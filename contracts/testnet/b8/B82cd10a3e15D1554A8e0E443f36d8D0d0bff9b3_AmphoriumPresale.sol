contract AmphoriumPresale is Ownable {

    using SafeMath for uint256;

    address private amphoriumAddress = 0x6659E6589D67003c213443d552f55D6a088Febd0;
    address private usdtAddress = 0x70225e5e79e654Fde5d0c3C8C1479C8579e7D091;
    address private usdcAddress = 0x8801D7849EF7F9d7774298d2FC4f50213Cda596f;
    address private busdAddress = 0xe9e7CEA3DedcA5984780Bafc599bD69ADd087D56;
    address private daiAddress  = 0x1AF3F329e8BE154074D8769D1FFa4eE058B1DBc3;


    // price 0.02 $
    function buyAmhTokenByToken(uint256 _tokenQuantity, address _paymentToken) external {

        require(_paymentToken == usdtAddress ||
                _paymentToken == usdcAddress ||
                _paymentToken == busdAddress ||
                _paymentToken == daiAddress, "Error token address");


        require(IERC20(_paymentToken).allowance(_msgSender(),address(this)) >= _tokenQuantity,"Tokens not approved");

        // mul 50 == 0.02 $
        uint256 amphoriumAmount = _tokenQuantity.mul(50);

        // Process Payment
        require(IERC20(_paymentToken).transferFrom(_msgSender(), address(this), _tokenQuantity),"Payment token transfer error");

        require(IERC20(amphoriumAddress).transfer(_msgSender(), amphoriumAmount),"Payment Amhorium transfer error");

    }

    function buyAmhTokenByBnb() payable external {
        // mul 30.000
        uint256 amphoriumAmount = msg.value.mul(30000);
        // Process Payment
        require(IERC20(amphoriumAddress).transfer(_msgSender(), amphoriumAmount),"Payment Amhorium transfer error");
    }



    function withDraw(address _paymentToken,uint _qauntity) external onlyOwner() {
        require(_paymentToken == usdtAddress ||
                _paymentToken == usdcAddress ||
                _paymentToken == busdAddress ||
                _paymentToken == daiAddress, "Error token address");

        require(IERC20(_paymentToken).transfer(_msgSender(), _qauntity),"Payment token transfer error");
    }

}