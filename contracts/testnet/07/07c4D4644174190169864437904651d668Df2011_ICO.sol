pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/extensions/IERC20MetadataUpgradeable.sol";
import "@openzeppelin/contracts/utils/math/Math.sol";

import "hardhat/console.sol";


contract ICO is Initializable, AccessControlUpgradeable, ReentrancyGuardUpgradeable {
    // using SafeMath for uint256;
    using SafeERC20Upgradeable for IERC20MetadataUpgradeable;

    struct LockUserToken {
        uint256 lockToken;
        uint256 currentLockToken;
        uint256 chunkClaimed;  
        uint256 claimedToken;      
    }

    struct TokenBuy {
        address tokenBuyAddress;
        uint256 rate;
        uint256 min;
        uint256 max;
    }

    struct EtherBuy {
        uint256 rate;
        uint256 min;
        uint256 max;
    }

    // set enum per method
    enum saleMethod{ ETHER, TOKEN, DISTRIBUTOR, ADD_LOCK }

    // use from ICO TOKEN Contract
    IERC20MetadataUpgradeable public token;
    // min and max for raise with ether
    EtherBuy public etherBuyData;      
    
    // pause buy with crypto
    bool public isPauseSaleWithCrypto;
    // enable limit
    bool public isCheckLimitPerUser;
    // pause buy with distributor
    bool public isPauseSaleWithDistributor;

    // set token rate decimals
    uint256 public rateDecimals;
    // tell this time when ico start
    uint256 public icoStartTime;
    // tell this time when ico end
    uint256 public icoEndTime;        
    // how much money raised so far
    uint256 public fundingGoal;
    // token raised so far
    uint256 public tokenRaised;
    // ether get
    uint256 public etherRaised;
    // lock rate percent for example 70% set tokenLockRate = 70
    uint256 public tokenLockRate;
    // total lock token
    uint256 public totalTokenLock;
    // for calculate in claim
    uint256 public unlockStartTime;
    // set max chunk to unlock token
    uint256 public maxChunk;
    // set duration time per chunk
    uint256 public timePerChunk;
    // whitelist start time 
    uint256 public whitelistStartTime;
    // whitelist end time
    uint256 public whitelistEndTime;
    // user limit ico token receive
    uint256 public limitTokenReceivePerUser;
    // token buy address list
    address[] public tokenBuyAddresses;
    // count user who buy
    address[] public userBuyAddresses; 


    // keep track balance token buy raisd in separate method      
    mapping(address => uint256) public tokenBuyRaised;
    // keep token buy data
    mapping(address => TokenBuy) public tokenBuys;
    // keep user ico token receive balance 
    mapping(address => uint256) public userTokenReceiveBalance;    
    // user native buy balance
    mapping(address => uint256) public userNativeBuyBalance;
    // user seperate token buy balance
    mapping(address => mapping(address => uint256)) public userTokenBuyBalance;
    // whitelist user to buy
    mapping(address => bool) public whitelistUsers;
    // set token address to accept as buy token
    mapping(address => bool) public whitelistTokenBuyAddress;
    // set token exchange rate for token
    mapping(address => uint256) public tokenRateTokenBuys;
    // calculate Lock token
    mapping(address => LockUserToken) public lockUserToken;
    // keep token receive per method
    mapping(saleMethod => uint256) public totalTokenReceivePerMethod;

    // setup role
    bytes32 public constant DISTRIBUTOR_ROLE = keccak256("DISTRIBUTOR_ROLE");
    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
    bytes32 public constant FUND_OWNER_ROLE = keccak256("FUND_OWNER_ROLE");
    bytes32 public constant CONFIG_ROLE = keccak256("CONFIG_ROLE");

    // event
    event BuyToken(address indexed buyer, uint256 ethPaid, uint256 tokenReceived, uint256 tokensToLock, uint256 timestamp);    
    event BuyTokenWithToken(address indexed buyer, uint256 tokenPaid, uint256 tokenReceived, uint256 tokensToLock, address tokenBuyAddress, uint256 timestamp);
    event BuyTokenWithDistributor(address indexed receiver, uint256 tokenReceived, uint256 tokensToLock, string indexed referenceTx); 
    event ExtractEther(address indexed receiver, uint256 ethReceived);    
    event ExtractToken(address indexed receiver, uint256 tokenBuyReceived);
    event ExtractICOToken(address indexed receiver, uint256 tokenReceived);
    event ClaimLockToken(address indexed receiver, uint256 claimableToken, uint256 chunkClaimed);
    event AddClaimLockToken(address indexed receiver, uint256 lockTokenAmount);
    

    modifier whenIcoCompleted {
        require(block.timestamp > icoEndTime || tokenRaised >= fundingGoal, "TOKENSALE: NOT_COMPLETE_YET");
        _;
    }

    modifier whenSaleNotPause {
        require(!isPauseSaleWithCrypto, "TOKENSALE: PAUSE");
        _;
    }

     modifier whenSaleWithDistributorNotPause {
        require(!isPauseSaleWithDistributor, "TOKENSALE: DISTRIBUTOR_PAUSE");
        _;
    }
    
    function initialize(
        uint256 _icoStartTime,
        uint256 _icoEndTime,
        address _tokenAddress,
        uint256 _fundingGoal,        
        address _distributor,
        uint256 _unlockStartTime,
        uint256 _maxChunk,
        uint256 _timePerChunk,        
        uint256 _tokenLockRate,
        uint256 _keepRateDecimals,
        EtherBuy memory _etherBuy
    ) public initializer {

        require (
            _icoStartTime != 0 &&
            
            _icoEndTime != 0 &&            
            
            _tokenAddress != address(0) &&
            
            _fundingGoal != 0 &&

            _keepRateDecimals != 0

        , "TOKENSALE: INITIAL_INVALID_VALUE");   

        __AccessControl_init();
        __ReentrancyGuard_init();

        icoStartTime = _icoStartTime;
        
        icoEndTime = _icoEndTime;
        
        token = IERC20MetadataUpgradeable(_tokenAddress);

        fundingGoal = _fundingGoal;            

        tokenLockRate = _tokenLockRate;

        unlockStartTime = _unlockStartTime;

        maxChunk = _maxChunk;

        timePerChunk = _timePerChunk;

        rateDecimals = 10 ** _keepRateDecimals; 

        // set up role
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(DISTRIBUTOR_ROLE, _distributor);
        _setupRole(PAUSER_ROLE, msg.sender);
        _setupRole(FUND_OWNER_ROLE, msg.sender);
        _setupRole(CONFIG_ROLE, msg.sender);

        // set ether buy data
        etherBuyData = _etherBuy;
    }

    function setTokenBuy(TokenBuy[] memory _tokenBuys) external onlyRole(CONFIG_ROLE) {
        // set allow token to buy
        for (uint256 i = 0; i < _tokenBuys.length; i++) {
            TokenBuy memory tokenBuy = _tokenBuys[i];
            // whitelist token buy address
            whitelistTokenBuyAddress[tokenBuy.tokenBuyAddress] = true;
            // mapping token buy data
            tokenBuys[tokenBuy.tokenBuyAddress] = tokenBuy;
            // keep address in array for use when extract fund
            tokenBuyAddresses.push(tokenBuy.tokenBuyAddress);
        }
    }

    function addWhitelistUser(address[] calldata _whitelistUsersAddress) external onlyRole(CONFIG_ROLE) {
        for (uint256 i = 0; i < _whitelistUsersAddress.length; i++) {
            whitelistUsers[_whitelistUsersAddress[i]] = true;
        }
    }

    function setSalePeriod(uint256 _icoStartTime, uint256 _icoEndTime) external onlyRole(CONFIG_ROLE) {  
        require(_icoStartTime != 0, 'TOKENSALE: INPUT_ZERO_AMOUNT');
        require(_icoEndTime != 0, 'TOKENSALE: INPUT_ZERO_AMOUNT');   
        // set sale period
        icoStartTime = _icoStartTime;
        icoEndTime = _icoEndTime;
    }

    function setWhitelistPeriod(uint256 _whitelistStartTime, uint256 _whitelistEndTime) external onlyRole(CONFIG_ROLE) {
        require(_whitelistEndTime > _whitelistStartTime && _whitelistEndTime <= icoEndTime, "TOKENSALE: CONFIG_INVALID_ENDTIME");
        require(_whitelistStartTime >= icoStartTime  && _whitelistStartTime <= icoEndTime, "TOKENSALE: CONFIG_INVALID_STARTTIME");
        // set whitelist period
        whitelistStartTime = _whitelistStartTime;
        whitelistEndTime = _whitelistEndTime;        
    }

    function isWhitelistPeriod() public view returns (bool) {
        return block.timestamp >= whitelistStartTime && block.timestamp <= whitelistEndTime;
    }

    function isIcoCompleted() external view returns (bool) {
        return block.timestamp > icoEndTime || tokenRaised >= fundingGoal;
    }

    function setLimitTokenReceivePerUser(bool _isCheckLimitPerUser, uint256 _limitTokenReceivePerUser) external onlyRole(CONFIG_ROLE) {
        require(_limitTokenReceivePerUser != 0, "TOKENSALE: INPUT_ZERO_AMOUNT");
        limitTokenReceivePerUser = _limitTokenReceivePerUser;
        isCheckLimitPerUser = _isCheckLimitPerUser;
    }

    function setPauseSaleWithCrypto(bool _isPauseSaleWithCrypto) external onlyRole(PAUSER_ROLE) {
        isPauseSaleWithCrypto = _isPauseSaleWithCrypto;
    }

    function setPauseSaleWithDistributor(bool _isPauseSaleWithDistributor) external onlyRole(PAUSER_ROLE) {
        isPauseSaleWithDistributor = _isPauseSaleWithDistributor;
    }

    function getTotalUser() external view returns (uint256) {
        return userBuyAddresses.length;
    }

    function buy() public payable nonReentrant whenSaleNotPause {

        // check ico time
        require(block.timestamp >= icoStartTime && block.timestamp <= icoEndTime, "TOKENSALE: END_TIME");                   
        // check raise cap
        require(tokenRaised < fundingGoal, "TOKENSALE: CAP_REACH");

        // check and allow whitelist user to buy first
        // after whitelist period will allow any user to buy
        if (isWhitelistPeriod()) {
            require(whitelistUsers[msg.sender], "TOKENSALE: ALLOW_ONLY_WHITELIST_ADDRESS");    
        }

        // check min max
        require(msg.value >= etherBuyData.min, "TOKENSALE: AMOUNT_LEES_THAN_MIN");
        require(msg.value <= etherBuyData.max, "TOKENSALE: AMOUNT_MORE_THAN_MAX");

        
        uint256 tokensToReceived;
        
        uint256 etherUsed = msg.value;
        
        address payable sender = payable(msg.sender);
        
        uint256 etherExceed;
        
        uint256 etherUnit = 1 ether;

        uint256 tokenReceivedUnit = 10 ** token.decimals();
        
        uint256 tokensToLock;

        // calculate receive token and have convert unit receive token
        // which support token with decimals is not 18
        tokensToReceived = etherUsed * tokenReceivedUnit / etherUnit * etherBuyData.rate / rateDecimals;
        
        // Check if we have reached and exceeded the funding goal to refund the exceeding tokens and ether
        if (tokenRaised + tokensToReceived > fundingGoal) {
            
            uint256 tokensToReceivedExceed = tokenRaised + tokensToReceived - fundingGoal;
            // formular
            // convert exceed token back to ether exceed
            etherExceed = tokensToReceivedExceed * etherUnit / tokenReceivedUnit * rateDecimals / etherBuyData.rate;
            // reduce etherExceed exceed from etherUsed receive
            etherUsed -= etherExceed;
            // reduce token exceed from total receive
            tokensToReceived -= tokensToReceivedExceed;
            // send exceed ether back to user
            sender.transfer(etherExceed);
        }

        // check limit per user
        if (isCheckLimitPerUser) {
            require(userTokenReceiveBalance[sender] + tokensToReceived <= limitTokenReceivePerUser, "TOKENSALE: EXCEED_LIMIT");
        }

        // calculate lock token
        tokensToLock = calculateLockToken(sender, tokensToReceived);
        
        // keep track token raised
        tokenRaised += tokensToReceived;
        
        // keep track ether raised
        etherRaised += etherUsed;

        // count each user for native buy balance
        userNativeBuyBalance[msg.sender] += etherUsed;

        // count each user ico token receive balance
        userTokenReceiveBalance[msg.sender] += tokensToReceived;

        // count total token receive per method
        totalTokenReceivePerMethod[saleMethod.ETHER] += tokensToReceived;

        // send token to buyer
        token.safeTransfer(msg.sender, tokensToReceived - tokensToLock);

        emit BuyToken(msg.sender, etherUsed, tokensToReceived, tokensToLock, block.timestamp);
    }
    
    function buyWithToken(uint256 _amount, address _tokenBuyAddress) external nonReentrant whenSaleNotPause {
        // check ico time
        require(block.timestamp >= icoStartTime && block.timestamp <= icoEndTime, "TOKENSALE: END_TIME");   
        // check whitelist token buy
        require(whitelistTokenBuyAddress[_tokenBuyAddress], "TOKENSALE: ALLOW_ONLY_WHITELIST_TOKEN");
        // check raise cap
        require(tokenRaised < fundingGoal, "TOKENSALE: CAP_REACH");

        // check and allow whitelist user to buy first
        // after whitelist period will allow any user to buy
        if (isWhitelistPeriod()) {
            require(whitelistUsers[msg.sender], "TOKENSALE: ALLOW_ONLY_WHITELIST_ADDRESS");    
        }

        TokenBuy memory tokenBuyData = tokenBuys[_tokenBuyAddress];
        // check min max
        require(_amount >= tokenBuyData.min, "TOKENSALE: AMOUNT_LEES_THAN_MIN");
        require(_amount <= tokenBuyData.max, "TOKENSALE: AMOUNT_MORE_THAN_MAX");
    
        // set token buy instance
        IERC20MetadataUpgradeable tokenBuy = IERC20MetadataUpgradeable(_tokenBuyAddress);
        
        uint256 tokensToReceived;
        
        uint256 tokenBuyUsed = _amount;
        
        address sender = msg.sender;
        
        uint256 tokenBuyExceed;
        
        uint256 tokenBuyUnit = 10 ** tokenBuy.decimals();
        
        uint256 tokenReceivedUnit = 10 ** token.decimals();
        
        uint256 tokensToLock;

        uint256 tokenRateTokenBuy = tokenBuyData.rate;
 
        // trasfer token buy to crowdsale
        tokenBuy.safeTransferFrom(sender, address(this), tokenBuyUsed);
        
        // calculate receive token and have convert unit receive token
        // which support sale token with decimals is not 18
        // and decimals for token buy is not 18

        tokensToReceived = tokenBuyUsed * tokenReceivedUnit / tokenBuyUnit * tokenRateTokenBuy / rateDecimals;
        
        // Check if we have reached and exceeded the funding goal to refund the exceeding tokens and ether
        if (tokenRaised + tokensToReceived > fundingGoal) {
            
            uint256 tokensToReceivedExceed = tokenRaised + tokensToReceived - fundingGoal;
            
            // formular
            // convert exceed token back to token buy exceed;
            
            tokenBuyExceed = tokensToReceivedExceed * tokenBuyUnit / tokenReceivedUnit * rateDecimals / tokenRateTokenBuy;
                                        
            tokenBuyUsed -= tokenBuyExceed;
            
            tokensToReceived -= tokensToReceivedExceed;
            // transfer exceed token buy to user
            tokenBuy.safeTransfer(sender, tokenBuyExceed);
        }

        // check limit per user
        if (isCheckLimitPerUser) {
            require(userTokenReceiveBalance[sender] + tokensToReceived <= limitTokenReceivePerUser, "TOKENSALE: EXCEED_LIMIT");
        }

        // calculate lock token
        tokensToLock = calculateLockToken(sender, tokensToReceived);        

        // increase token raised and ether raised variable
        tokenRaised += tokensToReceived;
            
        // count raised token buy raised
        tokenBuyRaised[_tokenBuyAddress] += tokenBuyUsed;

        // count each user for token buy balance
        userTokenBuyBalance[sender][_tokenBuyAddress] += tokenBuyUsed;

        // count each user ico token receive balance
        userTokenReceiveBalance[sender] += tokensToReceived;

        // count total token receive per method
        totalTokenReceivePerMethod[saleMethod.TOKEN] += tokensToReceived;

        // send token to buyer with lock token
        token.safeTransfer(sender, tokensToReceived - tokensToLock);
        
        emit BuyTokenWithToken(sender, tokenBuyUsed, tokensToReceived, tokensToLock, _tokenBuyAddress, block.timestamp);
    }

    function buyTokenWithDistributor(address _receiver, uint256 _amount, string memory _reference) public nonReentrant onlyRole(DISTRIBUTOR_ROLE) whenSaleWithDistributorNotPause {    
        // check value address and amount
        require(_receiver != address(0), "TOKENSALE: INPUT_ZERO_ADDRESS");
        require(_amount != 0, "TOKENSALE: INPUT_ZERO_AMOUNT");
        // check token not already reach goal
        require(tokenRaised < fundingGoal, "TOKENSALE: CAP_REACH");
         // cannot buy more than funding goal
        require(tokenRaised + _amount <= fundingGoal, "TOKENSALE: INSUFFICIENT_TOKEN");   
        // check in ico time
        require(block.timestamp >= icoStartTime && block.timestamp <= icoEndTime, "TOKENSALE: END_TIME");

        // check and allow whitelist user to buy first
        // after whitelist period will allow any user to buy
        if (isWhitelistPeriod()) {
            require(whitelistUsers[_receiver], "TOKENSALE: ALLOW_ONLY_WHITELIST_ADDRESS");    
        }
        
        // check limit per user
        if (isCheckLimitPerUser) {
            require(userTokenReceiveBalance[_receiver] + _amount <= limitTokenReceivePerUser, "TOKENSALE: EXCEED_LIMIT");
        }

        uint256 tokensToReceived = _amount;
                                                        
        uint256 tokensToLock;
                
        // calculate token to token lock
        tokensToLock = calculateLockToken(_receiver, tokensToReceived);
        
        // increase token raised and ether raised variable
        tokenRaised += tokensToReceived;

        // count total token receive per method
        totalTokenReceivePerMethod[saleMethod.DISTRIBUTOR] += tokensToReceived;

        // count each user ico token receive balance
        userTokenReceiveBalance[_receiver] += tokensToReceived;

        // send token to buyer without lock token
        token.safeTransfer(_receiver, tokensToReceived - tokensToLock);
        
        // emit actual token receive and lock token
        emit BuyTokenWithDistributor(_receiver, tokensToReceived, tokensToLock, _reference);
    }
    
    function calculateLockToken(address _userAddress, uint256 _tokensToReceived) internal returns (uint256) {        
        require(_userAddress != address(0), "TOKENSALE: INPUT_ZERO_ADDRESS");
        require(_tokensToReceived != 0, "TOKENSALE: INPUT_ZERO_AMOUNT");
        // calculate lock percent
        uint256 tokenToLock = _tokensToReceived * tokenLockRate / 100;

        LockUserToken storage userToken = lockUserToken[_userAddress];
        // add address user to array only once for first time buy for keep count and track
        if (userToken.lockToken == 0 && userToken.currentLockToken == 0) {
            userBuyAddresses.push(_userAddress);
        }
        // add lock token to user        
        userToken.lockToken += tokenToLock;
        userToken.currentLockToken += tokenToLock;
        // sum all total token lock
        totalTokenLock += tokenToLock;        
        return tokenToLock;
    }    
    
    function claimLockToken() external nonReentrant {
        LockUserToken storage userToken = lockUserToken[msg.sender];
        require(msg.sender != address(0), "TOKENSALE: INPUT_ZERO_ADDRESS");
        // can claim only after sale end in case extend sale after unlock time and to migitate claim wrong amount
        // and need to manual flush user who bought and already claim after extend sale period
        // can claim only after unlock start time        
        require(block.timestamp >= unlockStartTime && block.timestamp > icoEndTime, "TOKENSALE: NOT_IN_CLAIM_TIME");
        require(userToken.chunkClaimed != maxChunk && userToken.currentLockToken != 0, "TOKENSALE: ALREADY_CLAIM_ALL_TOKEN");

        // formular 
        // unlockChunk = (block.timestamp - unlockStartTime)/timePerChunk + 1; 
        // and if unlockChuck more than maxChunk just use maxChunk as unlockChuck

        uint256 unlockChunk = Math.min(
            Math.ceilDiv(block.timestamp - unlockStartTime, timePerChunk),
            maxChunk
        );

        // in case multiple claim in same chunk
        require(unlockChunk > userToken.chunkClaimed, "TOKENSALE: ALREADY_CLAIM");
        // calculate remain chunk to claim
        uint256 claimableToken = userToken.lockToken * (unlockChunk - userToken.chunkClaimed)/maxChunk;        
        // reduce current lock token.
        userToken.currentLockToken -= claimableToken;
        // keep chunk claimed.
        userToken.chunkClaimed = unlockChunk;        
        // keep claimed token user
        userToken.claimedToken += claimableToken;

        // decrease total lock token
        totalTokenLock -= claimableToken;
       
        // call to send claim token
        token.safeTransfer(msg.sender, claimableToken);
        
        emit ClaimLockToken(msg.sender, claimableToken, userToken.chunkClaimed);
    }

    function addLockToken(address _userAddress, uint256 _amount) external nonReentrant onlyRole(CONFIG_ROLE) {

        require(_userAddress != address(0), "TOKENSALE: INPUT_ZERO_ADDRESS");
        uint256 lockTokenAmount = _amount;

        LockUserToken storage userToken = lockUserToken[_userAddress];                
        // add lock token to current user.
        userToken.currentLockToken += lockTokenAmount;        
        // reset lock token to curent lock because some user may claimed token earlier
        userToken.lockToken = userToken.currentLockToken;
        // reset chunk claimed to zero to prevent same address that already claim cannot claim new add lock token
        userToken.chunkClaimed = 0;
        // add total lock token
        totalTokenLock += lockTokenAmount;
        // count each user ico token receive balance
        userTokenReceiveBalance[_userAddress] += lockTokenAmount;
        // count total token receive per method
        totalTokenReceivePerMethod[saleMethod.ADD_LOCK] += lockTokenAmount;

        token.safeTransferFrom(msg.sender, address(this), lockTokenAmount);

        emit AddClaimLockToken(_userAddress, lockTokenAmount);
    }
    
    // withdraw ether raised to owner 
    function extractEther() external nonReentrant onlyRole(FUND_OWNER_ROLE) whenIcoCompleted {
        address payable fundOwner = payable(msg.sender);
        uint256 amount = address(this).balance;        
        // send ether to fund owner with amount more than zero
        if (amount != 0) {
            fundOwner.transfer(amount);
            emit ExtractEther(msg.sender, amount);
        }
    }
    
    // withdraw token buy raised to owner 
    function extractTokenBuy() external nonReentrant onlyRole(FUND_OWNER_ROLE) whenIcoCompleted {
        for (uint256 i = 0; i < tokenBuyAddresses.length; i++) {
            // set token buy
            IERC20MetadataUpgradeable tokenBuy = IERC20MetadataUpgradeable(tokenBuyAddresses[i]);
            uint256 amount = tokenBuy.balanceOf(address(this));
            // send token buy to fund owner with amount more than zero
            if (amount != 0) {
                tokenBuy.safeTransfer(msg.sender, amount);
                emit ExtractToken(msg.sender, amount);
            }           
        }        
    } 

    // withdraw remain token sale
    function extractRemainICOToken() external nonReentrant onlyRole(FUND_OWNER_ROLE) whenIcoCompleted {        
        // check cannot extract more than unsold token
        require(token.balanceOf(address(this)) - totalTokenLock > 0, "TOKENSALE: INSUFFICIENT_TOKEN_TO_EXTRACT");
        // send remain token to fund owner with amount more than zero
        uint256 amount = token.balanceOf(address(this)) - totalTokenLock;
        token.safeTransfer(msg.sender, amount);
        emit ExtractICOToken(msg.sender, amount);
    }
    
    // According to solidity version 0.6.0, we have a breaking change. The unnamed function commonly referred to as “fallback function” 
    // was split up into a new fallback function that is defined using the fallback keyword and a receive ether function defined using the receive keyword. 
    // If present, the receive ether function is called whenever the call data is empty (whether or not ether is received). This function is implicitly payable. 
    // The new fallback function is called when no other function matches (if the receive ether function does not exist then this includes calls with empty call data). 
    // You can make this function payable or not. If it is not payable then transactions not matching any other function which send value will revert. 
    // You should only need to implement the new fallback function if you are following an upgrade or proxy pattern.
    
    // https.//solidity.readthedocs.io/en/v0.6.7/060-breaking-changes.html#semantic-and-syntactic-changes


    fallback() external payable {        
        buy();        
    }
    
    receive() external payable {        
        buy();        
    }

}

