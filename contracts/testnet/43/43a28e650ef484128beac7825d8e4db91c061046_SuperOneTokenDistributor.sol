contract SuperOneTokenDistributor is Ownable {
  using SafeERC20 for IERC20;

	IERC20 public superOneToken;

	constructor(IERC20 _superOneToken) {
		superOneToken = _superOneToken;
	}

	function distribute(address[] memory _transferAddresses, uint256[] memory _transferAmounts) external onlyOwner {
		require(_transferAddresses.length == _transferAmounts.length, 'distributor: Transfer Data is invalid');
		for (uint256 index = 0; index < _transferAddresses.length; index++) {
			if (_transferAmounts[index] > 0) {
				superOneToken.safeTransfer(_transferAddresses[index], _transferAmounts[index]);
			}
		}
	}
}