contract EnglishAuction {

    // State Variables 
    address public owner;
    BEP721 public LandNFT;
    BEP721 public HeroNFT;
    BEP20 public HGB;

    // uint public nftId;
    uint public LandAuctionNo = 0;
    uint public LandAuctionrequestID = 1;
    uint public HeroAuctionNo = 0;
    uint public HeroAuctionrequestID = 1;

    // Structs
    struct _auctionLand{
        bool started;
        bool ended;
        uint highestBid;
        uint lowestbid;
        uint bidders;
    }
    struct _auctionHero{
        bool started;
        bool ended;
        uint highestBid;
        uint lowestbid;
        uint bidders;
    }

    struct LandBids{
        uint[] _bids;
    }
    struct HeroBids{
        uint[] _bids;
    }
    
    //  Events 
    event NewAuctionCreated (uint endsAt);
    event Bid (address indexed sender, uint bidNumber ,uint amount);
    event Winner (address indexed bidder);
    event End (address indexed winner, uint amount);
    event Lost (address indexed Loser);
    

    // Mappings for Land Auction 
    mapping(uint => mapping (uint => address)) public LanduserPerBidID;
    mapping(uint => _auctionLand) public auctionLand;
    mapping(address => mapping(uint => LandBids)) internal _LandNoOfBidsPerPerson;
    mapping(address => mapping(uint => mapping(uint => uint))) public _LandbidPerPerson;
    mapping(uint => address) public Landrequest;
    mapping(address => bool)public Landgrant;
    mapping(uint => uint) public LandRewardToken;

    // Mappings for Hero Auction 
    mapping(uint => mapping (uint => address)) public HerouserPerBidID;
    mapping(uint => _auctionHero) public auctionHero;
    mapping(address => mapping(uint => HeroBids)) internal _HeroNoOfBidsPerPerson;
    mapping(address => mapping(uint => mapping(uint => uint))) public _HerobidPerPerson;
    mapping(uint => address) public Herorequest;
    mapping(address => bool)public Herogrant;
    mapping(uint => uint) public HeroRewardToken;
    


    // modifier 
    modifier onlyOwner(){
        require(msg.sender == owner,"Only Owner can run this function");
        _;
    }

    // constructor 
    constructor(
        address _HGB,
        address _LandNFTAddress,
        address _HeroNFTAddress,
        address _owner
    )  {
        HGB = BEP20(_HGB);
        LandNFT = BEP721(_LandNFTAddress);
        HeroNFT = BEP721(_HeroNFTAddress);
        owner = _owner;
    }

    //  Functions 
    receive() external payable {}
    fallback() external payable {}

    // here onlyOwner can create New Land Auction 
    function createNewLandAuction( uint _lowestbid, uint TokenID ) public onlyOwner returns(bool) {

            LandAuctionNo += 1;

            _auctionLand storage a = auctionLand[LandAuctionNo];
            (   a.started = true,
                a.ended = false,
                a.highestBid = 0,
                a.bidders = 0,
                a.lowestbid = _lowestbid
            );

            LandRewardToken[LandAuctionNo] = TokenID;

            LandNFT.transferFrom(owner, address(this), TokenID);
            return true;

    }

    // here onlyOwner can create New Hero Auction 
    function createNewHeroAuction( uint _lowestbid, uint TokenID ) public onlyOwner returns(bool) {

            HeroAuctionNo += 1;

            _auctionHero storage a = auctionHero[HeroAuctionNo];
            (   a.started = true,
                a.ended = false,
                a.highestBid = 0,
                a.bidders = 0,
                a.lowestbid = _lowestbid
            );

            HeroRewardToken[HeroAuctionNo] = TokenID;

            HeroNFT.transferFrom(owner, address(this), TokenID);
            return true;

    }

    // here you can check Land Auction details
    function checkLandAuction( uint _auctionNumber ) public view returns ( bool, bool, uint, uint ) {

        _auctionLand storage a = auctionLand[_auctionNumber];
        return( a.started, a.ended, a.highestBid, a.lowestbid );

    }

    // here you can check Hero Auction details
    function checkHeroAuction( uint _auctionNumber ) public view returns ( bool, bool, uint, uint ) {

        _auctionHero storage a = auctionHero[_auctionNumber];
        return( a.started, a.ended, a.highestBid, a.lowestbid );

    }

    // here you can check current Land Auction Number
    function currentLandAuctionNumber() public view returns (uint) {
        return LandAuctionNo;
    }

    // here you can check current Hero Auction Number
    function currentHeroAuctionNumber() public view returns (uint) {
        return HeroAuctionNo;
    }

    // here you can Ask for permission From Owner For Land Auction
    function permissionFromOwnerForLandAuction () public {  

        Landrequest[LandAuctionrequestID] = msg.sender;
        LandAuctionrequestID += 1;

    }

    // here you can Ask for permission From Owner For Hero Auction
    function permissionFromOwnerForHeroAuction () public {  

        Herorequest[HeroAuctionrequestID] = msg.sender;
        HeroAuctionrequestID += 1;

    }

    // here onlyOwner can grant Permission For Land Auction
    function grantPermissionForLandAuction ( bool _answer, uint _LandAuctionrequestId ) public onlyOwner {

        Landgrant[Landrequest[_LandAuctionrequestId]] = _answer;

    }

    // here onlyOwner can grant Permission For Hero Auction
    function grantPermissionForHeroAuction ( bool _answer, uint _HeroAuctionrequestId ) public onlyOwner {

        Herogrant[Herorequest[_HeroAuctionrequestId]] = _answer;

    }

    // here only granted people can bid On Land
    function bidOnLand( uint _price ) external {

         _auctionLand storage a = auctionLand[LandAuctionNo];

        require(
             a.ended == false,
             "This auction has been ended"
        );

        require(
            Landgrant[msg.sender] == true,
             "you don't have permission to bid"
        );

        require(
            a.started == true ,
             "Auction is not started yet"
        );
        require(
            HGB.balanceOf(msg.sender) >= _price,
             "You must have HGB "
        );
        require(
            _price > a.highestBid,
             "please enter amount higher then Previous bid"
        );




        a.lowestbid = _price;
        a.highestBid = _price;
        a.bidders = a.bidders + 1;

        LandBids storage b = _LandNoOfBidsPerPerson[msg.sender][LandAuctionNo];
        b._bids.push(a.bidders) ;

        _LandbidPerPerson[msg.sender][LandAuctionNo][a.bidders] = _price;

        HGB.transferFrom(msg.sender,address(this), _price);

        LanduserPerBidID[LandAuctionNo][a.bidders] = msg.sender;

        emit Bid(msg.sender,a.bidders,_price);

    }

    // here only granted people can bid On Hero
    function bidOnHero( uint _price ) external {

        require(
            Herogrant[msg.sender] == true,
             "you don't have permission to bid"
        );

        _auctionHero storage a = auctionHero[HeroAuctionNo];
        require(
            a.started == true ,
             "Auction is not started yet"
        );
        require(
            HGB.balanceOf(msg.sender) >= _price,
             "You must have HGB "
        );
        require(
            _price > a.highestBid,
             "please enter amount higher then Previous bid"
        );

        a.lowestbid = _price;
        a.highestBid = _price;
        a.bidders = a.bidders + 1;

        HeroBids storage b = _HeroNoOfBidsPerPerson[msg.sender][HeroAuctionNo];
        b._bids.push(a.bidders) ;

        _HerobidPerPerson[msg.sender][HeroAuctionNo][a.bidders] = _price;

        HGB.transferFrom(msg.sender,address(this), _price);

        HerouserPerBidID[HeroAuctionNo][a.bidders] = msg.sender;

        emit Bid(msg.sender,a.bidders,_price);

    }

    // here you can check winner Of Land Auction
    function winnerOfLandAuction( uint _AuctionNo ) public view returns( address ) {

        if ( auctionLand[_AuctionNo].ended == true ){
            return LanduserPerBidID[_AuctionNo][auctionLand[_AuctionNo].bidders];
        } else {
            return address(0);
        }

    }

    // here you can check winner Of Hero Auction
    function winnerOfHeroAuction( uint _AuctionNo ) public view returns( address ) {

        if ( auctionHero[_AuctionNo].ended == true ){
            return HerouserPerBidID[_AuctionNo][auctionHero[_AuctionNo].bidders];
        } else {
            return address(0);
        }

    }

    // here you can check Land Bids Per Id
    function checkLandBidsPerId(address _user) public view returns(uint[] memory){

        _auctionLand storage a = auctionLand[LandAuctionNo];
        LandBids storage b = _LandNoOfBidsPerPerson[_user][LandAuctionNo];

        uint[] memory number = new uint[](a.bidders);
        for(uint i =0; i < b._bids.length; i++ ){
            number[i] =  b._bids[i];
        }

        return (number);
   
    }

    // here you can check Hero Bids Per Id
    function checkHeroBidsPerId(address _user) public view returns(uint[] memory){

        _auctionHero storage a = auctionHero[HeroAuctionNo];
        HeroBids storage b = _HeroNoOfBidsPerPerson[_user][HeroAuctionNo];

        uint[] memory number = new uint[](a.bidders);
        for(uint i =0; i < b._bids.length; i++ ){
            number[i] =  b._bids[i];
        }

        return (number);
   
    }

    // here onlyOwner can Land Auction End
    function LandAuctionEnd()public onlyOwner{

        _auctionLand storage a = auctionLand[LandAuctionNo];
        a.ended = true;
        
    }

    // here onlyOwner can Hero Auction End
    function HeroAuctionEnd()public onlyOwner{

        _auctionHero storage a = auctionHero[HeroAuctionNo];
        a.ended = true;
        
    }

    // here you can withdraw Land Reward
    function withdrawLandReward(uint _auctionNo,uint bidId) external {

        _auctionLand storage a = auctionLand[_auctionNo];
        require(
            a.ended == true,
            "Auction is Not Ended yet !"
        );
        
        if ( auctionLand[_auctionNo].bidders == bidId ){
            // Winner
            HGB.approve( address(this), _LandbidPerPerson[msg.sender][_auctionNo][bidId] );
            HGB.transferFrom( address(this), owner, _LandbidPerPerson[msg.sender][_auctionNo][bidId] );

            _LandbidPerPerson[msg.sender][_auctionNo][bidId] = 0;
            
            LandNFT.transferFrom( address(this), msg.sender, LandRewardToken[_auctionNo] );
            emit Winner(msg.sender);

        } else {
            // loser
            HGB.approve( address(this), _LandbidPerPerson[msg.sender][_auctionNo][bidId] );
            HGB.transferFrom( address(this), msg.sender, _LandbidPerPerson[msg.sender][_auctionNo][bidId] );
            _LandbidPerPerson[msg.sender][_auctionNo][bidId] = 0;
            emit Lost(msg.sender);
        }
    }

    // here you can withdraw Hero Reward
    function withdrawHeroReward(uint _auctionNo,uint bidId) external {

        _auctionHero storage a = auctionHero[_auctionNo];
        require(
            a.ended == true,
            "Auction is Not Ended yet !"
        );
        
        if ( auctionHero[_auctionNo].bidders == bidId ){
            // Winner
            HGB.approve( address(this), _HerobidPerPerson[msg.sender][_auctionNo][bidId] );
            HGB.transferFrom( address(this), owner, _HerobidPerPerson[msg.sender][_auctionNo][bidId] );

            _HerobidPerPerson[msg.sender][_auctionNo][bidId] = 0;
            
            HeroNFT.transferFrom( address(this), msg.sender, HeroRewardToken[_auctionNo] );
            emit Winner(msg.sender);

        } else {
            // loser
            HGB.approve( address(this), _HerobidPerPerson[msg.sender][_auctionNo][bidId] );
            HGB.transferFrom( address(this), msg.sender, _HerobidPerPerson[msg.sender][_auctionNo][bidId] );
            _HerobidPerPerson[msg.sender][_auctionNo][bidId] = 0;
            emit Lost(msg.sender);
        }
    }

    // here you can check Land Highest Bid 
    function checkLandHighestBid( uint _auctionNo ) public view returns (uint) {
        return ( auctionLand[_auctionNo].highestBid );   
    }

    // here you can check Hero Highest Bid 
    function checkHeroHighestBid( uint _auctionNo ) public view returns (uint) {
        return ( auctionHero[_auctionNo].highestBid );   
    }

    /// marketplace for land and hero start here >>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>

    // State Variables for Land
    uint public LandAssetNo;
    uint public LandBuyerID = 1;
    uint public LandSellerID = 1;

    // State Variables for Hero
    uint public HeroAssetNo;
    uint public HeroBuyerID = 1;
    uint public HeroSellerID = 1;

    struct _LandAssets{
        address _owner;
        uint _tokenID;
        uint _assetNo;
        uint _price;
        bool _onSell;
    }
    struct _HeroAssets{
        address _owner;
        uint _tokenID;
        uint _assetNo;
        uint _price;
        bool _onSell;
    }

    mapping ( uint => _LandAssets ) public LandAssets;
    mapping ( uint => _HeroAssets ) public HeroAssets;

    // maping for Land
    mapping(uint => address) public LandBuyer;
    mapping(uint => address) public LandSeller;
    mapping(address => bool) public LandGrantBuy;
    mapping(address => bool) public LandGrantSell;
    mapping(uint => bool) public LandOnSell;
    mapping(uint => uint) public LandTokenPrice;

    // mapping for hero
    mapping(uint => address) public HeroBuyer;
    mapping(uint => address) public HeroSeller;
    mapping(address => bool) public HeroGrantBuy;
    mapping(address => bool) public HeroGrantSell;
    mapping(uint => bool) public HeroOnSell;
    mapping(uint => uint) public HeroTokenPrice;

    // here everyone can request For Buy Land
    function requestForBuyLand() external{
        LandBuyer[LandBuyerID] = msg.sender; 
        LandBuyerID++;
    }

    // here everyone can request For Buy Hero
    function requestForBuyHero() external{
        HeroBuyer[HeroBuyerID] = msg.sender; 
        HeroBuyerID++;
    }

    // here everyone can request For Sell Land
    function requestForSellLand() external {
        LandSeller[LandSellerID] = msg.sender;
        LandSellerID++;
    }

    // here everyone can request For Sell Hero
    function requestForSellHero() external {
        HeroSeller[HeroSellerID] = msg.sender;
        HeroSellerID++;
    }

    // here everyone can sell Land
    function sellLand( uint _tokenID, uint _price ) external{

        require(
            LandGrantSell[msg.sender] == true,
             "You are not allowed to sell"
        );
        LandOnSell[_tokenID] = true;
        LandTokenPrice[_tokenID] = _price+((_price/100)*1);
        LandSeller[_tokenID] = msg.sender;
        LandNFT.transferFrom(msg.sender, address(this), _tokenID);

        LandAssetNo++;
        LandAssets[LandAssetNo] = _LandAssets(msg.sender, _tokenID, LandAssetNo, _price, true);

    }

    // here everyone can sell Hero
    function sellHero( uint _tokenID, uint _price ) external{

        require(
            HeroGrantSell[msg.sender] == true,
             "You are not allowed to sell"
        );
        HeroOnSell[_tokenID] = true;
        HeroTokenPrice[_tokenID] = _price+((_price/100)*1);
        HeroSeller[_tokenID] = msg.sender;
        HeroNFT.transferFrom(msg.sender, address(this), _tokenID);

        HeroAssetNo++;
        HeroAssets[HeroAssetNo] = _HeroAssets(msg.sender, _tokenID, HeroAssetNo, _price, true);

    }

    // here everyone can check asset Data of Land
    function assetDataLand( uint _assetNo ) public view returns( address, uint, uint, uint, bool ) {
        _LandAssets storage a = LandAssets[_assetNo];
        return ( a._owner, a._tokenID, a._assetNo, a._price, a._onSell );
    }

    // here everyone can check asset Data of Hero
    function assetDataHero( uint _assetNo ) public view returns( address, uint, uint, uint, bool ) {
        _HeroAssets storage a = HeroAssets[_assetNo];
        return ( a._owner, a._tokenID, a._assetNo, a._price, a._onSell );
    }

    // here everyone can buy Land
    function buyLand( uint _assetNo, uint _tokenID, uint _price ) external {

        require( 
            LandGrantBuy[msg.sender] == true,
            "You are not allowed to buy"
        );
        require(
            LandOnSell[_tokenID] == true,
            "this Token is not on Sell"
        );
        require(
            _price == LandTokenPrice[_tokenID],
            "Price is not valid"
        );

        HGB.approve( msg.sender, LandTokenPrice[_tokenID]-((_price/100)*1));
        HGB.transferFrom( msg.sender, LandSeller[_tokenID], LandTokenPrice[_tokenID]-((_price/100)*1));

        HGB.approve( msg.sender, ((_price/100)*1));
        HGB.transferFrom( msg.sender, owner, ((_price/100)*1));

        LandTokenPrice[_tokenID] = 0;
        LandNFT.transferFrom(address(this), msg.sender,_tokenID);

        _LandAssets storage a = LandAssets[_assetNo];
        a._owner = msg.sender;
    }

    // here everyone can buy Hero
    function buyHero( uint _assetNo, uint _tokenID, uint _price ) external {

        require( 
            HeroGrantBuy[msg.sender] == true,
            "You are not allowed to buy"
        );
        require(
            HeroOnSell[_tokenID] == true,
            "this Token is not on Sell"
        );
        require(
            _price == HeroTokenPrice[_tokenID],
            "Price is not valid"
        );

        HGB.approve( msg.sender, HeroTokenPrice[_tokenID]-((_price/100)*1));
        HGB.transferFrom( msg.sender, HeroSeller[_tokenID], HeroTokenPrice[_tokenID]-((_price/100)*1));

        HGB.approve( msg.sender, ((_price/100)*1));
        HGB.transferFrom( msg.sender, owner, ((_price/100)*1));

        HeroTokenPrice[_tokenID] = 0;
        HeroNFT.transferFrom(address(this), msg.sender,_tokenID);

        _HeroAssets storage a = HeroAssets[_assetNo];
        a._owner = msg.sender;
    }

    // <<<<<<<<<<<<< onlyOwner >>>>>>>>>>>>>

    // here onlyOwner can approve Land Buyer
    function approveLandBuyer( uint _buyerId , bool _grant ) external onlyOwner{
        LandGrantBuy[LandBuyer[_buyerId]] = _grant;
    }

    // here onlyOwner can approve Hero Buyer
    function approveHeroBuyer( uint _buyerId , bool _grant ) external onlyOwner{
        HeroGrantBuy[HeroBuyer[_buyerId]] = _grant;
    }

    // here onlyOwner can approve Land Seller
    function approveLandSeller( uint _sellerId , bool _grant ) external onlyOwner{
        LandGrantSell[LandSeller[_sellerId]] = _grant;
    }

    // here onlyOwner can approve Hero Seller
    function approveHeroSeller( uint _sellerId , bool _grant ) external onlyOwner{
        HeroGrantSell[HeroSeller[_sellerId]] = _grant;
    }

}

