    function allPairsLength() external view returns (uint);

    function createPair(address tokenA, address tokenB) external returns (address pair);

    function setFeeTo(address) external;
    function setFeeToSetter(address) external;
}

contract TestToken is ERC20, Ownable {
    using SafeMath for uint256;

    IUniswapV2Router02 public uniswapV2Router;
    address public uniswapV2Pair;
        
    address public buyToken;
    address public teamWallet;
    address public burnWallet;
    
    uint256 public dividend = 100;
    
    uint256 public teamFee = 200;
    uint256 public burnFee = 200;
    uint256 public liquidityFee = 200;
    uint256 public totalFee = 600;

    uint256 public buyFeeIncreaseFactor = 100;
    uint256 public sellFeeIncreaseFactor = 100;
    uint256 public transferFeeIncreaseFactor = 100;
    
    uint256 public swapTokensAtAmount = 1000 * 10**18;

    address public presaleAddress = address(0);
    
    bool private swapping;
    bool public tradingOpened;
    bool public pausedContract;

    mapping (address => bool) private _botList;
    mapping (address => bool) private _isExcludedFromFees;
    mapping (address => bool) private _automatedMarketMakerPairs;
    mapping (address => bool) private _canTransferWhenTradingDisabled;

    event ExcludeFromFees(address indexed account, bool isExcluded);

    event SetAutomatedMarketMakerPair(address indexed pair, bool indexed value);

    event SwapAndLiquify(uint256 tokensSwapped, uint256 ethReceived, uint256 tokensIntoLiqudity);


    constructor(address _teamWallet, address _burnWallet, address _buyToken) ERC20("TestToken", "$TEST") {
        buyToken = _buyToken;
        teamWallet = _teamWallet;
        burnWallet = _burnWallet;
        
        uniswapV2Router = IUniswapV2Router02(0xCc7aDc94F3D80127849D2b41b6439b7CF1eB4Ae0);
        uniswapV2Pair = IUniswapV2Factory(uniswapV2Router.factory()).createPair(address(this), uniswapV2Router.WETH());

        _setAutomatedMarketMakerPair(uniswapV2Pair, true);

        excludeFromFees(owner(), true);
        excludeFromFees(burnWallet, true);
        excludeFromFees(teamWallet, true);
        excludeFromFees(address(this), true);

        _canTransferWhenTradingDisabled[owner()] = true;
        
        _mint(owner(), 10**8 * (10**18));

        tradingOpened = true;
    }

    function setBuyToken(address _buyToken) external onlyOwner {
        buyToken = _buyToken;
    }

    function setTeamWallet(address _teamWallet) external onlyOwner {
        teamWallet = _teamWallet;
    }

    function setBurnWallet(address _burnWallet) external onlyOwner {
        burnWallet = _burnWallet;
    }
    
    function setTeamFee(uint256 _teamFee) external onlyOwner {
        teamFee = _teamFee;
        totalFee = teamFee.add(burnFee).add(liquidityFee);
    }

    function setBurnFee(uint256 _burnFee) external onlyOwner {
        burnFee = _burnFee;
        totalFee = teamFee.add(burnFee).add(liquidityFee);
    }

    function setLiquidityFee(uint256 _liquidityFee) external onlyOwner {
        liquidityFee = _liquidityFee;
        totalFee = teamFee.add(burnFee).add(liquidityFee);
    }

    function setDividend(uint256 _dividend) external onlyOwner {
        dividend = _dividend;
    }

    function setBuyFeeIncreaseFactor(uint256 _buyFeeIncreaseFactor) external onlyOwner {
        buyFeeIncreaseFactor = _buyFeeIncreaseFactor;
    }

    function setSellFeeIncreaseFactor(uint256 _sellFeeIncreaseFactor) external onlyOwner {
        sellFeeIncreaseFactor = _sellFeeIncreaseFactor;
    }
    
    function setTransferFeeIncreaseFactor(uint256 _transferFeeIncreaseFactor) external onlyOwner {
        transferFeeIncreaseFactor = _transferFeeIncreaseFactor;
    }
    
    function setSwapTokensAtAmount(uint256 _swapTokensAtAmount) external onlyOwner {
        swapTokensAtAmount = _swapTokensAtAmount;
    }
    
    function setTradingStatus(bool _status) external onlyOwner {
        tradingOpened = _status;
    }
    
    function setPausedStatus(bool _status) external onlyOwner() {
        pausedContract = _status;
    }
    
    function whitelistAddress(address _address, bool _enable) public onlyOwner {
        _canTransferWhenTradingDisabled[_address] = _enable;
    }

    function isExcludedFromFees(address account) public view returns(bool) {
        return _isExcludedFromFees[account];
    }
    
    function excludeFromFees(address account, bool excluded) public onlyOwner {
        _isExcludedFromFees[account] = excluded;

        emit ExcludeFromFees(account, excluded);
    }

    function setAntiBotslist(address[] calldata addresses, bool status) external onlyOwner {
        for (uint256 i; i < addresses.length; ++i) {
            _botList[addresses[i]] = status;
        }
    }

    function _setAutomatedMarketMakerPair(address pair, bool value) private {
        require(_automatedMarketMakerPairs[pair] != value, "TEST: Automated market maker pair is already set to that value");
        _automatedMarketMakerPairs[pair] = value;

        emit SetAutomatedMarketMakerPair(pair, value);
    }
    
    function recoverContractBNB(uint256 recoverRate) public onlyOwner{
        uint256 bnbAmount = address(this).balance;
        if(bnbAmount > 0){
            payable(teamWallet).transfer(bnbAmount.mul(recoverRate).div(100));
        }
    }
    
    function recoverContractToken(address token, uint256 recoverRate) public onlyOwner{
        uint256 tokenBalance = IERC20(token).balanceOf(address(this));
        if(tokenBalance > 0){
            uint256 amount = tokenBalance.mul(recoverRate).div(100);            
            IERC20(token).transfer(teamWallet, amount);
        }
    }
    
    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        require(!pausedContract || from == owner() || to == owner(), "BEP20: Transfers are paused"); // pause check
        require(from != address(0), "BEP20: transfer from the zero address");
        require(to != address(0), "BEP20: transfer to the zero address");
        require(!_botList[from] && !_botList[to], "Error: NTAM8L0K1 Try again later");

        bool tradingIsEnabled = tradingOpened;

        if(!tradingIsEnabled) {
            require(_canTransferWhenTradingDisabled[from], "TEST: This account cannot send tokens until trading is enabled");
        }

        if(amount == 0) {
            super._transfer(from, to, 0);
            return;
        }

        bool bWalletTransfer = false;

        if(from != owner() && to != owner() && tradingIsEnabled && !_automatedMarketMakerPairs[from] && !_automatedMarketMakerPairs[to] && from != address(uniswapV2Router) && to != address(uniswapV2Router)){
            bWalletTransfer = true;
        }

        uint256 contractTokenBalance = balanceOf(address(this));
        bool canSwap = contractTokenBalance >= swapTokensAtAmount;
        
        contractTokenBalance = swapTokensAtAmount;

        if(tradingIsEnabled && canSwap && !swapping &&
            !_automatedMarketMakerPairs[from] && from != uniswapV2Pair && to != uniswapV2Pair ) {
            swapping = true;
            
            uint256 swapTokens = contractTokenBalance.div(2);
            uint256 burnTokens = contractTokenBalance.sub(swapTokens);

            if(swapTokens > 0)
                swapAndLiquify(swapTokens);
            if(burnTokens > 0)
                swapAndBurn(burnTokens);

            swapping = false;
        }


        bool takeFee = tradingIsEnabled && !swapping;

        if(_isExcludedFromFees[from] || _isExcludedFromFees[to]) {
            takeFee = false;
        }

        if(takeFee) {
            uint256 fees = amount.mul(totalFee).div(100);
            
            if(_automatedMarketMakerPairs[to]) {
                fees = fees.mul(sellFeeIncreaseFactor).div(100);
            } else if(bWalletTransfer){
                fees = fees.mul(transferFeeIncreaseFactor).div(100);
            } else {
                fees = fees.mul(buyFeeIncreaseFactor).div(100);
            }
            
            amount = amount.sub(fees);
            
            uint256 toTeamFee = fees.mul(teamFee).div(totalFee);
            uint256 toContractFee = totalFee.sub(teamFee);

            super._transfer(from, teamWallet, toTeamFee);
            super._transfer(from, address(this), toContractFee);
        }

        super._transfer(from, to, amount);
    }

    function swapAndLiquify(uint256 tokens) private {
        uint256 half = tokens.div(2);
        uint256 otherHalf = tokens.sub(half);
        uint256 initialBalance = address(this).balance;
        swapTokensForEth(half); 
        uint256 newBalance = address(this).balance.sub(initialBalance);

        addLiquidity(otherHalf, newBalance);
        
        emit SwapAndLiquify(half, newBalance, otherHalf);
    }

    function swapTokensForEth(uint256 tokenAmount) private {
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = uniswapV2Router.WETH();

        _approve(address(this), address(uniswapV2Router), tokenAmount);

        uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0,
            path,
            address(this),
            block.timestamp
        );
        
    }

    function addLiquidity(uint256 tokenAmount, uint256 ethAmount) private {
        
        _approve(address(this), address(uniswapV2Router), tokenAmount);

       uniswapV2Router.addLiquidityETH{value: ethAmount}(
            address(this),
            tokenAmount,
            0, 
            0, 
            teamWallet,
            block.timestamp
        );
        
    }

    function swapTokensForRewardToken(uint256 tokenAmount, address recipient) private {
        address[] memory path = new address[](3);
        path[0] = address(this);
        path[1] = uniswapV2Router.WETH();
        path[2] = buyToken;

        _approve(address(this), address(uniswapV2Router), tokenAmount);

        uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            tokenAmount,
            0, 
            path,
            recipient,
            block.timestamp
        );
        
    }
    
    function swapAndBurn(uint256 amount) private {
        swapTokensForRewardToken(amount, address(this));
        
        uint256 tokenAmount = IERC20(buyToken).balanceOf(address(this));

        IERC20(buyToken).transfer(burnWallet, tokenAmount); 
    }
    
}