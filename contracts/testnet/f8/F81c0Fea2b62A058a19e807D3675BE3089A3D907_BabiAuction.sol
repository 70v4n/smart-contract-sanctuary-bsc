pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/AccessControl.sol";

interface IBabiFee {
    function assignFee(bytes32 feeType, uint256 newFee) external;
    function getFee(bytes32 feeType) external view returns(uint256);
    function calculatePay(bytes32[] memory feeTypes, uint256 baseValue) external view returns(uint256);
    function calculateTotalFee(bytes32[] memory feeType, uint256 baseValue) external view returns(uint256);
    function calculateFee(bytes32 feeType, uint256 baseValue) external view returns(uint256);
}

contract BabiFee is IBabiFee, AccessControl {
    uint256 public constant MATH_BASE = 10000;
    bytes32 public constant FEE_ASSIGNER_ROLE = keccak256("FEE_ASSIGNER_ROLE");

    mapping(bytes32 => uint256) public fees;
    bytes32 public constant SELLER_FEE = keccak256("SELLER_FEE");
    bytes32 public constant BUYER_FEE = keccak256("BUYER_FEE");

    event FeeAssigned(bytes32 feeType, uint256 oldFee, uint256 newFee);

    constructor(address fee_assigner) {
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(FEE_ASSIGNER_ROLE, fee_assigner);

        _assignFee(SELLER_FEE, 200);
        _assignFee(BUYER_FEE, 200);
    }

    function supportsInterface(bytes4 interfaceId) public view virtual override returns (bool) {
        return interfaceId == type(IBabiFee).interfaceId
            || super.supportsInterface(interfaceId);
    }

    function assignFee(bytes32 feeType, uint256 newFee) external override onlyRole(FEE_ASSIGNER_ROLE) {
        _assignFee(feeType, newFee);
    }

    function _assignFee(bytes32 feeType, uint256 newFee) internal {
        uint256 oldFee = fees[feeType];
        fees[feeType] = newFee;

        emit FeeAssigned(feeType, oldFee, newFee);
    }

    function getFee(bytes32 feeType) external view override returns(uint256) {
        return _getFee(feeType);
    }

    function _getFee(bytes32 feeType) internal view returns(uint256) {
        return fees[feeType];
    }

    function calculatePay(bytes32[] memory feeTypes, uint256 baseValue) external override view returns(uint256) {
        return _calculatePay(feeTypes, baseValue);
    }

    function _calculatePay(bytes32[] memory feeTypes, uint256 baseValue) internal view returns(uint256) {
        uint256 totalFee = _calculateTotalFee(feeTypes, baseValue);
        return baseValue + totalFee;
    }

    function calculateTotalFee(bytes32[] memory feeTypes, uint256 baseValue) external override view returns(uint256) {
        return _calculateTotalFee(feeTypes, baseValue);
    }

    function _calculateTotalFee(bytes32[] memory feeTypes, uint256 baseValue) internal view returns(uint256) {
        uint256 totalFee = 0;
        for (uint i = 0; i < feeTypes.length; i++) {
            bytes32 feeType = feeTypes[i];
            uint256 fee = _calculateFee(feeType, baseValue);
            totalFee = totalFee + fee;
        }
        return totalFee;
    }

    function calculateFee(bytes32 feeType, uint256 baseValue) external override view returns(uint256) {
        return _calculateFee(feeType, baseValue);
    }

    function _calculateFee(bytes32 feeType, uint256 baseValue) internal view returns(uint256) {
        uint256 fee = _getFee(feeType);
        return (fee * baseValue) / MATH_BASE;
    }
}

