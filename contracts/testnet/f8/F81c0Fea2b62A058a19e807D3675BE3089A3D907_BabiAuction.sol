pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/utils/ERC721HolderUpgradeable.sol";
import "./BabiFee.sol";
import "./interfaces/ITransferProxy.sol";
import "./interfaces/HasSecondarySaleFees.sol";

import "hardhat/console.sol";



contract AuctionDomain {

    enum AuctionState {
        WaitingReservePrice,
        Open,
        Closed
    }

    struct Auction {
        address owner;
        Lot lot;
        Bid highestBid;
        AuctionState state;
        uint256 endTime;
        bool exists;
    }

    struct Lot {
        address tokenAddress; // nft contract address
        uint256 tokenId; // nft id in the nft contract address
        address currency; // for eth: 0x00 or erc20 token contract address
        uint256 reservePrice; // auction starts after reserve price met.
        uint256 duration; // seconds | auction duration after it starts(reserve price met or instant)
        uint256 extensionDuration; // seconds | auction extension duration from endTime, 0 is no extension. "if bid on last 1 hour, it will extend to 1 hour from now."
        uint256 salt; // salting auctionId
    }

    struct Bid {
        address bidder;
        uint256 value; // bid value
        uint256 pay; // bid value + fees
    }

    struct Collaborator {
        address recipient;
        uint256 share; // of 10,000
    }
}

interface IBabiAuction {
    function start(AuctionDomain.Lot calldata lot) external;
    function bid(bytes32 auctionId, uint256 bidValue) external payable;
    function finalize(bytes32 auctionId) external;
    function cancel(bytes32 auctionId) external;
    function getAuction(bytes32 auctionId) external view returns(AuctionDomain.Auction memory);
    function getClaimBalance(address currency, address guy) external view returns(uint256);
    function withdraw(address currency) external returns(bool);
}

contract BabiAuction is IBabiAuction, Initializable, ERC721HolderUpgradeable, AccessControlUpgradeable {
    bytes32 public constant SELLER_FEE = keccak256("SELLER_FEE");
    bytes32 public constant BUYER_FEE = keccak256("BUYER_FEE");
    bytes4 private constant _INTERFACE_ID_FEES = 0xb7799584;

    bytes32[] private feeTypes;

    address payable beneficiary;
    IBabiFee public babiFee;
    ITransferProxy transferProxy;
    IERC20TransferProxy erc20TransferProxy;

    uint256 public minDuration; 
    uint256 public minExtensionDuration; 

    mapping(bytes32 => AuctionDomain.Auction) public auctions; // auctionId => auction
    mapping(address => mapping(address => uint256)) balances; // currency => user => amountWei

    event StartAuction(bytes32 auctionId, AuctionDomain.Lot lot);
    event Bid(bytes32 auctionId, address bidder, uint256 value, uint256 pay);
    event Outbid(bytes32 auctionId, address bidder, uint256 value, uint256 pay);
    event FinalizeAuction(bytes32 auctionId, AuctionDomain.Lot lot, AuctionDomain.Bid winnerBid);
    event CancelAuction(bytes32 auctionId);

    function initialize(address _beneficiary, IBabiFee _babiFee, ITransferProxy _transferProxy, IERC20TransferProxy _erc20TransferProxy) public initializer {
        __ERC721Holder_init();
        __AccessControl_init();

        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        
        beneficiary = payable(_beneficiary);
        babiFee = _babiFee;
        transferProxy = _transferProxy;
        erc20TransferProxy = _erc20TransferProxy;
        
        minDuration = 15 * 60; // 15 minutes
        minExtensionDuration = 1 * 60; // 1 minute

        feeTypes.push(SELLER_FEE);
        feeTypes.push(BUYER_FEE);
    }

    function setBeneficiary(address _beneficiary) external onlyRole(DEFAULT_ADMIN_ROLE) {
        beneficiary = payable(_beneficiary);
    }

    function setConfig(uint256 _minDuration, uint256 _minExtensionDuration) external onlyRole(DEFAULT_ADMIN_ROLE) {
        minDuration = _minDuration;
        minExtensionDuration = _minExtensionDuration;
    }

    function supportsInterface(bytes4 interfaceId) public view virtual override returns (bool) {
        return interfaceId == type(IBabiAuction).interfaceId
            || super.supportsInterface(interfaceId);
    }

    function start(AuctionDomain.Lot calldata lot) external override {
        _start(lot);
    }

    function _start(AuctionDomain.Lot calldata lot) internal {
        require(lot.reservePrice > 0, "BabiAuction:start Reserve price must be greater than zero.");
        require(lot.duration > minDuration, "BabiAuction:start Duration must be greater than minDuration.");
        require(lot.extensionDuration > minExtensionDuration, "BabiAuction:start Extension duration must be greater than minExtensionDuration.");

        bytes32 auctionId = _getAuctionId(lot.tokenAddress, lot.tokenId, lot.salt);
        AuctionDomain.Auction storage auction = auctions[auctionId];
        require(!auction.exists, "BabiAuction:start Auction is already started for this token, try a different salt.");

        transferProxy.erc721safeTransferFrom(IERC721(lot.tokenAddress), _msgSender(), address(this), lot.tokenId);

        auction.owner = _msgSender();
        auction.lot = lot;
        auction.endTime = type(uint256).max;
        auction.state = AuctionDomain.AuctionState.WaitingReservePrice;
        auction.exists = true;
        
        emit StartAuction(auctionId, lot);
    }
    
    function bid(bytes32 auctionId, uint256 bidValue) external payable override {
        _bid(auctionId, bidValue);
    }

    /**
        @dev Bid on only 
            1. Existing
            2. Not-closed
            3. Not-ended
            4. Now-owned auctions. 
        @dev Bid must be
            1. Greater than or equal to Reserve Price
            2. Not by current winner
            3. Greater than current bid
        @dev Bid starts auctions if reserve price met.
     */
    function _bid(bytes32 auctionId, uint256 bidValue) internal {
        AuctionDomain.Auction storage auction = auctions[auctionId];
        require(auctions[auctionId].exists, "BabiAuction:bid Cannot bid on an auction which does not exist.");
        require(auction.state != AuctionDomain.AuctionState.Closed, "BabiAuction:bid Cannot bid on closed auction.");
        require(auction.endTime > block.timestamp, "BabiAuction:bid Auction is ended, waiting for finalization.");
        require(auction.owner != _msgSender(), "BabiAuction:bid Cannot bid on own auction.");

        AuctionDomain.Lot storage lot = auction.lot;
        require(bidValue >= lot.reservePrice, "BabiAuction:bid Bid must be greater than or equal to ReservePrice.");

        AuctionDomain.Bid storage highestBid = auction.highestBid;
        require(highestBid.bidder != _msgSender(), "BabiAuction:bid Cannot raise already winning bid.");
        require(bidValue > highestBid.value, "BabiAuction:bid Bid must be greater than current winning bid.");

        if (auction.state == AuctionDomain.AuctionState.WaitingReservePrice) {
            auction.endTime = block.timestamp + lot.duration;
            auction.state = AuctionDomain.AuctionState.Open;
        } else if (auction.state == AuctionDomain.AuctionState.Open) {
            if (lot.extensionDuration > 0) {
                uint256 extendTime = auction.endTime - lot.extensionDuration;
                if (block.timestamp > extendTime) {
                    auction.endTime = block.timestamp + lot.extensionDuration;
                }
            }
        }

        address curr = lot.currency;

        uint256 pay = babiFee.calculatePay(feeTypes, bidValue);
        uint256 balance = balances[curr][_msgSender()];

        if (pay > balance) {
            uint256 remainder = pay - balance;
            _transfer(curr, remainder, _msgSender(), address(this));
        } else {
            balances[curr][_msgSender()] = balance - pay;
        }

        // add previous winner pay to balances
        if (highestBid.bidder != address(0)) {
            uint256 prevBalance = balances[curr][highestBid.bidder];
            balances[curr][highestBid.bidder] = prevBalance + highestBid.pay;
            emit Outbid(auctionId, highestBid.bidder, highestBid.value, highestBid.pay);
        }

        // set new winner
        highestBid.bidder = _msgSender();
        highestBid.value = bidValue;
        highestBid.pay = pay;

        emit Bid(auctionId, highestBid.bidder, highestBid.value, highestBid.pay);
    }

    function finalize(bytes32 auctionId) external override {
        _finalize(auctionId);
    }

    function _finalize(bytes32 auctionId) internal {
        AuctionDomain.Auction storage auction = auctions[auctionId];
        require(auction.exists, "BabiAuction:finalize Cannot finalize an auction which does not exist.");
        require(auction.state == AuctionDomain.AuctionState.Open, "BabiAuction:finalize Cannot finalize not open auction.");
        require(auction.endTime <= block.timestamp, "BabiAuction:finalize Auction end time is not met yet.");
        require(auction.owner == _msgSender() || auction.highestBid.bidder == _msgSender(), "BabiAuction:bid Cannot finalize an auction which you not own or win.");

        AuctionDomain.Bid storage highestBid = auction.highestBid;
        AuctionDomain.Lot storage lot = auction.lot;
        require(highestBid.value >= lot.reservePrice, "BabiAuction:bid Reserve price not met.");

        // set auction to closed first
        auction.state = AuctionDomain.AuctionState.Closed;

        // send nft to winner
        // transferProxy.erc721safeTransferFrom(IERC721(lot.tokenAddress), address(this), highestBid.bidder, lot.tokenId);
        IERC721(lot.tokenAddress).transferFrom(address(this), highestBid.bidder, lot.tokenId);

        // send fee to beneficiary
        uint256 totalFee = babiFee.calculateTotalFee(feeTypes, highestBid.value);
        _transfer(lot.currency, totalFee, address(this), beneficiary);

        // send remaining amount to seller
        uint256 payToSeller = highestBid.pay - totalFee;
        _transferToSellerWithFees(payToSeller, auction);
        

        emit FinalizeAuction(auctionId, lot, highestBid);
    }

    function cancel(bytes32 auctionId) external override {
        _cancel(auctionId);
    }

    function _cancel(bytes32 auctionId) internal {
        AuctionDomain.Auction storage auction = auctions[auctionId];
        require(auction.exists, "BabiAuction:cancel Cannot cancel an auction which does not exist.");
        require(auction.owner == _msgSender(), "BabiAuction:cancel Cannot cancel an auction which you not own.");
        require(auction.state == AuctionDomain.AuctionState.WaitingReservePrice, "BabiAuction:cancel You can only cancel an auction which is in WaitingReservePrice state.");

        auction.state = AuctionDomain.AuctionState.Closed;

        AuctionDomain.Lot storage lot = auction.lot;
        // send nft to owner
        IERC721(lot.tokenAddress).transferFrom(address(this), auction.owner, lot.tokenId);

        emit CancelAuction(auctionId);
    }

    function getAuction(bytes32 auctionId) external view override returns(AuctionDomain.Auction memory) {
        return _getAuction(auctionId);
    }

    function _getAuction(bytes32 auctionId) internal view returns(AuctionDomain.Auction memory) {
        AuctionDomain.Auction memory auction = auctions[auctionId];
        require(auctions[auctionId].exists, "BabiAuction:getAuction Auction does not exist.");

        return auction;
    }

    function getClaimBalance(address currency, address guy) external view override returns(uint256) {
        return _getClaimBalance(currency, guy);
    }

    function _getClaimBalance(address currency, address guy) internal view returns(uint256) {
        return balances[currency][guy];
    }

    function _getAuctionId(address tokenAddress, uint256 tokenId, uint256 salt) internal pure returns(bytes32) {
        return keccak256(abi.encodePacked(tokenAddress, tokenId, salt));
    }

    function withdraw(address currency) external override returns(bool) {
        return _withdraw(currency);
    }

    function _withdraw(address currency) internal returns(bool) {
        uint256 freeBalance = _getClaimBalance(currency, _msgSender());
        require(freeBalance > 0, "BabiAuction:withdraw nothing to withdraw.");

        balances[currency][_msgSender()] = 0;

        _transfer(currency, freeBalance, address(this), _msgSender());

        return true;
    }

    function _transferToSellerWithFees(uint256 pay, AuctionDomain.Auction memory auction) internal returns(bool) {
        AuctionDomain.Lot memory lot = auction.lot;
        AuctionDomain.Bid memory highestBid = auction.highestBid;

        // transfer secondary sale fees to recipients
        if (IERC721(lot.tokenAddress).supportsInterface(_INTERFACE_ID_FEES)) {
            HasSecondarySaleFees withFees = HasSecondarySaleFees(lot.tokenAddress);
            address payable[] memory recipients = withFees.getFeeRecipients(lot.tokenId);
            uint[] memory fees = withFees.getFeeBps(lot.tokenId);
            require(fees.length == recipients.length);
            for (uint256 i = 0; i < fees.length; i++) {
                uint256 fee = highestBid.value * fees[i] / 10000;
                pay = pay - fee;
                // todo: use local balance
                _transfer(lot.currency, fee, address(this), recipients[i]);
            }
        }

        // transfer remaining amount to auction owner
        // todo: use local balance
        _transfer(lot.currency, pay, address(this), auction.owner);

        return true;
    }

    function _transfer(address currency, uint256 value, address from, address to) internal returns(bool) {
        if (currency == address(0)) {
            if (to == address(this)) {
                require(msg.value == value, "BabiAuction:transfer msg.value must be equal to value.");
            } else {
                address payable recipient = payable(to);
                (bool success, ) = recipient.call{value: value, gas: 20000}("");

                // if fail with a low gas limit, save it for future withdraw
                if (!success) {
                    balances[currency][to] = balances[currency][to] + value;
                }
            }
        } else {
            erc20TransferProxy.erc20safeTransferFrom(currency, from, to, value);
        }

        return true;
    }
    
}

