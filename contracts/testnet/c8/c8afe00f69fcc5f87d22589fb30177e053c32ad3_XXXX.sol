contract XXXX is ERC20, Ownable {
    using SafeMath for uint256;
    event ProcessedDividendTracker(uint256 iterations, uint256 claims, uint256 lastProcessedIndex, bool indexed automatic, uint256 gas, address indexed processor);


    IUniswapV2Router02 public uniswapV2Router;
    address public  uniswapV2Pair;

    address public husdtAddress=0x28c0357F19Eb4D91Fd1C6d41D3415E24C96020Af;

    uint256 public _minBuyUBalance=1*10**18;

    uint256 public total = 100;
    uint256 public _totalFee=14;

    uint256 public _burnFee = 2;
    uint256 public _rewardFee = 6;
    uint256 public _inviterFeeOne = 3;
    uint256 public _inviterFeeTwo = 2;
    uint256 public _inviterFeeThree = 1;
    uint256 public rewardNum = 300;


    uint256 public _rewardBalance = 1000000 * 10 ** 18;

    // 当前轮次
    uint256 public currentRound;

    uint256 public INDEX_NO_UPDATE=100000000000000;

    RewardPool rewardPool;

    //todo 被奖励过的人 不会被再次奖励
    mapping(address => bool) alreadyRewardUsers;// 已经被奖励过的用户

    //轮次信息
    struct roundInfo{
        uint256 currentRewardNum; // 当前奖励了多少人
        uint256 minTimestamp; // 第三百人的时间搓
        uint256 minTimestampIndex; //
        address[] rewardUsers; //奖励的用户
        txByTime[] tbts; // 每个时间的奖励用户最后一次购买数量
        mapping(uint256=>uint256) tbtIndex; // 时间对应的tbts的位置
    }

    //当前时间买币的用户
    struct txByTime {
        uint256 timestamp; //买币的时间 ，同一区块的交易 时间戳一致
        uint256 size;
    }

    // 奖励进度
    rewardProgress _rewardProgress;

    // 奖励进度
    struct rewardProgress {
        uint256 round; // 轮次
        uint256 index; // 第几个人
    }

    mapping(uint256 => roundInfo) roundMap;

    struct BuyTokenMsg{
        uint256 balance; // 买币的数量
        uint256 canRewardTimestamp; // 最近一次买币的时间，自有满足奖励才计
    }

    // 用户买币的信息
    mapping (address => mapping(uint256 => BuyTokenMsg)) buyTokenMsg;

    // 每个轮次可以平分奖励的用户
    mapping(uint256 => address[]) rewardUsers;

    address public deadWallet = 0x000000000000000000000000000000000000dEaD;

    address public marketValueWallet = 0xC3F4a42f51CB3f1c73053a745F6dB3a085e3BC58;

    mapping(address => bool) public _isBlacklisted;

    uint256 public gasForProcessing = 300000;


    // exlcude from fees and max transaction amount
    mapping (address => bool) private _isExcludedFromFees;

    // inviter
    mapping (address => address) inviter;

    event UpdateUniswapV2Router(address indexed newAddress, address indexed oldAddress);

    event ExcludeFromFees(address indexed account, bool isExcluded);

    event ExcludeMultipleAccountsFromFees(address[] accounts, bool isExcluded);

    event GasForProcessingUpdated(uint256 indexed newValue, uint256 indexed oldValue);


    constructor(
        string memory name_,
        string memory symbol_,
        uint256 totalSupply_
    ) payable ERC20(name_, symbol_)  {
        uint256 totalSupply = totalSupply_ * (10**18);

        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3);
        address _uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
        .createPair(address(this), husdtAddress);

        uniswapV2Router = _uniswapV2Router;
        uniswapV2Pair = _uniswapV2Pair;


        // exclude from paying fees or having max transaction amount
        excludeFromFees(owner(), true);
        excludeFromFees(address(this), true);

        _mint(owner(), totalSupply);
    }

    receive() external payable {}

    function excludeFromFees(address account, bool excluded) public onlyOwner {
        if(_isExcludedFromFees[account] != excluded){
            _isExcludedFromFees[account] = excluded;
            emit ExcludeFromFees(account, excluded);
        }
    }

    function excludeMultipleAccountsFromFees(address[] calldata accounts, bool excluded) public onlyOwner {
        for(uint256 i = 0; i < accounts.length; i++) {
            _isExcludedFromFees[accounts[i]] = excluded;
        }
        emit ExcludeMultipleAccountsFromFees(accounts, excluded);
    }


    function blacklistAddress(address account, bool value) external onlyOwner{
        _isBlacklisted[account] = value;
    }


    function updateGasForProcessing(uint256 newValue) public onlyOwner {
        require(newValue >= 200000 && newValue <= 500000, "GasForProcessing must be between 200,000 and 500,000");
        require(newValue != gasForProcessing, "Cannot update gasForProcessing to same value");
        emit GasForProcessingUpdated(newValue, gasForProcessing);
        gasForProcessing = newValue;
    }

    function isExcludedFromFees(address account) public view returns(bool) {
        return _isExcludedFromFees[account];
    }




    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        require(!_isBlacklisted[from] && !_isBlacklisted[to], 'Blacklisted address');


        if (_isExcludedFromFees[from] || _isExcludedFromFees[to]){
            // 白名单  或者 普通转账
            // 白名单买币也能够被奖励
            bool canInviter =  balanceOf(to) == 0 && inviter[to] == address(0);

            super._transfer(from,to,amount);

            if(canInviter) {
                inviter[to] = from;
            }
            return;
        }

        if (from != uniswapV2Pair && to != uniswapV2Pair) {
            bool canInviter =  balanceOf(to) == 0 && inviter[to] == address(0);
            if(canInviter) {
                inviter[to] = from;
            }
        }

        if(from == uniswapV2Pair || to == uniswapV2Pair){
            //买币或者卖币
            super._transfer(from,deadWallet,amount.div(100).mul(_burnFee));
            // 奖金池分配
            super._transfer(from,address(this),amount.div(100).mul(_rewardFee));

            uint256 rate;
            address reciver;
            address cur = from;
            if (from == uniswapV2Pair) {
                cur = to;
            } else {
                cur = from;
            }
            for (int256 i = 0; i < 3; i++) {
                if(i == 0){
                    rate = _inviterFeeOne;
                }else if(i == 1){
                    rate = _inviterFeeTwo;
                }else{
                    rate = _inviterFeeThree;
                }
                cur = inviter[cur];
                if (cur == address(0)) {
                    reciver = marketValueWallet;
                }else{
                    reciver = cur;
                }
                super._transfer(from,reciver,amount.div(100).mul(rate));
            }
            super._transfer(from,to,amount.div(100).mul((total).sub(_totalFee)));
        }

        if(from == uniswapV2Pair){
            // 更新账户 todo 白名单用户
            uint256 uBuy = getBuyUsdtBalance(amount);
            buyTokenMsg[from][currentRound].balance = buyTokenMsg[from][currentRound].balance.add(uBuy);

            if(buyTokenMsg[from][currentRound].balance >= _minBuyUBalance){
                //更新 roundInfo
                updateRoundInfo(from,buyTokenMsg[from][currentRound].canRewardTimestamp);
                buyTokenMsg[from][currentRound].canRewardTimestamp = block.timestamp;
            }
        }

        uint256 contractTokenBalance = balanceOf(address(this));
        if(contractTokenBalance >= _rewardBalance){
            //如果存满
            //super._transfer(address(this),address(rewardPool),_rewardBalance);
            currentRound = currentRound.add(1);
        }

        if(_rewardProgress.round < currentRound){
            processReward(gasForProcessing);
        }
    }

    function processReward(uint256 gas) public {
        uint256 gasUsed = 0;
        uint256 gasLeft = gasleft();
        uint256 rewardBalance;
        if (_rewardProgress.index == INDEX_NO_UPDATE){
            // INDEX_NO_UPDATE 代表还未更新
            _rewardProgress.index = roundMap[_rewardProgress.round].rewardUsers.length.sub(1);
        }
        if(roundMap[_rewardProgress.round].rewardUsers.length >=  rewardNum){
            rewardBalance = _rewardBalance.div(rewardNum);
        }else{
            rewardBalance = _rewardBalance.div(roundMap[_rewardProgress.round].rewardUsers.length);
        }

        while(gasUsed < gas) {
            address to = roundMap[_rewardProgress.round].rewardUsers[_rewardProgress.index];
            if(buyTokenMsg[to][_rewardProgress.round].canRewardTimestamp >=roundMap[_rewardProgress.round].minTimestamp){
                this.transfer(to,rewardBalance);
                roundMap[_rewardProgress.round].currentRewardNum = roundMap[_rewardProgress.round].currentRewardNum.add(1);
            }
            if (_rewardProgress.index == 0 || roundMap[_rewardProgress.round].currentRewardNum == rewardNum){
                _rewardProgress.round = _rewardProgress.round.add(1);
                if (_rewardProgress.round == currentRound){
                    _rewardProgress.index = INDEX_NO_UPDATE;
                    return ;
                }

                _rewardProgress.index = roundMap[_rewardProgress.round].rewardUsers.length.sub(1);
                if(roundMap[_rewardProgress.round].rewardUsers.length >=  rewardNum){
                    rewardBalance = _rewardBalance.div(rewardNum);
                }else{
                    rewardBalance = _rewardBalance.div(roundMap[_rewardProgress.round].rewardUsers.length);
                }

            }else {
                _rewardProgress.index = _rewardProgress.index.sub(1);
            }


            uint256 newGasLeft = gasleft();
            if(gasLeft > newGasLeft) {
                gasUsed = gasUsed.add(gasLeft.sub(newGasLeft));
            }

            gasLeft = newGasLeft;
        }
    }

    function updateRoundInfo(address from , uint256 timestamp) public {
        roundInfo storage rdInfo = roundMap[currentRound];
        if (timestamp == 0){
            rdInfo.rewardUsers.push(from);
        }
    }

    function getBuyUsdtBalance(uint256 cxBalance) public view returns(uint256){
        address[] memory routerAddress = new address[](2);
        routerAddress[0] = husdtAddress;
        routerAddress[1] = address(this);
        uint[] memory amounts = uniswapV2Router.getAmountsIn(cxBalance,routerAddress);
        return amounts[0];
    }

}