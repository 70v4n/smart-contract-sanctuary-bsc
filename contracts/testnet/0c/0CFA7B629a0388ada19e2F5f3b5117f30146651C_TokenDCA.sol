pragma solidity 0.8.11;

import "./ERC20.sol";
import "./Pausable.sol";
import "./IPancakeRouter02.sol";
interface IWBNB {
    function deposit() external payable;
    function transfer(address to, uint value) external returns (bool);
    function withdraw(uint) external;
}

/**
 * @title Token DCA.
 */
contract TokenDCA is Pausable {
    ERC20 public token;
    IPancakeRouter02 public router; // router where the token is listed and has most of its USDC liquidity
    address public constant USDC = 0x64544969ed7EBf5f083679233325356EbE738930; // USDC smart contract address
    address public constant WBNB = 0xae13d989daC2f0dEbFf460aC112a837C89BAa7cd; // WBNB testnet

    uint256 private _percentageOfUSDCtoSwapForTokens = 5000; // percentage of USDC to swap for TNEC - the rest will be swap for BNB
    uint256 public constant DENOMINATOR = 10_000;

    mapping(address => bool) private _owners; // list of owners

    ///@notice Throws if called by any account other than the owner.
    modifier onlyOwner() {
        require(_owners[msg.sender], "TNECDCA: caller is not owner");
        _;
    }

    ///@notice constructor, to initialize the smart contract 
    ///and to approve all usdc of this sc on 
    ///@param _router, router used
    ///@param _token, token used
    constructor(
        ERC20 _token,
        IPancakeRouter02 _router
    )
    {
        _setOwner(address(0), msg.sender);
        token = _token;
        router = _router;
        _setApprove();
    }

    ///@notice receive, required to recieve BNB
    receive() external payable {}
    
    /**
    * @notice Leaves the contract without owner. It will not be possible to call
    * `onlyOwner` functions anymore. Can only be called by the current owner.
    *
    * NOTE: Renouncing ownership will leave the contract without an owner,
    * thereby removing any functionality that is only available to the owner.
    */
    function renounceOwnership() public onlyOwner {
       _owners[msg.sender] = false;
       _setOwner(msg.sender, address(0));
    }

    /**
    * @notice Returns the address of the current owner.
    */
    function owner(address _owner) public view returns (bool) {
        return _owners[_owner];
    }

    ///@notice Transfers ownership of the contract to a new account (`newOwner`), can only be called by the current owner.
    ///@param _newOwner, addresss of the new owner
    function transferOwnership(address _newOwner) public onlyOwner {
        require(
            _newOwner != address(0),
            "Ownable: new owner is the zero address"
        );
        _setOwner(msg.sender, _newOwner);
    }

    ///@notice addNewOwner, add a new owner
    ///@param _newOwner, addresss of the new owner
    function addNewOwner(address _newOwner) public onlyOwner {
        require(
            _newOwner != address(0),
            "Ownable: new owner is the zero address"
        );
        _setOwner(address(0), _newOwner);
    }

    /// @notice setPercentageOfUSDCtoSwapForTokens, only Owner can call this function
    /// @param _percentage, percentage of USDC to be swapped to tokens - the rest will be swapped to BNB 
    function setPercentageOfUSDCtoSwapForTokens(uint256 _percentage) 
    public 
    onlyOwner 
    {
        _percentageOfUSDCtoSwapForTokens = _percentage;
    }

    /// @notice getPercent, only Owner can call this function
    /// @return percentage of USDC to be swapped to tokens - the rest will be swapped to BNB 
    function getPercent() 
    public 
    view
    onlyOwner 
    returns (uint256)
    {
        return _percentageOfUSDCtoSwapForTokens;
    }

    ///@dev transfert tokens for DCA
    ///@param _recipient, recipient address
    ///@param _valueTokens, amount of tokens to be send
    ///@param _valueBNB, amount of BNB to be send
    function distributeDCA(
        address _recipient,
        uint256 _valueTokens,
        uint256 _valueBNB
    )
        external
        whenNotPaused
        onlyOwner
    {
        token.transfer(_recipient, _valueTokens);
        payable(_recipient).transfer(_valueBNB);
    }
   
    ///@notice swapTokens, required to swap USDC for two parts : one for Tokens and BNB 
    ///@param _amount, amount to be swaped
    ///@return _tokensSwapped amount of tokens swapped 
    ///@return _BNBSwapped amount of BNB swapped
    function swapTokens(uint256 _amount)
        external
        whenNotPaused
        onlyOwner
        returns(uint256 _tokensSwapped, uint256 _BNBSwapped)
    {
        if (_amount > 0) {
            address[] memory _path = new address[](2);
            address[] memory _path2 = new address[](2);

            _path[0] = USDC;
            _path[1] = address(token);

            uint256 _forTokens = _amount * _percentageOfUSDCtoSwapForTokens / DENOMINATOR;
            uint256 _forBNB = _amount - _forTokens;
            uint256 _balanceTokensbefore = token.balanceOf(address(this));

            router.swapExactTokensForTokens(
                _forTokens,
                0,
                _path,
                address(this),
                1e18
            );

            _tokensSwapped = token.balanceOf(address(this)) - _balanceTokensbefore;

            _path2[0] = USDC;
            _path2[1] = WBNB; 
            uint256 _balanceWBNBbefore = ERC20(WBNB).balanceOf(address(this));

            router.swapExactTokensForTokens(
                _forBNB,
                0,
                _path2,
                address(this),
                1e18
            );

            _BNBSwapped = ERC20(WBNB).balanceOf(address(this)) - _balanceWBNBbefore;
            IWBNB(WBNB).withdraw(_BNBSwapped);

            return(_BNBSwapped, _tokensSwapped);
        }
    }

    ///@notice _setOwner, the address of the current owner
    ///@param _newOwner, addresss of the new owner
    ///@param _oldOwner, addresss of the old owner
    function _setOwner(address _oldOwner, address _newOwner) internal {
        _owners[_oldOwner] = false;
        _owners[_newOwner] = true;
    }

    function _setApprove() internal {
        ERC20(USDC).approve(address(router), type(uint256).max);
    }
}

