contract TheLastOneStandingToken is Context, Ownable, BEP20Token{

// TAX FEE IS LOCKED 20% ON THE BUY
    using SafeMath for uint;
    using Address for address;
    uint8 private taxFee;
//=====================================================================================//
// Game Wallets //
    address public prizeFeeReceiverAddress;
    uint8   public prizePoolFee;

    address public marketingFeeReceiverAddress;
    uint8   public marketingFee;

    address public teamFeeReceiverAddress;
    uint8   public teamFee;
    uint8   public teamPrizeFee;

    address public autoLiquidityReceiverAddress;
    uint8   public liquidityFee;
// Game Wallets //
//=====================================================================================//
// Player Information //
    struct Player {
        uint    playerId;
        bool    playing;
        address playerAddress;
        uint    playerAmount;
        uint    gameEdition;
        uint    gameBlock;
        uint    gamemode;
    }

    Player[] players;

    uint idNewPlayer;
// Player Information //
//=====================================================================================//
// Game Information //
    uint private _amountRequired;
    uint private _totalAmount;
    uint private _qtyDays;
    uint private _totalPlayersPlaying;
    uint private _gameEdition;
    bool private _winner;
// Game Information //
 
  constructor() {
    prizePoolFee = 15;
    marketingFee = 1;
    teamFee = 2;
    liquidityFee = 2;
    _totalAmount = 0;
    _qtyDays = 0;
    idNewPlayer = 0;
    _totalPlayersPlaying = 0;
    teamPrizeFee = 10;
    _winner = false;
  }

//=====================================================================================//
// Owner Section //

    function setPrizeFeeReceiverAddress(address newPrizeFeeReceiver) public onlyOwner{
      prizeFeeReceiverAddress = newPrizeFeeReceiver;
    }

    function setMarketingFeeReceiverAddress(address  newMarketingFeeReceiverAddress) public onlyOwner{
      marketingFeeReceiverAddress = newMarketingFeeReceiverAddress;
    }

    function setTeamFeeReceiverAddress(address  newTeamFeeReceiverAddress) public onlyOwner{
      teamFeeReceiverAddress = newTeamFeeReceiverAddress;
    }

    function setAutoLiquidityReceiverAddress(address  newAutoLiquidityReceiverAddress) public onlyOwner{
      autoLiquidityReceiverAddress = newAutoLiquidityReceiverAddress;
    }

    function setBuyAmount(uint16 newAmount) public onlyOwner{
      _amountRequired = newAmount;
    }

//Finding the Last One Standing//
    function findWinner() public  onlyOwner returns (bool){
     uint _id;
     uint countPlaying;
      for (_id = 0; _id < players.length || countPlaying > 1; _id++){
        if(players[_id].playerAmount < _totalAmount && players[_id].playing){
          setPlayerStatus(_id,false);
        }else{
          countPlaying.add(1);
        }
      }
      if(countPlaying > 1){
        return true;
      }else{
        return false;
      }
    }

// Reseting Game
    function resetGame() private onlyOwner{
        delete players;
    }

// Owner Section //
//=====================================================================================//
//Creating new player //
    function newPlayer(address payable _playerAddress, bool _playing) private{
      uint _id = idNewPlayer + 1;
      players.push(Player(_id,_playing,_playerAddress,0,_gameEdition,_id/500,1));
      idNewPlayer++;
    }

//Updating player Amount//
    function setPlayerAmount(uint _id, uint amount) view private{
      players[_id].playerAmount.add(amount);
    }

//Updating player Status(if he/she is playing or not)//
    function setPlayerStatus(uint _id, bool status) private{
      players[_id].playing = status;
    }

//Get players list//
    function getPlayerList() view public returns (Player[] memory) {
      return (players);
    }

//Getting player info//
    function findPlayer(address recipient) view private returns (uint) {
     uint _id;
      for (_id = 0; _id < players.length; _id++){
        if(players[_id].playerAddress == recipient){
          break;
        }
      }
     return (players[_id].playerId);
    }

//Get total players//
    function getTotalPlayer() view public returns (uint) {
      return players.length;
    }

//Getting player info//
    function getPlayerInfo(uint _id) view public returns (Player memory) {
      return (players[_id]);
    }

//Getting player Status//
    function getPlayerStatus(uint _id) view public returns (bool ) {
      return (players[_id].playing);
    }    

//Getting total players playing //
    function getTotalAddressPlaying() public returns(uint){
      uint id = 0;
      for (id = 0; id < players.length - 1; id++){
        if(players[id].playing = true){
          _totalPlayersPlaying.add(1);
        }else{
          continue;
        }
      }
      return(_totalPlayersPlaying);
    }

  function _transfer(address sender, address payable recipient , uint256 amount) internal virtual {
    if (sender != owner()){
        require(sender != address(0), "BEP20: transfer from the zero address");
        require(recipient != address(0), "BEP20: transfer to the zero address");
        require(sender.isContract() == false, "Must be a wallet to play");
        require(amount <= _amountRequired,"Amount can't be greater than maximum");
        if(recipient.balance == 0 && _qtyDays == 0){
          newPlayer(recipient,true);
        }else if(recipient.balance == 0 && _qtyDays != 0) {
          newPlayer(recipient,false);
        }else if(getPlayerStatus(findPlayer(recipient))){
          setPlayerAmount(findPlayer(recipient),amount);
        }
        super._transfer(sender, prizeFeeReceiverAddress, amount.mul(prizePoolFee).div(1000));
        super._transfer(sender, marketingFeeReceiverAddress, amount.mul(marketingFee).div(1000));
        super._transfer(sender, teamFeeReceiverAddress, amount.mul(teamFee).div(1000));
        super._transfer(sender, autoLiquidityReceiverAddress, amount.mul(liquidityFee).div(1000));
        amount = amount.sub(taxFee);
      }else if (sender == owner() && _winner){
          super._transfer(sender, teamFeeReceiverAddress, amount.mul(teamPrizeFee).div(1000));
          _winner = false;
      }
    super._transfer(sender, recipient, amount);
  }

}