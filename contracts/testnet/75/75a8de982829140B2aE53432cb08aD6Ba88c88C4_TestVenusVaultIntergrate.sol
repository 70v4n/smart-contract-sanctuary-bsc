/*
    ETH DODOV2Proxy: 0xa356867fDCEa8e71AEaF87805808803806231FdC
    BSC DODOV2Proxy: 0x8F8Dd7DB1bDA5eD3da8C9daf3bfa471c12d58486
    Polygon DODOV2Proxy: 0xa222e6a71D1A1Dd5F279805fbe38d5329C1d0e70
    Heco DODOV2Proxy: 0xAc7cC7d2374492De2D1ce21e2FEcA26EB0d113e7
    Arbitrum DODOV2Proxy: 0x88CBf433471A0CD8240D2a12354362988b4593E5
*/

/*
    ETH DODOApprove: 0xCB859eA579b28e02B87A1FDE08d087ab9dbE5149
    BSC DODOApprove: 0xa128Ba44B2738A558A1fdC06d6303d52D3Cef8c1
    Polygon DODOApprove: 0x6D310348d5c12009854DFCf72e0DF9027e8cb4f4
    Heco DODOApprove: 0x68b6c06Ac8Aa359868393724d25D871921E97293
    Arbitrum DODOApprove: 0xA867241cDC8d3b0C07C85cC06F25a0cD3b5474d8
*/
interface IDODOV1Helper {
    function querySellQuoteToken(address dodoV1Pool, uint256 quoteAmount)
        external
        view
        returns (uint256 receivedBaseAmount);

    function querySellBaseToken(address dodoV1Pool, uint256 baseAmount)
        external
        view
        returns (uint256 receivedQuoteAmount);
}

interface IDODOV1 {
    function _BASE_TOKEN_() external view returns (address);

    function _QUOTE_TOKEN_() external view returns (address);
}

interface IDODOV2 {
    function _BASE_TOKEN_() external view returns (address);

    function _QUOTE_TOKEN_() external view returns (address);

    function querySellBase(address trader, uint256 payBaseAmount)
        external
        view
        returns (uint256 receiveQuoteAmount, uint256 mtFee);

    function querySellQuote(address trader, uint256 payQuoteAmount)
        external
        view
        returns (uint256 receiveBaseAmount, uint256 mtFee);
}

interface IDODOProxy {
    function dodoSwapV1(
        address fromToken,
        address toToken,
        uint256 fromTokenAmount,
        uint256 minReturnAmount,
        address[] memory dodoPairs,
        uint256 directions,
        bool,
        uint256 deadLine
    ) external payable returns (uint256 returnAmount);

    function dodoSwapV2TokenToToken(
        address fromToken,
        address toToken,
        uint256 fromTokenAmount,
        uint256 minReturnAmount,
        address[] memory dodoPairs,
        uint256 directions,
        bool isIncentive,
        uint256 deadLine
    ) external returns (uint256 returnAmount);
}

abstract contract DODOProxyIntegrate {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    struct Pool {
        address pool;
        bool v1;
    }

    mapping(address => bool) public allowedPoolV1;
    mapping(address => bool) public allowedPoolV2;
    Pool[] public allowedPools;
    address public dodoApprove;
    address public dodoProxy;
    address public dodoHelper;

    struct DODOProxyIntegrateParams {
        address dodoApprove;
        address dodoProxy;
        address dodoHelper;
    }

    constructor(DODOProxyIntegrateParams memory params) {
        dodoApprove = params.dodoApprove;
        dodoProxy = params.dodoProxy;
        dodoHelper = params.dodoHelper;
    }

    function _addAllowPool(address pool, bool v1) internal {
        if (v1) {
            require(!allowedPoolV1[pool], "DODOProxyIntegrate: Already exists");
            allowedPoolV1[pool] = true;
        } else {
            require(!allowedPoolV2[pool], "DODOProxyIntegrate: Already exists");
            allowedPoolV2[pool] = true;
        }
        allowedPools.push(Pool({pool: pool, v1: v1}));
    }

    function _removeAllowPool(address pool, bool v1) internal {
        if (v1) {
            require(allowedPoolV1[pool], "DODOProxyIntegrate: Not exists");
            allowedPoolV1[pool] = false;
        } else {
            require(allowedPoolV2[pool], "DODOProxyIntegrate: Not exists");
            allowedPoolV2[pool] = false;
        }
        uint256 poolCount = allowedPools.length;
        for (uint256 idx = 0; idx < poolCount; idx++) {
            if (allowedPools[idx].pool == pool) {
                allowedPools[idx] = allowedPools[poolCount - 1];
                delete allowedPools[poolCount - 1];
                break;
            }
        }
    }

    function _generalApproveMax(
        address token,
        address to,
        uint256 amount
    ) internal {
        uint256 allowance = IERC20(token).allowance(address(this), to);
        if (allowance < amount) {
            if (allowance > 0) {
                IERC20(token).safeApprove(to, 0);
            }
            IERC20(token).safeApprove(to, amount);
        }
    }

    function _useDODOSwapV1(
        address dodoV1Pool,
        address tokenIn,
        address tokenOut,
        uint256 amountIn,
        uint256 slippage,
        uint256 directions,
        uint256 deadline
    ) internal returns (uint256 amount) {
        /*
            DODOV1 Pool is DODO Pool
            Note: (only used for DODOV1 pool)

            Sell baseToken and estimate received quoteToken
            Sell quoteToken and estimate received baseToken

            We provide a helper contract to help user easily estimating the received amount.

            function querySellBaseToken(
                address dodoV1Pool,
                uint256 baseAmount
            ) public view returns (uint256 receiveQuoteAmount);

            function querySellQuoteToken(
                address dodoV1Pool,
                uint256 quoteAmount
            ) public view returns (uint256 receiveBaseAmount);

            Helper Contract address on multi chain:
            - ETH: 0x533dA777aeDCE766CEAe696bf90f8541A4bA80Eb
            - BSC: 0x0F859706AeE7FcF61D5A8939E8CB9dBB6c1EDA33
            - Polygon: 0xDfaf9584F5d229A9DBE5978523317820A8897C5A
            - HECO: 0xA0Bb1FbC23a547a8D448C7c8a2336F69A9dBa1AF
            - Arbitrum: 0xA5F36E822540eFD11FcD77ec46626b916B217c3e
        */
        require(
            allowedPoolV1[dodoV1Pool],
            "DODOProxyIntegrate: Invalid v1pool"
        );
        require(slippage < 100, "DODOProxyIntegrate: Invalid slippage");
        require(dodoHelper != address(0), "DODOProxyIntegrate: Invalid helper");
        uint256 amountOut;
        if (tokenIn == IDODOV1(dodoV1Pool)._BASE_TOKEN_()) {
            amountOut = IDODOV1Helper(dodoHelper).querySellBaseToken(
                dodoV1Pool,
                amountIn
            );
        } else {
            require(
                tokenIn == IDODOV1(dodoV1Pool)._QUOTE_TOKEN_(),
                "DODOProxyIntegrate: Invalid quote token"
            );
            amountOut = IDODOV1Helper(dodoHelper).querySellQuoteToken(
                dodoV1Pool,
                amountIn
            );
        }
        uint256 amountOutMin = amountOut.mul(100 - slippage).div(100);
        address[] memory dodoPairs = new address[](1);
        dodoPairs[0] = dodoV1Pool;
        _generalApproveMax(tokenIn, dodoApprove, amountIn);
        amount = IDODOProxy(dodoProxy).dodoSwapV1(
            tokenIn,
            tokenOut,
            amountIn,
            amountOutMin,
            dodoPairs,
            directions,
            false, // isIncentive need to be false
            deadline
        );
    }

    function _useDODOSwapV2(
        address dodoV2Pool,
        address tokenIn,
        address tokenOut,
        uint256 amountIn,
        uint256 slippage,
        uint256 directions,
        uint256 deadline
    ) internal returns (uint256 amount) {
        // DODOV2 Pool is DVM or DPP
        require(
            allowedPoolV2[dodoV2Pool],
            "DODOProxyIntegrate: Invalid V2pool"
        );
        require(slippage < 100, "DODOProxyIntegrate: Invalid slippage");
        uint256 amountOut;
        if (tokenIn == IDODOV2(dodoV2Pool)._BASE_TOKEN_()) {
            (amountOut, ) = IDODOV2(dodoV2Pool).querySellBase(
                msg.sender,
                amountIn
            );
        } else {
            require(
                tokenIn == IDODOV2(dodoV2Pool)._QUOTE_TOKEN_(),
                "DODOProxyIntegrate: Invalid quote token"
            );
            (amountOut, ) = IDODOV2(dodoV2Pool).querySellQuote(
                msg.sender,
                amountIn
            );
        }
        uint256 amountOutMin = amountOut.mul(100 - slippage).div(100);
        address[] memory dodoPairs = new address[](1); //one-hop
        dodoPairs[0] = dodoV2Pool;
        _generalApproveMax(tokenIn, dodoApprove, amountIn);
        amount = IDODOProxy(dodoProxy).dodoSwapV2TokenToToken(
            tokenIn,
            tokenOut,
            amountIn,
            amountOutMin,
            dodoPairs,
            directions,
            false,
            deadline
        );
    }
}

