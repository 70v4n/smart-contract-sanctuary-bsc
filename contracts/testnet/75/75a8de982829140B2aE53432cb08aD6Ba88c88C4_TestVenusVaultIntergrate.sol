pragma solidity ^0.7.6;
pragma abicoder v2;

import "@openzeppelin/contracts/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/SafeERC20.sol";

interface IVenusVault {
    function mint(uint256 mintAmount) external returns (uint256);

    function redeem(uint256 redeemTokens) external returns (uint256);

    function underlying() external view returns (address);
}

abstract contract VenusVaultIntegrate {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    mapping(address => bool) public allowVTokened;
    mapping(address => uint256) private _vTokenBalances;
    address[] public allowVTokens;

    modifier approveMax(
        address token,
        address to,
        uint256 amount
    ) {
        if (IERC20(token).allowance(address(this), to) < amount) {
            IERC20(token).safeApprove(to, amount);
        }
        _;
        IERC20(token).safeApprove(to, 0);
    }

    function showVTokenBalance(address vToken) external view returns (uint256) {
        return _vTokenBalances[vToken];
    }

    function showUnderlying(address vToken) public view returns (address) {
        return IVenusVault(vToken).underlying();
    }

    function _addAllowVToken(address vToken) internal {
        require(!allowVTokened[vToken], "VenusVaultIntegrate: vToken exists");
        allowVTokened[vToken] = true;
        allowVTokens.push(vToken);
    }

    function _removeAllowVToken(address vToken) internal {
        require(allowVTokened[vToken], "VenusVaultIntegrate: Not exists");
        allowVTokened[vToken] = false;
        uint256 length = allowVTokens.length;
        for (uint256 idx = 0; idx < length; idx++) {
            if (allowVTokens[idx] == vToken) {
                allowVTokens[idx] = allowVTokens[length - 1];
                delete allowVTokens[length - 1];
                break;
            }
        }
    }

    function _useMint(
        address token,
        address vToken,
        uint256 amountIn
    )
        internal
        approveMax(token, vToken, amountIn)
        returns (uint256 mintAmount)
    {
        uint256 beforeMint = IERC20(vToken).balanceOf(address(this));
        uint err = IVenusVault(vToken).mint(amountIn);
        require(err == 0, "VenusVaultIntegrate: Mint Error");
        _vTokenBalances[vToken] = IERC20(vToken).balanceOf(address(this));
        mintAmount = _vTokenBalances[vToken].sub(beforeMint);
    }

    function _useRedeem(address vToken, uint256 amountIn)
        internal
        returns (uint256 redeemAmount)
    {
        require(amountIn > 0, "VenusVaultIntegrate: AM0");
        require(
            _vTokenBalances[vToken] >= amountIn,
            "VenusVaultIntegrate: Insufficient"
        );
        uint256 beforeRedeem = IERC20(vToken).balanceOf(address(this));
        uint err = IVenusVault(vToken).redeem(amountIn);
        require(err == 0, "VenusVaultIntegrate: Redeem Error");
        _vTokenBalances[vToken] = IERC20(vToken).balanceOf(address(this));
        redeemAmount = beforeRedeem.sub(_vTokenBalances[vToken]);
        require(redeemAmount <= amountIn, "VenusVaultIntegrate: Max Exceed redeem");
    }
}

