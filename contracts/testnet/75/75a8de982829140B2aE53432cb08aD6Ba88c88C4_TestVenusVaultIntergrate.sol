import "@openzeppelin/contracts/math/Math.sol";
import "@openzeppelin/contracts/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/utils/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/Pausable.sol";

import "../abstract/AccessControl.sol";
import "../abstract/VenusVaultIntegrate.sol";
import "../abstract/DODOProxyIntegrate.sol";

contract TestVenusVaultIntergrate is VenusVaultIntegrate {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    function stakeInVenus(
        address tokenIn,
        address vToken,
        uint256 amountIn
    ) external returns (uint256 amountOut) {
        require(amountIn > 0, "TestVenusVaultIntergrate: amountIn must larger than 0");
        IERC20(tokenIn).approve(msg.sender, amountIn);
        IERC20(tokenIn).safeTransferFrom(msg.sender, address(this), amountIn);
        amountOut = _useMint(tokenIn, vToken, amountIn);
        require(amountOut > 0, "TestVenusVaultIntergrate: amountOut must larger than 0");
    }

    function unstakeFromVenus(
        address vToken,
        uint256 amountIn
    ) external returns (uint256 amountOut) {
        require(amountIn > 0, "TestVenusVaultIntergrate: amountIn must larger than 0");
        amountOut = _useRedeem(vToken, amountIn);
        require(amountOut > 0, "TestVenusVaultIntergrate: amountOut must larger than 0");
        address tokenIn = showUnderlying(vToken);
        require(tokenIn != address(0), "TestVenusVaultIntergrate: AT0");
        IERC20(tokenIn).safeTransfer(msg.sender, amountOut);
    }

    function addAllowVToken(
        address vToken
    ) external {
        _addAllowVToken(vToken);
    }

    function removeAllowVToken(
        address vToken
    ) external {
        _removeAllowVToken(vToken);
    }
}

