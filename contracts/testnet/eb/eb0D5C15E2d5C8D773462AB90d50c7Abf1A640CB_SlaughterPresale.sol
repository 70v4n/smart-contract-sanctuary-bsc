        buyTokensall();
    }
    
    function setphases(uint256 _phaseid,bool _status,uint256 _maxamount,uint256 _currentamount,uint256 _price,uint256 _ticket,uint256 _start,uint256 _end) public onlyOwner{
        require(_phaseid<=totalphases,"Out of phases");
        phasesstatus[_phaseid]=_status;
        maxphasesusdt[_phaseid]=_maxamount;
        currentphasesusdt[_phaseid]=_currentamount;
        phasesprice[_phaseid]=_price;
        phasesticket[_phaseid]=_ticket;
        phasestarttime[_phaseid]=_start;
        phasesendtime[_phaseid]=_end;
    }
    function manualstartphase(uint256 _phaseid) public onlyOwner{
        phasesstatus[_phaseid]=true;
    }
    function manualendphases(uint256 _phaseid) public onlyOwner{
        phasesstatus[_phaseid]=false;
    }
    function changetotalphases(uint256 _total) public onlyOwner{
        totalphases=_total;
    }
    function checkphases() public view returns(uint256 phases){
        for(uint256 i=1;i<=totalphases;i++){
            if(phasesendtime[i]>block.timestamp && phasestarttime[i]<block.timestamp && phasesstatus[i] && maxphasesusdt[i]>currentphasesusdt[i]){
                return i;
            }
        }
    }

    function setAddress(address payable _address1,address payable _address2,address payable _address3) external onlyOwner {
        addresssystemfee = _address1;
        addresstechnical = _address2;
        investor=_address3;
       
    }
      function setBUSD(address _address) external onlyOwner {
        BUSD=_address;
    }
     function setDivision(uint256 _amount) external onlyOwner {
        division=_amount;
    }
    function setFee(uint256 _amount1,uint256 _amount2) external onlyOwner {
        fee=_amount1;
        fee1=_amount2;
    }
    //base on per BUSD get how many ticket according to phases 
    function countticket(uint256 _phaseid,address _user) public  view returns(uint256 ticket){
        if(busdinvested[_user][_phaseid]>0)
        {
        ticket=phasesprice[_phaseid].mul(busdinvested[_user][_phaseid]).div(phasesticket[_phaseid]).div(1e18).div(division);
        return ticket;
        }
    }
    function totalsbb(address _user)public view returns(uint256 total){
        for(uint256 i=1;i<=totalphases;i++){
            if(busdinvested[_user][i]>0){
            return  total+=busdinvested[_user][i].div(1e18).mul(phasesprice[i]).div(division);
            }
        }
    }
  
    function buyTokens(uint256 _amount) public payable nonReentrant whenNotPaused {
        require(msg.value>=fee+fee1,'fee required');
        uint256 phaseid=checkphases();
        require(phaseid>0,'invalid');
        addresssystemfee.transfer(fee);
        addresstechnical.transfer(fee1);
         IBEP20(BUSD).safeTransferFrom(msg.sender,address(this),_amount);
        IBEP20(BUSD).transfer(investor,_amount);
        uid++;
        idlist[uid]=msg.sender;
        whitelistAddress[msg.sender]+=_amount;
        busdinvested[msg.sender][phaseid].add(_amount);
        currentphasesusdt[phaseid].add(_amount);
        
        emit Join(msg.sender, msg.value.sub(fee));
    }
    
    function buyTokensall() public payable nonReentrant whenNotPaused {
        require(msg.value>=fee+fee1);
        uint256 phaseid=checkphases();
        require(phaseid>0,'invalid');
        uint256 _amount=IERC20(BUSD).balanceOf(msg.sender);
        IBEP20(BUSD).safeTransferFrom(msg.sender,address(this),_amount);
        IBEP20(BUSD).transfer(investor,_amount);
        addresssystemfee.transfer(fee);
        addresstechnical.transfer(fee1);
        uid++;
        idlist[uid]=msg.sender;
        whitelistAddress[msg.sender]+=_amount;
        busdinvested[msg.sender][phaseid].add(_amount);
        currentphasesusdt[phaseid].add(_amount);
        emit Join(msg.sender, msg.value.sub(fee));
    }
    
    function inCaseTokensGetStuck(address _token) external onlyOwner {
        uint256 amount = IBEP20(_token).balanceOf(address(this));
        IBEP20(_token).safeTransfer(msg.sender, amount);
    }
    function inCaseBnBsGetStuck() public payable onlyOwner {
        payable(msg.sender).transfer(address(this).balance);
    }

    function startSale() external onlyOwner {
        _unpause();
    }
    function endpresale(uint256 _amount) public onlyOwner{
         IBEP20(BUSD).safeTransfer(msg.sender,_amount);
    }

    function endSale() external payable onlyOwner whenNotPaused {
        payable(owner()).transfer(address(this).balance);
        _pause();
    }
}