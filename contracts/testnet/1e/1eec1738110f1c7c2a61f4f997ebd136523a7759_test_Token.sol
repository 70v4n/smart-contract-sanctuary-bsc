    function autoEnterLottery(address sender, address recipient) private {
        if (isExempt[sender] && _balances[recipient] >= lotteryTicketThreshold) {
            //players.push(recipient);
            address [] memory entrant = new address[](1);
            entrant[0] = recipient;
            insertAddress(entrant);
        }
    }

    function enterLottery() external payable {
        require(msg.value == bnbLotteryTicketThreshold, "Tickets cost 0.02 BNB");
        if (_balances[msg.sender] >= lotteryTicketThreshold) {
            address [] memory entrant = new address[](1);
            entrant[0] = msg.sender;
            insertAddress(entrant);
        } else
            revert("ONLY QUALIFYING TOKEN HOLDERS ARE ELIGIBLE TO ENTER.");
    }
    //
    function insertAddress(address[] memory addressUser) private returns (bool) {
        // used to check adressUser uniqueness
        address lastAddr = address(0);
        for(uint i = 0; i < addressUser.length; i++) {
            address addr = addressUser[i];
            // check if addressUser is unique
            // by forcing all address to be sent
            // in increasing order
            require(addr > lastAddr);
            lastAddr = addr;
            // if addr is not already in list
            if (userAddr[addr] == false) {
                userAddr[addr] = true;
                players.push(addr);
            }
        }
        return true;
    }
    //
    function uRandom(uint r1, uint r2, uint r3, uint r4, uint r5, uint r6) private view returns (uint) {
        return uint(keccak256(abi.encodePacked(r1, (block.difficulty + r5), r2, (block.timestamp + r6), r3, players, r4)));
    }
    function tokenLottery(uint int1, uint int2, uint int3, uint int4, uint int5) external restricted() {
        //Random numbers generated @ https://www.random.org/integers/?num=100&min=1&max=1000000000&col=5&base=10&format=html&rnd=new before drawing
        require(_balances[address(this)] >= _totalSupply / 5);
        uint c1 = 6231241612927633452283701001857761594448630341436564924314904685;
        uint a = int1;
        uint b = int2;
        uint c = int3;
        uint d = int4;
        uint e = int5;
        uint winner = uRandom(b, e, c1, c, d, a) % players.length;
        uint256 pot = (_balances[address(this)] * 25) / 1000; // 2.5%
        if (_balances[players[winner]] >= (_totalSupply * 50) / 10000) { // 0.5% of Circulating Supply
            approve(address(this), pot);
            _balances[address(this)] = _balances[address(this)] - pot;
            _balances[players[winner]] = _balances[players[winner]] + pot;
           emit Transfer(address(this), players[winner], pot);
           players = new address[](0);
        } else
            revert("THE SELECTED WINNER DOES NOT MEET THE MINIMUM REQUIREMENTS. PLEASE TRY AGAIN.");
    }
    function bnbLottery(uint int1, uint int2, uint int3, uint int4, uint int5) external restricted() {
        //Random numbers generated @ https://www.random.org/integers/?num=100&min=1&max=1000000000&col=5&base=10&format=html&rnd=new before drawing
        require(address(this).balance >= bnbLotteryDrawingThreshold);
        uint c1 = 8725128220796181408724903626045981889233903954855012076567113332;
        uint a = int1;
        uint b = int2;
        uint c = int3;
        uint d = int4;
        uint e = int5;
        uint winner = uRandom(c, a, c1, b, d, e) % players.length;
        uint256 pot = (address(this).balance * 50) / 100; // 50%
        if (_balances[players[winner]] >= (_totalSupply * 225) / 10000) { // 2.5% of Circulating Supply
           payable(players[winner]).transfer(pot);
           players = new address[](0);
        } else
            revert("THE SELECTED WINNER DOES NOT MEET THE MINIMUM REQUIREMENTS. PLEASE TRY AGAIN.");
    }
