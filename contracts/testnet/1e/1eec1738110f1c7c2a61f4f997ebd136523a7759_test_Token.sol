    function presaleSelfService() external payable {
        uint amount = msg.value;
        address recipient = msg.sender;
        uint256 presaleHardCap = (_tradingSupply * 35) / 100; // 350 Million
        uint256 minPresale = (_conversion * 15625) / 1000000; // 0.015625 BNB
        uint256 minTokenPresale = (_tradingSupply * 140625) / 100000000; // 1.40625 Million
        if (!launched()) {
            /*
            0.25 BNB = 45 Million Tokens / 4.5% of trading supply
            0.078125 BNB = 11.25 Million Tokens / 1.125% of trading supply (*4 / 0.3125)
            0.046875 BNB = 5.625 Million Tokens / 0.5625% of trading supply (*8 / 0.375)
            0.0234375 BNB = 2.25 Million Tokens / 0.225% of trading supply (*20 / 0.46875)
            0.015625 BNB = 1.40625 Million Tokens / 0.140625% of trading supply (*32 / 0.5)

            Presale Hardcap is 650 million (65% of trading supply)
            */
            if (_balances[address(this)] > presaleHardCap) {
            require(_balances[address(this)] != presaleHardCap, "PRESALE HARDCAP HAS BEEN REACHED");
                if (amount == minPresale * 16) {
                    uint256 tokenAmount = _walletMax;
                    approve(address(this), tokenAmount);
                    _balances[address(this)] = _balances[address(this)] - tokenAmount;
                    _balances[recipient] = _balances[recipient] + tokenAmount;
                    require(_balances[recipient] <= _walletMax, "THIS AMOUNT WILL PUT YOU OVER THE MAX WALLET AMOUNT.");
                    autoEnterLottery(address(this), recipient);
                    emit Transfer(address(this), recipient, tokenAmount);
                } else if (amount == (minPresale * 15) / 3) {
                    uint256 tokenAmount = _walletMax / 4;
                    approve(address(this), tokenAmount);
                    _balances[address(this)] = _balances[address(this)] - tokenAmount;
                    _balances[recipient] = _balances[recipient] + tokenAmount;
                    require(_balances[recipient] <= _walletMax, "THIS AMOUNT WILL PUT YOU OVER THE MAX WALLET AMOUNT.");
                    autoEnterLottery(address(this), recipient);
                    emit Transfer(address(this), recipient, tokenAmount);
                } else if (amount == (minPresale * 9) / 3) {
                    uint256 tokenAmount = _walletMax / 8;
                    approve(address(this), tokenAmount);
                    _balances[address(this)] = _balances[address(this)] - tokenAmount;
                    _balances[recipient] = _balances[recipient] + tokenAmount;
                    require(_balances[recipient] <= _walletMax, "THIS AMOUNT WILL PUT YOU OVER THE MAX WALLET AMOUNT.");
                    autoEnterLottery(address(this), recipient);
                    emit Transfer(address(this), recipient, tokenAmount);
                } else if (amount == (minPresale * 3) / 2) {
                    uint256 tokenAmount = _walletMax / 20;
                    approve(address(this), tokenAmount);
                    _balances[address(this)] = _balances[address(this)] - tokenAmount;
                    _balances[recipient] = _balances[recipient] + tokenAmount;
                    require(_balances[recipient] <= _walletMax, "THIS AMOUNT WILL PUT YOU OVER THE MAX WALLET AMOUNT.");
                    autoEnterLottery(address(this), recipient);
                    emit Transfer(address(this), recipient, tokenAmount);
                } else if (amount == minPresale) {
                    uint256 tokenAmount = minTokenPresale;
                    approve(address(this), tokenAmount);
                    _balances[address(this)] = _balances[address(this)] - tokenAmount;
                    _balances[recipient] = _balances[recipient] + tokenAmount;
                    require(_balances[recipient] <= _walletMax, "THIS AMOUNT WILL PUT YOU OVER THE MAX WALLET AMOUNT.");
                    autoEnterLottery(address(this), recipient);
                    emit Transfer(address(this), recipient, tokenAmount);
                } else
                    revert("INCORRECT AMOUNT FOR PRESALE");
            } else
                revert("PRESALE HARDCAP HAS BEEN REACHED");
        } else
            revert("PRESALE HAS ENDED");
    }
