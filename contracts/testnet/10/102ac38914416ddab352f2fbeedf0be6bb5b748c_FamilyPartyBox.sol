contract FamilyPartyNFT is OwnableContract, ERC721("FamilyPartyNFT", "FamilyPartyNFT") {

    using Counters for Counters.Counter;
    Counters.Counter private _tokenIds;

    struct CardAttribute {
        uint256 rarity;
        string name;
    }

    mapping(uint256 => CardAttribute) public tokenIdAttributeMapping;


    function getTokenIdCardAttribute(uint256 tokenId) public view returns (CardAttribute memory){
        return tokenIdAttributeMapping[tokenId];
    }

    function createNFT(uint256 rarity, string memory name, address to) public onlyAdmin returns (uint256){
        uint256 tokenId = _mintNFT(to);
        CardAttribute memory cardAttribute;
        cardAttribute.name = name;
        cardAttribute.rarity = rarity;
        tokenIdAttributeMapping[tokenId] = cardAttribute;
        return tokenId;
    }

    function _mintNFT(address owner) private returns (uint256){
        _tokenIds.increment();

        uint256 newItemId = _tokenIds.current();
        _mint(owner, newItemId);
        return newItemId;
    }

    function setBaseURI(string memory newBaseURI) public onlyOwner {
        setBaseURI(newBaseURI);
    }

    function tokenURI(uint256 tokenId) public view virtual override returns (string memory){
        require(tokenId >= _tokenIds.current(), "URI query for nonexistent token");
        return string(abi.encodePacked(_baseURI(), address(this), '/', tokenIdAttributeMapping[tokenId].name));
    }

    function snapshot() public view returns (address[] memory){
        address[] memory whiteList;
        for (uint256 i = 0; i < _tokenIds.current(); i++) {
            CardAttribute memory cardAttribute = tokenIdAttributeMapping[i];
            if (cardAttribute.rarity >= 3) {
                whiteList[i] = ownerOf(i);
            }
        }
        return whiteList;
    }

    function tokensOfOwner(address _owner) public view returns (uint256[] memory ownerTokens) {
        uint256 tokenCount = balanceOf(_owner);

        if (tokenCount == 0) {
            // Return an empty array
            return new uint256[](0);
        } else {
            uint256[] memory result = new uint256[](tokenCount);
            uint256 totalToken = _tokenIds.current();
            uint256 resultIndex = 0;

            uint256 tokenId;

            for (tokenId = 1; tokenId <= totalToken; tokenId++) {
                if (_owners[tokenId] == _owner) {
                    result[resultIndex] = tokenId;
                    resultIndex++;
                }
            }

            return result;
        }
    }

}

contract Random is RandomNumberConsumer {

    uint256 private constant ROLL_IN_PROGRESS = 42;

    mapping(bytes32 => address) public s_rollers;
    mapping(address => uint256) public s_results;

    uint256 public randomValue;

    function initParams(address roller) public returns (bytes32 requestId) {
        require(LINK.balanceOf(address(this)) >= fee, "Not enough LINK to pay fee");
        require(s_results[roller] == 0, "Already rolled");
        requestId = requestRandomness(keyHash, fee);
        s_rollers[requestId] = roller;
        s_results[roller] = ROLL_IN_PROGRESS;
    }

    function nextRandom(uint256 index) public view returns (uint256){
        return uint256(keccak256(abi.encode(randomValue, index))) % 100 + 1;
    }

    function fulfillRandomness(bytes32 requestId, uint256 randomness) internal override {
        uint256 d20Value = (randomness % 100) + 1;
        s_results[s_rollers[requestId]] = d20Value;
        randomValue = randomness;
    }
}

contract FamilyPartyBox is OwnableContract, Random {

    using Counters for Counters.Counter;
    Counters.Counter private _next;//_next

    uint256 basicBoxMax = 2000;
    uint256 basicBoxMin = 0;
    uint256 luxuryBoxMax = 5000;
    uint256 luxuryBoxMin = 0;

    address familyPartyNFT;
    uint256 openTime;
    uint256 basicBoxPrice;
    uint256 luxuryBoxPrice;

    mapping(address => uint256) public maxBasicBoxRecord;
    mapping(address => uint256) public maxLuxuryBoxRecord;

    uint256[] public probability;
    uint256[] public remain;
    uint256[] public rarity = [1, 2, 3, 4];
    string[] cardsName = ["Skeleton soldier", "Jumping zombie", "Goblin Bombardier", "Orc Brothers", "Fire element", "Slime", "Arrow Rain", "Veteran", "Apprentice mage", "Dwarf fighter", "Ninja", "Rock giant", "Meteorite", "Cyclop", "Warlock", "Flying Ball"];

    constructor(){
        familyPartyNFT = 0x6e7EeF63Fe073B56853F2919c89d1DAE92Bc9E6a;
        basicBoxPrice = 10000000000000000;
        luxuryBoxPrice = 10000000000000000;
        for (uint256 i = 0; i < 16; i++) {
            if (i < 8) {
                probability.push(105);
                remain.push(525);
            } else if (i < 13) {
                probability.push(30);
                remain.push(150);
            } else {
                probability.push(3);
                remain.push(i == 15 ? 17 : 16);
            }
        }
    }

    //    modifier canBuy {
    //        require(openTime >= block.timestamp);
    //        _;
    //    }

    modifier onlyEOA() {
        require(tx.origin == msg.sender, "FamilyPartyBox: should be EOA");
        _;
    }

    function setOpenTime(uint256 _openTime) public onlyAdmin {
        openTime = _openTime;
    }

    function buyBasicBox() payable public {
        require(basicBoxMin < basicBoxMax, "FamilyPartyBoxL BasicBox sold out");
        require(maxBasicBoxRecord[msg.sender] == 0, "FamilyPartyBox: BasicBox sold out");
        require(msg.value == basicBoxPrice, "FamilyPartyBox: INSUFFICIENT_AMOUNT");
        //TODO 测试阶段不限制购买次数
        //        maxBasicBoxRecord[msg.sender] = 1;

        openCommonBox();
    }

    function buyLuxuryBox() payable public {
        require(basicBoxMin < basicBoxMax, "FamilyPartyBox: LuxuryBox sold out");
        require(maxLuxuryBoxRecord[msg.sender] < 10, "FamilyPartyBox: LuxuryBox sold out");
        require(msg.value == luxuryBoxPrice, "FamilyPartyBox: INSUFFICIENT_AMOUNT");
        //TODO 测试阶段不限制购买次数
        //        maxLuxuryBoxRecord[msg.sender] = maxLuxuryBoxRecord[msg.sender] + 1;

        openLuxuryBox();
    }

    function openCommonBox() internal {
        for (uint256 i = 1; i < 9; i++) {
            FamilyPartyNFT instance = FamilyPartyNFT(familyPartyNFT);
            instance.createNFT(rarity[0], cardsName[i - 1], msg.sender);
        }
    }

    function openLuxuryBox() internal {
        uint256 lotteryIndex = lottery();
        uint256 remainCount = remain[lotteryIndex];
        remainCount = remainCount - 1;
        remain[lotteryIndex] = remainCount;
        if (remainCount == 0) {
            probability[lotteryIndex] = 0;
        }
        FamilyPartyNFT instance = FamilyPartyNFT(familyPartyNFT);
        uint256 rarity = 2;
        if (lotteryIndex > 4) {
            rarity = 3;
        }
        instance.createNFT(rarity, cardsName[lotteryIndex + 7], msg.sender);
    }

    function lottery() internal returns (uint256){

        uint256 size = probability.length;

        uint256 sumProbability = 0;
        for (uint256 i = 0; i < size; i++) {
            sumProbability = sumProbability + probability[i];
        }

        uint256[] memory sortProbability = new uint256[](size + 1);
        uint256 tempSumProbability = 0;
        for (uint256 i = 0; i < size; i++) {
            tempSumProbability = tempSumProbability + probability[i];
            sortProbability[i] = (tempSumProbability * 1000 / sumProbability);
        }

        _next.increment();
        uint256 randProbability = nextRandom(_next.current());
        sortProbability[sortProbability.length - 1] = randProbability;
        sort(sortProbability);
        uint256 index;
        for (uint256 i = 0; i < sortProbability.length; i++) {
            if (sortProbability[i] == randProbability) {
                index = i;
                break;
            }
        }
        return index;
    }

    function sort(uint[] memory data) internal returns (uint[] memory) {
        quickSort(data, int(0), int(data.length - 1));
        return data;
    }

    function quickSort(uint[] memory arr, int left, int right) internal {
        int i = left;
        int j = right;
        if (i == j) return;
        uint pivot = arr[uint(left + (right - left) / 2)];
        while (i <= j) {
            while (arr[uint(i)] < pivot) i++;
            while (pivot < arr[uint(j)]) j--;
            if (i <= j) {
                (arr[uint(i)], arr[uint(j)]) = (arr[uint(j)], arr[uint(i)]);
                i++;
                j--;
            }
        }
        if (left < j)
            quickSort(arr, left, j);
        if (i < right)
            quickSort(arr, i, right);
    }

    function setBasicBoxPrice(uint256 _price) public onlyAdmin() {
        basicBoxPrice = _price;
    }

    function setLuxuryPrice(uint256 _price) public onlyAdmin() {
        luxuryBoxPrice = _price;
    }

    function remainBasicBox() public view returns (uint256){
        return basicBoxMax - basicBoxMin;
    }

    function remainLuxuryBox() public view returns (uint256){
        return luxuryBoxMax - luxuryBoxMin;
    }

    function withdrawBNB() public onlyAdmin {

        uint256 balance = address(this).balance;

        (bool success,) = payable(msg.sender).call{value : balance}("");
        require(success, "BNB withdraw failed");
    }
}