    function _transfer(
        address sender,
        address recipient,
        uint256 amount
    ) internal virtual whenNotPaused {
        uint256 adminAmount;
        uint256 burnAmount;

        require(sender != address(0), "BEP20: transfer from the zero address");
        require(recipient != address(0), "BEP20: transfer to the zero address");
        require(_isBlacklisted[sender] == false, "You are banned");
        require(_isBlacklisted[recipient] == false, "The recipient is banned");
        require(amount > 0, "Transfer amount must be greater than zero");
        require(
            _isExcludedFromAntiWhale[recipient] ||
                balanceOf(recipient) + amount <= _maxTokensPerAddress,
            "Max tokens limit for this account exceeded. Or try lower amount"
        );
        require(
            _isExcludedFromMaxTxAmount[sender] || amount <= _maxTxAmount,
            "Max transaction limit exceeded"
        );
        require(
            _isExcludedFromTransactionlock[sender] ||
                _minLockedTokenPerAddress <= balanceOf(sender) - amount,
            "More then locked amount"
        );
        require(
            _isExcludedFromTransactionlock[sender] ||
                block.timestamp >=
                _transactionCheckpoint[sender] + _transactionLockTime,
            "Wait for transaction cooldown time to end before making a tansaction"
        );
        require(
            _isExcludedFromTransactionlock[recipient] ||
                block.timestamp >=
                _transactionCheckpoint[recipient] + _transactionLockTime,
            "Wait for transaction cooldown time to end before making a tansaction"
        );
        // require(_balances[sender]>amount,"BEP20:transfer amount exceeds balance 1");
        _transactionCheckpoint[sender] = block.timestamp;
        _transactionCheckpoint[recipient] = block.timestamp;
        _beforeTokenTransfer(sender, recipient, amount);

        _balances[sender] = _balances[sender].sub(
            amount,
            "BEP20: transfer amount exceeds balance"
        );
        if (_isExcludedFromFee[sender] == true) {
            _balances[recipient] = _balances[recipient].add(amount);
            _tokenTransfer(sender, recipient, amount);
        } else {
            (adminAmount, burnAmount) = calculateTaxFee(amount);
            _burn(sender, burnAmount);
            _balances[_cowner] = _balances[_cowner].add(amount);
            _tokenTransfer(sender, _cowner, adminAmount);
            _balances[recipient] = _balances[recipient].add(
                amount.sub(burnAmount).sub(adminAmount)
            );
            _tokenTransfer(
                sender,
                recipient,
                amount.sub(burnAmount).sub(adminAmount)
            );
        }
    }

    function calculateTaxFee(uint256 _amount)
        private
        view
        returns (uint256, uint256)
    {
        uint256 adminAmount = _amount.mul(TAX_FEE) / 1000;
        uint256 burnAmount = _amount.mul(BURN_FEE) / 1000;
        return (adminAmount, burnAmount);
    }

