pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/Strings.sol";

import "./BytesLibrary.sol";
import "./StringsLibrary.sol";
import "./IDomainNameRegister.sol";

contract DomainNameRegister is IDomainNameRegister, Ownable {

    using StringsLibrary for *;

    using BytesLibrary for bytes32;

    using Strings for uint256;

    using EnumerableSet for EnumerableSet.Bytes32Set;

    string public constant version = "1.0";

    mapping(address => EnumerableSet.Bytes32Set) private _curatorDomainNameBytes32List;

    event ADD_CURATOR_DOMAIN_NAME(address _curator, string _domainName);

    event REMOVE_CURATOR_DOMAIN_NAME(address _curator, string _domainName);

    //批量添加策展人域名
    function addList(address curator, bytes32[] calldata domainNameBytes32List) override external onlyOwner {
        EnumerableSet.Bytes32Set storage curatorDomainNameBytes32List = _curatorDomainNameBytes32List[curator];
        for (uint256 i = 0; i < domainNameBytes32List.length; i++) {
            bytes32 _domainNameListItemBytes32 = domainNameBytes32List[i];
            //require(!containsBlank(_domainNameListItemString), "DomainNameRegister: domain name incorrect format");
            if (!curatorDomainNameBytes32List.contains(_domainNameListItemBytes32)) {
                curatorDomainNameBytes32List.add(_domainNameListItemBytes32);
                //
                //emit ADD_CURATOR_DOMAIN_NAME(curator, _domainNameListItemString);
            }
        }
    }

    //批量添加策展人域名
    function addList2(address curator, string[] calldata domainNameStringList) override external onlyOwner {
        EnumerableSet.Bytes32Set storage domainNameBytes32List = _curatorDomainNameBytes32List[curator];
        for (uint256 i = 0; i < domainNameStringList.length; i++) {
            string memory _domainNameListItemString = domainNameStringList[i];
            bytes32 _domainNameListItemBytes32 = domainNameStringList[i].toBytes32();
            require(!containsBlank(_domainNameListItemString), "DomainNameRegister: domain name incorrect format");
            if (!domainNameBytes32List.contains(_domainNameListItemBytes32)) {
                domainNameBytes32List.add(_domainNameListItemBytes32);
                //
                emit ADD_CURATOR_DOMAIN_NAME(curator, _domainNameListItemString);
            }
        }
    }

    //批量移除策展人域名
    function removeList(address curator, string[] calldata domainNameStringList) override external onlyOwner {
        EnumerableSet.Bytes32Set storage domainNameBytes32List = _curatorDomainNameBytes32List[curator];
        for (uint256 i = 0; i < domainNameStringList.length; i++) {
            string memory _domainNameListItemString = domainNameStringList[i];
            bytes32 _domainNameListItemBytes32 = _domainNameListItemString.toBytes32();
            domainNameBytes32List.remove(_domainNameListItemBytes32);
            //
            emit REMOVE_CURATOR_DOMAIN_NAME(curator, _domainNameListItemString);
        }
    }

    //查询策展人域名
    function getList(address curator) override external view returns (string[] memory) {
        EnumerableSet.Bytes32Set storage domainNameBytes32List = _curatorDomainNameBytes32List[curator];
        uint256 length = domainNameBytes32List.length();
        string[] memory domainNameStringList = new string[](length);
        for (uint256 i = 0; i < length; i++) {
            domainNameStringList[i] = domainNameBytes32List.at(i).toString();
        }
        return domainNameStringList;
    }

    //是否已包含
    function contains(address curator, string memory domainName) override external view returns (bool){
        EnumerableSet.Bytes32Set storage domainNameList = _curatorDomainNameBytes32List[curator];
        return domainNameList.contains(domainName.toBytes32());
    }

    function containsBlank(string memory str) internal pure returns (bool) {
        return str.toSlice().contains(" ".toSlice());
    }

}

