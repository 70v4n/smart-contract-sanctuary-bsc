pragma solidity ^0.8.2;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "hardhat/console.sol";

contract PerformanceToken is ERC20 {
    // 默认的钱包地址
    address public owner;
    address public adminer;
    address public freezer;

    // 悬赏任务结构
    struct RewardTask {
        uint256 taskId;
        address creater;
        uint256 amount;
        bool isUnfreezed;
        bool isRewarded;
        bool isDeduced;
        bool isWithdrawed;
        mapping(address => uint256) freezerMap;
        address[] freezerAddressArr;
        mapping(address => uint256) rewardMap;
        address[] rewardAddressArr;
        mapping(address => uint256) deduceMap;
        mapping(address => uint256) withdrawMap;
    }
    mapping(uint256 => RewardTask) public rewardTaskMap;

    // 用户钱包结构
    enum WalletLevel { ADMIN, DEPARTMENT, USER, FREEZE }
    mapping(address => ClientWallet) public clientWalletMap;
    struct ClientWallet {
        address walletAddress;
        WalletLevel walletLevel;
        uint256 userId;
        uint256 tokens;
    }

    // 悬赏任务账本结构
    enum RewardTaskTransactionType { FREEZE, UN_FREEZE, REWARD, DEDUCT, WITHDRAW }
    struct RewardTaskLedger {
        uint256 taskId;
        RewardTaskTransactionType taskTransactionType;
        address from;
        address to;
        uint256 amount;
        uint createTime;
    }
    RewardTaskLedger[] public allLedgers;
    mapping(address => RewardTaskLedger[]) public userLedgers;
    mapping(uint256 => RewardTaskLedger[]) public taskLedgers;

    constructor() ERC20("PerformanceToken", "PTC") {
        _mint(msg.sender, 1000000 * 10 ** decimals());
        owner = msg.sender;
    }

    // 校验智能合约交易只能由归属者发起
    modifier onlyOwner() {
        require(msg.sender == owner, "Not owner");
        _;
    }

    // 校验智能合约交是否已经初始化
    modifier onlyInited() {
        require(adminer != address(0), "Adminer not exist");
        require(freezer != address(0), "Freezer not exist");
        _;
    }

    // 校验悬赏任务是否存在
     modifier onlyRewardExisted(uint256 _taskId) {
        require(rewardTaskMap[_taskId].taskId > 0, "Reward task not exist");
        _;
    }

    // 初始化代码，初始化管理员钱包，冻结账号钱包
    function init(address _adminerAddress, address _freezerAddress) onlyOwner external {
        require(adminer == address(0), "Adminer has existed");
        require(freezer == address(0), "Freezer has existed");
        adminer = _adminerAddress;
        transferFrom(owner, adminer, 1000000 * 10 ** decimals());
        freezer = _freezerAddress;
    }

    // 创建用户钱包
    function createClientWallet(address _address, WalletLevel _walletLevel, uint256 _userId) onlyOwner onlyInited external {
        require(clientWalletMap[_address].walletAddress == address(0) , "The wallet has existed");
        ClientWallet memory wallet = ClientWallet(_address, _walletLevel, _userId, 0);
        wallet.tokens = balanceOf(_address);
        clientWalletMap[_address] = wallet;
    }

    // 创建悬赏任务
    function createRewardTask(uint256 _taskId, address _creater, uint256 _amount) onlyOwner onlyInited external {
        require(rewardTaskMap[_taskId].taskId > 0, "The rewadTask has existed");
        RewardTask storage rewardTask = rewardTaskMap[_taskId];
        rewardTask.taskId = _taskId;
        rewardTask.creater = _creater;
        rewardTask.amount = _amount;
    }

    // 冻结操作
    function freeze(address _address, uint256 _amount, uint256 _taskId) onlyOwner onlyInited onlyRewardExisted(_taskId) external {
        require(rewardTaskMap[_taskId].freezerMap[_address] == 0, "The rewardTask address has freezed");
        transferTo(_address, freezer, _amount, _taskId, RewardTaskTransactionType.FREEZE);
        rewardTaskMap[_taskId].freezerMap[_address] = _amount;
        rewardTaskMap[_taskId].freezerAddressArr.push(_address);
    }

    // 解冻操作
    function unFreeze(uint256 _taskId) onlyOwner onlyInited onlyRewardExisted(_taskId) external {
        require(!rewardTaskMap[_taskId].isUnfreezed, "The rewardTask has been unFreezed");
        for(uint i = 0; i < rewardTaskMap[_taskId].freezerAddressArr.length; i++) {
            address _address = rewardTaskMap[_taskId].freezerAddressArr[i];
            uint256 _amount =  rewardTaskMap[_taskId].freezerMap[_address];
            transferTo(freezer, _address, _amount, _taskId, RewardTaskTransactionType.UN_FREEZE);
        }
        rewardTaskMap[_taskId].isUnfreezed = true;
    }

    // 发放奖励操作
    function reward(address[] memory _addressArr, uint256[] memory _amountArr, uint256 _taskId) onlyOwner onlyInited onlyRewardExisted(_taskId) external {
        require(_addressArr.length == _amountArr.length, "Not match addrees and amount numbers");
        require(_addressArr.length == _amountArr.length, "Not match addrees and amount numbers");
        address _from = rewardTaskMap[_taskId].creater;
        for (uint i = 0; i < _addressArr.length; i++) {
            address _address = _addressArr[i];
            uint256 _amount = _amountArr[i];
            require(rewardTaskMap[_taskId].rewardMap[_address] == 0, "The rewardTask address has rewarded");
            transferTo(_from, _addressArr[i], _amountArr[i], _taskId, RewardTaskTransactionType.REWARD);
            rewardTaskMap[_taskId].rewardMap[_address] = _amount;
            rewardTaskMap[_taskId].rewardAddressArr.push(_address);
        }
        rewardTaskMap[_taskId].isRewarded = true;
    }

    // 惩罚扣除操作
    function deduct(address[] memory _addressArr, uint256[] memory _amountArr, uint256 _taskId) onlyOwner onlyInited onlyRewardExisted(_taskId) external {
        require(_addressArr.length == _amountArr.length, "Not match addrees and amount numbers");
        for (uint i = 0; i < _addressArr.length; i++) {
            address _address = _addressArr[i];
            uint256 _amount = _amountArr[i];
            require(rewardTaskMap[_taskId].deduceMap[_address] == 0, "The rewardTask address has deduced");
            transferTo(adminer, _addressArr[i], _amountArr[i], _taskId, RewardTaskTransactionType.DEDUCT);
            rewardTaskMap[_taskId].rewardMap[_address] = _amount;
            rewardTaskMap[_taskId].rewardAddressArr.push(_address);
        }
        rewardTaskMap[_taskId].isDeduced = true;
    }

    // 退回奖励操作
    function withdraw(uint256 _taskId) onlyOwner onlyInited onlyRewardExisted(_taskId) external {
        require(rewardTaskMap[_taskId].isRewarded, "The rewardTask has not reward");
        require(rewardTaskMap[_taskId].isWithdrawed, "The rewardTask has been withdrawed");
        address _to = rewardTaskMap[_taskId].creater;
        for(uint i = 0; i < rewardTaskMap[_taskId].rewardAddressArr.length; i++) {
            address _address = rewardTaskMap[_taskId].rewardAddressArr[i];
            uint256 _amount =  rewardTaskMap[_taskId].rewardMap[_address];
            transferTo(_address, _to, _amount, _taskId, RewardTaskTransactionType.WITHDRAW);
        }
        rewardTaskMap[_taskId].isWithdrawed = true;
    }

    // 通用的转账逻辑
    function transferTo(address _from, address _to, uint256 _amount, uint256 _taskId, RewardTaskTransactionType _rewardTaskTransactionType) onlyOwner onlyInited private {

        // 校验要转账的钱包是否充足
        require(balanceOf(_from) >= _amount, "Not enough tokens");

        // console.log("Sender balance is %s tokens", balanceOf(from));
        // console.log("Trying to send %s tokens to %s", amount, to);

        // 转账交易
        transferFrom(_from, _to, _amount);

        // 添加账本信息
        RewardTaskLedger memory _taskLedger;
        _taskLedger.taskId = _taskId;
        _taskLedger.taskTransactionType = _rewardTaskTransactionType;
        _taskLedger.from = _from;
        _taskLedger.to = _to;
        _taskLedger.amount = _amount;
        _taskLedger.createTime = block.timestamp;
        allLedgers.push(_taskLedger);
        userLedgers[_from].push(_taskLedger);
        userLedgers[_to].push(_taskLedger);
        taskLedgers[_taskId].push(_taskLedger);

        // 更新用户钱包余额信息
        clientWalletMap[_from].tokens = balanceOf(_from);
        clientWalletMap[_to].tokens = balanceOf(_to);
    }

    // 获取所有账本
    function getAllLedgers() view external returns(RewardTaskLedger[] memory) {
        return allLedgers;
    }

    // 获取悬赏任务账本
    function getTaskLedgers(uint256 _taskId) view external returns(RewardTaskLedger[] memory) {
        return taskLedgers[_taskId];
    }

    // 获取用户账本
     function getUserLedgers(address _address) view external returns(RewardTaskLedger[] memory) {
        return userLedgers[_address];
    }
}

