/// @title Smartcontract batch auction and sales in marketplace
/// @notice This contract allows batchs of more than 2 sales or auctions to be executed in one transaction.
/// @author Mariano Salazar A.
contract batchmarket {
    using Strings for uint256;

    address public Market;
    NFTMarket market;
    string public uriSuffix = ".json";

    mapping(address => uint256) public lastid;

    constructor(address _market) {
        Market = _market;
        market = NFTMarket(Market);
    }

    ///@dev Configuration parameters applicable to auctions batchs:
    ///@dev --> _batchamount: number of auctions to be registered in the marketplace.
    ///@dev --> _baseUri: This is the address of the base IPFS with which the metadata will be loaded.
    ///@dev --> ERC20 Token for payment (if specified by the seller) : _erc20Token
    ///@dev --> minimum price : _minPrice
    ///@dev --> buy now price : _buyNowPrice
    ///@dev --> the nft seller: msg.sender
    ///@dev --> The fee recipients & their respective percentages for a sucessful auction/sale
    function batchAuctions(
        uint256 _batchamount,
        string memory _baseUri,
        address _nftContractAddress,
        address _erc20Token,
        uint256 _minPrice,
        uint256 _buyNowPrice,
        uint32 _auctionBidPeriod,
        uint32 _bidIncreasePercentage,
        address[] memory _feeRecipients,
        uint32[] memory _feePercentages
    ) public {
        require(_batchamount > 1, "amount must exceed 1");
        uint256 _tokenId;
        uint256 total = lastid[_nftContractAddress] + _batchamount;
        uint256 id = 0;
        string memory _metadata = "";
        if (lastid[_nftContractAddress] == 0) {
            _tokenId = 0;
        } else {
            _tokenId = lastid[_nftContractAddress];
        }
        for (uint256 i = lastid[_nftContractAddress]; i < total; i++) {
            console.log("i es %s", i);
            _tokenId++;
            console.log("Token ID es %s", _tokenId);
            _metadata = string(
                abi.encodePacked(_baseUri, _tokenId.toString(), uriSuffix)
            );
            console.log("Metadata es %s", _metadata);
            market.createNewNftAuction(
                _nftContractAddress,
                _tokenId,
                _erc20Token,
                _minPrice,
                _buyNowPrice,
                _auctionBidPeriod,
                _bidIncreasePercentage,
                _feeRecipients,
                _feePercentages,
                true,
                _metadata
            );
            id = id + 1;
        }
        lastid[_nftContractAddress] = lastid[_nftContractAddress] + id;
        console.log("lastid es %s", lastid[_nftContractAddress]);
    }

    ///@dev Configuration parameters applicable to sales batchs:
    ///@dev --> _batchamount: number of auctions to be registered in the marketplace.
    ///@dev --> _baseUri: This is the address of the base IPFS with which the metadata will be loaded.
    ///@dev --> ERC20 Token for payment (if specified by the seller) : _erc20Token
    ///@dev --> buy now price : _buyNowPrice
    ///@dev --> the nft seller: msg.sender
    ///@dev --> The fee recipients & their respective percentages for a sucessful auction/sale
    function batchSales(
        uint256 _batchamount,
        string memory _baseUri,
        address _nftContractAddress,
        address _erc20Token,
        uint256 _buyNowPrice,
        address[] memory _feeRecipients,
        uint32[] memory _feePercentages
    ) public {
        require(_batchamount > 1, "amount must exceed 1");
        uint256 _tokenId;
        uint256 total = lastid[_nftContractAddress] + _batchamount;
        uint256 id = 0;
        string memory _metadata = "";
        if (lastid[_nftContractAddress] == 0) {
            _tokenId = 0;
        } else {
            _tokenId = lastid[_nftContractAddress];
        }
        for (uint256 i = lastid[_nftContractAddress]; i < total; i++) {
            console.log("i es %s", i);
            _tokenId++;
            console.log("Token ID es %s", _tokenId);
            _metadata = string(
                abi.encodePacked(_baseUri, _tokenId.toString(), uriSuffix)
            );
            console.log("Metadata es %s", _metadata);
            market.createSale(
                _nftContractAddress,
                _tokenId,
                _erc20Token,
                _buyNowPrice,
                _feeRecipients,
                _feePercentages,
                true,
                _metadata
            );
            id = id + 1;
        }
        lastid[_nftContractAddress] = lastid[_nftContractAddress] + id;
        console.log("lastid es %s", lastid[_nftContractAddress]);
    }
}

