pragma solidity ^0.8.0;

import "./MyToken.sol";
import "./StakingUtil.sol";
import "hardhat/console.sol";
import "@openzeppelin/contracts/utils/Strings.sol";

library Library  {
    struct staker {
        uint256 dayStartedStaking; // TODO: rename to sinceBlockNumber //TODO: should be block number or timestamp??
        uint256 stakedAmount; // TODO: rename to stakingBalance
        bool exists; // might be useful
        //
        uint256 lockupPeriodEnd; 
    }
}

contract Staking {
    // usings
    using Library for Library.staker;

    // constants
    // penalty to pay
    uint8 constant INITIAL_PENALTY_PERCENTAGE = 15; // this is the max penalty percentage to pay (at the beginning)
    uint8 constant MONTHLY_PENALTY_PERCENTAGE_DISCOUNT = 1; // this is how much (in percentage) the penalty percentage reduces per month (it has to be a full month though)
    uint8 constant MINIMUM_PENALTY_PERCENTAGE = 3; // this is the minimum penalty percentage to pay 
    // rewards to get
    uint8 constant DAILY_PERCENTAGE_DISCOUNT = 1; // this is how much (in %) more of your rewards you will get waiting 1 more day (after 100 days there is no difference)
    // percentage allocation of penalty paid
    uint8 constant PERCENTAGE_ALLOCATION_FOR_PENALTY_POOL = 75;
    uint8 constant PERCENTAGE_ALLOCATION_FOR_DEV_POOL = 20;
    uint8 constant PERCENTAGE_ALLOCATION_FOR_ADS_POOL = 3;
    uint8 constant PERCENTAGE_ALLOCATION_FOR_CHARITY_POOL = 2;
    // multiply and divide values to support fractional numbers (decimal points)
    uint256 public constant MULTIPLIED_FOR_DECIMAL_POINTS = 10000000000;
    // pump penalty pool by a percentage from the staking amount (when someone stakes), until the pre penalty pool runs out
    uint8 public constant PERCENTAGE_TO_PUMP_PENALTY_POOL = 10;

    // variables 
    // business logic
    //uint256 currentDayNumber; // TODO: rename to currentBlockNumber ... TODO: does it need to be public??
    uint256 private _previousLastStakerStartDay; // TODO: rename to _previousLastStakerStartBlockNumber
    uint256 private _lastStakerStartDay; // TODO: rename to _lastStakerStartBlockNumber
    uint256 private _totalAmountStaked; // in HodlersPool: totalStakingBalance
    uint256 private _totalNumberOfStakers;
    uint256 private _previousTotalAmountStaked;
    uint256 private _totalCoinsOverTime = 0;
    uint256 private _lastDayTotalCoinsOverTimeCalculation; // TODO: rename to _lastBlockNumberTotalCoinsOverTimeCalculation
    mapping(address => Library.staker) private _allStakers;
    // Pools (Bonus, Penalty, Dev, Ads, Charity)
    uint256 private _bonusPoolBalance;
    uint256 private _prePenaltyPoolBalance; // balance to use to automatically keep refilling the penalty pool (until it runs out)
    uint256 private _penaltyPoolBalance; // the actual penalty pool balance
    uint256 private _devPoolBalance;
    uint256 private _adsPoolBalance;
    uint256 private _charityPoolBalance;
    //
    bool private _hasInitialized = false;
    // other variables
    MyToken private _stakingToken;
    IStakingUtil private _stakingUtil;

    // events
    event Staked(address indexed user, uint256 amount);
    event Unstaked(address indexed user, uint256 amount);
    event PenaltyPaid(address indexed user, uint256 amount);
    event StakingRewardsPaid(address indexed user, uint256 amount);
    event StakingRewardsUserIsEntitledTo(address indexed user, uint256 amount);
    event LockupBonusGiven(uint256 amount);

    // constructor(address tokenAddress) {
    //     _stakingToken = MyToken(tokenAddress);   
    // }

    // modifiers
    modifier toBeCalledOnlyOnce() {
        // TODO: check if this works after upgrading the contract (in theory it should work!)
        require(_hasInitialized == false, "Has been initialized already.");
        _;
    }

    modifier checkLockupHasEnded() {
        //require(_stakingUtil.getLockupPeriodEndOf(_allStakers[msg.sender]) < block.timestamp, "Lockup period has not ended yet.");
        //require(_stakingUtil.getLockupPeriodEndOf(_allStakers[msg.sender]) < _stakingUtil.getBlockTimestamp(), "Lockup period has not ended yet.");
        //require(_allStakers[msg.sender].lockupPeriodEnd < block.timestamp, "Lockup period has not ended yet.");
        //console.log("lockupPeriodEnd: ", _allStakers[msg.sender].lockupPeriodEnd);
        //console.log("getBlockTimestamp: ", _stakingUtil.getBlockTimestamp());
        require(_allStakers[msg.sender].lockupPeriodEnd < _stakingUtil.getBlockTimestamp(), "Lockup period has not ended yet.");
        _;
    }

    // functions
    // function to be called as soon as the contract is deployed (it can be called only once)
    // why not using a constructor?
    // 1- there was a circular dependency, both contracts needed each other
    // 2- this method doesn't have limitations for upgrading the contract
    //function initialize(address tokenAddress, uint256 bonusBalance) external toBeCalledOnlyOnce {
    function initialize(address tokenAddress, IStakingUtil stakingUtil) external toBeCalledOnlyOnce {
        _hasInitialized = true;
        _stakingToken = MyToken(tokenAddress);
        _stakingUtil = stakingUtil;
        _bonusPoolBalance = _stakingToken.getAmountMintedForBonusPool();
        _prePenaltyPoolBalance = _stakingToken.getAmountMintedForPrePenaltyPool();
        _penaltyPoolBalance = 0;
        _devPoolBalance = 0;
        _adsPoolBalance = 0;
        _charityPoolBalance = 0;
        _totalNumberOfStakers = 0;
    }

    function stake(uint256 amount, uint8 lockupInYears) external {
        stakeForStaker(amount, lockupInYears, msg.sender);
    }

    function stakeForStaker(uint256 amount, uint8 lockupInYears, address staker) public {
        require(amount > 0, "Cannot stake 0");
        require(staker != address(0), "Staker address should not be 0x0!");
        require(_allStakers[staker].stakedAmount == 0, "Staker's staking balance has to be 0 to stake");
        //
        pumpPenaltyPool(amount);
        uint256 amountPlusBonuses = handleLockupBonus(amount, lockupInYears, staker);
        // core staking
        _previousTotalAmountStaked = _totalAmountStaked;
        _previousLastStakerStartDay = _lastStakerStartDay;
        _lastStakerStartDay = _stakingUtil.getBlockTimestamp();
        _totalAmountStaked = _totalAmountStaked + amountPlusBonuses;
        _allStakers[staker].dayStartedStaking = _stakingUtil.getBlockTimestamp();
        _allStakers[staker].stakedAmount = amountPlusBonuses;
        _allStakers[staker].exists = true;
        calculateLatestTotalCoinsOverTimeWhenEntering();
        // note we only send the amount as the bonus is already there. And the sender is always msg.sender (regardless of who is the staker)!
        _stakingToken.transferFrom(msg.sender, address(this), amount);
        //console.log("stake: balanceOf =", _stakingToken.balanceOf(staker));
        //
        _totalNumberOfStakers = _totalNumberOfStakers + 1;
        emit Staked(staker, amountPlusBonuses);
    }    

    function unstake() external checkLockupHasEnded {
        require(_allStakers[msg.sender].stakedAmount > 0, "Staker's staking balance has to be greater than 0 to unstake.");
        //
        calculateLatestTotalCoinsOverTimeWhenExiting();
        uint256 percentageToGetFromTaxPool = getLatestPercentageToGetFromTaxPool(_totalCoinsOverTime);
        console.log("percentageToGetFromTaxPool (multiplied by 10000000000): ", percentageToGetFromTaxPool);
        uint256 personalTotalCoinsOverTime = getPersonalTotalCoinsOverTime();
        console.log("personalTotalCoinsOverTime: ", personalTotalCoinsOverTime);
        _totalAmountStaked = _totalAmountStaked - _allStakers[msg.sender].stakedAmount;
        console.log("_allStakers[msg.sender].stakedAmount: ", _allStakers[msg.sender].stakedAmount);
        console.log("_totalCoinsOverTime: ", _totalCoinsOverTime);
        console.log("personalTotalCoinsOverTime: ", personalTotalCoinsOverTime);
        _totalCoinsOverTime = _totalCoinsOverTime - personalTotalCoinsOverTime;        
        console.log("BEFORE TRANSFER..._totalCoinsOverTime: ", _totalCoinsOverTime);
        transferRewardsFromPenaltyPool(percentageToGetFromTaxPool);
        transferStakedAmountSubstractingPenalty();
        delete _allStakers[msg.sender];
        _totalNumberOfStakers = _totalNumberOfStakers - 1;
    }

    // calculates the 10% of the saked amount, and transfers that amount of tokens from the prePenaltyPool to the penaltyPool (until the prePenaltyPool runs out)
    function pumpPenaltyPool(uint256 amount) private {
        uint8 pumpPercentage = _stakingUtil.getPumpPenaltyPoolPercentage(PERCENTAGE_TO_PUMP_PENALTY_POOL); // set the percentage
        uint256 amountToTransfer = amount * pumpPercentage / 100; // get the % of the amount being staked. this is the amount to transfer to the penalty pool
        // check how much the prePenaltyPool has left
        // if it has less than the expected amount, return everything left (and update the amount variable!)
        if (amountToTransfer <= getPrePenaltyPoolBalance()) {
            // there is still enough balance
            _penaltyPoolBalance = getPenaltyPoolBalance() + amountToTransfer; // increase the balance for the penaltyPool
            _prePenaltyPoolBalance = getPrePenaltyPoolBalance() - amountToTransfer; // reduce the prePenaltyPool amount available
        }
        else {
            // there is not enough remaining prePenaltyPool balance, so we'll use ALL the remaining balance...
            _penaltyPoolBalance = getPenaltyPoolBalance() + getPrePenaltyPoolBalance();
            _prePenaltyPoolBalance = 0;
        }
    }

    function handleLockupBonus(uint256 amount, uint8 lockupInYears, address staker) private returns (uint256) {
        uint256 amountPlusBonuses;
        if (lockupInYears > 0) {            
            _allStakers[staker].lockupPeriodEnd = calculateLockupPeriodEnd(lockupInYears);
            //console.log("XXXXXXXXXXX lockupPeriodEnd: ", _allStakers[staker].lockupPeriodEnd);
            //console.log("XXXXXXXXXXX getBlockTimestamp: ", _stakingUtil.getBlockTimestamp());
            uint256 bonus = calculateBonusBasedOnLockupPeriod(amount, lockupInYears);
            // check how much the bonus pool has left
            // if it has less than the expected amount, return everything left (and update the amount variable!)
            if (bonus <= getBonusPoolBalance()) {
                amountPlusBonuses = amount + bonus;
                emit LockupBonusGiven(bonus); //TODO: make sure if tx reverts this event is not emitted
                _bonusPoolBalance = getBonusPoolBalance() - bonus; //reduce the bonus amount available
            }
            else {
                // there is not enough remaining bonus balance, so we'll use ALL the remaining balance...
                amountPlusBonuses = amount + getBonusPoolBalance();
                emit LockupBonusGiven(getBonusPoolBalance()); //TODO: make sure if tx reverts this event is not emitted
                _bonusPoolBalance = 0;
            }
        }
        else
        {
            amountPlusBonuses = amount;
            _allStakers[staker].lockupPeriodEnd = 0; // no lock-up
        }
        //
        return amountPlusBonuses;
    }

    function getPersonalTotalCoinsOverTime() private view returns (uint256) {
        uint256 millisecondsSinceStartedStaking = getMillisecondsSinceStartedStaking(msg.sender);
        //console.log("getPersonalTotalCoinsOverTime ==> millisecondsSinceStartedStaking: ", millisecondsSinceStartedStaking);
        // uint256 daysSinceStartedStaking = getDaysSinceStartedStaking(msg.sender);
        // console.log("getPersonalTotalCoinsOverTime ==> daysSinceStartedStaking: ", daysSinceStartedStaking);
        //console.log("getPersonalTotalCoinsOverTime ==> stakedAmount: ", _allStakers[msg.sender].stakedAmount);
        //console.log("getPersonalTotalCoinsOverTime ==> result: ", millisecondsSinceStartedStaking * _allStakers[msg.sender].stakedAmount);
        //console.log("getPersonalTotalCoinsOverTime ==> result: ", daysSinceStartedStaking * _allStakers[msg.sender].stakedAmount);
        return millisecondsSinceStartedStaking * _allStakers[msg.sender].stakedAmount;
        //return daysSinceStartedStaking * _allStakers[msg.sender].stakedAmount;
    }

    // returns the calculated value of totalCoinsOverTime when exiting
    function calculateTotalCoinsOverTimeWhenExiting() private view returns (uint256) {
        //
        // uint256 differenceInDays = getDaysDifference(getCurrentDayNumber(), _lastDayTotalCoinsOverTimeCalculation);
        // console.log("differenceInDays: ", differenceInDays);
        uint256 differenceInMilliseconds = getMillisecondsDifference(getCurrentDayNumber(), _lastDayTotalCoinsOverTimeCalculation);
        //console.log("differenceInMilliseconds: ", differenceInMilliseconds);
        //
        //console.log("_totalAmountStaked: ", _totalAmountStaked);
        //uint256 totalCoinsOverTime = _totalCoinsOverTime + (differenceInDays * _totalAmountStaked);
        uint256 totalCoinsOverTime = _totalCoinsOverTime + (differenceInMilliseconds * _totalAmountStaked);
        //
        return totalCoinsOverTime;
    }

    // returns the calculated value of totalCoinsOverTime when Entering
    function calculateTotalCoinsOverTimeWhenEntering() private view returns (uint256) {
        //
        // uint256 differenceInDays = getDaysDifference(getCurrentDayNumber(), _previousLastStakerStartDay);
        // console.log("differenceInDays: ", differenceInDays);
        //uint256 differenceInMilliseconds = getMillisecondsDifference(getCurrentDayNumber(), _previousLastStakerStartDay);
        uint256 differenceInMilliseconds = getMillisecondsDifference(getCurrentDayNumber(), _lastDayTotalCoinsOverTimeCalculation);
        //console.log("differenceInMilliseconds: ", differenceInMilliseconds);
        //
        //console.log("_totalAmountStaked: ", _totalAmountStaked);
        //uint256 totalCoinsOverTime = _totalCoinsOverTime + (differenceInDays * _previousTotalAmountStaked);
        uint256 totalCoinsOverTime = _totalCoinsOverTime + (differenceInMilliseconds * _previousTotalAmountStaked);
        //
        return totalCoinsOverTime;
    }

    // function to be called externally when the staker wants to check what's their percentage of the penalty pool they would get if unstaking
    function getPercentageToGetFromPenaltyPool() external view returns (uint256) {
        require(_allStakers[msg.sender].stakedAmount > 0, "Staker's staking balance has to be greater than 0.");
        //
        // offChainTotalCoinsOverTime has the number of total coins over time, but this value is not yet persisted in the blockchain!
        uint256 offChainTotalCoinsOverTime = calculateTotalCoinsOverTimeWhenExiting();
        //console.log("getLatestPercentageToGetFromTaxPool: ", getLatestPercentageToGetFromTaxPool(offChainTotalCoinsOverTime));
        return getLatestPercentageToGetFromTaxPool(offChainTotalCoinsOverTime);
    }

    // This is the method that pays the rewards from the penalty pool
    function transferRewardsFromPenaltyPool(uint256 percentageToGetFromTaxPool) private {
        console.log("BEGIN - transferRewardsFromPenaltyPool");
        // percentageToGetFromTaxPool is multiplied by 10000000000 (to support 9 decimal points), so we need to divide back by 10000000000 before using it
        console.log("getPenaltyPoolBalance(): ", getPenaltyPoolBalance());
        console.log("percentageToGetFromTaxPool: ", percentageToGetFromTaxPool);
        uint256 rewardsStakerIsEntitledTo = ((getPenaltyPoolBalance() * percentageToGetFromTaxPool) / 100) / MULTIPLIED_FOR_DECIMAL_POINTS;
        console.log("rewardsStakerIsEntitledTo: ", rewardsStakerIsEntitledTo);
        uint256 percentageToGet = calculatePercentageOfRewardsToGet();
        console.log("percentageToGet: ", percentageToGet);
        uint256 rewardsToPayToStaker = (rewardsStakerIsEntitledTo * percentageToGet) / 100;
        console.log("rewardsToPayToStaker: ", rewardsToPayToStaker);
        //
        _penaltyPoolBalance = getPenaltyPoolBalance() - rewardsToPayToStaker;
        console.log("NEW getPenaltyPoolBalance(): ", getPenaltyPoolBalance());
        _stakingToken.transfer(msg.sender, rewardsToPayToStaker);
        //
        emit StakingRewardsUserIsEntitledTo(msg.sender, rewardsStakerIsEntitledTo);
        emit StakingRewardsPaid(msg.sender, rewardsToPayToStaker);
        console.log("END transferRewardsFromPenaltyPool");
    }

    // This method pays back the staked amount substracting the penalty to pay for unstaking
    function transferStakedAmountSubstractingPenalty() private {
        // from the penalty paid:
        // 75% goes to penalty pool
        // 20% to dev fund
        // 3% to ads
        // 2% for charity
        console.log("BEGIN transferStakedAmountSubstractingPenalty");
        uint256 stakedAmount = _allStakers[msg.sender].stakedAmount;
        console.log("stakedAmount: ", stakedAmount);
        uint256 penaltyPercentageToPay = calculatePercentageForPenaltyToPay();
        console.log("penaltyPercentageToPay: ", penaltyPercentageToPay);
        uint256 penaltyToPay = (stakedAmount * penaltyPercentageToPay) / 100;
        console.log("penaltyToPay: ", penaltyToPay);
        uint256 amountToPayToStaker = stakedAmount - penaltyToPay;
        console.log("amountToPayToStaker: ", amountToPayToStaker);
        _stakingToken.transfer(msg.sender, amountToPayToStaker);
        //
        uint256 amountForPenaltyPoolBalance = (penaltyToPay * PERCENTAGE_ALLOCATION_FOR_PENALTY_POOL) / 100;
        uint256 amountForDevPoolBalance = (penaltyToPay * PERCENTAGE_ALLOCATION_FOR_DEV_POOL) / 100;
        uint256 amountForAdsPoolBalance = (penaltyToPay * PERCENTAGE_ALLOCATION_FOR_ADS_POOL) / 100;
        uint256 amountForCharityPoolBalance = (penaltyToPay * PERCENTAGE_ALLOCATION_FOR_CHARITY_POOL) / 100;
        console.log("getPenaltyPoolBalance(): ", getPenaltyPoolBalance());
        _penaltyPoolBalance = getPenaltyPoolBalance() + amountForPenaltyPoolBalance;
        console.log("NEW getPenaltyPoolBalance(): ", getPenaltyPoolBalance());
        _devPoolBalance = getDevPoolBalance() + amountForDevPoolBalance;
        _adsPoolBalance = getAdsPoolBalance() + amountForAdsPoolBalance;
        _charityPoolBalance = getCharityPoolBalance() + amountForCharityPoolBalance;
        //
        emit Unstaked(msg.sender, amountToPayToStaker);
        emit PenaltyPaid(msg.sender, penaltyToPay);
        console.log("END transferStakedAmountSubstractingPenalty");
    }
    
    // make it public so people can check what's the percentage they would get discounted from their rewards if they unstake
    function calculatePercentageOfRewardsToGet() public view returns (uint256) {      
        // initially the staker would get 0% of the rewards (if they unstake and have rewards)
        // this percentage keeps increasing 1% daily until it reaches 100%, when they can get all the rewards they are entitled to

        // divide the result of getDaysSinceStartedStaking by 100 (getDaysSinceStartedStaking returns the number of days multiplied by 100 to support 2 decimal points)
        uint256 percentageToGet = getDaysSinceStartedStaking(msg.sender) / 100 * DAILY_PERCENTAGE_DISCOUNT;
        //
        if (percentageToGet < 100)
            return percentageToGet;
        else
            return 100; // 100 is the max! 
    }

    // make it public so people can check what's the percentage they would pay if they unstake
    function calculatePercentageForPenaltyToPay() public view returns (uint256) {      
        // reducing 12%  per year
        // reducing ~ 1% per month

        uint256 monthlyDiscount = MONTHLY_PENALTY_PERCENTAGE_DISCOUNT;
        //console.log("monthlyDiscount : ", monthlyDiscount);
        uint256 discountToApply = getMonthsSinceStartedStaking(msg.sender) * monthlyDiscount;
        //console.log("discountToApply : ", discountToApply);
        if (discountToApply > INITIAL_PENALTY_PERCENTAGE) {
            return MINIMUM_PENALTY_PERCENTAGE;
        }            
        else {
            uint256 penaltyPercentage = INITIAL_PENALTY_PERCENTAGE - discountToApply;
            //
            if (penaltyPercentage > MINIMUM_PENALTY_PERCENTAGE)
                return penaltyPercentage;
            else
                return MINIMUM_PENALTY_PERCENTAGE;
        }
    }

    // returns the percentage to get from the tax pool (multiplied by 1,000,000,000 (1 billion) so we can get up to 9 decimal points)
    function getLatestPercentageToGetFromTaxPool(uint256 totalCoinsOverTime) private view returns (uint256) {
        //console.log("getLatestPercentageToGetFromTaxPool ==> totalCoinsOverTime: ", totalCoinsOverTime);
        return (getPersonalTotalCoinsOverTime() * 100 * MULTIPLIED_FOR_DECIMAL_POINTS) / totalCoinsOverTime; // multiply the result by 10000000000 so we can get up to 9 decimal points
    }

    function getMonthsSinceStartedStaking(address account) public view returns (uint256) {
        // we consider that a month has 30.44 days
        // we have to divide by 3044 as getDaysSinceStartedStaking already returns a value multiplied by 100
        return getDaysSinceStartedStaking(account) / 3044; 
    }

    // returns the number of days since started staking (multiplied by 100 to support 2 decimal points) 
    function getDaysSinceStartedStaking(address account) public view returns (uint256) {
        // multiply milliseconds by 100 first to support 2 decimal points
        return (getMillisecondsSinceStartedStaking(account) * 100 / 1000) / 60 / 60 / 24; 
    }

    function getDaysDifference(uint256 timestamp1, uint256 timestamp2) private pure returns (uint256) {
        return (timestamp1 - timestamp2) / 1000 / 60 / 60 / 24;
    }

    function getMillisecondsSinceStartedStaking(address account) private view returns (uint256) {
        //console.log("IIIIIIIIIIIIII getBlockTimestamp: ", _stakingUtil.getBlockTimestamp());
        //console.log("IIIIIIIIIIIIII dayStartedStaking: ", _allStakers[account].dayStartedStaking);
        return getMillisecondsDifference(_stakingUtil.getBlockTimestamp(), _allStakers[account].dayStartedStaking);
    }

    function getMillisecondsDifference(uint256 timestamp1, uint256 timestamp2) private pure returns (uint256) {
        return timestamp1 - timestamp2;
    }

    function calculateLatestTotalCoinsOverTimeWhenExiting() private {       
        //console.log("------- calculateLatestTotalCoinsOverTimeWhenExiting -----------");
        //console.log("_totalCoinsOverTime: ", _totalCoinsOverTime);
        //console.log("(getCurrentDayNumber(): ", getCurrentDayNumber());
        //console.log("_lastDayTotalCoinsOverTimeCalculation: ", _lastDayTotalCoinsOverTimeCalculation);
        if (getCurrentDayNumber() > _lastDayTotalCoinsOverTimeCalculation) {
            //console.log("IN");
            //console.log("(getCurrentDayNumber(): ", getCurrentDayNumber());
            //console.log("_lastDayTotalCoinsOverTimeCalculation: ", _lastDayTotalCoinsOverTimeCalculation);
            //console.log("_totalAmountStaked: ", _totalAmountStaked);
            _totalCoinsOverTime = calculateTotalCoinsOverTimeWhenExiting();
            //console.log("_totalCoinsOverTime: ", _totalCoinsOverTime);
        }
        //console.log("OUT");
        _lastDayTotalCoinsOverTimeCalculation = getCurrentDayNumber();
        //console.log("_totalCoinsOverTime: ", _totalCoinsOverTime);
        //console.log("------------------");
    }

    function calculateLatestTotalCoinsOverTimeWhenEntering() private {
        //console.log("------- calculateLatestTotalCoinsOverTimeWhenEntering -----------");
        //console.log("_totalCoinsOverTime: ", _totalCoinsOverTime);
        //console.log("(getCurrentDayNumber(): ", getCurrentDayNumber());
        //console.log("_lastDayTotalCoinsOverTimeCalculation: ", _lastDayTotalCoinsOverTimeCalculation);
        //console.log("_totalAmountStaked: ", _totalAmountStaked);
        if (getCurrentDayNumber() > _lastDayTotalCoinsOverTimeCalculation) {
            _totalCoinsOverTime = calculateTotalCoinsOverTimeWhenEntering();
        }
        _lastDayTotalCoinsOverTimeCalculation = getCurrentDayNumber();
        //console.log("_totalCoinsOverTime: ", _totalCoinsOverTime);
        //console.log("------------------");
    }

    function calculateBonusBasedOnLockupPeriod(uint256 amount, uint8 lockupInYears) private pure returns (uint256) {
        uint8 percentage = 0;
        if (lockupInYears == 1) {
            percentage = 10;
        } else if (lockupInYears == 2)  {
            percentage = 25;
        } else if (lockupInYears == 3)  {
            percentage = 40;
        } else if (lockupInYears == 4)  {
            percentage = 60;
        } else if (lockupInYears == 5)  {
            percentage = 80;
        } else if (lockupInYears >= 6)  {
            percentage = 100;
        }
        //
        return (amount * percentage) / 100;
    }

    function calculateLockupPeriodEnd(uint8 lockupInYears) private view returns (uint256) {
        //return _stakingUtil.getBlockTimestamp() + (lockupInYears * 365 days);
        // on average a year has 31556952 seconds (The Gregorian (western) solar calendar has 365.2425 days, taking into account leap years)
        // but we need to multiply that number by 1000 to convert it to milliseconds
        return _stakingUtil.getBlockTimestamp() + (lockupInYears * (31556952 * 1000));
    }

    function getCurrentDayNumber() internal view returns (uint256) {
        return _stakingUtil.getBlockTimestamp(); 
    }

    function getTotalAmountStaked() external view returns (uint256) {
        return _totalAmountStaked;
    }

    function getAmountStakedOf(address account) external view returns (uint256) {
        return _allStakers[account].stakedAmount;
    }

    function getBonusPoolBalance() public view returns (uint256) {
        return _stakingUtil.getBonusPoolBalance(_bonusPoolBalance);
    }

    function getPrePenaltyPoolBalance() public view returns (uint256) {
        //return _prePenaltyPoolBalance;
        return _stakingUtil.getPrePenaltyPoolBalance(_prePenaltyPoolBalance);
    } 

    function getPenaltyPoolBalance() public view returns (uint256) {
        return _penaltyPoolBalance;
    } 

    function getDevPoolBalance() public view returns (uint256) {
        return _devPoolBalance;
    }   

    function getAdsPoolBalance() public view returns (uint256) {
        return _adsPoolBalance;
    }   

    function getCharityPoolBalance() public view returns (uint256) {
        return _charityPoolBalance;
    }

    function getTotalNumberOfStakers() public view returns (uint256) {
        return _totalNumberOfStakers;
    }
}

