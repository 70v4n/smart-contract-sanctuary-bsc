interface IPancakeFactory{
    function createPair(address tokenA, address tokenB)
        external
        returns (address pair);
}

contract OXFIV3 is IERC20, Ownable, AccessControl {
    using SafeMath for uint256;

    address DEAD = 0x000000000000000000000000000000000000dEaD;
    IERC20 constant BUSD = IERC20(0x78867BbEeF44f2326bF8DDd1941a4439382EF2A7);
    bytes32 public constant DESTROYER_ROLE = keccak256("DESTROYER_ROLE");
    OXFIvault public immutable OXFIVault;

    string constant _name = "OXFI"; // Name
    string constant _symbol = "OX"; // Symbol
    uint8 constant _decimals = 18; // Decimals

    uint256 _totalSupply = 900_000 ether; // Total Supply
    uint256 public _maxTxAmount = _totalSupply.div(100); // 1%

    mapping(address => uint256) _balances;
    mapping(address => mapping(address => uint256)) _allowances;

    mapping(address => bool) isFeeExempt;
    mapping(address => bool) isTxLimitExempt;
    mapping(address => bool) isDividendExempt;
    mapping(address => bool) isSniper;

    // buy tax fee
    uint256 public vaultFeeOnBuying = 70;   // 7% will be added to the OXFI vaulr
    uint256 public marketFeeOnBuying = 30;  // 3% will be added to the liquidity pool
    uint256 private buyFees;     // fee counter
    uint256 private marketFees;  // fee counter


    // sell tax fee
    uint256 public liquidityFeeOnSelling = 50;  // 5% will be added to the liquidity pool
    uint256 public burnFeeOnSelling = 50;       // 5% will be destroyed

    // for smart contract use
    uint256 _vaultFee;
    uint256 _liquidityFee;
    uint256 _burnFee;
    uint256 _marketingFee;
    uint256 feeDenominator = 1000;
    uint256 totalFee = 150;

    uint256 targetLiquidity = 45;
    uint256 targetLiquidityDenominator = 100;

    address public autoLiquidityReceiver = 0x0600061016d090f004925000D80008e0fA1610c0;
    address public marketingFeeReceiver = 0x0600061016d090f004925000D80008e0fA1610c0;

    IPancakeRouter public router;
    address public pair;

    uint256 public launchedAt;
    uint256 public launchedAtTimestamp;
    uint256 public antiSnipingTime = 60 seconds;

    address public vaultAddress;

    bool public swapEnabled;
    bool public tradingOpen;
    bool private antibot;

    uint256 public swapThreshold = _totalSupply / 2000; // 0.005%
    bool inSwap;
    modifier swapping() {
        inSwap = true;
        _;
        inSwap = false;
    }

    constructor()  {
        router = IPancakeRouter(0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3);
        pair = IPancakeFactory(router.factory()).createPair(address(this),address(BUSD));

        // Create vault
        OXFIVault = new OXFIvault(address(this));
        vaultAddress = address(OXFIVault);

        isFeeExempt[msg.sender] = true;
        isFeeExempt[address(this)] = true;
        isFeeExempt[vaultAddress] = true;
        isFeeExempt[marketingFeeReceiver] = true;
        isTxLimitExempt[msg.sender] = true;
 
        //approve(addressrouter, _totalSupply);
        //approve(address(this), _totalSupply);
        //approve(address(pair), _totalSupply);

        _allowances[address(this)][address(router)] = _totalSupply;
        _allowances[address(this)][address(pair)] = _totalSupply;
        _allowances[address(this)][address(this)] = _totalSupply;


        // balances and roles
        _balances[vaultAddress] = 600_000 ether;          // 600_000 to presale
        _balances[msg.sender] += 240_000 ether;         // 240_000 liquidity
        _balances[marketingFeeReceiver] = 60_000 ether;   // 60_000 to marketing

        _setupRole(DESTROYER_ROLE, vaultAddress);
        _setupRole(DESTROYER_ROLE, address(this));
        emit Transfer(address(0), vaultAddress, 600_000 ether);
        emit Transfer(address(0), vaultAddress, 240_000 ether);
        emit Transfer(address(0), marketingFeeReceiver, 60_000 ether);
    }

    function viewFees() public view returns (uint256[] memory) {
        uint256[] memory fees = new uint256[](2);
        fees[0] = buyFees;
        fees[1] = marketFees;
        return fees;
    }


    receive() external payable {}

    function totalSupply() external view override returns (uint256) {
        return _totalSupply;
    }

    function decimals() external pure returns (uint8) {
        return _decimals;
    }

    function symbol() external pure returns (string memory) {
        return _symbol;
    }

    function name() external pure returns (string memory) {
        return _name;
    }

    function getOwner() external view returns (address) {
        return owner();
    }

    function balanceOf(address account) public view override returns (uint256) {
        return _balances[account];
    }

    function allowance(address holder, address spender)
        public
        view
        override
        returns (uint256)
    {
        return _allowances[holder][spender];
    }

    function approve(address spender, uint256 amount)
        public
        override
        returns (bool)
    {
        _allowances[msg.sender][spender] = amount;
        emit Approval(msg.sender, spender, amount);
        return true;
    }

    function approveMax(address spender) external returns (bool) {
        return approve(spender, _totalSupply);
    }

    function transfer(address recipient, uint256 amount)
        external
        override
        returns (bool)
    {
        return _transferFrom(msg.sender, recipient, amount);
    }

    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) external override returns (bool) {
        if (_allowances[sender][msg.sender] != _totalSupply) {
            _allowances[sender][msg.sender] = _allowances[sender][msg.sender]
                .sub(amount, "Insufficient Allowance");
        }

        return _transferFrom(sender, recipient, amount);
    }

    function _transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) internal returns (bool) {
        // if (!isTxLimitExempt[sender] && !isTxLimitExempt[recipient]) {
        //     // trading disable till launch
        //     if (!tradingOpen) {
        //         require(
        //             sender != pair && recipient != pair,
        //             "Trading is not enabled yet"
        //         );
        //     }
        //     // antibot
        //     if (
        //         block.timestamp < launchedAtTimestamp + antiSnipingTime &&
        //         sender != address(router)
        //     ) {
        //         if (sender == pair) {
        //             isSniper[recipient] = true;
        //         } else if (recipient == pair) {
        //             isSniper[sender] = true;
        //         }
        //     }

        //     require(amount <= _maxTxAmount, "TX Limit Exceeded");
        // }

        if (inSwap) {
            console.log("[_basicTransfer]");
            return _basicTransfer(sender, recipient, amount);
        }

        if (shouldSwapBack()) {
            console.log("[shouldSwapBack]");
            swapBack();
        }

        _balances[sender] = _balances[sender].sub( amount, "Insufficient Balance");

        uint256 tFee;
        if (isFeeExempt[sender] || isFeeExempt[recipient]) {
            console.log("Excluded no fee");
            _balances[recipient] = _balances[recipient].add(amount);
            emit Transfer(sender, recipient, amount);
        }
        // selling handler
        else if (recipient == pair) {
            //require(!antibot, "Bots restricted");
            console.log("[SELL]");

            setSellFee();
            tFee = totalFeePerTx(amount);
            _balances[recipient] = _balances[recipient].add(amount.sub(tFee));
            emit Transfer(sender, recipient, amount.sub(tFee));
            _takeSellFee(sender, amount);
        }
        // buying && normal transaction handler
        else {
            console.log("[BUY]");

            setBuyFee();
            tFee = totalFeePerTx(amount);
            _balances[recipient] = _balances[recipient].add(amount.sub(tFee));
            emit Transfer(sender, recipient, amount.sub(tFee));
            _takeBuyFee(sender, amount);
        }

        return true;
    }

    function _basicTransfer(
        address sender,
        address recipient,
        uint256 amount
    ) internal returns (bool) {
        if(recipient == pair){
            console.log("Token address is", address(this));
            console.log("%s sends pair %s", sender, amount);
        }
        _balances[sender] = _balances[sender].sub(amount,"Insufficient Balance");
        _balances[recipient] = _balances[recipient].add(amount);
        emit Transfer(sender, recipient, amount);
        console.log("[_basicTransfer returns true]");
        return true;
    }
    function shouldTakeFee(address sender) internal view returns (bool) {
        return !isFeeExempt[sender];
    }

    function totalFeePerTx(uint256 amount)
        public
        view
        returns (uint256 feeAmount)
    {
        feeAmount = amount * 1000 / 10000; // 10%
    }

    // take fees for buy
    function _takeBuyFee(address sender, uint256 tAmount) internal {
        console.log("[_takeBuyFee]");

        //require(!isSniper[sender], "Sniper detected");
        uint256 vFee = tAmount.mul(_vaultFee).div(1e3);
        uint256 lFee = tAmount.mul(_liquidityFee).div(1e3);
        _balances[address(this)] = _balances[address(this)].add(vFee.add(lFee));
        buyFees += vFee.add(lFee);
        emit Transfer(sender, address(this), vFee.add(lFee));
    }

    // take fees for sell
    function _takeSellFee(address sender, uint256 tAmount) internal {
        //require(!isSniper[sender], "Sniper detected");
        uint256 bFee = tAmount.mul(_burnFee).div(1e3);
        uint256 mFee = tAmount.mul(_marketingFee).div(1e3);
        _balances[address(this)] = _balances[address(this)].add(bFee.add(mFee));
        marketFees += bFee.add(mFee);
        emit Transfer(sender, address(this), bFee.add(mFee));
    }



    function shouldSwapBack() internal view returns (bool) {
        return
            msg.sender != pair &&
            !inSwap &&
            //swapEnabled &&
            _balances[address(this)] >= swapThreshold;
    }

    function swapBack() internal swapping {
        uint256 dynamicLiquidityFee = isOverLiquified(
            targetLiquidity,
            targetLiquidityDenominator
        )
            ? 0
            : liquidityFeeOnSelling;
        uint256 amountToLiquify = swapThreshold
            .mul(dynamicLiquidityFee)
            .div(totalFee)
            .div(2);
        uint256 amountToSwap = swapThreshold.sub(amountToLiquify);

        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = address(BUSD);
        uint256 balanceBefore = BUSD.balanceOf(address(this));
        uint256 oxfiBalance = balanceOf(address(this));

        //approve(address(router), amountToSwap);

        //uint256 oxfiAllowanceThis = allowance(address(this),address(this));
        uint256 oxfiAllowancePancake = allowance(address(this), address(router));
        uint256 oxfiAllowancePair = allowance(address(this), pair);
        console.log("[Contract Balance]-", oxfiBalance);
        console.log("[amountToSwap]-----", amountToSwap);
        console.log("[Allowance Router]- %s",  oxfiAllowancePancake);
        console.log("[Allowance Pair]--- %s",  oxfiAllowancePair);
        //console.log("[oxfiAllowanceThis]    %s", oxfiAllowanceThis);

        router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            amountToSwap,
            0,
            path,
            address(this),
            block.timestamp + 360
        );

        console.log("[swapExactTokensForTokensSupportingFeeOnTransferTokens]");


        uint256 amountBUSD = BUSD.balanceOf(address(this)).sub(balanceBefore);

        uint256 totalBNBFee = totalFee.sub(dynamicLiquidityFee.div(2));

        uint256 amountBUSDLiquidity = amountBUSD
            .mul(dynamicLiquidityFee)
            .div(totalBNBFee)
            .div(2);

        if (amountToLiquify > 0) {
            router.addLiquidityETH{value: amountBUSDLiquidity}(
                address(this),
                amountToLiquify,
                0,
                0,
                autoLiquidityReceiver,
                block.timestamp
            );
            emit AutoLiquify(amountBUSDLiquidity, amountToLiquify);
        }
    }

    function setBuyFee() private {
        _vaultFee = vaultFeeOnBuying;
        _marketingFee = marketFeeOnBuying;
    }

    function setSellFee() private {
        _liquidityFee = liquidityFeeOnSelling;
        _burnFee = burnFeeOnSelling;
    }

    function launch() public onlyOwner {
        require(launchedAt == 0, "Already launched boi");
        launchedAt = block.number;
        launchedAtTimestamp = block.timestamp;
        tradingOpen = true;
        swapEnabled = true;
    }

    function setTxLimit(uint256 amount) external onlyOwner {
        require(amount >= _totalSupply / 1000);
        _maxTxAmount = amount;
    }

    function setTargetLiquidity(uint256 _target, uint256 _denominator)
        external
        onlyOwner
    {
        targetLiquidity = _target;
        targetLiquidityDenominator = _denominator;
    }


    function setIsFeeExempt(address holder, bool exempt) external onlyOwner {
        isFeeExempt[holder] = exempt;
    }

    function setIsTxLimitExempt(address holder, bool exempt)
        external
        onlyOwner
    {
        isTxLimitExempt[holder] = exempt;
    }

    function setFeeReceivers(
        address _autoLiquidityReceiver,
        address _marketingFeeReceiver
    ) external onlyOwner {
        autoLiquidityReceiver = _autoLiquidityReceiver;
        marketingFeeReceiver = _marketingFeeReceiver;
    }

    function setSwapBackSettings(bool _enabled, uint256 _amount)
        external
        onlyOwner
    {
        swapEnabled = _enabled;
        swapThreshold = _amount;
    }

    function getCirculatingSupply() public view returns (uint256) {
        return _totalSupply.sub(balanceOf(DEAD)).sub(balanceOf(address(0)));
    }

    function addSniperInList(address _account) external onlyOwner {
        require(_account != address(router), "We can not blacklist router");
        require(!isSniper[_account], "Sniper already exist");
        isSniper[_account] = true;
    }

    function removeSniperFromList(address _account) external onlyOwner {
        require(isSniper[_account], "Not a sniper");
        isSniper[_account] = false;
    }

    function enableOrDisableAntibot(bool _status) external onlyOwner {
        antibot = _status;
    }

    function getLiquidityBacking(uint256 accuracy)
        public
        view
        returns (uint256)
    {
        return accuracy.mul(balanceOf(pair).mul(2)).div(getCirculatingSupply());
    }

    function isOverLiquified(uint256 target, uint256 accuracy)
        public
        view
        returns (bool)
    {
        return getLiquidityBacking(accuracy) > target;
    }

    event AutoLiquify(uint256 amountBUSD, uint256 amountBOG);
    event BuybackMultiplierActive(uint256 duration);
}

