pragma solidity ^0.8.0;

import "./interfaces/IConstants.sol";
import "./interfaces/ISessionRegistrar.sol";
import "./interfaces/ISessionFees.sol";

abstract contract SessionFees is ISessionFees {

    FeeStores public override feeStores;
    mapping(SessionType => FeeRates) public override feeRates;

    modifier canChangeFees virtual;

    function setFeeStores(FeeStores memory _feeStores) public override virtual canChangeFees {
        require(_feeStores.develop != address(0) && _feeStores.buyback != address(0) && _feeStores.liquidity != address(0), "Zero address");
        feeStores = _feeStores;
    }

    function setFeeRates(SessionType _sessionType, FeeRates memory _feeRates) public override virtual canChangeFees {
        require(uint256(_sessionType) < NumberSessionTypes, "Wrong SessionType");
        require(_feeRates.develop + _feeRates.buyback + _feeRates.liquidity < FeeMagnifier, "Fee rates exceed limit");

        feeRates[_sessionType] = _feeRates;
    }

    function initializeFees(FeeStores memory _feeStores, FeeRates[NumberSessionTypes] memory _feeRatesArray) public override virtual canChangeFees {
        setFeeStores(_feeStores);
        for(uint256 i = 0; i < NumberSessionTypes; i ++) {
            feeRates[SessionType(i)] = _feeRatesArray[i];
        }
    }

    modifier onlySessionManager virtual;
    function payFeeImplementation(address account, uint256 principal, FeeRates memory rates, bool fromAllowance ) public override virtual onlySessionManager returns (uint256 feesPaid) {
    }

}

