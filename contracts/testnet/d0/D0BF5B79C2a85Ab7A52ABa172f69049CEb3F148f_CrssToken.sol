import "./interfaces/IWiredToken.sol";
import "./interfaces/IWiredMaker.sol";
import "./interfaces/IWiredTaker.sol";
import "./interfaces/IWiredFarm.sol";

import "./WireBase.sol";

abstract contract WiredToken is IWiredToken, WireBase {

    address public override maker;
    address public override taker;
    address public override farm;

    string private constant sForbidden = "WiredToken: Forbidded";
    string private constant sZeroAddress = "WiredToken: Zero address";

    modifier pairInformer () override virtual {
        require(msg.sender == maker, sForbidden);
        _;
    }

    modifier canWire() virtual;
    function getOwner() external view override virtual returns (address);

    function _isExternalCall() internal view virtual returns (bool) {
        return ! (msg.sender == maker || msg.sender == taker || msg.sender == farm);
    }

    function setMaker(address _maker) public override virtual canWire {
        require(msg.sender == IWiredMaker(_maker).getOwner(), sForbidden);
        require(_maker != address(0), sZeroAddress);
        maker = _maker;
        emit SetMaker(_maker);
    }

    function setTaker(address _taker) public override virtual canWire {
        require(msg.sender == IWiredTaker(_taker).getOwner(), sForbidden);
        require(_taker != address(0), sZeroAddress);
        taker = _taker;
        emit SetTaker(_taker);
    }

    function setFarm(address _farm) external override virtual canWire {
        require(msg.sender == IWiredFarm(_farm).getOwner(), sForbidden);
        require(_farm != address(0), sZeroAddress);
        farm = _farm;
        emit SetFarm(_farm);
    }

    function informOfPair(address pair, address token0, address token1) public override(IWireBase, WireBase) virtual pairInformer {
        super.informOfPair(pair, token0, token1);
        IWiredFarm(farm).informOfPair(pair, token0, token1);
    }
}

