contract MainToken is ERC20, Pausable  
{
    using SafeMath for uint256;
    
    mapping (address => bool) private blacklist;
    bool private cooldownEnabled = false;
    IUniswapV2Router02 private uniswapV2Router;
    address private uniswapV2Pair;
    mapping(address => uint256) private cooldown;
    address public adminAddress = 0xeE2f1c0E59393A37a7Cef741B85694a7023dc64A; //admin address

    constructor() ERC20("Chipz", "CHPZ") {
      IUniswapV2Router02 _uniswapV2Router =
            IUniswapV2Router02(0xD99D1c33F9fC3444f8101754aBC46c52416550D1); //BSC testnet
      uniswapV2Router = _uniswapV2Router;
      uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
          .createPair(address(this), _uniswapV2Router.WETH());
      _mint(adminAddress, 105000000 * (10**18));
    }
    
    function transferFrom(address _from, address _to, uint256 _value) public override returns (bool _success) {
        require(!paused);
        require(!isBlacklist(msg.sender), "Chipz: sender blacklisted");
        require(!isBlacklist(_to), "Chipz: recipient blacklisted");
        require(!isBlacklist(tx.origin), "Chipz: sender blacklisted");
        if (
            _from == uniswapV2Pair &&
            _to != address(uniswapV2Router) &&
            cooldownEnabled
        ) {
            require(cooldown[_to] < block.timestamp);
            cooldown[_to] = block.timestamp.add(30);
        }
        return super.transferFrom(_from, _to, _value);
    }

    function transfer(address _to, uint256 _value) public override returns (bool _success) {
        require(!paused);
        require(!isBlacklist(msg.sender), "Chipz: sender blacklisted");
        require(!isBlacklist(_to), "Chipz: recipient blacklisted");
        require(!isBlacklist(tx.origin), "Chipz: sender blacklisted");
        if (
            msg.sender == uniswapV2Pair &&
            _to != address(uniswapV2Router) &&
            cooldownEnabled
        ) {
            require(cooldown[_to] < block.timestamp);
            cooldown[_to] = block.timestamp.add(30);
        }
        return super.transfer(_to, _value);
    }

    /**
     * @dev Blacklists the specified account (Disables transfers to and from the account).
     */
    function addBlacklist(address account) public onlyOwner {
        require(!blacklist[account], "Chipz: Account is already blacklisted");
        blacklist[account] = true;
    }
    
    /**
     * @dev Remove the specified account from the blacklist.
     */
    function removeBlacklist(address account) public onlyOwner {
        require(blacklist[account], "Chipz: Account is not blacklisted");
        blacklist[account] = false;
    }

    /**
     * @dev Returns true if the account is blacklisted, and false otherwise.
     */
    function isBlacklist(address account) public view returns (bool) {
        return blacklist[account];
    }

    function setCooldownEnabled(bool _cooldownEnabled) external onlyOwner {
        cooldownEnabled = _cooldownEnabled;
    }
