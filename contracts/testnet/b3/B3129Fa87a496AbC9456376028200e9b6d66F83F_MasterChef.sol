// File: contracts/MasterChef.sol



pragma solidity >=0.8.0;





contract MasterChef is Ownable {
    using SafeMath for uint256;
    using SafeBEP20 for IBEP20;

    // Info of each user.
    struct UserInfo {
        uint256 amount; // How many LP tokens the user has provided.
        uint256 rewardDebt;
        //
        // The amount of pending WUST entitled to a user is:
        //   pending reward = (user.amount * pool.accWustPerShare) - user.rewardDebt
        //
        // Whenever a user deposits or withdraws LP tokens to a pool. Here's what happens:
        //   1. The pool's 'accWustPerShare' (and 'lastRewardBlock') gets updated.
        //   2. User receives the pending reward sent to his/her address.
        //   3. User's `amount` gets updated.
        //   4. User's `rewardDebt` gets updated.
    }

    // Info of each pool.
    struct PoolInfo {
        IBEP20 lpToken; // Address of LP token contract.
        uint256 allocPoint; // How many allocation points assigned to this pool. WUST to distribute per block.
        uint256 lastRewardBlock; // Last block number that WUST distribution occurs.
        uint256 accWustPerShare; // Accumulated WUST per share, times 1e12.
        uint16 depositFeeBP; // Deposit fee in basis points
    }

    // The WUST token
    WraithDust public wust;
    // WUST tokens created per block.
    uint256 public wustPerBlock;
    // Deposit Fee address
    address public treasuryAddress;
    // Dev address.
    address public devAddress;

    // Info of each pool.
    PoolInfo[] public poolInfo;
    // Info of each user that stakes LP tokens.
    mapping(uint256 => mapping(address => UserInfo)) public userInfo;
    // Total allocation points. Must be the sum of all allocation points in all pools.
    uint256 public totalAllocPoint = 0;
    // The block number when WUST mining starts.
    uint256 public startBlock;
    // Total staked WUST to display on frontend
    uint256 public stakedWust = 0;

    event Deposit(address indexed user, uint256 indexed pid, uint256 amount);
    event Withdraw(address indexed user, uint256 indexed pid, uint256 amount);
    event EmergencyWithdraw(
        address indexed user,
        uint256 indexed pid,
        uint256 amount
    );

    constructor(
        WraithDust _wust,
        address _devaddr,
        address _treasuryAddress,
        uint256 _wustPerBlock,
        uint256 _startBlock
    ) {
        wust = _wust;
        devAddress = _devaddr;
        treasuryAddress = _treasuryAddress;
        wustPerBlock = _wustPerBlock;
        startBlock = _startBlock;
    }

    function poolLength() external view returns (uint256) {
        return poolInfo.length;
    }

    // Add a new lp to the pool. Can only be called by the owner.
    // XXX DO NOT add the same LP token more than once. Rewards will be messed up if you do.
    function add(
        uint256 _allocPoint,
        IBEP20 _lpToken,
        uint16 _depositFeeBP,
        bool _withUpdate
    ) public onlyOwner {
        require(
            _depositFeeBP <= 10000,
            "add: invalid deposit fee basis points"
        );
        if (_withUpdate) {
            massUpdatePools();
        }
        uint256 lastRewardBlock = block.number > startBlock
            ? block.number
            : startBlock;
        totalAllocPoint = totalAllocPoint.add(_allocPoint);
        poolInfo.push(
            PoolInfo({
                lpToken: _lpToken,
                allocPoint: _allocPoint,
                lastRewardBlock: lastRewardBlock,
                accWustPerShare: 0,
                depositFeeBP: _depositFeeBP
            })
        );
    }

    // Update the given pool's WUST allocation point and deposit fee. Can only be called by the owner.
    function set(
        uint256 _pid,
        uint256 _allocPoint,
        uint16 _depositFeeBP,
        bool _withUpdate
    ) public onlyOwner {
        require(
            _depositFeeBP <= 10000,
            "set: invalid deposit fee basis points"
        );
        if (_withUpdate) {
            massUpdatePools();
        }
        totalAllocPoint = totalAllocPoint.sub(poolInfo[_pid].allocPoint).add(
            _allocPoint
        );
        poolInfo[_pid].allocPoint = _allocPoint;
        poolInfo[_pid].depositFeeBP = _depositFeeBP;
    }

    // Return reward multiplier over the given _from to _to block.
    function getMultiplier(uint256 _from, uint256 _to)
        public
        pure
        returns (uint256)
    {
        return _to.sub(_from);
    }

    // View function to see pending WUST on frontend.
    function pendingWust(uint256 _pid, address _user)
        external
        view
        returns (uint256)
    {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_user];
        uint256 accWustPerShare = pool.accWustPerShare;
        uint256 lpSupply = pool.lpToken.balanceOf(address(this));
        if (block.number > pool.lastRewardBlock && lpSupply != 0) {
            uint256 multiplier = getMultiplier(
                pool.lastRewardBlock,
                block.number
            );
            uint256 wustReward = multiplier
            .mul(wustPerBlock)
            .mul(pool.allocPoint)
            .div(totalAllocPoint);

            accWustPerShare = accWustPerShare.add(
                wustReward.mul(1e12).div(lpSupply)
            );
        }
        return user.amount.mul(accWustPerShare).div(1e12).sub(user.rewardDebt);
    }

    // Update reward variables for all pools. Be careful of gas spending!
    function massUpdatePools() public {
        uint256 length = poolInfo.length;
        for (uint256 pid = 0; pid < length; ++pid) {
            updatePool(pid);
        }
    }

    // Update reward variables of the given pool to be up-to-date.
    function updatePool(uint256 _pid) public {
        PoolInfo storage pool = poolInfo[_pid];
        if (block.number <= pool.lastRewardBlock) {
            return;
        }
        uint256 lpSupply = pool.lpToken.balanceOf(address(this));
        if (lpSupply == 0 || pool.allocPoint == 0) {
            pool.lastRewardBlock = block.number;
            return;
        }
        uint256 multiplier = getMultiplier(pool.lastRewardBlock, block.number);
        uint256 wustReward = multiplier
        .mul(wustPerBlock)
        .mul(pool.allocPoint)
        .div(totalAllocPoint);
        wust.mint(devAddress, wustReward.div(10));
        wust.mint(address(this), wustReward);
        pool.accWustPerShare = pool.accWustPerShare.add(
            wustReward.mul(1e12).div(lpSupply)
        );
        pool.lastRewardBlock = block.number;
    }

    // Deposit LP tokens to MasterChef for WUST allocation.
    function deposit(uint256 _pid, uint256 _amount) public {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        updatePool(_pid);
        if (user.amount > 0) {
            uint256 pending = user
            .amount
            .mul(pool.accWustPerShare)
            .div(1e12)
            .sub(user.rewardDebt);
            if (pending > 0) {
                safeWustTransfer(msg.sender, pending);
            }
        }
        if (_amount > 0) {
            pool.lpToken.safeTransferFrom(
                address(msg.sender),
                address(this),
                _amount
            );
            if (pool.depositFeeBP > 0) {
                uint256 depositFee = _amount.mul(pool.depositFeeBP).div(10000);
                pool.lpToken.safeTransfer(treasuryAddress, depositFee);
                user.amount = user.amount.add(_amount).sub(depositFee);
                if (pool.lpToken == wust) {
                    stakedWust = stakedWust.add(_amount).sub(depositFee);
                }
            } else {
                user.amount = user.amount.add(_amount);
                if (pool.lpToken == wust) {
                    stakedWust = stakedWust.add(_amount);
                }
            }
        }
        user.rewardDebt = user.amount.mul(pool.accWustPerShare).div(1e12);
        emit Deposit(msg.sender, _pid, _amount);
    }

    // Withdraw LP tokens from MasterChef.
    function withdraw(uint256 _pid, uint256 _amount) public {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        require(user.amount >= _amount, "withdraw: not good");
        updatePool(_pid);
        uint256 pending = user.amount.mul(pool.accWustPerShare).div(1e12).sub(
            user.rewardDebt
        );
        if (pending > 0) {
            safeWustTransfer(msg.sender, pending);
        }
        if (_amount > 0) {
            user.amount = user.amount.sub(_amount);
            pool.lpToken.safeTransfer(address(msg.sender), _amount);
            if (pool.lpToken == wust) {
                stakedWust = stakedWust.sub(_amount);
            }
        }
        user.rewardDebt = user.amount.mul(pool.accWustPerShare).div(1e12);
        emit Withdraw(msg.sender, _pid, _amount);
    }

    // Withdraw without caring about rewards. EMERGENCY ONLY.
    function emergencyWithdraw(uint256 _pid) public {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        uint256 amount = user.amount;
        user.amount = 0;
        user.rewardDebt = 0;
        pool.lpToken.safeTransfer(address(msg.sender), amount);
        emit EmergencyWithdraw(msg.sender, _pid, amount);
    }

    // Safe wust transfer function, just in case if rounding error causes pool to not have enough WUST.
    function safeWustTransfer(address _to, uint256 _amount) internal {
        uint256 wustBal = wust.balanceOf(address(this));
        if (_amount > wustBal) {
            wust.transfer(_to, wustBal);
        } else {
            wust.transfer(_to, _amount);
        }
    }

    function setDevAddress(address _devaddr) public {
        require(msg.sender == devAddress, "dev: wut?");
        devAddress = _devaddr;
    }

    function setTreasuryAddress(address _treasuryAddress) public {
        require(msg.sender == treasuryAddress, "setTreasuryAddress: FORBIDDEN");
        treasuryAddress = _treasuryAddress;
    }

    function updateEmissionRate(uint256 _wustPerBlock) public onlyOwner {
        massUpdatePools();
        wustPerBlock = _wustPerBlock;
    }
}