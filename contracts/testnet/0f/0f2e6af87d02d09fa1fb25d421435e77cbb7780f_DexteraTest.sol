contract DexteraTest is Context, IBEP20, Ownable {
    using SafeMath for uint256;
    using Address for address;
    
    address payable public marketingAddress = payable(0xA747bbc59611049A137b4b1c8132D8e3B14980C1); // Marketing Address
    address public constant deadAddress = 0x000000000000000000000000000000000000dEaD;
    mapping (address => uint256) private _balances;
    mapping (address => mapping (address => uint256)) private _allowances;

    mapping (address => bool) private _isExcludedFromFee;

    mapping (address => bool) private _isExcluded;
    address[] private _excluded;
   
    uint256 private constant _tTotal = 10000000000 * 10**9;

    string private constant _name = "DexteraTest";
    string private constant _symbol = "DXTAT";
    uint8 private constant _decimals = 9;


    uint256 public _buysMarketingFee = 2; // Buys Marketing Fee 
    uint256 private _previousBuysMarketingFee = _buysMarketingFee;
    uint256 public _sellsMarketingFee = 2; // Sells Marketing Fee 
    uint256 private _previousSellsMarketingFee = _sellsMarketingFee;
    
    uint256 public _buysLiquidityFee = 3; // Buys liquidity Fee
    uint256 private _previousBuysLiquidityFee = _buysLiquidityFee;
    uint256 public _sellsLiquidityFee = 2; // Sells liquidity Fee
    uint256 private _previousSellsLiquidityFee = _sellsLiquidityFee;

    uint256 public _buyBackFee = 5; // Sells liquidity Fee
    uint256 private _previousBuyBackFee = _buyBackFee;
    
    
    uint256 public _maxTxAmount = 100000000 * 10**9;

    IPancakeRouter02 public immutable pancakeRouter;
    address public immutable pancakePair;
    
    bool inSwapAndLiquify;
    bool public _swapAndLiquifyEnabled = false;
    bool public _buyBackEnabled = true;

    event SwapAndLiquify(
        uint256 tokensSwapped,
        uint256 ethReceived,
        uint256 tokensIntoLiqudity
    );
    
    event SwapETHForTokens(
        uint256 amountIn,
        address[] path
    );
    
    event SwapTokensForETH(
        uint256 amountIn,
        address[] path
    );
    
    event SwapTokensForTokens(uint256 amountIn, address[] path);

    modifier lockTheSwap {
        inSwapAndLiquify = true;
        _;
        inSwapAndLiquify = false;
    }
    
    constructor () {
        _balances[_msgSender()] = _tTotal;
        
        IPancakeRouter02 _pancakeRouter = IPancakeRouter02(0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3);
        pancakePair = IPancakeFactory(_pancakeRouter.factory()).createPair(address(this), _pancakeRouter.WETH());

        pancakeRouter = _pancakeRouter;

        
        _isExcludedFromFee[owner()] = true;
        _isExcludedFromFee[address(this)] = true;
        
        emit Transfer(address(0), _msgSender(), _tTotal);
    }

    function name() public view returns (string memory) {
        return _name;
    }

    function symbol() public view returns (string memory) {
        return _symbol;
    }

    function decimals() public view returns (uint8) {
        return _decimals;
    }

    function totalSupply() public view override returns (uint256) {
        return _tTotal;
    }

    function balanceOf(address account) public view override returns (uint256) {
        return _balances[account];
    }

    function transfer(address recipient, uint256 amount) public override returns (bool) {
        _transfer(_msgSender(), recipient, amount);
        return true;
    }

    function allowance(address owner, address spender) public view override returns (uint256) {
        return _allowances[owner][spender];
    }

    function approve(address spender, uint256 amount) public override returns (bool) {
        _approve(_msgSender(), spender, amount);
        return true;
    }

    function transferFrom(address sender, address recipient, uint256 amount) public override returns (bool) {
        _transfer(sender, recipient, amount);
        _approve(sender, _msgSender(), _allowances[sender][_msgSender()].sub(amount, "BEP20: transfer amount exceeds allowance"));
        return true;
    }

    function increaseAllowance(address spender, uint256 addedValue) public virtual returns (bool) {
        _approve(_msgSender(), spender, _allowances[_msgSender()][spender].add(addedValue));
        return true;
    }

    function decreaseAllowance(address spender, uint256 subtractedValue) public virtual returns (bool) {
        _approve(_msgSender(), spender, _allowances[_msgSender()][spender].sub(subtractedValue, "BEP20: decreased allowance below zero"));
        return true;
    }

    function isExcludedFromReward(address account) public view returns (bool) {
        return _isExcluded[account];
    }

    function buysMarketingFee() public view returns (uint256) {
        return _buysMarketingFee;
    }
    function sellsMarketingFee() public view returns (uint256) {
        return _sellsMarketingFee;
    }
    function buysLiquidityFee() public view returns (uint256) {
        return _buysLiquidityFee;
    }
    function sellsLiquidityFee() public view returns (uint256) {
        return _sellsLiquidityFee;
    }
    function buyBackFee() public view returns (uint256) {
        return _buyBackFee;
    }
    
  


    function _approve(address owner, address spender, uint256 amount) private {
        require(owner != address(0), "BEP20: approve from the zero address");
        require(spender != address(0), "BEP20: approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) private {
        require(from != address(0), "BEP20: transfer from the zero address");
        require(to != address(0), "BEP20: transfer to the zero address");
        require(amount > 0, "Transfer amount must be greater than zero");
        if(from != owner() && to != owner()) {
            require(amount <= _maxTxAmount, "Transfer amount exceeds the maxTxAmount.");
        }
        uint256 newAmount = amount;
        uint256 marketingAmount = 0;
        uint256 liquidityAmount = 0;
        uint256 buyBackAmount = 0;
        uint256 initialBalance = address(this).balance;
        if(_isExcludedFromFee[to] || _isExcludedFromFee[from]){
            _buysMarketingFee = 0;
            _buysLiquidityFee = 0;
            _sellsMarketingFee = 0;
            _sellsLiquidityFee = 0;
            _buyBackFee = 0;
        }
        if(from==pancakePair){
            if(_swapAndLiquifyEnabled){
                marketingAmount = amount.mul(_buysMarketingFee).div(100);
                liquidityAmount = amount.mul(_buysLiquidityFee).div(100);
                newAmount = amount.sub(marketingAmount).sub(liquidityAmount);
                if(!inSwapAndLiquify){
                    swapAndLiquify(marketingAmount, liquidityAmount, initialBalance);
                }
            }
        } else if(to==pancakePair){
            if(_swapAndLiquifyEnabled){
                marketingAmount = amount.mul(_sellsMarketingFee).div(100);
                liquidityAmount = amount.mul(_sellsLiquidityFee).div(100);
                buyBackAmount = amount.mul(_buyBackFee).div(100);
                newAmount = newAmount.sub(marketingAmount).sub(liquidityAmount);
                if(_buyBackEnabled && buyBackAmount > 0){
                    newAmount = newAmount.sub(buyBackAmount);
                }
                if(!inSwapAndLiquify){
                    if(_buyBackEnabled && buyBackAmount > 0){
                        swapAndLiquifyWithBuyBack(marketingAmount, liquidityAmount, buyBackAmount, initialBalance);
                    } else if(!_buyBackEnabled){
                        swapAndLiquify(marketingAmount, liquidityAmount, initialBalance);
                    }
                }
            }
        }
        _balances[from] = _balances[from].sub(newAmount);
        _balances[to] = _balances[to].add(newAmount);

        emit Transfer(from, to, newAmount);
    }   
    
    function swapAndLiquify(uint256 marketingAmount, uint256 liquidityAmount, uint256 initialBalance) private lockTheSwap{
        swapTokensForEth(address(this), address(this), marketingAmount.add(liquidityAmount.div(2)));
        uint256 transferredETH = (address(this).balance).sub(initialBalance);
        uint256 transferredETHForMarketing = transferredETH.mul(marketingAmount).div(marketingAmount.add(liquidityAmount.div(2)));
        uint256 transferredETHForLiquidity = transferredETH.mul(liquidityAmount.div(2)).div(marketingAmount.add(liquidityAmount.div(2)));
        marketingAddress.transfer(transferredETHForMarketing);
        addLiquidity(liquidityAmount.div(2), transferredETHForLiquidity);
        emit SwapAndLiquify(marketingAmount.add(liquidityAmount.div(2)), transferredETHForLiquidity, liquidityAmount.div(2));
    }
    function swapAndLiquifyWithBuyBack(uint256 marketingAmount, uint256 liquidityAmount, uint256 buyBackAmount, uint256 initialBalance) private lockTheSwap{
        swapTokensForEth(address(this), address(this), marketingAmount.add(liquidityAmount.div(2)).add(buyBackAmount.div(2)));
        uint256 transferredETH = (address(this).balance).sub(initialBalance);
        uint256 transferredETHForMarketing = transferredETH.mul(marketingAmount).div(marketingAmount.add(liquidityAmount.div(2)).add(buyBackAmount.div(2)));
        uint256 transferredETHForLiquidity = transferredETH.mul(liquidityAmount.div(2)).div(marketingAmount.add(liquidityAmount.div(2)).add(buyBackAmount.div(2)));
        uint256 transferredETHForBuyBack = transferredETH.mul(buyBackAmount.div(2)).div(marketingAmount.add(liquidityAmount.div(2)).add(buyBackAmount.div(2)));
        marketingAddress.transfer(transferredETHForMarketing);
        addLiquidity((liquidityAmount.div(2)).add(buyBackAmount.div(2)), transferredETHForLiquidity.add(transferredETHForBuyBack));
        emit SwapAndLiquify(marketingAmount.add(liquidityAmount.div(2)).add(buyBackAmount.div(2)), transferredETHForLiquidity.add(transferredETHForBuyBack), (liquidityAmount.div(2)).add(buyBackAmount.div(2)));
    }

    function swapTokensForEth(
        address tokenAddress,
        address toAddress,
        uint256 tokenAmount
    ) private {
        // generate the pancake pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = tokenAddress;
        path[1] = pancakeRouter.WETH();

        IBEP20(tokenAddress).approve(address(pancakeRouter), tokenAmount);

        // make the swap
        pancakeRouter.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            toAddress, // The contract
            block.timestamp.add(300)
        );
    }
    
    function swapETHForTokens(
        address tokenAddress,
        address toAddress,
        uint256 amount
    ) private {
        // generate the pancake pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = pancakeRouter.WETH();
        path[1] = tokenAddress;

        // make the swap
        pancakeRouter.swapExactETHForTokensSupportingFeeOnTransferTokens{
            value: amount
        }(
            0, // accept any amount of Tokens
            path,
            toAddress, // The contract
            block.timestamp.add(300)
        );
    }
    

    function addLiquidity(uint256 tokenAmount, uint256 ethAmount) private {
        // approve token transfer to cover all possible scenarios
        _approve(address(this), address(pancakeRouter), tokenAmount);

        // add the liquidity
        pancakeRouter.addLiquidityETH{value: ethAmount}(
            address(this),
            tokenAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            owner(),
            block.timestamp.add(300)
        );
    }

