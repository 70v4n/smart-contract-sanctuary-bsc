pragma solidity ^0.8.0;


import "../OpenZeppelin/ERC20Token.sol";

import "../OpenZeppelin/Burnable.sol";
import "../OpenZeppelin/SafeMath.sol";


contract PLOZ is ERC20Token, Burnable {
    
    using SafeMath for uint256;
   
    constructor(
        address _developmentLock, 
        address _tokenSale,
        address _gameLock,
        address _stakingLock,
        address _treasuryLock,
        address _marketingLock,
        address _advisorLock,
        address _helper
    ) ERC20Token("PLOZ Token", "PLOZ") {

        uint256 supply = 230000000   * 10 ** decimals();
      
        //Setting helper address for whitelist contracts
        setHelper(_helper);

        //Mint 15% to Development Wallet Timelock
        _mint(_developmentLock, supply.div(100).mul(15));   

        //Mint 15% to Token Sale Timelock
        _mint(_tokenSale, supply.div(100).mul(15));

        //Mint 30% to Game Timelock
        _mint(_gameLock, supply.div(100).mul(30));

        //Mint 30% to Staking Timelock
        _mint(_stakingLock, supply.div(100).mul(30));

        //Mint 2% to Treasury Timelock
        _mint(_treasuryLock, supply.div(100).mul(2));
      
        //Mint 5% to Marketing Timelock
       _mint(_marketingLock, supply.div(100).mul(5));

        //Mint 3% to Advisor Timelock
        _mint(_advisorLock, supply.div(100).mul(3));
    }
    
   

}

