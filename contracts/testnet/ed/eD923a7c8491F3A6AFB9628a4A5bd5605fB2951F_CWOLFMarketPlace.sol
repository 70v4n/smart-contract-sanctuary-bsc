  event Approval(address indexed owner, address indexed spender, uint256 value);
}
contract CWOLFMarketPlace {
    
    using SafeMath for uint256;
    address public CWOLFContractAddress = 0x0e5903749fCa586a0d22F2d190A49008D21894a6;
    address public variablesContractAddress = 0x49f1FbdBF25A55F220dd22aea728B621c9223212;
    address public rewardsPoolAddress = 0xA2fB3C193d11D5B7e7C07475DAa3fbFC43553EA6;
    address public commissionWalletAddress = 0xA2fB3C193d11D5B7e7C07475DAa3fbFC43553EA6;
    uint256 public idCount = 0;
    mapping(uint256 =>  bytes32) public nftId;
    mapping(address => bool) public nftContractsAllowed;

    mapping(bytes32 => address) public tokenAddress;
    mapping(bytes32 => uint256) public tokenId;
    mapping(bytes32 => address) public sellerAddress;
    mapping(bytes32 => uint256) public askingPriceInDollars;
    event itemAdded(
        bytes32 indexed id,
        address tokenAddress,
        uint256 tokenId,
        address sellerAddress,
        uint256 askingPriceInDollars
    );

    event itemRemoved(bytes32 indexed id);

    event itemSold(
        bytes32 indexed id,
        address tokenAddress,
        uint256 tokenId,
        address sellerAddress,
        uint256 askingPriceInDollars,
        address buyerAddress
    );

    function addItemToMarket(
        address _tokenAddress,
        uint256 _tokenId,
        uint256 _askingPriceInDollars
    )   external
        returns (bytes32) {
        IERC721 tokenContract = IERC721(_tokenAddress);
        require(tokenContract.ownerOf(_tokenId) == msg.sender,"Owner can add item");
        require(tokenContract.getApproved(_tokenId) == address(this),"Not approved");
        // // Check if sender is owner not necessary. Checked in approve.
        // // Approve contract to move this token
        // tokenContract.approve(address(this), _tokenId);
        // Generate id
        bytes32 id = keccak256(abi.encode(_tokenAddress, _tokenId));
        require(sellerAddress[id] != msg.sender,"Item is already up for sale");
        // Save data to mappings
        nftId[idCount]=id;
        idCount = idCount + 1;
        tokenAddress[id] = _tokenAddress;
        tokenId[id] = _tokenId;
        sellerAddress[id] = msg.sender;
        askingPriceInDollars[id] = _askingPriceInDollars;
        emit itemAdded(
            id,
            _tokenAddress,
            _tokenId,
            msg.sender,
            _askingPriceInDollars
        );

        return id;
    }

    function getSellerAddress(
        address _tokenAddress,
        uint256 _tokenId
    ) public view returns (address) {
        // Generate id
        bytes32 id = keccak256(abi.encode(_tokenAddress, _tokenId));
        return sellerAddress[id];
    }

    function removeItem(bytes32 id) external returns (bytes32) {
        // TODO: Remove element if is burned or transferred (WolfsNFT, MaterialsNFT, WolfPacksNFT)

        require(
            sellerAddress[id] == msg.sender || nftContractsAllowed[msg.sender] == true,
            "Not allowed"
        );

        delete tokenAddress[id];
        delete tokenId[id];
        delete sellerAddress[id];
        delete askingPriceInDollars[id];
        for (uint256 i = 0; i < idCount; i++){
            if(nftId[i] == id){
                delete nftId[i];
                idCount = idCount - 1;
            }
        }
        emit itemRemoved(id);

        return (id);
    }

    function buyItem(bytes32 _id) external payable {
        require(msg.sender != sellerAddress[_id], "Seller can not buy");

        // Front-end: Need allowance to spend his CWOLF
        address _tokenAddress = tokenAddress[_id];
        uint256 _tokenId = tokenId[_id];
        address _sellerAddress = sellerAddress[_id];
        uint256 _askingPriceInDollars = askingPriceInDollars[_id];

        uint256 _sellPriceInCWOLF = Variables(variablesContractAddress)
            .getDollarsInCWOLF(_askingPriceInDollars);

        // Send 85% CWOLF to sellerAddress, 15% to rewardsPool
        IBEP20(CWOLFContractAddress).transferFrom(
            msg.sender,
            _sellerAddress,
            (_sellPriceInCWOLF * 85 * (10**9)) / 100
        );

        IBEP20(CWOLFContractAddress).transferFrom(
            msg.sender,
            rewardsPoolAddress,
            (_sellPriceInCWOLF * 15 * (10**9)) / 100
        );

        // Send 5% commission in BNB
        uint256 commissionInBNB = calculateCommission(_id);
        payable(commissionWalletAddress).transfer(commissionInBNB);

        // Transfer the token
        IERC721(_tokenAddress).safeTransferFrom(
            _sellerAddress,
            msg.sender,
            _tokenId
        );
        delete tokenAddress[_id];
        delete tokenId[_id];
        delete sellerAddress[_id];
        delete askingPriceInDollars[_id];
        for (uint256 i = 0; i < idCount; i++){
            if(nftId[i] == _id){
                delete nftId[i];
                idCount = idCount - 1;
            }
        }
        emit itemSold(
            _id,
            _tokenAddress,
            _tokenId,
            _sellerAddress,
            _askingPriceInDollars,
            msg.sender
        );
    }

    function calculateCommission(bytes32 _id) public view returns (uint256) {
        uint256 _commissionInDollars = (askingPriceInDollars[_id] * 5 * (10**9)) /
            100;
        uint256 _commissionInBNB = _commissionInDollars /
            Variables(variablesContractAddress).priceBNB();
        return _commissionInBNB;
    }
}