// File: contracts/Access/GameAccessManager.sol

abstract contract GameAccessManager is IGameAccessManager, AccessControlEnumerable, GameRoles
{
    //========================
    // ATTRIBUTES
    //========================

    //super admin
    address public superAdmin;

    //========================
    // EVENTS
    //========================

    event SuperAdminTransferred(address _old, address _new);

    //========================
    // CONSTRUCT
    //========================

    constructor()
    {   
        //init access control              
        _setRoleAdmin(ROLE_ADMIN, ROLE_SUPER_ADMIN);                
        _setRoleAdmin(ROLE_SECURITY_ADMIN, ROLE_SUPER_ADMIN);
        _setRoleAdmin(ROLE_SECURITY_MOD, ROLE_SUPER_ADMIN);
        _setRoleAdmin(ROLE_GAME_MANAGER, ROLE_SUPER_ADMIN);
        _setRoleAdmin(ROLE_TOKEN_MINTER, ROLE_SUPER_ADMIN);
        _setRoleAdmin(ROLE_TOKEN_BURNER, ROLE_SUPER_ADMIN);
        _setRoleAdmin(ROLE_NFT_MINTER, ROLE_SUPER_ADMIN);
        _setRoleAdmin(ROLE_NFT_BURNER, ROLE_SUPER_ADMIN);

        //setup roles
        _setupRole(ROLE_SUPER_ADMIN, msg.sender);
        _setupRole(ROLE_GAME_MANAGER, address(this));
        _setupRole(ROLE_NFT_MINTER, address(this));
        superAdmin = msg.sender;
    }

    //========================
    // FUNCTIONS
    //========================

    function transferSuperAdmin(address _newSuperAdmin) external
    {
        require(hasRole(ROLE_SUPER_ADMIN, msg.sender), "User is not SuperAdmin");
        require(_newSuperAdmin != superAdmin, "User already is Superadmin");
        _setupRole(ROLE_SUPER_ADMIN, _newSuperAdmin);
        renounceRole(ROLE_SUPER_ADMIN, superAdmin);
        superAdmin = _newSuperAdmin; 
        emit SuperAdminTransferred(msg.sender, superAdmin);  
    }

    //========================
    // SECURITY FUNCTIONS
    //========================

    function isAdmin(address _user) private view returns (bool)
    {
        return hasRole(ROLE_SUPER_ADMIN, _user)
            || hasRole(ROLE_ADMIN, _user);
    }

    function requireAdmin(address _user) public view
    {
        require(
            isAdmin(_user),
            "User is not Admin");
    }

    function requireTokenMinter(address _user) public view
    {
        require(
            hasRole(ROLE_TOKEN_MINTER, _user),
            "User is not TokenMinter");
    }

    function requireTokenBurner(address _user) public view
    {
        require(
            hasRole(ROLE_TOKEN_BURNER, _user),
            "User is not TokenBurner");
    }

    function requireNFTMinter(address _user) public view
    {
        require(
            hasRole(ROLE_NFT_MINTER, _user),
            "User is not NFTMinter");
    }

    function requireNFTBurner(address _user) public view
    {
        require(
            hasRole(ROLE_NFT_BURNER, _user),
            "User is not NFTBurner");
    }

    function requireGameManager(address _user) public view
    {
        require(
            isAdmin(_user)
                || hasRole(ROLE_GAME_MANAGER, _user),
            "User is not Admin/Manager");
    }

    function requireSecurityAdmin(address _user) public view
    {
        require(
            isAdmin(_user)
                || hasRole(ROLE_SECURITY_ADMIN, _user),
            "User is not Admin/SecurityAdmin");
    }

    function requireSecurityMod(address _user) public view
    {
        require(
            isAdmin(_user)
                || hasRole(ROLE_SECURITY_ADMIN, _user)
                || hasRole(ROLE_SECURITY_MOD, _user),
            "User is not Admin/SecurityAdmin/SecurityMod");
    }
}

