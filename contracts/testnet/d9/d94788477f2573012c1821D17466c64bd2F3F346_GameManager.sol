// File: contracts/GameManager.sol

contract GameManager is IGameManager, GameAccessManager
{
    //========================
    // LIBS
    //========================

    using SafeERC20 for IGameToken;
    using SafeMath for uint256;

    //========================
    // ATTRIBUTES
    //========================

    //general
    IGameToken public override token;
    IRandom public override randoms;       
    ILevels public override levels; 
    IReward public override reward;   
    uint256 public mintCounter;

    //payment
    address public override devWallet;

    //workers
    IWorkers public override workers;
    uint256 public override workerPrice;
    uint256 public override workerBurnValue;
    uint256[] public workerRarities;
    uint256 public override maxWorkers;    
    uint256 public override workerDevFee;

    //teams
    ITeams public override teams;
    uint256 public override teamPrice;
    uint256 public override teamBurnValue;
    uint256[] public teamRarities;    
    uint256 public override maxTeams;
    uint256 public override teamDevFee;

    //organizations    
    IOrganizations public override organizations;
    uint256 public override organizationPrice;
    uint256 public override organizationBurnValue;
    uint256 public override maxTeamsPerOrganization;
    uint256 public override levelLockDuration;
    uint256 public override organizationDevFee;

    //tax    
    uint256 public override minTax;
    uint256 public override maxTax;    
    uint256 public override reduceTax;
    uint256 public override reduceTaxInterval;

    //========================
    // CREATE
    //========================

    constructor()
    {
        //worker rarity
        uint256[] memory wRar = new uint256[](5);
        wRar[0] = 50;
        wRar[1] = 30;
        wRar[2] = 15;
        wRar[3] = 4;
        wRar[4] = 1;
        _setWorkerRarities(wRar);

        //team rarity
        uint256[] memory tRar = new uint256[](5);
        tRar[0] = 50;
        tRar[1] = 30;
        tRar[2] = 15;
        tRar[3] = 4;
        tRar[4] = 1;
        _setTeamRarities(tRar);

        //init
        _setPrices(10 ether, 10 ether, 1 ether);
        _setBurnValues(2 ether, 2 ether, 0 ether);
        _setTax(0, 2500, 100, 1 days);
        _setDevFees(2000, 2000, 2000);
        devWallet = msg.sender;
    }  

    //========================
    // CONFIG MODULES FUNCTIONS
    //========================  

    function setToken(IGameToken _token) external
    {
        requireAdmin(msg.sender);        
        token = _token;
    }

    function setRandoms(IRandom _randoms) external
    {
        requireAdmin(msg.sender);        
        randoms = _randoms;
    }

    function setReward(IReward _reward) external
    {
        requireAdmin(msg.sender);        
        reward = _reward;
    }

    function setLevels(ILevels _levels) external
    {
        requireAdmin(msg.sender);
        levels = _levels;
    }

    function setWorkers(IWorkers _workers) external
    {
        requireAdmin(msg.sender);
        token.safeApprove(address(_workers), type(uint256).max);
        workers = _workers;
    }

    function setTeams(ITeams _teams) external
    {
        requireAdmin(msg.sender);
        token.safeApprove(address(_teams), type(uint256).max);
        teams = _teams;
    }

    function setOrganizations(IOrganizations _organizations) external
    {
        requireAdmin(msg.sender);
        token.safeApprove(address(_organizations), type(uint256).max);
        organizations = _organizations;
    }

    //========================
    // CONFIG FUNCTIONS
    //========================

    function setDevWallet(address _devWallet) external
    {
        requireAdmin(msg.sender);
        devWallet = _devWallet;
    }

    function setDevFees(uint256 _workerDevFee, uint256 _teamDevFee, uint256 _organizationDevFee) external
    {
        requireAdmin(msg.sender);
        _setDevFees(_workerDevFee, _teamDevFee, _organizationDevFee);
    }

    function _setDevFees(uint256 _workerDevFee, uint256 _teamDevFee, uint256 _organizationDevFee) private
    {
        workerDevFee = (_workerDevFee == 0 ? workerDevFee : _workerDevFee);
        teamDevFee = (_teamDevFee == 0 ? teamDevFee : _teamDevFee);
        organizationDevFee = (_organizationDevFee == 0 ? organizationDevFee : _organizationDevFee);
    }

    function setTax(uint256 _minTax, uint256 _maxTax, uint256 _reduceTax, uint256 _reduceTaxInterval) external
    {
        requireAdmin(msg.sender);
        _setTax(_minTax, _maxTax, _reduceTax, _reduceTaxInterval);
    }

    function _setTax(uint256 _minTax, uint256 _maxTax, uint256 _reduceTax, uint256 _reduceTaxInterval) private
    {
        require(_minTax <= _maxTax, "Min Tax invalid");
        require(_reduceTax <= _maxTax, "Reduce Tax invalid");

        minTax = _minTax;
        maxTax = _maxTax;
        reduceTax = _reduceTax;
        reduceTaxInterval = _reduceTaxInterval;
    }

    function setBurnValues(uint256 _workerBurnValue, uint256 _teamBurnValue, uint256 _organizationBurnValue) external
    {
        requireAdmin(msg.sender);
        _setBurnValues(_workerBurnValue, _teamBurnValue, _organizationBurnValue);
    }

    function _setBurnValues(uint256 _workerBurnValue, uint256 _teamBurnValue, uint256 _organizationBurnValue) private
    {
        workerBurnValue = (_workerBurnValue == 0 ? workerBurnValue : _workerBurnValue);
        teamBurnValue = (_teamBurnValue == 0 ? teamBurnValue : _teamBurnValue);
        organizationBurnValue = (_organizationBurnValue == 0 ? organizationBurnValue : _organizationBurnValue);
    }
    
    function setPrices(uint256 _workerPrice, uint256 _teamPrice, uint256 _organizationPrice) external
    {
        requireAdmin(msg.sender);
        _setPrices(_workerPrice, _teamPrice, _organizationPrice);
    }

    function _setPrices(uint256 _workerPrice, uint256 _teamPrice, uint256 _organizationPrice) private
    {
        workerPrice = (_workerPrice == 0 ? workerPrice : _workerPrice);
        teamPrice = (_teamPrice == 0 ? teamPrice : _teamPrice);
        organizationPrice = (_organizationPrice == 0 ? organizationPrice : _organizationPrice);
    }

    function setWorkerRarities(uint256[] memory _rarities) external
    {
        requireAdmin(msg.sender);
        _setWorkerRarities(_rarities);
    }

    function _setWorkerRarities(uint256[] memory _rarities) private
    {
        workerRarities = _rarities;
    }

    function setTeamRarities(uint256[] memory _rarities) external
    {
        requireAdmin(msg.sender);
        _setTeamRarities(_rarities);
    }

    function _setTeamRarities(uint256[] memory _rarities) private
    {
        teamRarities = _rarities;
    }

    //========================
    // MINT FUNCTIONS
    //========================

    function getWorkerRarities() external override view returns (uint256[] memory)
    {
        return workerRarities;
    }

    function getTeamRarities() external override view returns (uint256[] memory)
    {
        return teamRarities;
    }

    //========================
    // MINT FUNCTIONS
    //========================

    function mintWorker() external
    {
        if (workerPrice > 0)
        {
            token.safeTransferFrom(msg.sender, address(this), workerPrice);
            addReward(payDevs(workerPrice, workerDevFee));
        }
        workers.mint(msg.sender, mintCounter);
        mintCounter = mintCounter.add(1);
    }

    function mintTeam() external
    {
        if (teamPrice > 0)
        {
            token.safeTransferFrom(msg.sender, address(this), teamPrice);
            addReward(payDevs(teamPrice, teamDevFee));
        }
        teams.mint(msg.sender, mintCounter);
        mintCounter = mintCounter.add(1);
    }

    function mintOrganization() external
    {
        if (organizationPrice > 0)
        {
            token.safeTransferFrom(msg.sender, address(this), organizationPrice);
            addReward(payDevs(organizationPrice, organizationDevFee));
        }
        organizations.mint(msg.sender, mintCounter);
        mintCounter = mintCounter.add(1);
    }

    //========================
    // BURN FUNCTIONS
    //========================

    function burnWorker(uint256 _id) external
    {
        require(msg.sender == workers.ownerOf(_id), "Not Owner");
        workers.burn(_id);
    }

    function burnTeam(uint256 _id) external
    {
        require(msg.sender == teams.ownerOf(_id), "Not Owner");
        teams.burn(_id);
    }

    function burnOrganization(uint256 _id) external
    {
        require(msg.sender == organizations.ownerOf(_id), "Not Owner");
        organizations.burn(_id);
    }

    //========================
    // HELPER FUNCTIONS
    //========================

    function addReward(uint256 _amount) private
    {
        if (_amount > 0)
        {
            token.safeTransfer(address(reward), _amount);
        }
    }

    function payDevs(uint256 _amount, uint256 _fee) private returns (uint256)
    {
        uint256 devShare = _amount.mul(_fee).div(10000);
        if (devShare > 0)
        {
            token.safeTransfer(devWallet, devShare);
        }
        return _amount.sub(devShare);        
    }
}