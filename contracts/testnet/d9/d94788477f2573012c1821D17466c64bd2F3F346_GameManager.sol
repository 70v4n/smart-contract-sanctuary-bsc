    //========================
    // ATTRIBUTES
    //========================

    //roles
    bytes32 public constant ROLE_SUPER_ADMIN = keccak256("ROLE_SUPER_ADMIN"); //role management + admin
    bytes32 public constant ROLE_ADMIN = keccak256("ROLE_ADMIN"); //highest security. required to change important settings (security risk)    
    bytes32 public constant ROLE_SECURITY_ADMIN = keccak256("ROLE_SECURITY_ADMIN"); //can pause and unpause (no security risk, but trust is required)
    bytes32 public constant ROLE_SECURITY_MOD = keccak256("ROLE_SECURITY_MOD"); //can pause but not unpause (no security risk, minimal trust required)

    //game
    bytes32 public constant ROLE_GAME_MANAGER = keccak256("ROLE_GAME_MANAGER"); //required to change settings to optimize behaviour (no security risk, but trust is required)

    //token
    bytes32 public constant ROLE_TOKEN_MINTER = keccak256("ROLE_TOKEN_MINTER"); //can mint tokens (high security risk, even admins are not allowed to do this!)
    bytes32 public constant ROLE_TOKEN_BURNER = keccak256("ROLE_TOKEN_BURNER"); //can burn tokens (high security risk, even admins are not allowed to do this!)

    //nft
    bytes32 public constant ROLE_NFT_MINTER = keccak256("ROLE_NFT_MINTER"); //can mint NFTs (high security risk, even admins are not allowed to do this!)
    bytes32 public constant ROLE_NFT_BURNER = keccak256("ROLE_NFT_BURNER"); //can burn NFTs (high security risk, even admins are not allowed to do this!)
}

