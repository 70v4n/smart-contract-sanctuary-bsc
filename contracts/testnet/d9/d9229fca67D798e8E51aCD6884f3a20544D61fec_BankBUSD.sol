pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/math/Math.sol";

// import "@openzeppelin/contracts/access/Ownable.sol";
// import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";

import "./Worker.sol";
import "./IPancakeRouter02.sol";
import "hardhat/console.sol";

contract BankBUSD is OwnableUpgradeable, ReentrancyGuardUpgradeable {
    using SafeMath for uint256;

    address public dev;

    // uint256 public constant CEIL_SLOPE_1 = 60e18;
    // uint256 public constant CEIL_SLOPE_2 = 90e18;
    // uint256 public constant CEIL_SLOPE_3 = 100e18;

    // uint256 public constant MAX_INTEREST_SLOPE_1 = 20e16;
    // uint256 public constant MAX_INTEREST_SLOPE_2 = 20e16;
    // uint256 public constant MAX_INTEREST_SLOPE_3 = 150e16;

    uint256 public constant MAX_BORROW = 8000; // 80%
    uint256 public constant BORROW_INTERATE_RATE = 1000; // 10%
    uint256 public constant LIQUIDATE_BPS = 500; // 5%

    uint256 public constant RESERVE_POOL_BPS = 300; // 3%

    Worker public worker;
    address public busdToken;
    uint256 public lastAccrueBlock;
    uint256 public lastAccrueTime;
    uint256 public vaultDebtShare;
    uint256 public vaultDebtVal;
    uint256 public reservePool;

    struct BorrowPosition {
        address owner;
        address collateralToken;
        uint256 collateralAmount;
        uint256 debtShare;
        uint256 startBlock;
        uint256 interateRate;
    }

    mapping(uint256 => BorrowPosition) public borrowPositions;
    uint256 public nextPositionID;

    // user => share
    mapping(address => uint256) public shares;

    /// @dev Require that the caller must be an EOA account if not whitelisted.
    modifier onlyEOAorWhitelisted() {
        require(msg.sender == tx.origin, "not eoa");
        _;
    }

    modifier transferToVault(address token, uint256 value) {
        if (token == address(0)) {
            require(msg.value == value, "amount!");
        } else {
            IERC20(token).transferFrom(
                address(msg.sender),
                address(this),
                value
            );
        }
        _;
    }

    modifier accrue(uint256 value) {
        if (block.number > lastAccrueTime) {
            console.log("block.number: ", block.number);
            console.log("value: ", value);
            uint256 interest = pendingInterest(value);
            console.log("interest: ", interest);
            uint256 toReserve = interest.mul(RESERVE_POOL_BPS).div(10000);
            console.log("toReserve: ", toReserve);
            reservePool = reservePool.add(toReserve);
            console.log("reservePool: ", reservePool);
            vaultDebtVal = vaultDebtVal.add(interest);
            console.log("vaultDebtVal: ", vaultDebtVal);
            lastAccrueTime = block.number;
        }
        _;
    }

    // constructor(
    //     address _busdToken,
    //     address _worker,
    //     address _dev
    // ) {
    //     busdToken = _busdToken;
    //     worker = _worker;
    //     lastAccrueTime = block.number;
    //     dev = _dev;
    // }

    function initialize(
        address _busdToken,
        Worker _worker,
        address _dev
    ) public initializer {
        busdToken = _busdToken;
        worker = _worker;
        lastAccrueTime = block.number;
        dev = _dev;
    }

    function totalBUSD() public view returns (uint256) {
        return totalSupply().add(vaultDebtVal).sub(reservePool);
    }

    function totalSupply() public view returns (uint256) {
        return IERC20(busdToken).balanceOf(address(this));
    }

    function deposit(uint256 amount)
        external
        payable
        transferToVault(busdToken, amount)
        accrue(amount)
        nonReentrant
    {
        // uint256 total = totalBUSD().sub(amount);
        // uint256 share = total == 0
        //     ? amount
        //     : amount.mul(totalSupply()).div(total);

        shares[msg.sender] += debtValToShare(amount);
        require(totalSupply() > 1e17, "no tiny shares");
    }

    function pendingInterest(uint256 value) public view returns (uint256) {
        if (block.number > lastAccrueTime) {
            console.log("pendingInterest block.number: ", block.number);
            uint256 timePast = block.number.sub(lastAccrueTime);
            console.log("pendingInterest timePast: ", timePast);
            uint256 balance = totalSupply().sub(value);
            console.log("pendingInterest balance: ", balance);
            uint256 ratePerSec = interestRate(vaultDebtVal, balance);
            console.log("pendingInterest ratePerSec: ", ratePerSec);
            return ratePerSec.mul(vaultDebtVal).mul(timePast).div(1e18);
        } else {
            return 0;
        }
    }

    function withdraw(uint256 share) external accrue(0) nonReentrant {
        // console.log("withdraw share: ", share);
        // console.log("withdraw totalBUSD: ", totalBUSD());
        // console.log("withdraw totalSupply: ", totalSupply());
        // uint256 amount = share.mul(totalBUSD()).div(totalSupply());

        uint256 amount = debtShareToVal(share);
        shares[msg.sender] -= share;
        IERC20(busdToken).transfer(msg.sender, amount);
        require(totalSupply() > 1e17, "no tiny shares");
    }

    function interestRate(uint256 debt, uint256 floating)
        public
        view
        returns (uint256)
    {
        console.log("interestRate debt ", debt);
        console.log("interestRate floating ", floating);
        uint256 total = debt.add(floating);
        uint256 utilization = total == 0 ? 0 : debt.mul(100e18).div(total);
        if (utilization < 80e18) {
            console.log("interestRate ============ here: ", utilization);
            // Less than 80% utilization - 0%-20% APY
            return utilization.mul(20e16).div(80e18) / 365 days;
        } else if (utilization < 90e18) {
            // Between 80% and 90% - 20% APY
            return uint256(20e16) / 365 days;
        } else if (utilization < 100e18) {
            // Between 90% and 100% - 20%-200% APY
            return
                (20e16 + utilization.sub(90e18).mul(180e16).div(10e18)) /
                365 days;
        } else {
            // Not possible, but just in case - 200% APY
            return uint256(200e16) / 365 days;
        }
    }

    function borrow(
        uint256 id,
        address collateralToken,
        uint256 collateralAmount,
        uint256 borrowAmount
    )
        public
        payable
        transferToVault(collateralToken, collateralAmount)
        accrue(collateralAmount)
        nonReentrant
    {
        require(borrowAmount > 0, "borrow: amount");
        require(borrowAmount <= totalBUSD(), "borrow: totalBUSD");

        address[] memory path = new address[](2);
        path[0] = collateralToken;
        path[1] = busdToken;

        uint256 collateralPrice = worker.price(collateralAmount, path);
        console.log("collateralPrice: ", collateralPrice);
        uint256 max = collateralPrice.mul(MAX_BORROW).div(10000);
        require(borrowAmount <= max, "borrow: max");

        BorrowPosition storage pos;
        if (id == 0) {
            id = nextPositionID++;
            pos = borrowPositions[id];
            pos.owner = msg.sender;
        } else {
            pos = borrowPositions[id];
            require(id < nextPositionID, "bad position id");
            require(pos.owner == msg.sender, "not position owner");
        }

        pos.startBlock = block.number;
        pos.collateralToken = collateralToken;
        pos.collateralAmount = borrowAmount;
        _addDebt(id, borrowAmount);
        require(
            IERC20(busdToken).transfer(msg.sender, borrowAmount),
            "failed to transfer BUSD"
        );
    }

    function healthPositon(uint256 id) public view returns (uint256, uint256) {
        BorrowPosition storage pos = borrowPositions[id];
        address[] memory path = new address[](2);
        path[0] = pos.collateralToken;
        path[1] = busdToken;
        uint256 collateralPrice = worker.price(pos.collateralAmount, path);
        console.log("healthPositon collateralPrice: ", collateralPrice);
        uint256 max = collateralPrice.mul(MAX_BORROW).div(10000);
        return (pos.debtShare, max);
    }

    function liquidate(uint256 id) public {
        (uint256 debtShare, uint256 max) = healthPositon(id);
        require(debtShare > max, "liquidate: health good");

        uint256 debt = _removeDebt(id);
        uint256 liquidatorPrize = debtShare.mul(LIQUIDATE_BPS).div(10000);
        uint256 tresauryFees = debtShare.mul(LIQUIDATE_BPS).div(10000);
        uint256 prize = liquidatorPrize.add(tresauryFees);
        uint256 rest = debtShare.sub(prize);

        BorrowPosition storage pos = borrowPositions[id];
        if (liquidatorPrize > 0) {
            IERC20(pos.collateralToken).transfer(
                address(msg.sender),
                liquidatorPrize
            );
        }

        if (tresauryFees > 0) {
            IERC20(pos.collateralToken).transfer(dev, tresauryFees);
        }

        uint256 left = rest > debt ? rest - debt : 0;
        if (left > 0) {
            IERC20(pos.owner).transfer(dev, left);
        }
    }

    function _addDebt(uint256 id, uint256 debtVal) internal {
        BorrowPosition storage pos = borrowPositions[id];
        uint256 debtShare = debtValToShare(debtVal);
        pos.debtShare = pos.debtShare.add(debtShare);
        vaultDebtShare = vaultDebtShare.add(debtShare);
        vaultDebtVal = vaultDebtVal.add(debtVal);
    }

    function _removeDebt(uint256 id) internal returns (uint256) {
        BorrowPosition storage pos = borrowPositions[id];
        uint256 debtShare = pos.debtShare;
        if (debtShare == 0) {
            return 0;
        }
        uint256 debtVal = debtShareToVal(debtShare);
        pos.debtShare = 0;
        vaultDebtShare = vaultDebtShare.sub(debtShare);
        vaultDebtVal = vaultDebtVal.sub(debtVal);
        return debtVal;
    }

    function debtShareToVal(uint256 debtShare) public view returns (uint256) {
        if (vaultDebtShare == 0) return debtShare; // When there's no share, 1 share = 1 val.
        return debtShare.mul(vaultDebtVal).div(vaultDebtShare);
    }

    function debtValToShare(uint256 debtVal) public view returns (uint256) {
        if (vaultDebtShare == 0) return debtVal; // When there's no share, 1 share = 1 val.
        return debtVal.mul(vaultDebtShare).div(vaultDebtVal);
    }

    function addCollateral(
        uint256 id,
        address token,
        uint256 amount
    )
        public
        payable
        transferToVault(token, amount)
        accrue(amount)
        nonReentrant
    {
        BorrowPosition storage pos = borrowPositions[id];
        require(id < nextPositionID, "bad position id");
        require(pos.owner == msg.sender, "!position owner");

        (uint256 debtShare, uint256 max) = healthPositon(id);
        require(debtShare < max, "health no good");

        pos.collateralAmount = pos.collateralAmount.add(amount);
    }

    function withdrawCollateral(uint256 id, uint256 amount)
        public
        nonReentrant
    {
        BorrowPosition storage pos = borrowPositions[id];
        require(id < nextPositionID, "bad position id");
        require(pos.owner == msg.sender, "!position owner");

        (uint256 debtShare, uint256 max) = healthPositon(id);
        require(debtShare < max, "health no good");

        pos.collateralAmount = pos.collateralAmount.add(amount);

        address[] memory path = new address[](2);
        path[0] = pos.collateralToken;
        path[1] = busdToken;
        uint256 collateralPrice = worker.price(pos.collateralAmount, path);
        console.log("collateralPrice: ", collateralPrice);
        uint256 maxAmount = collateralPrice.mul(MAX_BORROW).div(10000);
        require(pos.debtShare <= maxAmount, "borrow: max");

        IERC20(pos.collateralToken).transfer(address(msg.sender), amount);
    }

    function repay(uint256 id, uint256 amount)
        public
        payable
        transferToVault(busdToken, amount)
        nonReentrant
    {
        BorrowPosition storage pos = borrowPositions[id];
        require(id < nextPositionID, "bad position id");
        require(pos.owner == msg.sender, "!position owner");

        (uint256 debtShare, uint256 max) = healthPositon(id);
        require(debtShare < max, "health no good");

        pos.debtShare = pos.debtShare.sub(amount);
    }
}

