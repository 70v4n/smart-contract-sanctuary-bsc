contract Worker is Ownable, ReentrancyGuard {
    using SafeMath for uint256;
    IPancakeRouter02 public router;
    IERC20 public busd;

    constructor(address _router, address _busd) {
        // testnet 0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3
        router = IPancakeRouter02(_router);
        busd = IERC20(_busd);
    }

    function price(uint256 amountIn, address[] calldata path)
        public
        view
        returns (uint256)
    {
        uint256[] memory amounts = router.getAmountsOut(amountIn, path);
        return amounts[1];
    }
}

