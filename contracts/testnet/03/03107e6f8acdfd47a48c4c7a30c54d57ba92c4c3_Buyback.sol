// File: contracts\Buyback_testnet.sol



pragma solidity ^0.6.2;




contract Buyback is Ownable {
    using SafeMath for uint256;

    IUniswapV2Router02 public uniswapV2Router;
    address public deadAddress = 0x000000000000000000000000000000000000dEaD;
    address public immutable BETH = address(0x2170Ed0880ac9A755fd29B2688956BD959F933F8); //newBETH
    address public burnToken;

    event UpdateUniswapV2Router(address indexed newAddress, address indexed oldAddress);
    event UpdateBurnToken(address indexed newAddress, address indexed oldAddress);
    event SwapETHForTokens(uint256 amountIn, address[] path);

    constructor(address _burnToken) public {
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3);
        uniswapV2Router = _uniswapV2Router;
        burnToken = _burnToken;
    }

    function buyback () external onlyOwner {

        if (address(this).balance > 0) {	
            swapETHForTokens(address(this).balance);	
        }
    }

    receive() external payable {

  	}

    function swapETHForTokens(uint256 amount) private {	
        // generate the uniswap pair path of token -> weth	
        address[] memory path = new address[](3);	
        path[0] = BETH;
        path[1] = uniswapV2Router.WETH();	
        path[2] = burnToken;
        
        // make the swap	
        uniswapV2Router.swapExactETHForTokensSupportingFeeOnTransferTokens{value: amount}(	
        0, // accept any amount of Tokens	
        path,	
        deadAddress, // Burn address	
        block.timestamp.add(300)	
        );	

        emit SwapETHForTokens(amount, path);	
    }      

    function updateUniswapV2Router(address newAddress) public onlyOwner {
        require(newAddress != address(uniswapV2Router), "Buyback: The router already has that address");
        emit UpdateUniswapV2Router(newAddress, address(uniswapV2Router));
        uniswapV2Router = IUniswapV2Router02(newAddress);
    }

    function updateBurnTokenAddress(address newAddress) public onlyOwner {
        require(burnToken != newAddress, "Buyback: New token is already set");
        emit UpdateBurnToken(newAddress, burnToken);
        burnToken = newAddress;
    }
}