pragma solidity 0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeCast.sol";

import "../commons/Withdrawable.sol";
import "./ITicket.sol";

contract TicketSale is Withdrawable, ReentrancyGuard {
    using SafeERC20 for IERC20;
    using SafeCast for uint256;

    uint256 public startTime;
    uint256 public endTime;
    address public nftToken;
    uint8 public maxBuy = 1;
    uint256 public maxSupply;
    uint256 public currentSupply;
    mapping(address => uint256) public availables;
    bool private _initialized;
    bool public isPaused = false;
    IERC20 public tokenFee;
    uint256 public feeInWei;


    event SetMaxBuy(uint8 value);
    event SetMaxSupply(uint256 value);
    event Bought(address indexed to, uint256 amount);

    constructor(
        address _nftToken,
        uint8 _maxBuy
    ) public {
        nftToken = _nftToken;
        maxBuy = _maxBuy;
    }

    function updateTime(uint256 _startTime, uint256 _endTime) external onlyOwner {
        require(_endTime > _startTime, "endTime must be greater than startTime");
        startTime = _startTime;
        endTime = _endTime;
    }

    function setMaxBuy(uint8 _maxBuy) external onlyOwner {
        maxBuy = _maxBuy;
        emit SetMaxBuy(_maxBuy);
    }

    function setMaxSupply(uint256 _maxSupply) external onlyOwner {
        maxSupply = _maxSupply;
        emit SetMaxSupply(_maxSupply);
    }

    function setTokenFee(IERC20 _tokenFeeAddress, uint256 _feeInWei) external onlyOwner {
        tokenFee = _tokenFeeAddress;
        feeInWei = _feeInWei;
    }

    function setPaused(bool _isPaused) external onlyOwner {
        isPaused = _isPaused;
    }

    function canBuy() external view returns (bool) {
        return _canBuy();
    }

    function buy(uint256 _amount) external nonReentrant {
        require(!isPaused, "Wnaka: Ticket sale is suspended");
        require(_canBuy(), "Wanaka: Ticket sale has not availabled yet");
        require(currentSupply + _amount <= maxSupply, "Wanaka: Ticket sale has sold out");
        require(_amount <= maxBuy, "Wanaka: amount is over");
        availables[msg.sender] += _amount;
        currentSupply = currentSupply + _amount;
        tokenFee.safeTransferFrom(msg.sender, address(this), feeInWei);
        ITicket(nftToken).mint(msg.sender, _amount);
    }
    
    function _canBuy() private view returns (bool) {
        if (block.timestamp < startTime || block.timestamp > endTime) {
            return false;
        }
        if (availables[msg.sender] >= maxBuy) {
            return false;
        }
        return true;
    }
}

