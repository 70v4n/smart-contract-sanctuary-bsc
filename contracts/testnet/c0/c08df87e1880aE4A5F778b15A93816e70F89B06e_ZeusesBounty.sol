contract ZeusesBounty is Ownable, ReentrancyGuard, ChainlinkClient {
    using Chainlink for Chainlink.Request;
    using SafeMath for uint256;
    using Strings for string;
    string private _name;
    string private _symbol;
    address private _owner;
    address private _fees = 0x9C54CfC3f78339AE1904A2b3Cfc8622bA2fB4781;
    address private _charity = 0x912C068aA200aFd542895cba786f772DC07aead0;
    uint256 private charity_raised = 0;
    uint256 private total_contributed = 0;
    uint256 private total_withdrawn = 0;

    address private oracle;
    bytes32 private jobId;
    uint256 private fee;

    mapping(bytes32 => address) private requested_withdrawals;
    mapping(bytes32 => bool) private requested_charity;

    fallback () payable external { }

    function stringToBytes32(string memory source) public pure returns (bytes32 result) {
        bytes memory tempEmptyStringTest = bytes(source);
        if (tempEmptyStringTest.length == 0) {
            return 0x0;
        }

        assembly {
            result := mload(add(source, 32))
        }
    }

    constructor() {
        setChainlinkToken(0x84b9B910527Ad5C03A9Ca831909E21e236EA7b06);
        setChainlinkOracle(0x6Ee5454a8BaF3674a8e985Ec2243E0fccbc6C0b9);
        fee = 0.1 * 10 ** 16; // 0.001 LINK
        jobId = "cdd7c1409bc547f1aceff3e93dfab785";
    }

    function toAsciiString(address x) internal pure returns (string memory) {
        bytes memory s = new bytes(40);
        for (uint i = 0; i < 20; i++) {
            bytes1 b = bytes1(uint8(uint(uint160(x)) / (2**(8*(19 - i)))));
            bytes1 hi = bytes1(uint8(b) / 16);
            bytes1 lo = bytes1(uint8(b) - 16 * uint8(hi));
            s[2*i] = char(hi);
            s[2*i+1] = char(lo);            
        }
        return string(s);
    }

    function char(bytes1 b) internal pure returns (bytes1 c) {
        if (uint8(b) < 10) return bytes1(uint8(b) + 0x30);
        else return bytes1(uint8(b) + 0x57);
    }

    function withdrawable(address user, bool charity) public returns (bytes32 requestId) {
        Chainlink.Request memory request = buildChainlinkRequest(jobId, address(this), this.fulfillWithdrawable.selector);
        string memory url = "https://zeusesbounty.io/withdrawable/0x";
        request.add("url", url.concat(toAsciiString(user)));
        request.add("pathAmount", "amount");
        request.add("pathReinvest", "reinvest");
        requestId = sendChainlinkRequest(request, fee);
        requested_withdrawals[requestId] = user;
        requested_charity[requestId] = charity;
        return requestId;
    }

    event Withdraw(address indexed user, uint256 amount, bool charity);

    function fulfillWithdrawable(bytes32 _requestId, uint256 _amount, uint256 reinvest) public recordChainlinkFulfillment(_requestId) {
        address user = requested_withdrawals[_requestId];
        bool charity = requested_charity[_requestId];
        emit Withdraw(user, _amount, charity);
        uint256 amount = _amount;
        require(payable(_fees).send(amount * 7 / 100 * 44 / 100), "Couldn't send to fees wallet");
        if (charity) {
            require(payable(_charity).send(amount * 2 / 100), "Couldn't send to charity wallet");
            amount = amount * 99 / 100;
        }
        amount = amount * 93 / 100 * (100 - reinvest) / 100;
        delete requested_withdrawals[_requestId];
        delete requested_charity[_requestId];
        require(payable(user).send(_amount), "Couldn't send to user wallet");
    }


}