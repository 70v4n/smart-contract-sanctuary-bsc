pragma solidity ^0.8.0;

// SPDX-License-Identifier: MIT

// Deploy step
// 0. Deploy the token buy  contract such as stable if not exist
// 1. Deploy the token contract with the ICO end time
// 2. Deploy the Crowdsale contract using the token address of the contract just deployed
// 3. Set the Crowdsale address inside the token contract with the setCrowdsale() function to distribute the tokens
// 4. Make the Crowdsale address public to your investors so that they can send ether to it to buy tokens

import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
// import "./interface/IERC20.sol";
import "hardhat/console.sol";


contract ICO is AccessControl, ReentrancyGuard {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    
    // tell this sell is complete or not
    bool icoCompleted;
    // tell this time when ico start
    uint256 public icoStartTime;
    // tell this time when ico end
    uint256 public icoEndTime;
    // set token exchange rate
    uint256 public tokenRateEth;
    // use from ICO TOKEN Contract
    IERC20 public token;
    // how much money raised so far
    uint256 public fundingGoal;
    // this is owner of this money raised
    address payable public owner;
    // token raised so far
    uint256 public tokenRaised;
    // ether get
    uint256 public etherRaised;
    // keep exceed ethers
    uint256 public etherExceeds;     
    
    // token get
    uint256 public tokenBuyRaised;
    // keep exceed token buy
    uint256 public tokenBuyExceeds;   
    // set token exchange rate for token
    mapping(address => uint256) public tokenRateTokenBuys;
    // set token address to accept as buy token
    mapping(address => bool) public whitelistTokenBuyAddress;
    
    struct LockUserToken {
        uint256 lockToken;
        uint256 currectLockToken;
        uint256 chunkClaimed;        
    }

    struct TokenBuy {
        address tokenBuyAddress;
        uint256 rate;
        uint256 min;
        uint256 max;
        uint8 decimals;
    }

    // calculate Lock token
    mapping(address => LockUserToken) public lockUserToken;
    // lock rate percent for example 70% set tokenLockRate = 70
    uint256 public tokenLockRate;
    // total lock token
    uint256 public totalTokenLock;
    // for calculate in claim
    uint256 public unlockStartTime;
    // set max chunk to unlock token
    uint256 public maxChunk;
    // set duration time per chunk
    uint256 public timePerChunk;
    // user seperate token buy balance
    mapping(address => mapping(address => uint256)) public userTokenBuyBalance;
    // user native buy balance
    mapping(address => uint256) public userNativeBuyBalance;
    // whitelist user to buy
    mapping(address => bool) public whitelistUsers;
    // whitelist period
    uint256 public whitelistStartTime;
    uint256 public whitelistEndTime;
    // keep token buy data
    mapping(address => TokenBuy) public tokenBuys;
    // keep user ico token receive balance 
    mapping(address => uint256) public userTokenReceiveBalance;
    // user limit ico token receive
    uint256 public limitTokenReceivePerUser;
    // enable limit
    bool public checkLimitPerUser;
    // setup role
    bytes32 public constant DISTRIBUTOR_ROLE = keccak256("DISTRIBUTOR_ROLE");
    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
    bytes32 public constant FUND_OWNER_ROLE = keccak256("FUND_OWNER_ROLE");
    bytes32 public constant CONFIG_ROLE = keccak256("CONFIG_ROLE");
    // pause buy with crypto
    bool public isPauseSaleWithCrypto;
    // pause buy with distributor
    bool public isPauseSaleWithDistributor;
    // set token ico decimal
    uint8 public icoTokenDecimals = 18;


    event BuyToken(address indexed buyer, uint256 ethPaid, uint256 tokenReceived, uint256 tokensToLock, uint256 timestamp);
    event BuyTokenWithToken(address indexed buyer, uint256 tokenPaid, uint256 tokenReceived, uint256 tokensToLock, address tokenBuyAddress, uint256 timestamp);
    event BuyTokenWithDistributor(address indexed receiver, uint256 tokenReceived, uint256 tokenLocked, string indexed referenceTx); 
    event ExtractEther(address indexed withdrawer, address indexed receiver, uint256 ethReceived);
    event EmergencyWithdrawEther(address indexed withdrawer, uint256 ethReceived);
    event ExtractToken(address indexed withdrawer, address indexed receiver, uint256 tokenReceived);
    event EmergencyWithdrawToken(address indexed withdrawer, uint256 tokenReceived);
    
    
    modifier whenIcoCompleted {
        require(icoCompleted, "This ico not complete yet");
        _;
    }

    modifier whenSaleNotPause {
        require(!isPauseSaleWithCrypto, "Token sale is pause");
        _;
    }

     modifier whenSaleWithDistributorNotPause {
        require(!isPauseSaleWithDistributor, "Token sale with distributor is pause");
        _;
    }
    
    constructor(
        uint256 _icoStartTime,
        uint256 _icoEndTime,
        uint256 _tokenRateEth,
        address _tokenAddress,
        uint256 _fundingGoal,        
        address _distributor,
        uint256 _unlockStartTime,
        uint256 _maxChunk,
        uint256 _timePerChunk,
        TokenBuy[] memory _tokenBuys,
        uint256 _tokenLockRate
    ) {
        require (
            _icoStartTime != 0 &&
            
            _icoEndTime != 0 &&
            
            _tokenRateEth != 0 &&
            
            _tokenAddress != address(0) &&
            
            _fundingGoal != 0

        , "Some value are not valid");
        
        icoCompleted = false;
        
        icoStartTime = _icoStartTime;
        
        icoEndTime = _icoEndTime;
        
        token = IERC20(_tokenAddress);
        
        tokenRateEth = _tokenRateEth;
        
        fundingGoal = _fundingGoal;
        
        owner = payable(msg.sender);

        tokenLockRate = _tokenLockRate;

        unlockStartTime = _unlockStartTime;

        maxChunk = _maxChunk;

        timePerChunk = _timePerChunk;
        // setup pause sale to false
        isPauseSaleWithCrypto = false;

        isPauseSaleWithDistributor = false;

        // set allow token to buy
        for (uint256 i = 0; i < _tokenBuys.length; i++) {
            TokenBuy memory tokenBuy = _tokenBuys[i];
            // whitelist token buy address
            whitelistTokenBuyAddress[tokenBuy.tokenBuyAddress] = true;
            // mapping token buy data
            tokenBuys[tokenBuy.tokenBuyAddress] = tokenBuy;
        }    

        // set up role
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(DISTRIBUTOR_ROLE, _distributor);
        _setupRole(PAUSER_ROLE, msg.sender);
        _setupRole(FUND_OWNER_ROLE, msg.sender);
        _setupRole(CONFIG_ROLE, msg.sender);
    }

    function addWhitelistUser(address[] calldata _whitelistUsersAddress) public onlyRole(CONFIG_ROLE) {
        for (uint256 i = 0; i < _whitelistUsersAddress.length; i++) {
            whitelistUsers[_whitelistUsersAddress[i]] = true;
        }
    }

    function setWhitelistPeriod(uint256 _whitelistStartTime, uint256 _whitelistEndTime) public onlyRole(CONFIG_ROLE) {
        require(_whitelistEndTime > _whitelistStartTime, "Endtime must more than StartTime");
        require(_whitelistStartTime >= icoStartTime, "Whitelist Starttime must more than icoStartTime");
        require(_whitelistEndTime <= icoEndTime, "Whitelist Endtime must less than icoEndTime");
        // set whitelist period
        whitelistStartTime = _whitelistStartTime;
        whitelistEndTime = _whitelistEndTime;        
    }

    function isWhitelistPeriod() public view returns (bool) {
        return block.timestamp >= whitelistStartTime && block.timestamp <= whitelistEndTime;
    }

    function setLimitTokenReceivePerUser(uint256 _limitTokenReceivePerUser) public onlyRole(CONFIG_ROLE) {
        require(_limitTokenReceivePerUser != 0);
        limitTokenReceivePerUser = _limitTokenReceivePerUser;
    }

    function enableCheckLimitPerUser() public onlyRole(CONFIG_ROLE) {
        checkLimitPerUser = true;
    }

    function disableCheckLimitPerUser() public onlyRole(CONFIG_ROLE) {
        checkLimitPerUser = false;
    }

    function pauseSaleWithCrypto() external onlyRole(PAUSER_ROLE) {
        isPauseSaleWithCrypto = true;
    }

    function unpauseSaleWithCrypto() external onlyRole(PAUSER_ROLE)  {
        isPauseSaleWithCrypto = false;
    }

    function pauseSaleWithDistributor() external onlyRole(PAUSER_ROLE) {
        isPauseSaleWithDistributor = true;
    }

    function unpauseSaleWithDistributor() external onlyRole(PAUSER_ROLE)  {
        isPauseSaleWithDistributor = false;
    }

    function setICOComplete() external onlyRole(CONFIG_ROLE) {
        require(block.timestamp > icoEndTime || tokenRaised >= fundingGoal, "Can complete after sale end of cap is reached");
        icoCompleted = true;
    }

    function buy() public payable whenSaleNotPause nonReentrant {
        
        require(tokenRaised < fundingGoal, "ICO Raise to CAP");
        
        require(block.timestamp > icoStartTime && block.timestamp < icoEndTime, "End time for ICO");
        
        uint256 tokensToBuy;
        
        uint256 etherUsed = msg.value;
        
        address payable sender = payable(msg.sender);
        
        uint256 etherExceed;
        
        uint256 etherUnit = 1 ether;

        uint256 tokensToLock;
        
        // incase token have 5 decimal but ether is 18 decimal
        // also multiply with token rate 
        // tokensToBuy = msg.value * 1e5 / 1 ether * tokenRateEth;
        
        
        // if have token instance can change to use decimals
        // formular 
        // ######
        // etherUsed * (10 ** token.decimals()) / 1 ether * tokenRateEth;
        // ######
        tokensToBuy = etherUsed.mul(10 ** icoTokenDecimals).div(etherUnit).mul(tokenRateEth);
        
        // Check if we have reached and exceeded the funding goal to refund the exceeding tokens and ether
        if (tokenRaised.add(tokensToBuy) > fundingGoal) {
            
            uint256 tokenExceed = tokenRaised.add(tokensToBuy).sub(fundingGoal);
            
            
            // test convert rate
            // formular
            // etherExceed = tokenExceed * 1 ether / 10 ** token.decimals() / tokenRateEth;
            
            etherExceed = tokenExceed.mul(etherUnit).div(10 ** icoTokenDecimals).div(tokenRateEth);
            
            // from tutorial
            // exceedingEther = exceedingTokens * 1 ether / tokenRateEth / token.decimals();
            
            sender.transfer(etherExceed);
            
            etherUsed = etherUsed.sub(etherExceed);
            
            tokensToBuy = tokensToBuy.sub(tokenExceed);
            
        }

        // calculate lock token
        tokensToLock = calculateLockToken(sender, tokensToBuy);

        // send token to buyer
        token.safeTransfer(msg.sender, tokensToBuy.sub(tokensToLock));
        
        // increase token raised and ether raised variable
        
        tokenRaised = tokenRaised.add(tokensToBuy);
        
        // etherRaised += etherUsed / 1 ether;
        etherRaised = etherRaised.add(etherUsed);
        
        // total eth exceed
        etherExceeds = etherExceeds.add(etherExceed);

        // count each user for native buy balance
        userNativeBuyBalance[msg.sender] += etherUsed;

        // count each user ico token receive balance
        userTokenReceiveBalance[msg.sender] += tokensToBuy;

        emit BuyToken(msg.sender, etherUsed, tokensToBuy, tokensToLock, block.timestamp);
    }
    
    function buyWithToken(uint256 _amount, address _tokenBuyAddress) external whenSaleNotPause nonReentrant {
        // check ico time
        require(block.timestamp >= icoStartTime && block.timestamp <= icoEndTime, "End time for ICO");   
        // check whitelist token buy
        require(whitelistTokenBuyAddress[_tokenBuyAddress], "Not allow this token to buy");
        // check raise cap
        require(tokenRaised < fundingGoal, "ICO Raise to CAP");

        // check and allow whitelist user to buy first
        // after whitelist period will allow any user to buy
        if (isWhitelistPeriod()) {
            require(whitelistUsers[msg.sender], "Not allow none whitelist");    
        }

        TokenBuy memory tokenBuyData = tokenBuys[_tokenBuyAddress];
        // check min max
        require(_amount >= tokenBuyData.min, "Amount purchase less than min");
        require(_amount <= tokenBuyData.max, "Amount purchase more than max");
    
        // set token buy instance
        IERC20 tokenBuy = IERC20(_tokenBuyAddress);
        
        uint256 tokensToReceived;
        
        uint256 tokenBuyUsed = _amount;
        
        address sender = msg.sender;
        
        uint256 tokenBuyExceed;
        
        uint256 tokenBuyUnit = 10 ** tokenBuyData.decimals;
        
        uint256 tokenReceivedUnit = 10 ** icoTokenDecimals;
        
        uint256 tokensToLock;

        uint256 tokenRateTokenBuy = tokenBuyData.rate;
 
        // trasfer token buy to crowdsale
        tokenBuy.safeTransferFrom(sender, address(this), tokenBuyUsed);
        
        // calculate token buy to token receive
        tokensToReceived = tokenBuyUsed.mul(tokenReceivedUnit).div(tokenBuyUnit).mul(tokenRateTokenBuy);

        console.log("tokensToReceived", tokensToReceived);
        
        // Check if we have reached and exceeded the funding goal to refund the exceeding tokens and ether
        if (tokenRaised.add(tokensToReceived) > fundingGoal) {
            
            uint256 tokensToReceivedExceed = tokenRaised.add(tokensToReceived).sub(fundingGoal);
            
            // formular
            // tokenBuyExceed = tokenExceed * 10 ** tokenBuyUnit / 10 ** token.decimals() / tokenRateEth;
            
            tokenBuyExceed = tokensToReceivedExceed.mul(tokenBuyUnit).div(tokenReceivedUnit).div(tokenRateTokenBuy);

            // transfer exceed token buy to user
                
            tokenBuy.safeTransfer(sender, tokenBuyExceed);
            
            tokenBuyUsed = tokenBuyUsed.sub(tokenBuyExceed);
            
            tokensToReceived = tokensToReceived.sub(tokensToReceivedExceed);
            
        }

        // check limit per user
        if (checkLimitPerUser) {
            require(userTokenReceiveBalance[sender] + tokensToReceived <= limitTokenReceivePerUser, "Exceed token receive limit");
        }

        // calculate lock token
        tokensToLock = calculateLockToken(sender, tokensToReceived);
        console.log("tokensToLock", tokensToLock);
        // send token to buyer with lock token
        token.safeTransfer(sender, tokensToReceived.sub(tokensToLock));

        // increase token raised and ether raised variable
        tokenRaised = tokenRaised.add(tokensToReceived);

        console.log("tokenRaised", tokenRaised);
        
        // count raised token
        tokenBuyRaised = tokenBuyRaised.add(tokenBuyUsed);
        // count each user for token buy balance
        userTokenBuyBalance[sender][_tokenBuyAddress] += tokenBuyUsed;

        // count each user ico token receive balance
        userTokenReceiveBalance[sender] += tokensToReceived;
        console.log("userTokenReceiveBalance[sender]", userTokenReceiveBalance[sender]);

        emit BuyTokenWithToken(sender, tokenBuyUsed, tokensToReceived.sub(tokensToLock), tokensToLock, _tokenBuyAddress, block.timestamp);
    }

    function buyTokenWithDistributor(address _receiver, uint256 _amount, string memory _reference) public onlyRole(DISTRIBUTOR_ROLE) whenSaleWithDistributorNotPause nonReentrant {    
        // check value address and amount
        require(_receiver != address(0), "Receiver must not be zero address");
        require(_amount != 0, "Not receive zero amount");
        // check token not already reach goal
        require(tokenRaised < fundingGoal, "ICO Raise to CAP");        
        // check in ico time
        require(block.timestamp >= icoStartTime && block.timestamp <= icoEndTime, "End time for ICO");

        // check and allow whitelist user to buy first
        // after whitelist period will allow any user to buy
        if (isWhitelistPeriod()) {
            require(whitelistUsers[_receiver], "Not allow none whitelist");    
        }

        // check contract have enough token to buy
        require(token.balanceOf(address(this)) - totalTokenLock >= _amount, "Insufficientcy token to sale");

        
        // check limit per user
        if (checkLimitPerUser) {
            require(userTokenReceiveBalance[_receiver] + _amount <= limitTokenReceivePerUser, "Exceed token receive limit");
        }

        uint256 tokensToReceived;
                                                        
        uint256 tokensToLock;
                                                
        // calculate token to token
        tokensToReceived = _amount;        
                
        tokensToLock = calculateLockToken(_receiver, tokensToReceived);
        
        // send token to buyer without lock token
        token.safeTransfer(_receiver, tokensToReceived.sub(tokensToLock));
        
        // increase token raised and ether raised variable        
        tokenRaised = tokenRaised.add(tokensToReceived);         

        // emit actual token receive and lock token
        emit BuyTokenWithDistributor(_receiver, tokensToReceived.sub(tokensToLock), tokensToLock, _reference);
    }
    
    function calculateLockToken(address _userAddress, uint256 _tokensToReceived) internal returns (uint256) {
        uint256 tokenToLock;
        require(_userAddress != address(0), "Not allow address zero");
        require(_tokensToReceived != 0, "Not allow amount equal zero");
        // calculate lock percent
        tokenToLock = _tokensToReceived.mul(tokenLockRate).div(100);

        LockUserToken storage userToken = lockUserToken[_userAddress];
        // add lock token to user        
        userToken.lockToken = userToken.lockToken.add(tokenToLock);
        userToken.currectLockToken = userToken.currectLockToken.add(tokenToLock);
        // sum all total token lock
        totalTokenLock = totalTokenLock.add(tokenToLock);
        return tokenToLock;
    }    
    
    function claimLockToken() public {        
        LockUserToken storage userToken = lockUserToken[msg.sender];
        console.log("###### Initial claim ######");
        console.log("userToken.chunkClaimed", userToken.chunkClaimed);
        console.log("userToken.currectLockToken", userToken.currectLockToken);
        console.log("maxChunk", maxChunk);
        console.log("timePerChunk", timePerChunk);
        console.log("unlockStartTime", unlockStartTime);
        console.log("block.timestamp", block.timestamp);
        console.log("###### End Initial claim ######");
        require(msg.sender != address(0), "Not allow burn address");
        require(block.timestamp >= unlockStartTime, "Not in time for claim.");
        require(userToken.chunkClaimed != maxChunk && userToken.currectLockToken != 0, "Already claim all lock token");
        
        // TODO: Uncomment this after test.
        // require(block.timestamp > icoEndTime, "Not in time for claim.");
        
        uint256 claimableToken;        

        uint256 currentTime = block.timestamp;
        uint256 unlockChunk = (currentTime - unlockStartTime)/timePerChunk + 1; 
        console.log("unlockChunk", unlockChunk);
    
        // for example max chunk is 10 and timePerChunk is 10 min so unlock 10% every 10 mins
        if (unlockChunk > maxChunk) unlockChunk = maxChunk;

        console.log("unlockChunk manipulate", unlockChunk);

        console.log("before userToken.chunkClaimed", userToken.chunkClaimed);
        // in case multiple claim in same chunk
        require(unlockChunk > userToken.chunkClaimed, "No remain lock token to claim right now");
        // calculate remain chunk to claim
        claimableToken = userToken.lockToken * (unlockChunk - userToken.chunkClaimed)/maxChunk;
        console.log("claimableToken", claimableToken);
        // reduce lock token.
        userToken.currectLockToken = userToken.currectLockToken.sub(claimableToken);
        // cummulative chunk claimed.
        userToken.chunkClaimed = userToken.chunkClaimed.add(unlockChunk - userToken.chunkClaimed);

        console.log("userToken.currectLockToken", userToken.currectLockToken);
        console.log("userToken.chunkClaimed", userToken.chunkClaimed);
        // call to send claim token
        token.safeTransfer(msg.sender, claimableToken);
    } 
    
    // withdraw money raised to owner 
    function extractEther(address _fundOwner, uint256 _amount) external onlyRole(FUND_OWNER_ROLE) whenIcoCompleted nonReentrant {
        require(_fundOwner != address(0), "Not allow zero address");
        require(_amount != 0, "Not allow zero amount");

        address payable fundOwner = payable(_fundOwner);
        // send contract ether to fund owner
        fundOwner.transfer(_amount);

        emit ExtractEther(msg.sender, fundOwner, _amount);
    }
    
     // withdraw money raised to owner 
    function extractTokenBuy(address _tokenBuyAddress, address _fundOwner, uint256 _amount) external onlyRole(FUND_OWNER_ROLE) whenIcoCompleted nonReentrant {
        // check whitelist token buy
        require(whitelistTokenBuyAddress[_tokenBuyAddress], "Not allow this token to extract");
        require(_fundOwner != address(0), "Not allow zero address");
        require(_amount != 0, "Not allow zero amount");
        // set token buy
        IERC20 tokenBuy = IERC20(_tokenBuyAddress);
        // send token buy to fund owner
        tokenBuy.safeTransfer(_fundOwner, _amount);

        emit ExtractToken(msg.sender, _fundOwner, _amount);
    } 
    
    // According to solidity version 0.6.0, we have a breaking change. The unnamed function commonly referred to as “fallback function” 
    // was split up into a new fallback function that is defined using the fallback keyword and a receive ether function defined using the receive keyword. 
    // If present, the receive ether function is called whenever the call data is empty (whether or not ether is received). This function is implicitly payable. 
    // The new fallback function is called when no other function matches (if the receive ether function does not exist then this includes calls with empty call data). 
    // You can make this function payable or not. If it is not payable then transactions not matching any other function which send value will revert. 
    // You should only need to implement the new fallback function if you are following an upgrade or proxy pattern.
    
    // https://solidity.readthedocs.io/en/v0.6.7/060-breaking-changes.html#semantic-and-syntactic-changes



    fallback() external payable {        
        buy();        
    }
    
    receive() external payable {        
        buy();        
    }

}

