pragma solidity ^0.8.9;



import "./IBNBRagnarok.sol";
import "./HeroLibrary.sol";
import "./IPancakeswapV2Factory.sol";
import "./IPancakeswapV2Router02.sol";
import "../ORACLE/IPriceOracle.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721ReceiverUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";

interface IReferral {
    function recordReferral(address user, address referrer) external;
    function getReferrer(address user) external view returns (address);
}

contract PLAYBNBRO is Initializable, AccessControlUpgradeable, ReentrancyGuardUpgradeable, PausableUpgradeable, IERC721ReceiverUpgradeable {    
    using SafeMath for uint256;

    using EnumerableSet for EnumerableSet.UintSet;
    using SafeERC20Upgradeable for IERC20Upgradeable;

    struct RewardToken {
        address token;
        uint128 rewardInToken;
    }

    struct BaseMonsterXps {
        uint256 monsterxps;
    }

    struct Monster {
        uint128 minLevel;
        uint128 requiredHp; //minstamina
        uint chanceToWin; //base 10000
        BaseMonsterXps[] basemonsterxps;
        RewardToken[] rewardTokens;
        string name;
        bool enable; //true - enable; false - disable
    }

    struct MonsterInfo {
        uint index;
        Monster monster;
        bool Level;
        bool hpAmount;
    }
    
    IBNBRagnarok public bnbronft;
    IERC20Upgradeable public zenytoken;
    IPriceOracle public priceOracle;
    IReferral public referral;
    address public treasuryAddress;
    address public burnAddress;   
 
    uint constant MAX_WITHDRAWAL_FEE = 5000; //50%
    uint public decreaseWithdrawalFeeByDay; //400: -4% by day
    uint public withdrawalFee; //4000: 40%    
    uint256 public numTokensToSend;
    uint256 public maxHeroCount;
    uint256 public dividePercent;
    //uint256 public feeToLevelup;

    Monster[] public monsters;
    address[] rewardTokens;
    uint256[] basemonsterxps;
    
    mapping(address => uint) public withdrawTimeLock; //user address => block.timestamp; To calc withdraw fee
    mapping(address => mapping(address => uint128)) userBalances; //user balances: user address => token address => amount
    mapping(uint256 => uint256) private listings;
    mapping (address => EnumerableSet.UintSet) heroIDsInStorage;
    mapping (address => bool) public bannedList;
    mapping (uint256 => uint256) rewardsInCurLvl;

    event MonsterAdded(uint monsterIndex);
    event MonsterDisable(uint monsterIndex);
    event MonsterEnable(uint monsterIndex);
    event FightMonster(address indexed user, uint indexed monsterIndex, bool userWin, address[] rewardTokens, uint128[] rewardAmount, uint xpGained, uint hpLoss);
    event Withdrew(address indexed user, RewardToken[] _rewardBalance);
    event RewardTokenChanged(uint monsterIndex);
    event MonsterXpsChanged(uint monsterIndex);
    event SetNewMonsterParam(uint monsterIndex);
    event ReferralCommissionPaid(address indexed user, address indexed referrer, uint256 commissionAmount);
    event UpdatedTokenContract(address tokenAddress);
    event UpdatedNFTContract(address characterAddress);
    event UpgradedVip(address player, uint8 level);
    event Fight(address player, uint256 _attackingHero, uint256 enemyType, uint256 rewards, uint256 xpGained, uint256 hpLoss);
    event ClaimedRewards(address player, uint256 amount);
    event WarpPortalHero(address player, uint256 _heroId);
    event CreatedChar(address player, uint256 _heroId);
    event UpdatedBurnAddress(address account);
    event CreatedAndSendPrizeHero(address account, uint256 heroIds);
    event UnlockedLevel(address player, uint256 _heroId, uint256 level);
    event UpdatedPriceOracle(address account);
    event MovedHeroToStorage(address player, uint256 heroId);
    event TakeHeroFromStorage(address player, uint256 heroId);
    event UpdatedDividePercent(uint256 percent);


    // receive() external payable {}
    // modifier onlyOwner() {
    //     require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "Not admin");
    //     _;
    // }
    function initialize (
        IERC20Upgradeable  _zenytoken, 
        IBNBRagnarok _bnbronft, 
        IPriceOracle _priceOracle,
        IReferral _referral,  
        address  _treasuryAddress
        ) public initializer {
            require(
            address(_zenytoken) != address(0) &&
            address(_bnbronft) != address(0) &&
            address(_priceOracle) != address(0) &&
            address(_referral) != address(0) &&
            address(_treasuryAddress) != address(0),
            "Address cant be zero"
        );
        __AccessControl_init_unchained();
        __ReentrancyGuard_init();
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);

        zenytoken = _zenytoken;
        bnbronft = _bnbronft;
        priceOracle = _priceOracle;
        referral = _referral;
        treasuryAddress = _treasuryAddress;
        burnAddress = 0x000000000000000000000000000000000000dEaD;
        maxHeroCount = 10;
        dividePercent = 70;//set for burn
        //feeToLevelup = 25;
        numTokensToSend = 10000000000000000000000; //send when 10k Zeny
    }

    //Modifiers -------------------------------------------------------------------------------------------------------

    modifier notContract() {
        require(!_isContract(msg.sender), "Contract not allowed");
        require(msg.sender == tx.origin, "Proxy contract not allowed");
        _;
    }

    //External functions --------------------------------------------------------------------------------------------

    function addNewMonster(Monster calldata _monster) external onlyRole(DEFAULT_ADMIN_ROLE) {
        monsters.push(_monster);
        for (uint i = 0; i < _monster.rewardTokens.length; i++) {
            if (!_tokenInArray(_monster.rewardTokens[i].token)) {
                rewardTokens.push(_monster.rewardTokens[i].token);
            }
        }
        emit MonsterAdded(monsters.length);
    }
    function disableMonster(uint _monsterIndex) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(_monsterIndex < monsters.length, "Index out of bound");
        monsters[_monsterIndex].enable = false;
        emit MonsterDisable(_monsterIndex);
    }
    function enableMonster(uint _monsterIndex) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(_monsterIndex < monsters.length, "Index out of bound");
        monsters[_monsterIndex].enable = true;
        emit MonsterEnable(_monsterIndex);
    }
    function setMonsterParameters(uint _monsterIndex, uint128 _minLevel, uint128 _requiredHp, uint _chanceToWin) external onlyRole(DEFAULT_ADMIN_ROLE) {
        Monster storage _monster = monsters[_monsterIndex];
        _monster.minLevel = _minLevel;
        _monster.requiredHp = _requiredHp;
        _monster.chanceToWin = _chanceToWin;
        emit SetNewMonsterParam(_monsterIndex);
    }
    function setRewardTokensToMonster(uint _monsterIndex, RewardToken[] calldata _rewardTokens) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(_monsterIndex < monsters.length, "Index out of bound");
        delete monsters[_monsterIndex].rewardTokens;
        for(uint i = 0; i < _rewardTokens.length; i++){
            monsters[_monsterIndex].rewardTokens.push(_rewardTokens[i]);
        }
        Monster memory _monster = monsters[_monsterIndex];
        for (uint i = 0; i < _monster.rewardTokens.length; i++) {
            if (!_tokenInArray(_monster.rewardTokens[i].token)) {
                rewardTokens.push(_monster.rewardTokens[i].token);
            }
        }
        emit RewardTokenChanged(_monsterIndex);
    }
    function setXpsToMonster(uint _monsterIndex, BaseMonsterXps[] calldata _xps) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(_monsterIndex < monsters.length, "Index out of bound");
        delete monsters[_monsterIndex].basemonsterxps;
        for(uint i = 0; i < _xps.length; i++){
            monsters[_monsterIndex].basemonsterxps.push(_xps[i]);
        }
        Monster memory _monster = monsters[_monsterIndex];
        for (uint i = 0; i < _monster.basemonsterxps.length; i++) {
           
                basemonsterxps.push(_monster.basemonsterxps[i].monsterxps);
          
        }
        emit MonsterXpsChanged(_monsterIndex);
    }
    function pause() external onlyRole(DEFAULT_ADMIN_ROLE) {
        _pause();
    }
    function unpause() external onlyRole(DEFAULT_ADMIN_ROLE) {
        _unpause();
    }
    function setWithdrawalFee(uint _decreaseWithdrawalFeeByDay, uint _withdrawalFee)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        require(_withdrawalFee <= MAX_WITHDRAWAL_FEE, "Incorrect value withdrawal Fee");
        withdrawalFee = _withdrawalFee;
        decreaseWithdrawalFeeByDay = _decreaseWithdrawalFeeByDay;
    }
    function migrateBannedList(address[] memory accounts) external onlyRole(DEFAULT_ADMIN_ROLE) {
        for (uint i = 0; i < accounts.length; i++) {
            bannedList[accounts[i]] = true;
        }
    }
    function setBannAddress(address account, bool status) external onlyRole(DEFAULT_ADMIN_ROLE) {
        bannedList[account] = status;
    }
    function setBurnAddress(address account) external onlyRole(DEFAULT_ADMIN_ROLE) {
        burnAddress = account;
        emit UpdatedBurnAddress(account);
    }
    function setPriceOracle (address account) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(account != address(0), "Address cant be zero");
        priceOracle = IPriceOracle(account);
        emit UpdatedPriceOracle(account);
    }
    function setZenytokenContract(address tokenAddress) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(tokenAddress != address(0), "Address cant be zero");
        zenytoken = IERC20Upgradeable(tokenAddress);
        emit UpdatedTokenContract(tokenAddress);
    }
    function setNFTContract(address characterAddress) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(characterAddress != address(0), "Address cant be zero");
        bnbronft = IBNBRagnarok(characterAddress);
        emit UpdatedNFTContract(characterAddress);
    }
    function setTreasuryAddress(address _treasuryAddress) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(_treasuryAddress != address(0), "Address cant be zero");
        treasuryAddress = _treasuryAddress;
    }
    function setDividePercent(uint256 percent) external onlyRole(DEFAULT_ADMIN_ROLE) {
        dividePercent = percent;
        emit UpdatedDividePercent(percent);
    }
    function setNumTokensToSend(uint256 amount) external onlyRole(DEFAULT_ADMIN_ROLE) {
        numTokensToSend = amount;
    }  
    // function setFeeToLvlUp(uint256 amount) external onlyRole(DEFAULT_ADMIN_ROLE) {
    //     feeToLevelup = amount;
    // }    
    function setReferral(address _referralAddress) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(_referralAddress != address(0), "setReferral: ZERO");
        referral = IReferral(_referralAddress);
    }
    //Public functions ----------------------------------------------------------------------------------------------

    //fight
    function fight(uint _monsterIndex, uint _playersId) public notContract whenNotPaused nonReentrant {
        require(_monsterIndex < monsters.length, "Index out of bound");
        Monster memory _monster = monsters[_monsterIndex];
        require(_monster.enable, "Game is disabled");
        bool userWin = _getRandomForWin(_monsterIndex, _playersId);
        (uint256 discoverData)= bnbronft.fight(msg.sender, _playersId, _monsterIndex, userWin);
        (uint16 hpLoss, uint24 xpGained, uint128 bnbRewards) = unpackdiscoverData(discoverData);                  
        //puthereforwinornot
        address[] memory _rewardTokens = new address[](_monster.rewardTokens.length);
        uint128[] memory _rewardAmount = new uint128[](_monster.rewardTokens.length);
        if (userWin) {
            for (uint i = 0; i < _monster.rewardTokens.length; i++) {
               // tmpRewards = bnbRewards + _planet.rewardTokens[i].rewardInToken; 
                _rewardAmount[i] = ((bnbRewards / _monster.rewardTokens[i].rewardInToken) * 100) + _monster.rewardTokens[i].rewardInToken;
                _rewardTokens[i] = _monster.rewardTokens[i].token;
            }
            _balanceIncrease(msg.sender, _rewardTokens, _rewardAmount);
        }
        emit FightMonster(msg.sender, _monsterIndex, userWin, _rewardTokens, _rewardAmount, xpGained, hpLoss);
    }

    function withdrawReward() public notContract whenNotPaused nonReentrant {
        require(withdrawTimeLock[msg.sender] > 0, "Withdraw not allowed");
        require(bannedList[msg.sender] == false);
        uint calcFee;
        uint multipl = (block.timestamp - withdrawTimeLock[msg.sender]) / 86400;
        calcFee = (multipl * decreaseWithdrawalFeeByDay) >= withdrawalFee
            ? 0
            : withdrawalFee - (multipl * decreaseWithdrawalFeeByDay);

        RewardToken[] memory _rewardBalance = new RewardToken[](rewardTokens.length);
        for (uint i = 0; i < rewardTokens.length; i++) {
            address currentToken = rewardTokens[i];
            uint128 currentBalance = userBalances[msg.sender][currentToken];
            _rewardBalance[i].token = currentToken;
            _rewardBalance[i].rewardInToken = userBalances[msg.sender][currentToken];
            delete userBalances[msg.sender][currentToken];
            if (currentBalance > 0) {
                uint fee = (currentBalance * calcFee) / 10000;
                IERC20Upgradeable(currentToken).safeTransfer(treasuryAddress, fee);
                IERC20Upgradeable(currentToken).safeTransfer(msg.sender, currentBalance - fee);
            }
        }
        
        delete withdrawTimeLock[msg.sender];
        emit Withdrew(msg.sender, _rewardBalance);
    }

    function getUserRewardBalances(address _user) public view returns (address[] memory, uint128[] memory) {
        uint128[] memory balances = new uint128[](rewardTokens.length);
        for (uint i = 0; i < balances.length; i++) {
            balances[i] = userBalances[_user][rewardTokens[i]];
        }
        return (rewardTokens, balances);
    }

    function getMonsterCount() public view returns(uint count){
        count = monsters.length;
    }

    function getHero(uint256 _heroId, bool calcVip) internal view returns (HeroLibrary.Hero memory) {        
        return bnbronft.getHero(_heroId, calcVip);
    }

    function getHeroesByOwner(address account, bool calcVip) public view returns (HeroLibrary.Hero[] memory) {
        uint256 balance = bnbronft.balanceOf(account);
        HeroLibrary.Hero[] memory heroes = new HeroLibrary.Hero[](balance);
        for(uint256 i = 0; i < balance; i ++) {
            heroes[i] = getHero(bnbronft.tokenOfOwnerByIndex(account, i), calcVip);
        }
        return heroes;
    }

    function getVipsOfPlayer(address account) public view returns (HeroLibrary.VipRagnarok memory) {
        return bnbronft.getVipRagnarokOfPlayer(account);
    }

    function getVipLevel(address account) public view returns (uint256) {        
        return bnbronft.getVipRagnarokLevel(account);
    }
    
    function sendPrizeHero(address account) public onlyRole(DEFAULT_ADMIN_ROLE) {
        uint256 heroId = _createHero(msg.sender, account);
        emit CreatedAndSendPrizeHero(account, heroId);
    }    
    function createNewChar(address _referrer) public notContract nonReentrant{
        require(bannedList[msg.sender] == false, "bad boy!");
        if (_referrer != address(0) && _referrer != msg.sender) {
            referral.recordReferral(msg.sender, _referrer);
        }
        uint256 heroId = _createHero(msg.sender, msg.sender);
        emit CreatedChar(msg.sender, heroId);
    }

    function WarpPortal(uint256 _heroId) public nonReentrant{
        uint256 cost = priceOracle.getWarpPrice();
        require(zenytoken.balanceOf(msg.sender) >= cost, "Insufficient $ZENY balance");
        bnbronft.WarpPortal(_heroId);
        payForOperation(msg.sender, cost);    
        emit WarpPortalHero(msg.sender, _heroId);
    }
    
    function upgradeVip() public nonReentrant{
        uint8 level = bnbronft.getVipRagnarokLevel(msg.sender);
        require (level < 5, "Vip Level was fully upgraded");
        uint256 priceForUpdate = priceOracle.getVipUpgradePrice(level + 1); 
        require(zenytoken.balanceOf(msg.sender) >= priceForUpdate, "Insufficient $ZENY balance");
        bnbronft.upgradeVipRagnarok(msg.sender);
        payForOperation(msg.sender, priceForUpdate);     
        emit UpgradedVip(msg.sender, level + 1);
    }

    function moveHeroToStorage(uint256 heroId) public nonReentrant{
        require(bnbronft.ownerOf(heroId) == msg.sender, "You are not owner of this hero");
        require(heroIDsInStorage[msg.sender].contains(heroId) == false, "The hero was already moved");
        require(heroIDsInStorage[msg.sender].length() < maxHeroCount, "Your slots are full");
        bnbronft.safeTransferFrom(msg.sender, address(this), heroId);
        heroIDsInStorage[msg.sender].add(heroId);
        listings[heroId] = block.timestamp;
        emit MovedHeroToStorage(msg.sender, heroId);
    }

    function takeHeroFromStorage(uint256 heroId) public nonReentrant{
        require(heroIDsInStorage[msg.sender].contains(heroId) == true, "The hero does not exist in this storage");
        bnbronft.safeTransferFrom(address(this), msg.sender, heroId);
        bnbronft.resumeStaminaTimeStamp(heroId, block.timestamp - listings[heroId]);
        heroIDsInStorage[msg.sender].remove(heroId);
        delete listings[heroId];
        emit TakeHeroFromStorage(msg.sender, heroId);
    }

    function getHeroesInStorage(address account) public view returns (HeroLibrary.Hero[] memory) {
        HeroLibrary.Hero[] memory heroes = new HeroLibrary.Hero[](heroIDsInStorage[account].length());
        for (uint256 index = 0; index < heroIDsInStorage[account].length(); index ++) {
            uint256 heroId = heroIDsInStorage[account].at(index);
            HeroLibrary.Hero memory hero = bnbronft.getHero(heroId, false);
            hero.hp = bnbronft.calcHpForFreeHeroes(heroId, block.timestamp - listings[heroId]);      
            heroes[index] = hero;      
        }
        return heroes;
    }

    function unLockLevel(uint256 _heroId) public nonReentrant{
        uint256 price = priceOracle.getTokenPrice();
        if (rewardsInCurLvl[_heroId] < 0.01 ether) {
            rewardsInCurLvl[_heroId] = 0.01 ether;
        }
        price = rewardsInCurLvl[_heroId].mul(price).mul(25).div(100).div(10 ** 18);        
        require(zenytoken.balanceOf(msg.sender) >= price, "Insufficient ZENY balance");        
        uint256 level = bnbronft.unlockLevel(_heroId);
        rewardsInCurLvl[_heroId] = 0;
        payForOperation(msg.sender, price);
        emit UnlockedLevel(msg.sender, _heroId, level);
    }

    function getPriceToUnlockLevel(uint256 _heroId) public view returns (uint256) {
        uint256 price = priceOracle.getTokenPrice();
        if (rewardsInCurLvl[_heroId] < 0.01 ether) {
            return (price).mul(10 ** 16).mul(25).div(100).div(10 ** 18);      
        }
        return rewardsInCurLvl[_heroId].mul(price).mul(25).div(100).div(10 ** 18);      
    }

    //Internal functions --------------------------------------------------------------------------------------------


    function _isContract(address _addr) internal view returns (bool) {
        uint size;
        assembly {
            size := extcodesize(_addr)
        }
        return size > 0;
    }

    function _tokenInArray(address _token) internal view returns (bool) {
        for (uint i = 0; i < rewardTokens.length; i++) {
            if (_token == rewardTokens[i]) return true;
        }
        return false;
    }

    function _createHero(address payer, address fighter) internal returns(uint256) {
        require(address(priceOracle) != address(0), "Price Oracle was not set yet");
        uint256 characterPrice = priceOracle.getCharacterPrice();
        require(zenytoken.balanceOf(payer) >= characterPrice, "Insufficient ZENY balance");
        uint256 heroId = bnbronft.mint(fighter);
        payForOperation(payer, characterPrice);
        return heroId;
    }

    function payForOperation(address payer, uint256 amount) internal {
        uint256 refamount = 0;
        address referrer = referral.getReferrer(payer);
        if (referrer != address(0) && referrer != msg.sender) {
            refamount = amount.mul(5).div(100);
            IERC20Upgradeable(zenytoken).safeTransferFrom(payer, referrer, refamount);
            emit ReferralCommissionPaid(payer, referrer, refamount);
        }
        IERC20Upgradeable(zenytoken).safeTransferFrom(payer, address(this), amount.sub(refamount));  
                 
        if (numTokensToSend <= zenytoken.balanceOf(address(this))) {
            uint256 swapTokenAmount = numTokensToSend.mul(dividePercent).div(100);
            IERC20Upgradeable(zenytoken).safeTransfer(address(zenytoken), swapTokenAmount);
            IERC20Upgradeable(zenytoken).safeTransfer(burnAddress, numTokensToSend.sub(swapTokenAmount));
        }
    }

    function unpackdiscoverData(uint256 data) internal pure returns(uint16, uint24, uint128) {
        uint16 hpLoss = uint16(data & 0xFFFF);
        uint24 xpGained = uint24((data >> 16) & 0xFFFFFF);
        uint128 bnbRewards = uint128((data >> 40) & 0xFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFF);
        return (hpLoss, xpGained, bnbRewards);
    }

     function onERC721Received(
        address,
        address,
        uint256,
        bytes calldata
    ) external override pure returns (bytes4) {
        return this.onERC721Received.selector;
    }

    //Private functions --------------------------------------------------------------------------------------------

    function _getRandomForWin(uint256 _gameIndex, uint256 _playersId) private view returns (bool) {
        uint chancetowinlast = monsters[_gameIndex].chanceToWin + bnbronft.getStat(_playersId).div(3); //add extra chance to win . Example : Game Index = 5900 ( 59%) +  10%/3 from stat ) , Total 62% chance to win 
        uint random = (uint(keccak256(abi.encodePacked(blockhash(block.number - 1), gasleft()))) % 10000) + 1;
        return random < chancetowinlast;
    }

    function _balanceIncrease(address _user, address[] memory _token, uint128[] memory _amount) private {
        require(_token.length == _amount.length, "Wrong arrays length");
        if(withdrawTimeLock[_user] == 0) withdrawTimeLock[_user] = block.timestamp;
        for(uint i = 0; i < _token.length; i++){
            userBalances[_user][_token[i]] +=  _amount[i];
        }
    }

    function _balanceDecrease(address _user, address[] memory _token, uint128[] memory _amount) private {
        require(_token.length == _amount.length);
        for(uint i = 0; i < _token.length; i++){
            require(userBalances[_user][_token[i]] >=  _amount[i], "Insufficient balance");
            userBalances[_user][_token[i]] -=  _amount[i];
        }
    }

}

