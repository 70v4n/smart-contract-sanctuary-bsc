pragma solidity ^0.8.3;







interface ILiquidityLocker {

    function lockLPToken(address token_, uint256 _saleAmount, uint256 unlock_date_, address payable withdrawer_) external payable;

    function pairExists(address token_) external view returns(bool);

}



contract BaseILO is ReentrancyGuard {       

    //---------- Libraries ----------//

    using SafeMath for uint256;

    using EnumerableSet for EnumerableSet.AddressSet;



    //---------- Contracts ----------//

    IERC20 public TOKEN;

    ILiquidityLocker public LOCKER;



    //---------- Variables ----------//

    EnumerableSet.AddressSet private WHITELIST;

    address payable public OWNER;

    address public FACTORY;

    uint256 public VERSION = 1;

    uint256 public TOTAL_COLLECTED; 

    uint256 public TOTAL_SOLD; 

    uint256 public TOTAL_BUYERS;

    string public TOKEN_DATA;

      

    //---------- Enums -----------//

	enum State { NotExist, OnSale, Completed, Failed }



    //---------- Storage -----------//

    struct Info {

        uint256 Price; 

        uint256 Max4user; 

        uint256 Amount; 

        uint256 Hardcap;

        uint256 Softcap;

        uint256 DexRate; 

        uint256 Start;

        uint256 End;

        uint256 LockPercent;

        uint256 LockTime; 

        bool Whitelist; 

        State state;

    }

    

    struct Buyer {

        uint256 bought; 

        uint256 owed; 

    }



    Info public INFO;

    mapping(address => Buyer) public BUYERS;

    

    //---------- Events -----------//

    event WithdrawLP(uint256 amount);

    event Withdraw(address buyer, uint256 amount);

    event CreatedLP(address LP, uint256 amount);



    //---------- Constructor ----------//

    constructor(address _factory, address _locker) {

        FACTORY = _factory;

        LOCKER = ILiquidityLocker(_locker);

    }

    



    //---------- Modifiers ----------// 

    modifier onlyFactory() {

        require(FACTORY == msg.sender);

        _;

    }    

    modifier onlyOwner() {

        require(OWNER == msg.sender); 

        _;

    }



    //----------- External Functions -----------//

    function init(

        address payable _owner, 

        uint256 _amount,

        uint256 _tokenPrice, 

        uint256 _maxEthPerBuyer, 

        uint256 _hardcap, 

        uint256 _softcap,

        uint256 _liquidityPercent,

        uint256 _listingRate,

        uint256 _start,

        uint256 _end,

        uint256 _lockPeriod    

        ) external onlyFactory {          

        OWNER = _owner;

        INFO.Amount = _amount;

        INFO.Price = _tokenPrice;

        INFO.Max4user = _maxEthPerBuyer;

        INFO.Hardcap = _hardcap;

        INFO.Softcap = _softcap;

        INFO.LockPercent = _liquidityPercent;

        INFO.DexRate = _listingRate;

        INFO.Start = _start;

        INFO.End = _end;

        INFO.LockTime = _lockPeriod;

    }



    function init2(address _token) external onlyFactory {

        INFO.state = State.OnSale;

        TOKEN = IERC20(_token);    

    }



    

    function status() public view returns (uint256) {

        if (INFO.state == State.Failed) {

        return 3; 

        }

        if (block.timestamp > INFO.End && TOTAL_COLLECTED < INFO.Softcap) {

        return 3; 

        }        

        if (TOTAL_COLLECTED >= INFO.Hardcap) {

        return 2; 

        }

        if (block.timestamp > INFO.End && TOTAL_COLLECTED >= INFO.Softcap) {

        return 2; 

        }

        if (block.timestamp >= INFO.Start && block.timestamp <= INFO.End) {

        return 1; 

        }

        return 0; 

    } 



    function buy() external payable nonReentrant {

        require(status() == 1, 'Not active');

        if (INFO.Whitelist) {

        require(WHITELIST.contains(msg.sender), 'Not allowed');

        }

        Buyer storage b = BUYERS[msg.sender];

        uint256 amount_in = msg.value;

        uint256 allowance = INFO.Max4user.sub(b.bought);

        uint256 remaining = INFO.Hardcap.sub(TOTAL_COLLECTED);

        allowance = allowance > remaining ? remaining : allowance;

        if (amount_in > allowance) {

            amount_in = allowance;

        }

        uint256 tokensSold = amount_in.mul(INFO.Price).div(10 ** 18);

        require(tokensSold > 0, 'ZERO TOKENS');

        if (b.bought == 0) {

            TOTAL_BUYERS ++;

        }

        b.bought = b.bought.add(amount_in);

        b.owed = b.owed.add(tokensSold);

        TOTAL_COLLECTED = TOTAL_COLLECTED.add(amount_in);

        TOTAL_SOLD = TOTAL_SOLD.add(tokensSold);



        if (amount_in < msg.value) {

            payable(msg.sender).transfer(msg.value.sub(amount_in));

        }

    }



    function withdrawTokens() external nonReentrant {

        require(INFO.state == State.Completed, 'Waiting complete');

        Buyer storage b = BUYERS[msg.sender];

        uint256 tokensOwed = b.owed;

        require(tokensOwed > 0, 'Nothing to withdraw');

        b.owed = 0;

        TOKEN.transfer(msg.sender, tokensOwed); 

    }



    function withdrawBNB() external nonReentrant {

        require(status() == 3, 'Not failed'); 

        Buyer storage b = BUYERS[msg.sender];

        uint256 bnbSpend = b.bought;

        require(bnbSpend > 0, 'Nothing to withdraw');

        b.bought = 0;

        payable(msg.sender).transfer(bnbSpend); 

    }



    function ownerWithdrawTokens() external onlyOwner {

        require(status() == 3);

        TOKEN.transfer(msg.sender, TOKEN.balanceOf(address(this)));

    }



    function forceFailIfPairExists() external virtual {

        require(INFO.state != State.Completed);

        if (LOCKER.pairExists(address(TOKEN))) {

            INFO.state = State.Failed;

        } 

    }



    function forceFail() external onlyOwner {

        require(INFO.state != State.Completed);

        INFO.state = State.Failed;

    }



    function addLiquidity() external nonReentrant {

        require(INFO.state != State.Completed, 'GENERATION COMPLETE');

        require(status() == 2, 'NOT SUCCESS');

        if (LOCKER.pairExists(address(TOKEN))) {

            INFO.state = State.Failed;

            return;

        }    

        uint256 baseLiquidity = TOTAL_COLLECTED.mul(INFO.LockPercent).div(1000); 

        uint256 tokenLiquidity = baseLiquidity.mul(INFO.DexRate).div(10 ** 18);        

        TOKEN.transfer(address(LOCKER), tokenLiquidity);

        LOCKER.lockLPToken{value: baseLiquidity}(address(TOKEN), tokenLiquidity, block.timestamp.add(INFO.LockTime), OWNER);        

        

        

        uint256 remainingSBalance = TOKEN.balanceOf(address(this));

        if (remainingSBalance > TOTAL_SOLD) {

            uint256 burnAmount = remainingSBalance.sub(TOTAL_SOLD);

            TOKEN.transfer(0x000000000000000000000000000000000000dEaD, burnAmount);

        }

        

        uint256 remainingBaseBalance = address(this).balance;

        payable(OWNER).transfer(remainingBaseBalance);        

        INFO.state = State.Completed;

    }



    function setWhitelist(bool set_) external onlyOwner {

        INFO.Whitelist = set_;

    }



    function editWhitelist(address[] memory _users, bool _add) external onlyOwner {

        if (_add) {

            for (uint i = 0; i < _users.length; i++) {

            WHITELIST.add(_users[i]);

            }

        } else {

            for (uint i = 0; i < _users.length; i++) {

            WHITELIST.remove(_users[i]);

            }

        }

    }



    function getWhitelistLength() external view returns (uint256) {

        return WHITELIST.length();

    }

    

    function getUserWhitelistStatus(address _user) external view returns (bool) {

        return WHITELIST.contains(_user);

    }



}