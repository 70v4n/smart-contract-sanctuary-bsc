import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
//import "@openzeppelin/contracts/token/ERC1155/extensions/IERC1155MetadataURI.sol";
import "@openzeppelin/contracts/token/ERC1155/extensions/ERC1155Burnable.sol";
import "./Strings.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155Receiver.sol";
import "./ILoserUyangTreasury.sol";

contract OwnableDelegateProxy {}

contract ProxyRegistry {
    mapping(address => OwnableDelegateProxy) public proxies;
}

/**
 * @title ERC1155Tradable
 * ERC1155Tradable - ERC1155 contract that whitelists an operator address, has create and mint functionality, and supports useful standards from OpenZeppelin,
  like _exists(), name(), symbol(), and totalSupply()
 */
contract ERC1155Tradable is ERC1155, ERC1155Burnable, Ownable, IERC1155Receiver {
    using Strings for string;

    struct FutureProject {
        address creator;
        uint256 id;
        string uri;
        uint256 capacity;
        uint256 raiseToken;
        uint256 tokenSum;
        uint256 uyangGateId;
        
    }

    struct FutureProjectExtra {
        uint256 support;
        uint256 oppose;
        bool contractAudit;
        bool isClose;
        uint256 launchDuring;
        uint256 projectDuring;
        uint256 timestamp;
    }

    address proxyRegistryAddress;
    address uyangGate;
    address uyangStone;
    address lowbToken;
    address treasuryContract;

    uint256 private _currentTokenID = 0;
    uint256 public nftTypeSum = 0;
    mapping(uint256 => address) public creators;
    mapping(uint256 => uint256) public tokenSupply;
    mapping(address => uint256) public composedPunk;
    mapping(uint256 => string) public idToUri;

    mapping(uint256 => FutureProject) public idToProject;
    mapping(uint256 => FutureProjectExtra) public idToProjectExtra;
    mapping(address => mapping(uint256 => uint256)) public userSupportProject;
    mapping(address => mapping(uint256 => uint256)) public userOpposeProject;
    mapping(address => uint256) public pendingWithdrawals;
    mapping(address => uint256) public allHistoryClaim;
    // Contract name
    string public name;
    // Contract symbol
    string public symbol;

    bool private isCreating = false;

    uint256 public feeBase = 10000;

    uint256 public feeValue = 300;

    uint256 public GATES_TYPE = 10;

    uint256 public GATES_NFT_NUM = 1000;

    uint256 public allHistoryFee = 0;


    /**
     * @dev Require msg.sender to be the creator of the token id
     */
    modifier creatorOnly(uint256 _id) {
        require(creators[_id] == msg.sender, "ERC1155Tradable#creatorOnly: ONLY_CREATOR_ALLOWED");
        _;
    }

    /**
     * @dev Require msg.sender to own more than 0 of the token id
     */
    modifier ownersOnly(uint256 _id) {
        require(
            balanceOf(msg.sender, _id) > 0,
            "ERC1155Tradable#ownersOnly: ONLY_OWNERS_ALLOWED"
        );
        _;
    }

    event PublishProject(uint256 id, address creator);
    event ParticipateProject(address sender, uint256 id, uint256 token, uint256 lowb);
    event SupportProject(address sender, uint256 id, uint256 amount);
    event OpposeProject(address sender, uint256 id, uint256 amount);

    constructor(
        string memory _name,
        string memory _symbol,
        address _proxyRegistryAddress,
        address _uyangGate,
        address _uyangStone,
        address _lowbToken,
        address _treasury
    ) ERC1155("") {
        name = _name;
        symbol = _symbol;
        proxyRegistryAddress = _proxyRegistryAddress;
        uyangGate = _uyangGate;
        uyangStone = _uyangStone;
        lowbToken = _lowbToken;
        treasuryContract = _treasury;
    }

    function uri(uint256 _id) public view override returns (string memory) {
        require(_exists(_id), "ERC721Tradable#uri: NONEXISTENT_TOKEN");
        return idToUri[_id];
    }

    /**
     * @dev Returns the total quantity for a token ID
     * @param _id uint256 ID of the token to query
     * @return amount of token in existence
     */
    function totalSupply(uint256 _id) public view returns (uint256) {
        return tokenSupply[_id];
    }

    function depositLowb(uint256 amount) public  {
        IERC20 lowbERC20 = IERC20(lowbToken);
        lowbERC20.transferFrom(msg.sender, address(this), amount);
        pendingWithdrawals[msg.sender] += amount;
    }

    function withdrawLowb(uint256 amount) public {
        require(
            amount <= pendingWithdrawals[msg.sender],
            "amount larger than that pending to withdraw"
        );
        pendingWithdrawals[msg.sender] -= amount;
        IERC20 token = IERC20(lowbToken);
        require(token.transfer(msg.sender, amount), "Lowb transfer failed");
    }

    function publishFutureProject(
        uint256 _projectValue,
        uint256 _tokenSum,
        uint256 _uyangGateId,
        string calldata  _uri,
        bytes calldata  _data,
        uint256 _launchDuring,
        uint256 _projectDuring
    ) external returns (uint256) {
        require(!isCreating, "other article is creating");
        require(bytes(_uri).length > 0, "uri is empty");
        require(_uyangGateId > 0, "_uyangGateId is 0");
        isCreating = true;
        IERC1155 uyangGateNft = IERC1155(uyangGate);
        uyangGateNft.safeTransferFrom(msg.sender, address(this), _uyangGateId, 1, _data);

        uint256 _id = _getNextTokenID();
        _incrementTokenTypeId();
        creators[_id] = msg.sender;

        if (bytes(_uri).length > 0) {
            emit URI(_uri, _id);
        }
        idToUri[_id] = _uri;
        _mint(address(this), _id, _tokenSum, _data);
        idToProject[_id] = FutureProject(msg.sender, _id, _uri, _projectValue, 0, _tokenSum, _uyangGateId);
        idToProjectExtra[_id] = FutureProjectExtra(0, 0, false, false,  _launchDuring, _projectDuring, block.timestamp);

        tokenSupply[_id] = _tokenSum;
        nftTypeSum++;
        isCreating = false;
        emit PublishProject(_id, msg.sender);
        return _id;
    }

    function participateFutureProject(
        uint256 _id,
        uint256 _tokens
    ) external returns (uint256) {
        require(_id != 0, "id can not be 0 !");
        require(_tokens != 0, "tokens can not be 0 !");
        FutureProject storage project = idToProject[_id];
        FutureProjectExtra storage projectExtra = idToProjectExtra[_id];
        require(!projectExtra.isClose, "project has close !");
        require(projectExtra.launchDuring + projectExtra.timestamp > block.timestamp, "project luanch time is over !");
        require(project.raiseToken + _tokens <= project.tokenSum, "project has enough token!");
        require(_tokens != 0, "tokens can not be 0 !");
        uint256 needValue = (project.capacity * _tokens) / project.tokenSum;

        IERC20 uyangToken = IERC20(uyangStone);
        uint256 uyangBalance = uyangToken.balanceOf(msg.sender);
        require(uyangBalance * 1000 > needValue, "you need more UyangStone!");

        require(pendingWithdrawals[msg.sender] > needValue, "desposit lowb is less!");
        pendingWithdrawals[msg.sender] -= needValue;
        IERC20 lowbERC20 = IERC20(lowbToken);
        
        uint fee = needValue * feeValue / feeBase;
        require(lowbERC20.transfer(project.creator, needValue - fee), "transfer to creator lowb is less!");
        pendingWithdrawals[address(this)] += fee;
        allHistoryFee += fee;
        
        project.raiseToken += _tokens;
        IERC1155 loserDaoNft = IERC1155(address(this));
        loserDaoNft.safeTransferFrom(address(this), msg.sender, _id, _tokens, "0x00");
        emit ParticipateProject(msg.sender, _id, _tokens, needValue); 
    }

    function auditFutureProject(uint _id, bool _audit) external onlyOwner returns (uint256) {
        FutureProjectExtra storage project = idToProjectExtra[_id];
        project.contractAudit = _audit;
    }

    function closeFutureProject(uint256 _id, bool isClose) external returns (uint256) {
        FutureProject storage project = idToProject[_id];
        FutureProjectExtra storage projectExtra = idToProjectExtra[_id];
        require(project.creator == msg.sender, "only creator can closeFutureProject!");
        projectExtra.isClose = isClose;
    }

    function releaseFutureProject(uint256 _id) external onlyOwner returns (uint256) {
        FutureProject storage project = idToProject[_id];
        if (project.uyangGateId > 0) {
            IERC1155 uyangGateNft = IERC1155(uyangGate);
            uyangGateNft.safeTransferFrom(address(this), msg.sender, project.uyangGateId, 1, "0x00");
        }
    }

    function claimFutureProjectLowb() external returns (uint256) {
        require(allHistoryFee > 0, "allHistoryFee need > 0");
        ILoserUyangTreasury treasury = ILoserUyangTreasury(treasuryContract);
        treasury.sendAllTreasuryBalance(allHistoryFee);
        allHistoryFee = 0;
        pendingWithdrawals[address(this)] = 0;
    }

    

    // support FutureProject
    function supportFutureProject(uint256 _id, uint256 uyangAmount)
        public
        returns (bool)
    {
        IERC20 uyangToken = IERC20(uyangStone);
        uint256 uyangBalance = uyangToken.balanceOf(msg.sender);

        uint256 uyangUsed = userSupportProject[msg.sender][_id] +
            userOpposeProject[msg.sender][_id];
        require(uyangBalance > uyangUsed, "this article your uyang has used");
        uint256 maxSupport = uyangBalance - uyangUsed;
        require(
            maxSupport >= uyangAmount,
            "this FutureProject : you have not enough uyang"
        );
        userSupportProject[msg.sender][_id] += uyangAmount;
        emit SupportProject(msg.sender, _id, uyangAmount);
        return true;
    }

    // oppose Article
    function opposeFutureProject(uint256 _id, uint256 uyangAmount)
        public
        returns (bool)
    {
        IERC20 uyangToken = IERC20(uyangStone);
        uint256 uyangBalance = uyangToken.balanceOf(msg.sender);
        uint256 uyangUsed = userSupportProject[msg.sender][_id] +
            userOpposeProject[msg.sender][_id];
        require(uyangBalance > uyangUsed, "this article your uyang has used");
        uint256 maxOppose = uyangBalance - uyangUsed;
        require(
            maxOppose >= uyangAmount,
            "this article : you have not enough uyang"
        );
        userOpposeProject[msg.sender][_id] += uyangAmount;
        emit OpposeProject(msg.sender, _id, uyangAmount);
        return true;
    }

    // /**
    //  * never mint
    //  * @dev Mints some amount of tokens to an address
    //  * @param _to          Address of the future owner of the token
    //  * @param _id          Token ID to mint
    //  * @param _quantity    Amount of tokens to mint
    //  * @param _data        Data to pass if receiver is contract
    //  */
    // function mint(
    //     address _to,
    //     uint256 _id,
    //     uint256 _quantity,
    //     bytes memory _data
    // ) public creatorOnly(_id) {
    //     _mint(_to, _id, _quantity, _data);
    //     tokenSupply[_id] = tokenSupply[_id] + _quantity;
    // }

    // /**
    //  * never mint
    //  * @dev Mint tokens for each id in _ids
    //  * @param _to          The address to mint tokens to
    //  * @param _ids         Array of ids to mint
    //  * @param _quantities  Array of amounts of tokens to mint per id
    //  * @param _data        Data to pass if receiver is contract
    //  */
    // function batchMint(
    //     address _to,
    //     uint256[] memory _ids,
    //     uint256[] memory _quantities,
    //     bytes memory _data
    // ) public {
    //     for (uint256 i = 0; i < _ids.length; i++) {
    //         uint256 _id = _ids[i];
    //         require(
    //             creators[_id] == msg.sender,
    //             "ERC1155Tradable#batchMint: ONLY_CREATOR_ALLOWED"
    //         );
    //         uint256 quantity = _quantities[i];
    //         tokenSupply[_id] = tokenSupply[_id] + quantity;
    //     }
    //     _mintBatch(_to, _ids, _quantities, _data);
    // }


    /**
     * Override isApprovedForAll to whitelist user's OpenSea proxy accounts to enable gas-free listings.
     */
    function isApprovedForAll(address _owner, address _operator)
        public
        view
        override
        returns (bool isOperator)
    {
        // Whitelist OpenSea proxy contract for easy trading.
        // ProxyRegistry proxyRegistry = ProxyRegistry(proxyRegistryAddress);
        // if (address(proxyRegistry.proxies(_owner)) == _operator) {
        //     return true;
        // }

        return ERC1155.isApprovedForAll(_owner, _operator);
    }

    /**
     * @dev Change the creator address for given token
     * @param _to   Address of the new creator
     * @param _id  Token IDs to change creator of
     */
    function _setCreator(address _to, uint256 _id) internal creatorOnly(_id) {
        creators[_id] = _to;
    }

    /**
     * @dev Returns whether the specified token exists by checking to see if it has a creator
     * @param _id uint256 ID of the token to query the existence of
     * @return bool whether the token exists
     */
    function _exists(uint256 _id) internal view returns (bool) {
        return creators[_id] != address(0);
    }

    /**
     * @dev calculates the next token ID based on value of _currentTokenID
     * @return uint256 for the next token ID
     */
    function _getNextTokenID() private view returns (uint256) {
        return _currentTokenID + 1;
