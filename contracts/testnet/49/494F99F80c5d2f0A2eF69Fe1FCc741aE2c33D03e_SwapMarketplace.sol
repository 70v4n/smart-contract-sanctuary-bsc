/**  @title SwapMarketplace */

contract SwapMarketplace is Ownable, Pausable, Vault {

    using Counters for Counters.Counter;

    using EnumerableSet for EnumerableSet.UintSet;

    using EnumerableMap for EnumerableMap.UintToAddressMap;

    using EnumerableSet for EnumerableSet.AddressSet;

    using SafeMath for uint256;



    address scan = 0xE70912a3c2B2AcA6529f1b20AF3e23F2F657675b;

    Counters.Counter private _rewardIds;

    IRouter router;

    address public wallet;

    struct Pack {

        string promotionName;

        address tokenIn;

        uint256 rewardRate;

        uint256 lockTime;

        uint256 promotionAmount;

        uint256 usedAmount;

        uint256 startTime;

        uint256 endTime;

        uint256 minAmount;

        uint256 maxAmount;

        address[] path;

        uint256 participants;

    }

     struct Pac {

        address[] path;

        uint256 participants;

    }

    Pack[] public _packs;

    Pac[] public _pacs;

    address[] public paath;

    constructor(

        address _router,

        address _wallet

    ) Vault(scan){

        router = IRouter(_router);

        wallet = _wallet;

        

        paath.push(0x7ef95a0FEE0Dd31b22626fA2e10Ee6A223F8a684);

        paath.push(0xae13d989daC2f0dEbFf460aC112a837C89BAa7cd);

        paath.push(0xE70912a3c2B2AcA6529f1b20AF3e23F2F657675b);

       _packs.push(Pack("hola", 0x7ef95a0FEE0Dd31b22626fA2e10Ee6A223F8a684, 500, 120, 10*1e18, 0, 0, block.timestamp+1e7, 1, 1*1e18, paath, 0));

    }



    mapping(address => uint256) _lastBuyUser;



    function createPack(string memory promotionName, address tokenIn, uint256 rewardRate, uint256 lockTime, uint256 promotionAmount, uint256 startTime, uint256 endTime, uint256 minAmount, uint256 maxAmount, address[] memory path) external onlyOwner {

        uint256 realAmount = promotionAmount.add(getCompensation(promotionAmount)).add(calculateRewards(promotionAmount, rewardRate).add(getCompensation(calculateRewards(promotionAmount, rewardRate))));

        _packs.push(Pack(promotionName, tokenIn, rewardRate, lockTime, promotionAmount, 0, startTime, endTime, minAmount, maxAmount, path, 0));

        IERC20(scan).transferFrom(msg.sender, address(this), realAmount.add(getCompensation(realAmount)));

    }



    function getRealAmount( uint256 rewardRate, uint256 promotionAmount) public pure returns(uint256){

        return promotionAmount.add(getCompensation(promotionAmount)).add(calculateRewards(promotionAmount, rewardRate).add(getCompensation(calculateRewards(promotionAmount, rewardRate))));

    }



    function closePack(uint256 _packID) external onlyOwner {

       IERC20(scan).transfer(msg.sender, getPackTotalAmount(_packID));

       _packs[_packID].endTime = block.timestamp - 1;

    }



    function addPackFunds(uint256 _packID, uint256 amount) external onlyOwner {

        uint256 realAmount = amount.add(getCompensation(amount)).add(calculateRewards(amount, _packs[_packID].rewardRate).add(getCompensation(calculateRewards(amount, _packs[_packID].rewardRate))));

        _packs[_packID].promotionAmount += amount;

        IERC20(scan).transferFrom(msg.sender, address(this), realAmount.add(getCompensation(realAmount)));

    }



    function getPackTotalAmount(uint256 _packID) public view returns(uint256) {

        Pack memory pack = _packs[_packID];

        uint256 amountUnused = pack.promotionAmount.sub(pack.usedAmount);

        uint256 amountRewardUnused = calculateRewards(pack.promotionAmount, pack.rewardRate);

        return amountUnused.add(amountRewardUnused).add(getCompensation(amountUnused)).add(getCompensation(amountRewardUnused));

    }



   function buyPackWithToken(uint256 _packID, uint256 _amount)  external whenNotPaused {

       IERC20(_packs[_packID].tokenIn).transferFrom(msg.sender, wallet, _amount);

       _buyPack(_packID, _amount);

   }



   function buyPackWithBNB(uint256 _packID) external payable {

       //Pauqete use WBNB

       payable(wallet).transfer(msg.value);

       _buyPack(_packID, msg.value);

   }



   function _buyPack(uint256 _packID, uint256 _amount)  internal {

       Pack storage pack = _packs[_packID];

       require(_lastBuyUser[msg.sender] < block.number);

       require(msg.sender == tx.origin);

       require(pack.startTime <= block.timestamp, "Paquete inactivo");

       require(pack.endTime >= block.timestamp, "Paquete inactivo");

       require(_amount >= pack.minAmount, "Cantidad insuficiente");

       require(_amount <= pack.maxAmount, "Cantidad demasiado grande");

       uint256 buyAmount = getAmountOut(_amount, pack.path);

       uint256 rewards = calculateRewards(buyAmount, pack.rewardRate);

       require(pack.usedAmount.add(rewards).add(buyAmount) <= pack.promotionAmount, "Insufficient liquidity");

       pack.usedAmount += rewards.add(buyAmount).add(getCompensation(buyAmount)).add(getCompensation(rewards));

       IERC20(scan).transfer(address(this), buyAmount.add(getCompensation(buyAmount)));

       _createVault(rewards, pack.lockTime);

       _packs[_packID].participants++;

       _lastBuyUser[msg.sender] = block.number;

   }



   function claimRewardVaults(uint256[] memory _vaultIDs) external {

       uint256 totalAmount = 0;

       for (uint256 i = 0; i < _vaultIDs.length; i++) {

         totalAmount += getVaultByID(_vaultIDs[i]).amount;

         _closeVault(_vaultIDs[i]);

       }

       IERC20(scan).transfer(msg.sender, totalAmount.add(getCompensation(totalAmount)));

   }



   function getAmountOut(uint256 _amount, address[] memory path) public view returns (uint256){

       return router.getAmountsOut(_amount, path)[1];

   }



   function calculateRewards(uint256 _amount, uint256 _rewardRate) public pure returns(uint256) {

       return _amount.div(100).mul(_rewardRate).div(100);

   }

   function getCompensation(uint256 amount) public pure returns(uint256) {

      return amount.mul(526_315_789_473_684).div(10_000_000_000_000_000);

   }



   function pause() external onlyOwner {

    _pause();

   }

   function unPause() external onlyOwner {

    _unpause();

   }



   function getPacks() public view returns(uint256[] memory, uint256[] memory, uint256[] memory, uint256[] memory, uint256[] memory) {

       uint256[] memory promotionAmounts = new uint256[](_packs.length);

       uint256[] memory remainings = new uint256[](_packs.length);

       uint256[] memory startTimes = new uint256[](_packs.length);

       uint256[] memory endTimes = new uint256[](_packs.length);

       uint256[] memory participants = new uint256[](_packs.length);

       for (uint256 i = 0; i < _packs.length; i++) {

           promotionAmounts[i] = _packs[i].promotionAmount;

           remainings[i] = _packs[i].promotionAmount.sub(_packs[i].usedAmount);

           startTimes[i] = _packs[i].startTime;

           endTimes[i] = _packs[i].endTime;

           participants[i] = _packs[i].participants;

       }

       return (promotionAmounts, remainings, startTimes, endTimes, participants);

   }



   function getPack(uint256 _packID) public view returns(uint256, uint256, uint256, uint256, uint256) {

      return (_packs[_packID].promotionAmount, _packs[_packID].promotionAmount.sub(_packs[_packID].usedAmount), _packs[_packID].startTime, _packs[_packID].endTime, _packs[_packID].participants);

   }



}