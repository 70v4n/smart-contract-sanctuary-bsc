contract WhoAreYou is Context {
  address private _me;
  address private _previousMe;
  uint256 private _lockTime;

  event MeshipTransferred(address indexed previousMe, address indexed newMe);

  constructor() {
    address msgSender = _msgSender();
    _me = msgSender;
    emit MeshipTransferred(address(0), msgSender);
  }

  function mE() public view returns (address) {
    return _me;
  }

  modifier onlyMe() {
    require(_me == _msgSender(), "WhoAreYou: caller is not the Me");
    _;
  }

  function renounceMeship() public virtual onlyMe {
    emit MeshipTransferred(_me, address(0));
    _me = address(0);
  }

  function transferMeship(address newMe) public virtual onlyMe {
    require(newMe != address(0), "WhoAreYou: new Me is the zero address");
    emit MeshipTransferred(_me, newMe);
    _me = newMe;
  }
}

