// File: swap.sol

contract SwapTest {

    address public router = 0xD99D1c33F9fC3444f8101754aBC46c52416550D1;
    address public tkn_a = 0x12C11D584d9904BAb8a35878F116296458cDDd5b;
    address public tkn_b = 0x96417F791119000b6f284c78188e7C157364921a;
    address public tkn_c = 0x35446893204Dab4d0742941cA77d315407804478;


    constructor(){
    }

    function swap(uint256 amount) public {
        address[] memory _path = new address[](2);
        _path[0] = tkn_a;
        _path[1] = tkn_b;
        // _path[2] = tkn_c;

        IERC20(tkn_a).transfer(address(this), amount);

        IERC20(tkn_a).approve(router, amount);
        IPancakeRouter02(router).swapExactTokensForTokens(
            amount,
            0,
            _path,
            msg.sender,
            block.timestamp + 600
        );
    }
}