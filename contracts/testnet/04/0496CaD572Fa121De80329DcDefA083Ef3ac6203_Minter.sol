pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/math/Math.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "../interfaces/IPCRO.sol";
import "../interfaces/IPDEBT.sol";
import "../interfaces/IOracle.sol";

import "hardhat/console.sol";

/**
* @title PCRO Minter
* @author Passage Finance
*
* Stake PASS tokens and earn PCRO
* ...
* ...
*/

contract Minter is Ownable, Pausable, ReentrancyGuard {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    /* ========== CONSTANTS ========== */

    uint256 public constant SAFE_MULTIPLIER = 1e18;

    /* ========== ENUMS ========== */

    enum Status { EXPANSIONARY, CONTRACTIONARY, BALANCED }

    /* ========== STATE VARIABLES ========== */

    // Contract address for the staked token (PASS)
    address public PASS;
    // Contract address for yield token (PCRO)
    address public PCRO;
    // Contract address for debt token (PDEBT)
    address public PDEBT;
    // Contract address for PCRO-CRO-LP oracle
    address public oracle;
    // Current epoch number
    uint256 public epoch;
    // Block which this Minter was last updated at
    uint256 public lastUpdatedEpoch;
    // Block which this Minter was last updated at
    uint256 public lastUpdatedBlock;
    // Total amount of tokens staked in this Minter
    uint256 public totalStaked;
    // Targeted % of new token to be minted per epoch
    uint256 public emissionsPercentagePerEpoch;
    // Number of blocks in an epoch
    uint256 public blocksPerEpoch;
    // Block number where 1st epoch begins
    uint256 public startEpochBlock;
    // Current epoch policy status
    Status public status;
    // Price of PCRO at $1.00
    uint256 public pcroPriceOne;
    // Ceiling price of PCRO (initialized at $1.01)
    uint256 public pcroPriceCeiling;

    /* ========== STRUCTS ========== */

    struct User {
        // Amount of tokens staked by user
        uint256 staked;
        // Amount of tokens staked in warmup by user
        uint256 warmup;
        // Amount of tokens rewarded and not claimed
        uint256 rewards;
        // Block which staked tokens can be withdrawn
        uint256 unlockBlock;
        // Amount of debt issued
        uint256 debt;
        // TWAP when debt is issued
        uint256 debtTWAP;
        // Epoch that user deposit tokens, to account for 1 epoch warmup before it is staked
        uint256 lastDepositedEpoch;
        // Epoch last updated
        uint256 lastUpdatedEpoch;
        // Block last updated
        uint256 lastUpdatedBlock;
        // Stake is active or not
        bool active;
    }

    /* ========== ARRAYS ========== */

    // Array of users
    address[] public users;

    /* ========== MAPPINGS ========== */

    mapping(address => User) public user;

    /* ========== EVENTS ========== */

    event Deposit(address indexed user, uint256 amount);
    event Withdraw(address indexed user, uint256 amount);
    event Claim(address indexed user, uint256 amount);
    event Epoch(uint256 epoch);

    /* ========== CONSTRUCTOR ========== */

    constructor(
        address _PASS,
        address _PCRO,
        address _PDEBT,
        address _oracle
    ) {
        PASS = _PASS;
        PCRO = _PCRO;
        PDEBT = _PDEBT;
        oracle = _oracle;

        totalStaked = 0;
        epoch = 0;
        blocksPerEpoch = 10;
        startEpochBlock = 1;
        emissionsPercentagePerEpoch = 10;
        lastUpdatedEpoch = 0;
        lastUpdatedBlock = block.number;
        status = Status.EXPANSIONARY;
        pcroPriceOne = 1e18;
        pcroPriceCeiling = pcroPriceOne.mul(101).div(100);
    }

    /* ========== MODIFIERS ========== */

    /**
    * Check to allow staked tokens to be withdrawn from lock only when
    * current block number is greater than unlock block in vePASS Lock.
    */
    modifier checkUnlockBlock() {
        require(block.number >= user[msg.sender].unlockBlock, "Not passed unlock block");
        _;
    }

    /**
    * Check if epoch has started
    */
    modifier checkEpochStart() {
        require(block.number >= startEpochBlock, "Epoch not started yet");
        _;
    }

    /* ========== VIEW FUNCTIONS ========== */

    /**
    * Returns the total supply of seigniorage token
    * @return totalSupply      Total supply of token
    */
    function totalSupplyOfSeigniorageTokens() public view returns (uint256) {
        return IERC20(PCRO).totalSupply();
    }

    /**
    * Returns the targeted amount of tokens to be minted in this epoch
    * @return amount      Targeted amount of tokens to be minted
    */
    function emissionsPerEpoch() public view returns (uint256) {
        return totalSupplyOfSeigniorageTokens().mul(SAFE_MULTIPLIER)
                                          .mul(emissionsPercentagePerEpoch)
                                          .div(100);
    }

    /**
    * Returns the amount of seigniorage token emissions to be given per staked token
    * @return amount      Amount of token emissions per staked token
    */
    function emissionsPerStakedToken() public view returns (uint256) {
        return emissionsPerEpoch().div(totalStaked);
    }

    /**
    * Returns the current price of PCRO
    * @return price      Current price of PCRO
    */
    function pcroPrice() public view returns (uint256) {
        try IOracle(oracle).consult(PCRO, 1e18) returns (uint256 price) {
            return price;
        } catch {
            revert("Failed to consult PCRO price from the oracle");
        }
    }

    /**
    * Returns the TWAP price of PCRO
    * @return price      TWAP price of PCRO
    */
    function pcroTWAP() public view returns (uint256) {
        try IOracle(oracle).twap(PCRO, 1e18) returns (uint256 price) {
            return price;
        } catch {
            revert("Failed to consult PCRO price from the oracle");
        }
    }



    /* ========== MUTATIVE FUNCTIONS ========== */

    /**
    * Stake PASS tokens
    * @param _amount                Amount of tokens to deposit
    * @param _unlockBlock           Block where staked tokens can be withdrawn
    */
    function deposit(uint256 _amount, uint256 _unlockBlock) external nonReentrant whenNotPaused {
        require(_amount > 0, "Cannot deposit 0");

        if (user[msg.sender].active == false) {
            users.push(msg.sender);
            user[msg.sender].active = true;
        }

        user[msg.sender].warmup = user[msg.sender].warmup.add(_amount);
        user[msg.sender].unlockBlock = _unlockBlock; // TODO: add 1 epoch?
        user[msg.sender].lastDepositedEpoch = epoch;
        user[msg.sender].lastUpdatedEpoch = epoch;
        user[msg.sender].lastUpdatedBlock = block.number;

        IERC20(PASS).safeTransferFrom(msg.sender, address(this), _amount);
        emit Deposit(msg.sender, _amount);

        _updateEpoch();
    }

    /**
    * Withdraw PASS tokens in warmup from stake
    * @param _amount          Amount of tokens to withdraw
    */
    function withdrawFromWarmup(uint256 _amount) external nonReentrant whenNotPaused {
        require(_amount > 0, "Cannot withdraw 0");

        user[msg.sender].warmup = user[msg.sender].warmup.sub(_amount);
        user[msg.sender].lastUpdatedEpoch = epoch;
        user[msg.sender].lastUpdatedBlock = block.number;

        IERC20(PASS).safeTransfer(msg.sender, _amount);
        emit Withdraw(msg.sender, _amount);

        _updateEpoch();
    }

    /**
    * Withdraw staked PASS tokens from stake
    * @param _amount          Amount of tokens to withdraw
    */
    function withdrawFromStaked(uint256 _amount) external nonReentrant whenNotPaused checkUnlockBlock() {
        require(_amount > 0, "Cannot withdraw 0");

        user[msg.sender].staked = user[msg.sender].staked.sub(_amount);
        user[msg.sender].lastUpdatedEpoch = epoch;
        user[msg.sender].lastUpdatedBlock = block.number;

        totalStaked = totalStaked.sub(_amount);

        IERC20(PASS).safeTransfer(msg.sender, _amount);
        emit Withdraw(msg.sender, _amount);

        _updateEpoch();
    }

    /**
    * Claim reward PCRO tokens from stake
    * @param _amount          Amount of tokens to claim
    */
    function claim(uint256 _amount) external nonReentrant whenNotPaused {
        require(_amount > 0, "Cannot claim 0");

        user[msg.sender].rewards = user[msg.sender].rewards.sub(_amount);
        user[msg.sender].lastUpdatedEpoch = epoch;
        user[msg.sender].lastUpdatedBlock = block.number;

        IERC20(PCRO).safeTransfer(msg.sender, _amount);
        emit Claim(msg.sender, _amount);

        _updateEpoch();
    }

    /**
    * Withdraw all PASS tokens in warmup and staked as well as PCRO rewards
    */
    function exit() external nonReentrant whenNotPaused checkUnlockBlock() {
        uint256 stakedAmount = user[msg.sender].warmup.add(user[msg.sender].staked);
        totalStaked = totalStaked.sub(user[msg.sender].staked);

        user[msg.sender].warmup = 0;
        user[msg.sender].staked = 0;

        uint256 rewardsAmount = user[msg.sender].rewards;
        user[msg.sender].rewards = 0;

        user[msg.sender].lastUpdatedEpoch = epoch;
        user[msg.sender].lastUpdatedBlock = block.number;

        IERC20(PASS).safeTransfer(msg.sender, stakedAmount);
        IERC20(PCRO).safeTransfer(msg.sender, rewardsAmount);

        emit Withdraw(msg.sender, stakedAmount);
        emit Claim(msg.sender, rewardsAmount);

        _updateEpoch();
    }

    /**
    * Swap PCRO tokens to PDEBT
    * Only allowed when Status is CONTRACTIONARY and debt supply is not maxed out
    * @param _amount          Amount of tokens
    */
    function issueDebt(uint256 _amount) external nonReentrant whenNotPaused {
        require(_amount > 0, "Cannot swap 0");
        require(status == Status.CONTRACTIONARY, "Status is not contractionary");
        // TODO CHECK for total supply of PDEBT is more than 30% supply of PCRO

        if (user[msg.sender].active == false) {
            users.push(msg.sender);
            user[msg.sender].active = true;
        }

        user[msg.sender].debt = _amount;
        // TODO Calculate and store current TWAP
        user[msg.sender].debtTWAP = 90 * (SAFE_MULTIPLIER);

        IPCRO(PCRO).burn(msg.sender, _amount);
        IPDEBT(PDEBT).mint(msg.sender, _amount);
        // emit Claim(msg.sender, _amount);

        _updateEpoch();
    }

    /**
    * Swap PDEBT tokens to PCRO with potential bonus
    * Only allowed when Status is EXPANSIONARY
    * @param _amount          Amount of tokens to swap
    */
    function redeemDebt(uint256 _amount) external nonReentrant whenNotPaused {
        require(_amount > 0, "Cannot swap 0");
        require(status == Status.EXPANSIONARY, "Status is not expansionary");

        // TODO Calculate current TWAP
        uint256 currentDebtTWAP = 110 * (SAFE_MULTIPLIER);
        uint256 debtTWAPDifference = currentDebtTWAP.sub(user[msg.sender].debtTWAP);

        uint256 _amountToMint = debtTWAPDifference.mul(70)
                                                  .div(100)
                                                  .mul(user[msg.sender].debt)
                                                  .div(SAFE_MULTIPLIER);

        user[msg.sender].debt = 0;
        user[msg.sender].debtTWAP = 0;

        IPDEBT(PDEBT).burn(msg.sender, user[msg.sender].debt);
        IPCRO(PCRO).mint(msg.sender, _amountToMint);
        // emit Claim(msg.sender, _amount);

        _updateEpoch();
    }

    function forceCallUpdateEpoch() public {
        _updateEpoch();
    }

    function _updateStatus(Status _status) public {
        status = _status;
    }

    function updatePassPrice() public {
        try IOracle(oracle).update() {} catch {}
    }

    /* ========== RESTRICTED FUNCTIONS ========== */

    function _updatePassPrice() private {
        try IOracle(oracle).update() {} catch {}
    }

    function _updateEpoch() private {
        uint256 blocksDifference = block.number.sub(lastUpdatedBlock);

        if (blocksDifference >= blocksPerEpoch) {
            // TODO: CHECK TWAP OF LAST and CURRENT EPOCH TO SET STATUS
            _updatePassPrice();
            // TODO: do we use TWAP or spot price?
            // uint256 twap = pcroTWAP();
            uint256 twap = IOracle(oracle).twap(PCRO, 1e18);

            console.log("twap: ", twap);

            if (twap > pcroPriceCeiling) {
                status = Status.EXPANSIONARY;
            } else if (twap < pcroPriceOne) {
                status = Status.CONTRACTIONARY;
            } else {
                status = Status.BALANCED;
            }

            uint256 epochsElapsed = blocksDifference.div(blocksPerEpoch);
            epoch = epoch.add(epochsElapsed);

            lastUpdatedBlock = block.number;
            lastUpdatedEpoch = epoch;

            // TODO: Total "rewards" is actually just expansion of total supply
            // So instead of calculating total emissions per staked, we can just
            // multiply by current supply and disitribute out per staked token after...
            uint256 totalRewards = 0;

            for (uint256 i = 0; i < users.length; i++) {
                if (status == Status.EXPANSIONARY) {
                    if (user[users[i]].staked > 0) {
                        uint256 rewardsAccrued = epochsElapsed.mul(user[users[i]].staked)
                                                            .mul(emissionsPerStakedToken())
                                                            .div(SAFE_MULTIPLIER);

                        user[users[i]].rewards = user[users[i]].rewards.add(rewardsAccrued);
                        totalRewards = totalRewards.add(rewardsAccrued);
                    }
                }

                if ((epoch > user[users[i]].lastDepositedEpoch) && (user[users[i]].warmup > 0)) {
                    user[users[i]].staked = user[users[i]].staked.add(user[users[i]].warmup);
                    totalStaked = totalStaked.add(user[users[i]].warmup);
                    user[users[i]].warmup = 0;
                }

                user[users[i]].lastUpdatedBlock = block.number;
                user[users[i]].lastUpdatedEpoch = epoch;
            }

            if (status == Status.EXPANSIONARY) {
                // TODO: Read TODO logic above
                IPCRO(PCRO).mint(address(this), totalRewards);
            }

            emit Epoch(epoch);
        }
    }
}

