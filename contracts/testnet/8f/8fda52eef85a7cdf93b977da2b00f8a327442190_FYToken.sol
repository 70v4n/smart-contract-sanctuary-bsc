abstract contract FYTokenCover is ERC20, ERC20Burnable, IFYToken, Pausable {
    using SafeMath for uint256;
    address internal owner;
    uint256 private _totalMinted;
    uint256 private _cap = 50 * 100000000 * (10**uint256(decimals()));
    uint256 public minPrice = 10 * (10**9);
    uint256 public minTaxPrice = 3 * (10**9);

    address[] private blackMap;

    uint256 private minTotalMinted =
        10 * 100000000 * 100000000 * (10**uint256(decimals()));
    uint256 private maxTotalMinted =
        99 * 100000000 * 100000000 * (10**uint256(decimals()));
    address internal admin;

    address internal gamePoolAddress =
        0x7e48897438107496538434370BC98Ba3c048b510;
    address internal teamAddress = 0x3981162EC18b4D6ACc636adC83773013aee428c5;
    address internal communityAddress =
        0xE10a0543B5CA6DF4859acAbd69a37876C14De2B1;
    address internal mobilityAddress =
        0xbd99C2cc3a856b9FfAD9b4a0fb732224f12D00Ac;
    address internal bankAddress = 0x68216175f391948E74A3531b558d3D564461572B;
    address internal burnAddress = 0x2f60c8891ea01c7491145F7d1444BEA5A962Bf46;

    event UpdateOwner(address oldAddr, address newAddr);
    event UpdateAdmin(address oldAddr, address newAddr);
    event MintPlayer(address indexed addr, uint256 amount);
    event UpdateMinTotalMinted(address addr, uint256 amount);
    event UpdateMaxTotalMinted(address addr, uint256 amount);
    event UpdateBlackMap(address addr);
    event Recharge(address indexed from, address indexed to, uint256 value);
    event DrawCoin(address indexed from, uint256 value);

    constructor() {
        owner = msg.sender;
        super._mint(owner, _cap.mul(5).div(100));
    }

    modifier isAdmins() {
        require(msg.sender == admin || msg.sender == owner);
        _;
    }

    modifier onlyOwner() {
        require(msg.sender == owner, "caller not owner");
        _;
    }

    modifier onlyAdmin() {
        require(msg.sender == admin, "caller not admin");
        _;
    }

    function setMinTotalMinted(uint256 _minTotalMinted) external isAdmins {
        minTotalMinted = _minTotalMinted;

        emit UpdateMinTotalMinted(msg.sender, minTotalMinted);
    }

    function setMaxTotalMinted(uint256 _maxTotalMinted) external isAdmins {
        maxTotalMinted = _maxTotalMinted;

        emit UpdateMaxTotalMinted(msg.sender, maxTotalMinted);
    }

    function setOwer(address _owner) external onlyOwner {
        require(_owner != address(0));

        owner = _owner;

        emit UpdateOwner(msg.sender, _owner);
    }

    modifier onlyGamePoolAddress() {
        require(msg.sender == gamePoolAddress);
        _;
    }

    function setGamePoolAddress(address _gamePoolAddress) external isAdmins {
        require(_gamePoolAddress != address(0));

        gamePoolAddress = _gamePoolAddress;
    }

    function setTeamAddress(address _teamAddress) external isAdmins {
        require(_teamAddress != address(0));

        teamAddress = _teamAddress;
    }

    function setCommunityAddress(address _communityAddress) external isAdmins {
        require(_communityAddress != address(0));

        communityAddress = _communityAddress;
    }

    function setMobilityAddress(address _mobilityAddress) external isAdmins {
        require(_mobilityAddress != address(0));

        mobilityAddress = _mobilityAddress;
    }

    function setBankAddress(address _bankAddress) external isAdmins {
        require(_bankAddress != address(0));

        bankAddress = _bankAddress;
    }

    function setAdmin(address _admin) external isAdmins {
        require(_admin != address(0));

        admin = _admin;

        emit UpdateAdmin(msg.sender, _admin);
    }

    function decimals() public pure override returns (uint8) {
        return 9;
    }

    function setPause() external onlyOwner whenNotPaused {
        _pause();
    }

    function setUnpause() external onlyOwner whenPaused {
        _unpause();
    }

    function _mint(address account, uint256 amount) internal override {
        super._mint(account, amount);
        _totalMinted = _totalMinted.add(amount);
        emit MintPlayer(account, amount);
    }

    function totalMinted() public view returns (uint256) {
        return _totalMinted;
    }

    modifier minAmount(uint256 _amount) {
        require(_amount >= minTaxPrice, "amount < minTaxPrice");
        _;
    }

    function setBlackMap(address addr) public onlyOwner {
        blackMap.push(addr);
    }

    function getBlackMap() public view onlyOwner returns (address[] memory) {
        return blackMap;
    }

    function publicBurn(uint256 amount) public onlyOwner {
        require(_totalMinted < (minTotalMinted + amount));
        _burn(_msgSender(), amount);
    }

    function mint(address to, uint256 amount)
        public
        onlyOwner
        minAmount(amount)
    {
        require((_totalMinted + amount) > maxTotalMinted);
        _mint(to, amount);
    }

    struct mintBulkParam {
        address to;
        uint256 amount;
    }

    function mintBulk(mintBulkParam[] memory _minters) public onlyOwner {
        for (uint256 i = 0; i < _minters.length; i++) {
            mint(_minters[i].to, _minters[i].amount);
        }
    }

    function validBalanceWithTax(
        address from,
        uint256 _amount,
        uint256 _tax
    ) internal view returns (bool) {
        if (balanceOf(from) >= _amount.add(_tax)) {
            return true;
        } else {
            return false;
        }
    }

    function getCapValue() external view override returns (uint256) {
        return _cap;
    }

    function withdraw() public onlyOwner {
        payable(owner).transfer(address(this).balance);
    }
}

pragma solidity ^0.8.9;

contract FYToken is FYTokenCover {
    using SafeMath for uint256;

    uint256 private rechargeGamePoolPercentage = 50;
    uint256 private rechargeCommunityPercentage = 40;
    uint256 private rechargeBurnPercentage = 10;

    constructor() ERC20("FYToken", "FYT") FYTokenCover() {}

    modifier validBalance(uint256 _amount) {
        require(balanceOf(msg.sender) >= _amount, "insufficient balance");
        _;
    }

    function transferAccount(
        address from,
        address to,
        uint256 amount
    ) external override returns (bool) {
        require(validBalanceWithTax(from, amount, 0), "insufficient balance");
        _transfer(from, to, amount);
        return true;
    }

    function transferAccountToAddress(uint256 amount)
        external
        override
        returns (bool)
    {
        require(balanceOf(msg.sender) >= amount, "insufficient balance");
        toAddress(amount);
        return true;
    }

    function toAddress(uint256 _amount) internal {
        transfer(
            gamePoolAddress,
            _amount * rechargeGamePoolPercentage.div(100)
        );
        transfer(
            communityAddress,
            _amount * rechargeCommunityPercentage.div(100)
        );
        transfer(burnAddress, _amount * rechargeBurnPercentage.div(100));
    }

    function recharge(uint256 _amount)
        public
        minAmount(_amount)
        validBalance(_amount)
    {
        toAddress(_amount);
        emit Recharge(msg.sender, owner, _amount);
    }

    function drawCoin(
        uint256 toUserAmount,
        address userAddress,
        uint256 toSelfAmount
    ) public onlyGamePoolAddress validBalance(toUserAmount.sub(toSelfAmount)) {
        transfer(userAddress, toUserAmount);
        transfer(
            communityAddress,
            toSelfAmount * rechargeCommunityPercentage.div(100)
        );
        transfer(burnAddress, toSelfAmount * rechargeBurnPercentage.div(100));
        emit DrawCoin(userAddress, toUserAmount);
    }
}