    modifier updateRedeem(address account){
        redeemsCRS[account] = redeemEarnedCRS(account);
        redeemsColValue[account] = redeemEarnedColValue(account);
        userRedeemPerSharePaidCRS[account] = redeemPerShareStoredCRS;
        userRedeemPerSharePaidColValue[account] = redeemPerShareStoredColValue;
        _;
    }

    constructor(address _owner, address _factory, address _asc, address _crs, address _stakerBook) Ownable(_owner) CeresBase(_factory){
        asc = IERC20(_asc);
        crs = IERC20(_crs);
        stakerBook = IStakerBook(_stakerBook);
    }

    /* ---------- Views ---------- */

    function token() public override view returns (address){
        return address(asc);
    }

    function redeemEarnedCRS(address account) public override view returns (uint256) {
        return _shareBalances[account] * (redeemPerShareStoredCRS - userRedeemPerSharePaidCRS[account])
        / SHARE_PRECISION + redeemsCRS[account];
    }

    function redeemEarnedColValue(address account) public override view returns (uint256) {
        return _shareBalances[account] * (redeemPerShareStoredColValue - userRedeemPerSharePaidColValue[account])
        / SHARE_PRECISION + redeemsColValue[account];
    }

    /* ---------- Public Func ---------- */

    function stake(uint256 amount) public override nonReentrant updateReward(msg.sender) updateRedeem(msg.sender) {
        require(amount > 0, "cannot stake 0");

        SafeERC20.safeTransferFrom(asc, msg.sender, address(this), amount);
        _stake(msg.sender, amount);
    }

    function withdraw(uint256 shareAmount) public override nonReentrant updateReward(msg.sender) updateRedeem(msg.sender) {
        require(shareAmount > 0 && shareAmount <= unlockedShareBalanceOf(msg.sender), "your share balance is not enough!");

        uint256 withdrawAmount = shareAmount * _totalStaking / _totalShare;
        _totalStaking -= withdrawAmount;

        _totalShare -= shareAmount;
        _shareBalances[msg.sender] -= shareAmount;

        SafeERC20.safeTransfer(asc, msg.sender, withdrawAmount);
        emit Withdrawn(msg.sender, shareAmount);
    }

    function withdrawAll(uint256 shareAmount) external override {
        uint256 percent = shareAmount * 100 / unlockedShareBalanceOf(msg.sender);
        withdraw(shareAmount);
        claimRewardWithPercent(percent);
        claimRedeemWithPercent(percent);
    }

    function claimRedeemWithPercent(uint256 percent) public override nonReentrant updateRedeem(msg.sender) {
        require(percent > 0 && percent <= 100, "percent wrong");
        uint256 redeemCRS = redeemsCRS[msg.sender] * percent / 100;
        uint256 redeemColValue = redeemsColValue[msg.sender] * percent / 100;

        // TODO k
        emit RedeemPaid(msg.sender, percent);
    }

    function reinvestRedeem() public override nonReentrant updateRedeem(msg.sender) {
        // TODO k
    }

    function reinvestAll() external {
        reinvestReward();
        // reinvestRedeem();
    }

    /* ---------- Public Func : Notifies ---------- */
    function notifyRedeem(uint256 amountASC, uint256 amountCRS, uint256 amountCollateral) public override {
        if (amountASC > 0) {

            //            asc.burn(amountASC);

            uint256 leftRatio = (_totalStaking - amountASC) * SHARE_PRECISION / _totalStaking;
            uint256 newUnusedRatio = shareUnusedRatio * leftRatio / SHARE_PRECISION;
            if (newUnusedRatio > 0)
                shareUnusedRatio = newUnusedRatio;

            _totalStaking -= amountASC;
        }

        if (amountCRS > 0) {
            // lastRedeemAmountCRS = amountCRS;
            redeemPerShareStoredCRS += amountCRS * SHARE_PRECISION / _totalShare;
        }
        if (amountCollateral > 0) {
            // lastRedeemAmountColValue = amountCollateral;
            redeemPerShareStoredColValue += amountCollateral * SHARE_PRECISION / _totalShare;
        }

    }

    function notifyReinvest(address account, uint256 amount) external override updateReward(account) updateRedeem(account) onlyStakings {
        require(_totalStaking + amount <= asc.balanceOf(address(this)), "Notify amount exceeds balance!");

        if (amount > 0)
            _stake(account, amount);
    }

}

