pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "./NoLossLotteryERC1155.sol";
import "../nft/IERC1155AllowanceMintController.sol";

/**
 * No loss lottery for awarding ERC1155 tokens from an ERC1155AllowanceMintController.
 */
contract NoLossLotteryERC1155AllowanceMinter is NoLossLotteryERC1155 {
    IERC1155AllowanceMintController private _mintController;

    event AwardsSet(address awardToken, uint256 awardTokenId, uint256 awardsPerTicket, address mintController);

    function initialize(
        string memory name_,
        uint256 endTime_,
        IERC20 paymentToken_,
        uint256 pricePerTicket_,
        uint256 maxTicketsPerPlayer_,
        uint256[] memory tieredTickets_,
        uint256[] memory tieredAwards_,
        address paymentTarget_,
        address rng_,
        address owner_
    ) public virtual initializer {
        __NoLossLotteryERC1155_init(
            name_, endTime_, paymentToken_, pricePerTicket_, maxTicketsPerPlayer_,
            tieredTickets_, tieredAwards_, paymentTarget_, rng_, owner_
        );
        
        _mintController = IERC1155AllowanceMintController(address(0));
    }

    function mintController() public view returns (IERC1155AllowanceMintController) {
        return _mintController;
    }

    function setAwards(
        IERC1155 awardToken_,
        uint256 awardTokenId_,
        uint256 awardsPerTicket_,
        IERC1155AllowanceMintController mintController_
    ) external onlyOwner {
        _setAwards(awardToken_, awardTokenId_, awardsPerTicket_);

        require(address(mintController_) != address(0), "Mint controller cannot be zero address");
        _mintController = mintController_;

        emit AwardsSet(address(awardToken_), awardTokenId_, awardsPerTicket_, address(mintController_));
    }

    /**
     * Send award to player.
     */
    function _award(address player, uint256 ticketCount) internal virtual override {
        uint256 awardAmount = ticketCount * _awardsPerTicket;
        _mintController.mint(player, _awardTokenId, awardAmount, "");
    }

    /**
     * @dev This empty reserved space is put in place to allow future versions to add new
     * variables without shifting down storage in the inheritance chain.
     * See https://docs.openzeppelin.com/contracts/4.x/upgradeable#storage_gaps
     */
    uint256[41] private __gap; //TODO
}

