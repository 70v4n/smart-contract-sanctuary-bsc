pragma solidity ^0.8.0;

interface IRandomNumberGenerator {
    event RandomNumbersRequested(uint256 indexed requestId, uint32 count, address indexed sender);

    event RequestCompleted(uint256 indexed requestId, uint256[] randomNumbers);

    function requestRandomNumbers(uint32 count) external returns(uint256 requestId);

    function isRequestComplete(uint256 requestId) external view returns(bool isCompleted);

    function randomNumbers(uint256 requestId) external returns(uint256[] memory randomNum);
}