// helper methods for interacting with ERC20 tokens and sending ETH that do not consistently return true/false
library TransferHelper {
    function safeApprove(
        address token,
        address to,
        uint256 value
    ) internal {
        // bytes4(keccak256(bytes('approve(address,uint256)')));
        (bool success, bytes memory data) = token.call(
            abi.encodeWithSelector(0x095ea7b3, to, value)
        );
        require(
            success && (data.length == 0 || abi.decode(data, (bool))),
            "TransferHelper: APPROVE_FAILED"
        );
    }

    function safeTransfer(
        address token,
        address to,
        uint256 value
    ) internal {
        // bytes4(keccak256(bytes('transfer(address,uint256)')));
        (bool success, bytes memory data) = token.call(
            abi.encodeWithSelector(0xa9059cbb, to, value)
        );
        require(
            success && (data.length == 0 || abi.decode(data, (bool))),
            "TransferHelper: TRANSFER_FAILED"
        );
    }

    function safeTransferFrom(
        address token,
        address from,
        address to,
        uint256 value
    ) internal {
        // bytes4(keccak256(bytes('transferFrom(address,address,uint256)')));
        (bool success, bytes memory data) = token.call(
            abi.encodeWithSelector(0x23b872dd, from, to, value)
        );
        require(
            success && (data.length == 0 || abi.decode(data, (bool))),
            "TransferHelper: TRANSFER_FROM_FAILED"
        );
    }

    function safeTransferNative(address to, uint256 value) internal {
        (bool success, ) = to.call{value: value}(new bytes(0));
        require(success, "TransferHelper: Native_TRANSFER_FAILED");
    }
}

contract Car is ERC721Enumerable, ReentrancyGuard, Ownable {
    uint256 public next_car;

    HelperRandom public helperRandom;

    using SafeMath for uint256;

    string public constant name = "Car NFT";
    string public constant symbol = "Car";

    uint256 public buyBoxPrice = 0.001 ether;

    uint256 radio1 = 50;
    uint256 radio2 = 25;
    uint256 radio3 = 15;
    uint256 radio4 = 7;
    uint256 radio5 = 3;
    

    mapping(uint256 => uint256) public class;
    mapping(uint256 => CarInfo) public carInfo;

    string public tokenURIPrefix = "ipfs://nftc.com/erc/721/car/";
    string public tokenURISuffix = ".json";

    struct CarInfo {
        uint256 power;
        bool isOpening;
    }

    function setHelperRandom(address _helperRandom) public onlyOwner {
        helperRandom = HelperRandom(_helperRandom);
    }

    function setBuyBoxPrice(uint256 _price) public onlyOwner {
        buyBoxPrice = _price;
    }

    function setRadio(
        uint256 _radio1,
        uint256 _radio2,
        uint256 _radio3,
        uint256 _radio4,
        uint256 _radio5
    ) public {
        radio1 = _radio1;
        radio2 = _radio2;
        radio3 = _radio3;
        radio4 = _radio4;
        radio5 = _radio5;
    }

    event _buybox(address indexed owner, uint256 class, uint256 building);

    event _openBox(address indexed owner);

    function setTokenURIAffixes(
        string calldata _prefix,
        string calldata _suffix
    ) external onlyOwner {
        tokenURIPrefix = _prefix;
        tokenURISuffix = _suffix;
    }

    function buyBox(uint amount) public payable {
        require(buyBoxPrice.mul(amount) == msg.value, "value error");
        uint256 _power = dn(10000000, 100, msg.sender);
        uint256 _class = getClassByPower(_power);

        CarInfo storage _car = carInfo[next_car];
        _car.power = _power;
        _car.isOpening = false;

        class[next_car] = _class;
        _safeMint(msg.sender, next_car);
        emit _buybox(msg.sender, _class, next_car);
        next_car++;
    }

    function openBox(uint256 tokenId) public {
        require(ownerOf(tokenId) == msg.sender, "only owner");
        CarInfo storage _car = carInfo[tokenId];
        _car.isOpening = true;
        emit _openBox(msg.sender);
    }

    function withdraw(address _to, uint256 _value) public onlyOwner {
        TransferHelper.safeTransferNative(
            _to,
            _value
        );
    }

    function tokenURI(uint256 id) public view returns (string memory) {
        uint256 _class = getClassByID(id);
        string memory output = string(
            abi.encodePacked(tokenURIPrefix, classes(_class), tokenURISuffix)
        );
        return output;
    }

    function getClassByID(uint256 id) public view returns (uint256) {
        return class[id];
    }

    function classes(uint256 id)
        public
        pure
        returns (string memory description){
        if (id == 1) {
            return "A";
        } else if (id == 2) {
            return "B";
        } else if (id == 3) {
            return "C";
        } else if (id == 4) {
            return "D";
        }else if (id == 5) {
            return "E";
        }
    }

    function getClassByPower(uint256 power) public view returns (uint256) {
        if(power <= radio1){
            return 1;
        }else if (power <= radio2) {
            return 2;
        } else if (power <= radio3) {
            return 3;
        } else if(power <= radio4) {
            return 4;
        }else{
            return 5;
        }
    }

    function toString(uint256 value) internal pure returns (string memory) {
        // Inspired by OraclizeAPI's implementation - MIT license
        // https://github.com/oraclize/ethereum-api/blob/b42146b063c7d6ee1358846c198246239e9360e8/oraclizeAPI_0.4.25.sol

        if (value == 0) {
            return "0";
        }
        uint256 temp = value;
        uint256 digits;
        while (temp != 0) {
            digits++;
            temp /= 10;
        }
        bytes memory buffer = new bytes(digits);
        while (value != 0) {
            digits -= 1;
            buffer[digits] = bytes1(uint8(48 + uint256(value % 10)));
            value /= 10;
        }
        return string(buffer);
    }

    function dn(
        uint256 _seedValue,
        uint256 _number,
        address _sender
    ) public view returns (uint256) {
        return helperRandom.getRandomNumber(_seedValue, _number, _sender);
    }
}

