contract NSugarVault is Lockable, Ownable, ReentrancyGuard {
    using SafeMath for uint256;
    using SafeBEP20 for IBEP20;

    struct UserInfo {
        uint256 shares;
        uint256 rewardDebt;
        uint lastDeposit;
        // We do some fancy math here. Basically, any point in time, the amount of AUTO
        // entitled to a user but is pending to be distributed is:
        //
        //   amount = user.shares / sharesTotal * wantLockedTotal
        //   pending reward = (amount * pool.accPerShare) - user.rewardDebt
        //
        // Whenever a user deposits or withdraws want tokens to a pool. Here's what happens:
        //   1. The pool's `accPerShare` (and `lastRewardBlock`) gets updated.
        //   2. User receives the pending reward sent to his/her address.
        //   3. User's `amount` gets updated.
        //   4. User's `rewardDebt` gets updated.
    }

    struct PoolInfo {
        IBEP20 want; // Address of the want token.
        uint256 lastRewardBlock; // Last block number that Reward distribution occurs.
        uint256 accPerShare; // Accumulated reward per share, times 1e18. See below.
        address strategy; // Strategy address that will auto compound want tokens
        address reward; // Reward address that will return to user
    }

    address utilAddress; // Utility address for other activity before farm (ZAP)
    uint256 public startBlock = 0;
    PoolInfo[] public poolInfo; // Info of each pool.
    mapping(uint256 => mapping(address => UserInfo)) public userInfo; // Info of each user that stakes LP tokens.

    uint256 public constant FactorMax = 10000;
    uint256 public perfFee = 100; // performance fee = perfFee / FactorMax
    uint256 public callFee = 25; // 0.25%
    uint256 public constant perfFeeUL = 500; // performance fee upper limit
    uint256 public withdrawFee = 50; // divide by 10,000: 100 = 1%
    uint256 public constant withdrawFeeUL = 100; // withdrawal fee upper limit
    uint256 public minimumDepositTime = 259200; // seconds: 86,400 * 3 days
    uint256 public constant minimumDepositTimeUL = 7 days; // minimum deposit time before withdraw upper limit

    event Deposit(address indexed user, uint256 indexed pid, uint256 amount);
    event DepositAutoSwap(address indexed user, uint256 indexed pid, address token, uint256 amount);
    event DepositAutoSwapBNB(address indexed user, uint256 indexed pid, address token, uint256 amount);
    event Withdraw(address indexed user, uint256 indexed pid, uint256 amount);
    event WithdrawExpectOut(address indexed user, uint256 indexed pid, uint256 amount);
    event EmergencyWithdraw(
        address indexed user,
        uint256 indexed pid,
        uint256 amount
    );
    event Harvest(address indexed user, uint256 currentPerformanceFee, uint256 currentCallFee);

    constructor(address _util) public {
        utilAddress = _util;
    }

    function setUtilityAddress(address _util) external onlyOwner
    {
        utilAddress = _util;
    }

    function setPerformanceFee(uint256 _fee) external onlyLocker
    {
        require(_fee <= perfFeeUL, "setPerformanceFee: too high.");
        perfFee = _fee;
    }

    function setWithdrawFee(uint256 _fee) external onlyLocker
    {
        require(_fee <= withdrawFeeUL, "setWithdrawFee: too high.");
        withdrawFee = _fee;
    }

    function setMinimumDepositTime(uint256 _sec) external onlyLocker
    {
        require(_sec <= minimumDepositTimeUL, "setMinimumDepositTime: too long.");
        minimumDepositTime = _sec;
    }

    function getDepositTimeDiff(uint256 _pid, address _usr) public view returns (uint)
    {
        return block.timestamp - userInfo[_pid][_usr].lastDeposit;
    }

    // Return total pool
    function poolLength() external view returns (uint256)
    {
        return poolInfo.length;
    }

    function getWant(uint256 _pid) external view returns(address) {
        return address(poolInfo[_pid].want);
    }

    // Add a new lp to the pool. Can only be called by the owner.
    function add(IBEP20 _want, address _strategy, address _reward) public onlyOwner
    {
        uint256 lastRewardBlock = block.number > startBlock ? block.number : startBlock;
        poolInfo.push(
            PoolInfo({
                want: _want,
                lastRewardBlock: lastRewardBlock,
                accPerShare: 0,
                strategy: _strategy,
                reward: _reward
            })
        );
    }

    // Return reward multiplier over the given _from to _to block.
    function getMultiplier(uint256 _from, uint256 _to) public view returns (uint256) {
        return _to.sub(_from);
    }

    // Update reward variables of the given pool to be up-to-date.
    // - Retrive reward from strategy
    // - Pay development fee reward
    // - Calculate pool.accPerShare
    function updatePool(uint256 _pid) public {
        PoolInfo storage pool = poolInfo[_pid];
        if (block.number <= pool.lastRewardBlock) {
            return;
        }
        uint256 sharesTotal = IStrategy(pool.strategy).sharesTotal();
        if (sharesTotal == 0) {
            pool.lastRewardBlock = block.number;
            return;
        }
        uint256 multiplier = getMultiplier(pool.lastRewardBlock, block.number);
        if (multiplier <= 0) {
            return;
        }

        uint256 rewardClaimed = IStrategy(pool.strategy).earn();

        uint256 rewardPerf = rewardClaimed.mul(perfFee).div(FactorMax);
        uint256 maxBal = IBEP20(pool.reward).balanceOf(pool.strategy);
        if (rewardPerf > maxBal) {
            rewardPerf = maxBal;
        }
        IStrategy(pool.strategy).getReward(owner(), rewardPerf);

        uint rewardUser = rewardClaimed.sub(rewardPerf);
        pool.accPerShare = pool.accPerShare.add(
            rewardClaimed.mul(1e12).div(sharesTotal)
        );
        pool.lastRewardBlock = block.number;
    }

    // View function to see pending reward on frontend.
    function pendingReward(uint256 _pid, address _user)
        external view
        returns (uint256, uint256)
    {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_user];
        uint256 sharesTotal = IStrategy(pool.strategy).sharesTotal();

        if (block.number > pool.lastRewardBlock && sharesTotal != 0) {
            // Calculate current reward
            uint256 rewardCurrent = user.shares.mul(pool.accPerShare).div(1e18).sub(user.rewardDebt);

            uint256 multiplier = getMultiplier(pool.lastRewardBlock, block.number);
            uint[] memory earnAmts = IStrategy(pool.strategy).earnPending();
            uint256 rewardPending = earnAmts[1];
            uint256 rewardPerf = rewardPending.mul(perfFee).div(FactorMax);
            rewardPending = rewardPending.sub(rewardPerf).mul(user.shares).div(sharesTotal);

            return (rewardCurrent, rewardPending);
        } else {
            return (0, 0);
        }
    }

    function deposit(uint256 pid, uint256 amt) public nonReentrant {
        _deposit(pid, address(msg.sender), amt);
    }

    function deposit(uint256 pid, address usr, uint256 amt) public nonReentrant {
        require(msg.sender == utilAddress, "deposit from util address only.");
        _deposit(pid, usr, amt);
    }

    // Want tokens moved from user -> NSugarVault (reward allocation) -> Strat (compounding)
    function _deposit(uint256 _pid, address _usr, uint256 _wantAmt) private {
        updatePool(_pid);
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_usr];

        if (user.shares > 0) {
            uint256 pending = user.shares
                .mul(pool.accPerShare)
                .div(1e18)
                .sub(user.rewardDebt);
            if (pending > 0) {
                _rewardPay(_pid, _usr, pending);
            }
        }
        if (_wantAmt > 0) {
            pool.want.safeTransferFrom(
                address(msg.sender),
                address(this),
                _wantAmt
            );
            pool.want.safeIncreaseAllowance(pool.strategy, _wantAmt);
            uint256 sharesAdded = IStrategy(pool.strategy).deposit(_wantAmt);
            user.shares = user.shares.add(sharesAdded);
            user.lastDeposit = block.timestamp;
        }
        user.rewardDebt = user.shares.mul(pool.accPerShare).div(1e18);
        emit Deposit(_usr, _pid, _wantAmt);
    }

    function withdrawAll(uint256 _pid) public nonReentrant {
        _withdraw(_pid, address(msg.sender), uint256(-1), false);
    }

    function withdrawAll(uint256 _pid, address _usr) public nonReentrant {
        require(msg.sender == utilAddress, "withdrawAll from util address only.");
        _withdraw(_pid, _usr, uint256(-1), true);
    }

    // Withdraw LP tokens from MasterChef.
    function withdraw(uint256 _pid, uint256 _wantAmt) public nonReentrant {
        _withdraw(_pid, address(msg.sender), _wantAmt, false);
    }

    function withdraw(uint256 _pid, address _usr, uint256 _wantAmt) public nonReentrant {
        require(msg.sender == utilAddress, "withdraw from util address only.");
        _withdraw(_pid, _usr, _wantAmt, true);
    }

    function _withdraw(uint256 _pid, address _usr, uint256 _wantAmt, bool flagUtil) private {
        updatePool(_pid);
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_usr];

        uint256 wantLockedTotal = IStrategy(poolInfo[_pid].strategy).wantLockedTotal();
        uint256 sharesTotal = IStrategy(poolInfo[_pid].strategy).sharesTotal();

        require(user.shares > 0, "user.shares is 0");
        require(sharesTotal > 0, "sharesTotal is 0");

        // Withdraw pending AUTO
        uint256 pending = user.shares
            .mul(pool.accPerShare)
            .div(1e18)
            .sub(user.rewardDebt);
        if (pending > 0) {
            _rewardPay(_pid, _usr, pending);
        }

        // Withdraw want tokens
        uint256 amount = user.shares.mul(wantLockedTotal).div(sharesTotal);
        if (_wantAmt > amount) {
            _wantAmt = amount;
        }
        if (_wantAmt > 0) {
            uint256 withdrawalFee = 0;
            // Calculate withdraw fee when withdraw before minimum time
            if (getDepositTimeDiff(_pid, _usr) <= minimumDepositTime) {
                withdrawalFee = _wantAmt.mul(withdrawFee).div(FactorMax);
            }
            uint256 sharesRemoved = _wantAmt;
            if (flagUtil) {
                sharesRemoved = IStrategy(poolInfo[_pid].strategy).withdraw(utilAddress, _wantAmt, owner(), withdrawalFee);
            } else {
                sharesRemoved = IStrategy(poolInfo[_pid].strategy).withdraw(_usr, _wantAmt, owner(), withdrawalFee);
            }

            if (sharesRemoved > user.shares) {
                user.shares = 0;
            } else {
                user.shares = user.shares.sub(sharesRemoved);
            }
        }
        user.rewardDebt = user.shares.mul(pool.accPerShare).div(1e18);
        emit Withdraw(_usr, _pid, _wantAmt);
    }
 
    function emergencyWithdraw(uint256 _pid) public nonReentrant {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];

        uint256 wantLockedTotal = IStrategy(poolInfo[_pid].strategy).wantLockedTotal();
        uint256 sharesTotal = IStrategy(poolInfo[_pid].strategy).sharesTotal();
        uint256 amount = user.shares.mul(wantLockedTotal).div(sharesTotal);

        IStrategy(poolInfo[_pid].strategy).withdraw(amount);

        pool.want.safeTransfer(address(msg.sender), amount);
        emit EmergencyWithdraw(msg.sender, _pid, amount);
        user.shares = 0;
        user.rewardDebt = 0;
    }

    // Pay reward to user
    function _rewardPay(uint256 _pid, address _usr, uint256 _pendingAmt) private
    {
        PoolInfo storage pool = poolInfo[_pid];
        uint256 maxBal = IBEP20(pool.reward).balanceOf(pool.strategy);
        if (_pendingAmt > maxBal) {
            _pendingAmt = maxBal;
        }
        IStrategy(pool.strategy).getReward(_usr, _pendingAmt);
    }

    /**
     * @notice Checks if the msg.sender is a contract or a proxy
     */
    modifier notContract() {
        require(!_isContract(msg.sender), "contract not allowed");
        require(msg.sender == tx.origin, "proxy contract not allowed");
        _;
    }

    /**
     * @notice Checks if address is a contract
     * @dev It prevents contract from being targetted
     */
    function _isContract(address addr) internal view returns (bool) {
        uint256 size;
        assembly {
            size := extcodesize(addr)
        }
        return size > 0;
    }

    function harvest(uint256 _pid) external notContract {
        PoolInfo storage pool = poolInfo[_pid];
        if (block.number <= pool.lastRewardBlock) {
            return;
        }
        uint256 sharesTotal = IStrategy(pool.strategy).sharesTotal();
        if (sharesTotal == 0) {
            pool.lastRewardBlock = block.number;
            return;
        }
        uint256 multiplier = getMultiplier(pool.lastRewardBlock, block.number);
        if (multiplier <= 0) {
            return;
        }

        uint256 rewardClaimed = IStrategy(pool.strategy).earn();

        uint256 performanceFee = rewardClaimed.mul(perfFee).div(FactorMax);
        uint256 currentCallFee = rewardClaimed.mul(callFee).div(FactorMax);

        IStrategy(pool.strategy).getReward(owner(), performanceFee.sub(currentCallFee));
        IStrategy(pool.strategy).getReward(msg.sender, currentCallFee);

        uint rewardUser = rewardClaimed.sub(performanceFee);
        pool.accPerShare = pool.accPerShare.add(
            rewardUser.mul(1e18).div(sharesTotal)
        );
        pool.lastRewardBlock = block.number;

        emit Harvest(msg.sender, performanceFee.sub(currentCallFee), currentCallFee);
    }
}