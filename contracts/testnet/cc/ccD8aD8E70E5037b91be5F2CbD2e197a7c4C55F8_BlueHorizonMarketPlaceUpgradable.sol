pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./interfaces/IBlueHorizonFeeDistributor.sol";

contract BlueHorizonTierPool is Ownable, ReentrancyGuard {
    IERC20 private token;
    IERC1155 private nft;

    /// @notice mapping of verified artists
    mapping(address => bool) private artists;

    /// @notice minimum token holding requirement for 3 tiers: A, B & C
    uint256 private constant TIER_A_AMOUNT = 10e3 * 1e18;
    uint256 private constant TIER_B_AMOUNT = 50e3 * 1e18;
    uint256 private constant TIER_C_AMOUNT = 250e3 * 1e18;

    /// @notice addresses of distributor contracts
    IBlueHorizonFeeDistributor public artistDistributor;
    IBlueHorizonFeeDistributor public collectorDistributor;

    /// @notice mapping of stakers
    mapping(address => uint256) public stakers;

    constructor(IERC20 _token) {
        token = _token;
    }

    /// @notice Set distributors
    /// @param _artistDistributor Contract address of the reward distributor for artists
    /// @param _collectorDistributor Contract address of the reward distributor for collectors
    function setDistributors(
        IBlueHorizonFeeDistributor _artistDistributor,
        IBlueHorizonFeeDistributor _collectorDistributor
    ) external onlyOwner {
        require(address(_artistDistributor) != address(0), "setDistributors: INVALID");
        require(address(_collectorDistributor) != address(0), "setDistributors: INVALID");
        artistDistributor = _artistDistributor;
        collectorDistributor = _collectorDistributor;
    }

    /// @notice Set BlueHorizonSeries NFT address
    /// @dev NFT is used to check if an account is a collector
    /// revert if input is address(0)
    /// @param _nft The address of the BlueHorizon Series NFT
    function setNFT(address _nft) external onlyOwner {
        require(_nft != address(0), "setNFT: INVALID");
        nft = IERC1155(_nft);
    }

    /// @notice Deposit BLH
    /// @dev If the account is a registered artist, deposit into ArtistDistributor
    /// else if the account is a collector, deposit into CollectorDistributor
    /// else revert
    /// @param amount The amount of BLH deposited
    function deposit(uint256 amount, uint256 id) external nonReentrant {
        require(isArtist(msg.sender) || isCollector(msg.sender, id), "deposit: NOT_ARTIST_OR_COLLECTOR");

        token.transferFrom(msg.sender, address(this), amount);
        stakers[msg.sender] += amount;

        uint256 tier = getTier(stakers[msg.sender]);
        if (tier <= 0) return;
        if (isArtist(msg.sender)) {
            artistDistributor.setShare(msg.sender, tier);
        } else if (isCollector(msg.sender, id)) {
            collectorDistributor.setShare(msg.sender, tier);
        }
    }

    /// @notice Withdraw deposited BLH
    /// @dev If the account is a registered artist, deposit into ArtistDistributor
    /// else if the account is a collector, deposit into CollectorDistributor
    /// else revert
    /// @param amount The amount of BLH deposited
    function withdraw(uint256 amount, uint256 id) external nonReentrant {
        require(isArtist(msg.sender) || isCollector(msg.sender, id), "withdraw: NOT_ARTIST_OR_COLLECTOR");

        uint256 stakedAmount = stakers[msg.sender];
        if (amount > stakedAmount) {
            amount = stakedAmount;
        }
        stakers[msg.sender] -= amount;

        uint256 tier = getTier(stakers[msg.sender]);
        if (isArtist(msg.sender)) {
            artistDistributor.setShare(msg.sender, tier);
        } else if (isCollector(msg.sender, id)) {
            collectorDistributor.setShare(msg.sender, tier);
        }

        token.transfer(msg.sender, amount);
    }

    /// @notice Claim reward from artist and collector reward pools
    function claim() external nonReentrant {
        artistDistributor.distributeReward(msg.sender);
        collectorDistributor.distributeReward(msg.sender);
    }

    /// @notice Fetch tier of an account
    /// @param account The address of the account to check for tier
    function getTierOf(address account) public view returns (uint256 tier) {
        uint256 amount = stakers[account];
        tier = getTier(amount);
    }

    /// @notice Private function to fetch tier based on the input amount
    /// @param amount The quantity of BLH tokens
    function getTier(uint256 amount) public pure returns (uint256) {
        if (amount >= TIER_C_AMOUNT) return 3;
        if (amount >= TIER_B_AMOUNT) return 2;
        if (amount >= TIER_A_AMOUNT) return 1;
        return 0;
    }

    /// @notice Set artists' addresses according to the status
    /// @param accounts A list of artist addresses
    /// @param status status if the artist should be in the list
    function setArtists(address[] memory accounts, bool status) external onlyOwner {
        for (uint256 i = 0; i < accounts.length; i++) {
            artists[accounts[i]] = status;
        }
    }

    /// @notice Private function that check if an account is artist
    /// @param account The checked account
    function isArtist(address account) public view returns (bool) {
        return artists[account];
    }

    /// @notice Private function that check if an account is artist
    /// @param account The checked account
    function isCollector(address account, uint256 id) public view returns (bool) {
        return IERC1155(nft).balanceOf(account, id) > 0;
    }
}

