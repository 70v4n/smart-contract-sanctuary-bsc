pragma solidity ^0.8.10;

import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/IERC1155Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import '@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol';
import "hardhat/console.sol";
import "../fee/BlueHorizonTierPool.sol";

contract BlueHorizonMarketPlaceUpgradable is ReentrancyGuardUpgradeable, OwnableUpgradeable{

    using CountersUpgradeable for CountersUpgradeable.Counter;
    using SafeMathUpgradeable for uint256;
    using AddressUpgradeable for address;

    CountersUpgradeable.Counter private saleCounter;
    CountersUpgradeable.Counter private bidCounter;

    IERC20Upgradeable wrappedBNBContract;
    BlueHorizonTierPool blhTierPoolContract;

    uint256[4] listingFee;

	uint8 constant MAX_ROAYLTY = 50;
	uint8 constant DEFAULT_ROAYLTY = 2;

    struct SaleData {
        uint256 id;
        address tokenAddress;
        uint256 tokenId;
        address payable seller;
        uint256 price;// it will be used as min bid for auction
        uint256 quantity;
        string schema;
        bool isAuction;
    }

    struct BidData {
        uint256 id;
        uint256 tokenId;
        uint256 auctionId;
        address tokenAddress;
        address seller;
        address user;
        uint256 bidAmount;
        string schema;
    }

    mapping(uint256 => SaleData) private saleItems;

    mapping(address=> mapping(address => mapping(uint256 => uint256))) public itemsForSale;

    mapping(uint256 => BidData) public auctionBidItems;

    event salePublished(address tokenAddress, uint256 id, uint256 tokenId, uint256 price, address seller);
    event auctionPublished(address tokenAddress, uint256 id, uint256 tokenId, uint256 price, address seller);
    event itemSold(address tokenAddress, uint256 id, uint256 tokenid, address seller, address buyer, uint256 price);
    event bidPlaced(address tokenAddress, uint256 id, address buyer, uint256 auctionId);
    event itemRemovedFromSale(address tokenAddress, uint256 id, address seller);
    event itemRemovedFromBid(address tokenAddress, uint256 id, address user);

    modifier IsOnSale(uint256 id){
        require(saleItems[id].quantity>0, "Item is already sold");
        _;
    }

    modifier IsOnAuction(uint256 id){
        require(saleItems[id].isAuction, "Item not on Auction");
        _;
    }

    modifier IsValidBid(uint256 _bidId){
        require(auctionBidItems[_bidId].id > 0, "Invalid Bid");
        _;
    }
    /**
    * @notice Constructor to create BlueHorizon Marketplace contract
    *
    * @param _wbnb WBNB contract
    * @param _blhtoken Token contract
    */
    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() initializer {}

    function initialize(address _wbnb, address _blhTierPool) public initializer {

        __Ownable_init();
        wrappedBNBContract  =    IERC20Upgradeable(_wbnb);
        blhTierPoolContract    =    BlueHorizonTierPool(_blhTierPool);
        listingFee    =   [50, 30, 20, 10];

    }

    /**
    * @notice Put Item on Sale
    *
    * @param tokenAddress Nft Contract.
    * @param tokenId Token Id
    * @param price Price of Token
    * @param _quantity Quantity to be sold(Always will be 1 for ERC721)
    * @param schema Schema ERC721 or ERC1155
    *
	* @return _saleID newly created sale ID
    */
    function sale(address tokenAddress, uint256 tokenId, uint256 price, uint256 _quantity, string memory schema)  external  returns (uint256 _saleID){

        require(checkAllowedSchema(schema), "Invalid Schmea");
        require(tokenAddress != address(0), "Ownable: is the zero address");
        require(tokenAddress.isContract(), "Only Contract Address Allowed");

        if(keccak256(bytes(schema)) == keccak256(bytes("ERC721"))){
            return putERC721OnSale(tokenAddress, _msgSender(),  tokenId, price, _quantity);
        }else{
            return putERC1155OnSale(tokenAddress, _msgSender(),  tokenId, price, _quantity);
        }
    }

    /**
    * @notice Put Item on Auction
    *
    * @param tokenAddress Nft Contract.
    * @param tokenId Token Id
    * @param minbid Min Price of Bid
    * @param _quantity Quantity to be sold(Always will be 1 for ERC721)
    * @param schema Schema ERC721 or ERC1155
    *
	* @return _saleID newly created sale ID
    */
    function auction(address tokenAddress, uint256 tokenId, uint256 minbid, uint256 _quantity, string memory schema)  external  returns (uint256 _saleID){

        require(checkAllowedSchema(schema), "Invalid Schema");
        require(tokenAddress != address(0), "Ownable: is the zero address");
        require(tokenAddress.isContract(), "Only Contract Address Allowed");

        if(keccak256(bytes(schema)) == keccak256(bytes("ERC721"))){
            return putERC721OnAuction(tokenAddress, _msgSender(), tokenId, minbid, _quantity);
        }else{
            return putERC1155OnAuction(tokenAddress, _msgSender(), tokenId, minbid, _quantity);
        }
    }

    /**
    * @notice Check Valid schema
    *
    * @param schema Schema ERC721 or ERC1155
    *
	* @return bool
    */
    function checkAllowedSchema(string memory schema) internal pure returns(bool){
        if(keccak256(bytes(schema)) == keccak256(bytes("ERC721"))){
            return true;
        }else if(keccak256(bytes(schema)) == keccak256(bytes("ERC1155"))){
            return true;
        }
        return false;
    }

    /**
    * @notice Put ERC721 Item on Sale
    *
    * @param _tokenAddress Nft ERC721 Contract.
    * @param _from Seller Address
    * @param _tokenId Token Id
    * @param _price Price of Token
    * @param _quantity Quantity to be sold(Always will be 1 for ERC721)
    *
	* @return _saleItemID newly created sale Item ID
    */

    function putERC721OnSale(address _tokenAddress, address _from, uint256 _tokenId, uint256 _price, uint256 _quantity) internal returns(uint256 _saleItemID){

        IERC721Upgradeable nftContract     =   IERC721Upgradeable(_tokenAddress);

        require(nftContract.ownerOf(_tokenId) == _from, "Owner Not Matched");
        require(itemsForSale[_from][_tokenAddress][_tokenId].add(_quantity) == 1 , "Already on sale.");
        require(nftContract.isApprovedForAll(_from, address(this)) , "Does not have enough Approval.");

        saleCounter.increment();
        uint256 newSaleItemId = saleCounter.current();
        
        saleItems[newSaleItemId]    =   SaleData({
        id: newSaleItemId,
        tokenAddress: _tokenAddress,
        tokenId: _tokenId,
        seller: payable(_from),
        price: _price,
        quantity: _quantity,
        schema: "ERC721",
        isAuction: false
        });

        itemsForSale[_from][_tokenAddress][_tokenId]  =   itemsForSale[_from][_tokenAddress][_tokenId].add(_quantity);
        emit salePublished(_tokenAddress, newSaleItemId, _tokenId, _price, _from);
        return newSaleItemId;
    }

    /**
    * @notice Put ERC1155 Item on Sale
    *
    * @param _tokenAddress Nft ERC1155 Contract.
    * @param _from Seller Address
    * @param _tokenId Token Id
    * @param _price Price of Token
    * @param _quantity Quantity to be sold(Always will be 1 for ERC1155)
    *
	* @return _saleItemID newly created sale Item ID
    */

    function putERC1155OnSale(address _tokenAddress, address _from, uint256 _tokenId, uint256 _price, uint256 _quantity) internal returns(uint256 _saleItemID){

        IERC1155Upgradeable nftContract     =   IERC1155Upgradeable(_tokenAddress);

        require(_quantity>0, "Invalid Quantity.");
        require(nftContract.balanceOf(_from, _tokenId) >= itemsForSale[_from][_tokenAddress][_tokenId].add(_quantity), "Does not have enough balance.");
        require(nftContract.isApprovedForAll(_from, address(this)) , "Does not have enough Approval.");

        saleCounter.increment();
        uint256 newSaleItemId = saleCounter.current();

        saleItems[newSaleItemId]    =   SaleData({
        id: newSaleItemId,
        tokenAddress: _tokenAddress,
        tokenId: _tokenId,
        seller: payable(_from),
        price: _price,
        quantity: _quantity,
        schema: "ERC1155",
        isAuction: false
        });
        itemsForSale[_from][_tokenAddress][_tokenId]  =   itemsForSale[_from][_tokenAddress][_tokenId].add(_quantity);
        emit salePublished(_tokenAddress, newSaleItemId, _tokenId, _price, _from);
        return newSaleItemId;
    }

    /**
    * @notice Remove Item from sell
    *
    * @param saleId Sale Item Id
    *
    */
    function cancelSale(uint256 saleId)
    IsOnSale(saleId)
    external {
        require(_msgSender() == saleItems[saleId].seller, "Invalid");
        delete itemsForSale[saleItems[saleId].seller][saleItems[saleId].tokenAddress][saleItems[saleId].tokenId];
        delete saleItems[saleId];
        emit itemRemovedFromSale(saleItems[saleId].tokenAddress, saleId, _msgSender());
    }
		/**
    * @notice Remove Item from sell for seller
    *
    * @param seller Address of seller for notification.
    * @param saleId Sale Item Id
    *
    */
    function cancelSaleForInactive(uint256 saleId, address seller)
    IsOnSale(saleId) onlyOwner
    external {
        require(seller == saleItems[saleId].seller, "Invalid");
        emit itemRemovedFromSale(saleItems[saleId].tokenAddress, saleId, seller);
        delete saleItems[saleId];
        delete itemsForSale[saleItems[saleId].seller][saleItems[saleId].tokenAddress][saleItems[saleId].tokenId];
    }
    /**
    * @notice Buy an Item on Sell
    *
    * @param saleId Sale Item Id
    *
    */
    function buyItem(uint256 saleId, uint256 _quantity)
    IsOnSale(saleId)
    payable
    external {

        SaleData memory saleItem   =   saleItems[saleId];
        require(msg.value >= saleItem.price, "Not enough funds sent");
        require(_msgSender() != saleItem.seller, "Its Own Item");
        require(saleItem.quantity >=_quantity, "Invalid Quantity");

        uint256 itemPrice   =   saleItem.price.mul(_quantity);
        // send excess Payment return
        uint256 excessPayment = msg.value.sub(itemPrice);
        if (excessPayment > 0) {
            (bool returnExcessStatus, ) = payable(_msgSender()).call{value: excessPayment}("");
            require(returnExcessStatus, "Error returning excess payment");
        }
		
        transferPayments(saleItem, _quantity);

        emit itemSold(saleItem.tokenAddress, saleId, saleItem.tokenId, saleItem.seller, _msgSender(), saleItem.price.mul(_quantity));

        // remove the sale if all sold
        saleItem.quantity  =   saleItem.quantity.sub(_quantity);

        itemsForSale[saleItem.seller][saleItem.tokenAddress][saleItem.tokenId]      =   itemsForSale[saleItem.seller][saleItem.tokenAddress][saleItem.tokenId].sub(_quantity);
        if(saleItem.quantity<1){
            removeItemFromSale(saleId);
        }
    }

    function transferPayments(SaleData memory saleItem, uint256 _quantity) private {

        uint256 itemPrice   =   saleItem.price.mul(_quantity);
        address seller      =   saleItem.seller;

        address tokenAddress=   saleItem.tokenAddress;
        uint256 tokenId     =   saleItem.tokenId;
		
        //Distribute Royalty
        uint256 royaltyAmount   =   transferRoyaltyFees(tokenAddress, tokenId, itemPrice);

        uint256 listingFeeTier  =   getUserTierFee(saleItem.seller);

        //Distribute Payment
        uint256 sellerAmount = itemPrice.sub(royaltyAmount.add(itemPrice.mul(listingFeeTier.div(10)).div(100)));

        (bool returnSellerStatus, ) = payable(seller).call{value: sellerAmount}("");
        require(returnSellerStatus, "Error sending payment to seller");

        //send All remaining balance to owner
        uint256 remainingAmount = getBalance();
        (bool returnRemainingStatus, ) = owner().call{value: remainingAmount}("");
        require(returnRemainingStatus, "Error sending payment owner");


        tokenTransferFrom(saleItem, seller, _msgSender(), _quantity);
    }



    /**
     * @dev Transfer Royalty Fee.
     * @param _tokenAddress Token Address
     * @param _tokenId TokenID
     * @return Royalty Fee values and Addresses
    */
    function getRoyaltyFees(address _tokenAddress, uint256 _tokenId) internal returns (uint256[] memory, address[] memory){

        uint256[] memory royaltyFees;
        address[] memory resultFeeRecipients;
        (bool successvalue, bytes memory resultValues) 			= _tokenAddress.call(abi.encodeWithSignature("getFeeBps(uint256)", _tokenId));
        (bool successRecipient, bytes memory resultRecipients)  = _tokenAddress.call(abi.encodeWithSignature("getFeeRecipients(uint256)", _tokenId));
        if(successRecipient && successvalue){
            royaltyFees 				= 	abi.decode(resultValues, (uint256[]));
            resultFeeRecipients 	= 	abi.decode(resultRecipients, (address[]));
        }
        return (royaltyFees, resultFeeRecipients);
    }

    /**
     * @dev Transfer Royalty Fee.
     * @param _tokenAddress Token Address
     * @param _tokenId TokenID
     * @return Total Royalty Fee
    */
    function transferRoyaltyFees(address _tokenAddress, uint256 _tokenId, uint256 _itemPrice) internal returns (uint256){

        uint256 royaltyAmount	=	0;
        uint256 remainingAmount	=	_itemPrice;
        uint256 remainingPercent	=	0;

        (uint256[] memory royaltyFees, address[] memory resultFeeRecipients)	=	getRoyaltyFees(_tokenAddress, _tokenId);
		
		if(royaltyFees.length < 1){
			
			address _creator    =   address(0);
			(bool success, bytes memory result) = _tokenAddress.call(abi.encodeWithSignature("owner()"));
			if(success){
				_creator = abi.decode(result, (address));
				royaltyFees[0]	=	DEFAULT_ROAYLTY;
				resultFeeRecipients[0]	=	_creator;
			}
		}
        if(royaltyFees.length > 0 && royaltyFees.length==resultFeeRecipients.length){

			
            for (uint i = 0; i < royaltyFees.length; i++) {

                uint256 royaltyPercent 	=	royaltyFees[i];
                address royaltyAddress 	=	resultFeeRecipients[i];
                uint256 royaltyValue	=	_itemPrice.mul(royaltyPercent).div(100);
				
				if(remainingPercent.add(royaltyPercent) > MAX_ROAYLTY){
					royaltyPercent	=	MAX_ROAYLTY	- remainingPercent;
				}
                if(royaltyAddress != address(0x0)){

                    if(royaltyPercent >0 && royaltyPercent < 100 && royaltyValue < _itemPrice && remainingAmount>0){
                        remainingAmount	=	_itemPrice.sub(royaltyValue);
                        royaltyAmount	=	royaltyAmount.add(royaltyValue);

                        (bool returnRoyaltyStatus, ) = payable(royaltyAddress).call{value: royaltyValue}("");
                        require(returnRoyaltyStatus, "Error sending royalty Fee");
						remainingPercent		=	remainingPercent.add(royaltyPercent);
                    }
                }
            }
        }

        return royaltyAmount;
    }

    /**
     * @dev Transfer Royalty Fee as Wrapped.
     * @param _tokenAddress Token Address
     * @param _tokenId TokenID
     * @return Total Royalty Fee
    */
    function transferRoyaltyFeesWrapped(address _tokenAddress, uint256 _tokenId, uint256 _itemPrice, address _from) internal returns (uint256){

        uint256 royaltyAmount	=	0;
        uint256 remainingAmount	=	_itemPrice;
        uint256 remainingPercent=	0;

        (uint256[] memory royaltyFees, address[] memory resultFeeRecipients)	=	getRoyaltyFees(_tokenAddress, _tokenId);
		if(royaltyFees.length < 1){
			address _creator    =   address(0);
			(bool success, bytes memory result) = _tokenAddress.call(abi.encodeWithSignature("owner()"));
			if(success){
				_creator = abi.decode(result, (address));
				royaltyFees[0]			=	DEFAULT_ROAYLTY;
				resultFeeRecipients[0]	=	_creator;
			}
		}

        if(royaltyFees.length > 0 && royaltyFees.length==resultFeeRecipients.length){
			
            for (uint i = 0; i < royaltyFees.length; i++) {

                uint256 royaltyPercent 	=	royaltyFees[i];
                address royaltyAddress 	=	resultFeeRecipients[i];
                uint256 royaltyValue	=	_itemPrice.mul(royaltyPercent).div(100);
				
				if(remainingPercent.add(royaltyPercent) > MAX_ROAYLTY){
					royaltyPercent	=	MAX_ROAYLTY	- remainingPercent;
				}
                if(royaltyAddress != address(0x0)){

                    if(royaltyPercent >0 && royaltyPercent < 100 && royaltyValue < _itemPrice && remainingAmount>0){
                        remainingAmount	=	_itemPrice.sub(royaltyValue);
                        royaltyAmount	=	royaltyAmount.add(royaltyValue);
                        transferWrapped(_from, royaltyAddress, royaltyValue);
						remainingPercent		=	remainingPercent.add(royaltyPercent);
                    }
                }
            }
        }

        return royaltyAmount;
    }

    /**
    * @notice Remove Item from Sale or auction
    *
    * @param saleId SaleId.
    */
    function removeItemFromSale(uint256 saleId) internal{
        delete saleItems[saleId];
    }

    /**
    * @notice Put ERC721 Item on Auction
    *
    * @param _tokenAddress Nft ERC721 Contract.
    * @param _from Seller Address
    * @param _tokenId Token Id
    * @param _buynowprice Buy now price
    * @param _quantity Quantity to be sold(Always will be 1 for ERC721)
    *
	* @return _saleItemID newly created sale Item ID
    */

    function putERC721OnAuction(address _tokenAddress, address _from, uint256 _tokenId, uint256 _buynowprice, uint256 _quantity) internal returns(uint256 _saleItemID){

        IERC721Upgradeable nftContract     =   IERC721Upgradeable(_tokenAddress);

        require(_quantity==1, "Invalid Quantity.");
        require(nftContract.ownerOf(_tokenId) == _from, "Owner Not Matched");
        require(nftContract.balanceOf(_from) >=_quantity, "Does not have enough balance.");
        require(nftContract.isApprovedForAll(_from, address(this)) , "Does not have enough Approval.");

        saleCounter.increment();
        uint256 newSaleItemId = saleCounter.current();
        
        saleItems[newSaleItemId]    =   SaleData({
        id: newSaleItemId,
        tokenAddress: _tokenAddress,
        tokenId: _tokenId,
        seller: payable(_from),
        price: _buynowprice,
        quantity: _quantity,
        schema: "ERC721",
        isAuction: true
        });
        itemsForSale[_from][_tokenAddress][_tokenId]  =   itemsForSale[_from][_tokenAddress][_tokenId].add(_quantity);
        emit auctionPublished(_tokenAddress, newSaleItemId, _tokenId, _buynowprice, _from);
        return newSaleItemId;
    }
    /**
    * @notice Put ERC1155 Item on Auction
    *
    * @param _tokenAddress Nft ERC1155 Contract.
    * @param _from Seller Address
    * @param _tokenId Token Id
    * @param _buynowprice Buy now price
    * @param _quantity Quantity to be sold(Always will be 1 for ERC1155)
    *
	* @return _saleItemID newly created sale Item ID
    */

    function putERC1155OnAuction(address _tokenAddress, address _from, uint256 _tokenId, uint256 _buynowprice, uint256 _quantity) internal returns(uint256 _saleItemID){

        IERC1155Upgradeable nftContract     =   IERC1155Upgradeable(_tokenAddress);

        require(_quantity>0, "Invalid Quantity.");
		
		require(nftContract.balanceOf(_from, _tokenId) >= itemsForSale[_from][_tokenAddress][_tokenId].add(_quantity), "Does not have enough balance.");
        require(nftContract.isApprovedForAll(_from, address(this)) , "Does not have enough Approval.");

        saleCounter.increment();
        uint256 newSaleItemId = saleCounter.current();
        
        saleItems[newSaleItemId]    =   SaleData({
        id: newSaleItemId,
        tokenAddress: _tokenAddress,
        tokenId: _tokenId,
        seller: payable(_from),
        price: _buynowprice,
        quantity: _quantity,
        schema: "ERC1155",
        isAuction: true
        });
        itemsForSale[_from][_tokenAddress][_tokenId]  =   itemsForSale[_from][_tokenAddress][_tokenId].add(_quantity);
        emit auctionPublished(_tokenAddress, newSaleItemId, _tokenId, _buynowprice, _from);
        return newSaleItemId;
    }

    /**
    * @notice Bid an Item
    *
    * @param _tokenAddress Address of token
    * @param _tokenId token Id
    * @param _auctionId auction Id if on Auction else 0
    * @param _bidAmount Amount of bid
    *
    */
    function bidItem(address _tokenAddress, uint256 _tokenId, uint256 _auctionId, uint256 _bidAmount, string memory schema)
    external {

        require(wrappedBNBContract.balanceOf(_msgSender())>=_bidAmount,"Insufficent Balance");

        require(checkAllowedSchema(schema), "Invalid Schema");
        require(_tokenAddress != address(0), "Ownable: is the zero address");
        require(_tokenAddress.isContract(), "Only Contract Address Allowed");
		require(_msgSender() != saleItems[_auctionId].seller, "Own item.");
		require(_bidAmount >= saleItems[_auctionId].price, "Min amount required to bid.");

        address seller    =   address(0);
        if(_auctionId > 0)
        {
            seller = saleItems[_auctionId].seller;
        }

        bidCounter.increment();
        uint256 newBidItemId    = bidCounter.current();

        auctionBidItems[newBidItemId]  =    BidData({
        id:newBidItemId,
        tokenId:_tokenId,
        auctionId:_auctionId,
        tokenAddress:_tokenAddress,
        seller: seller,
        user:_msgSender(),
        bidAmount:_bidAmount,
        schema: schema
        });

        emit bidPlaced(_tokenAddress, newBidItemId, _msgSender(), _auctionId);
    }

    /**
    * @notice Remove Bid from Auction
    *
    * @param _bidId Bid Item Id
    *
    */
    function cancelBid(uint256 _bidId)
    IsValidBid(_bidId)
    external {

        require(_msgSender() == auctionBidItems[_bidId].user, "Invalid");

        emit itemRemovedFromBid(auctionBidItems[_bidId].tokenAddress, _bidId, _msgSender());
        delete auctionBidItems[_bidId];
    }



    /**
    * @notice Accept Bid for Auction
    *
    * @param _bidId Bid Item Id
    *
    */
    function acceptBid(uint256 _bidId)
    IsValidBid(_bidId)
    external {

        if(auctionBidItems[_bidId].auctionId>0){
            acceptBidOnAuction(auctionBidItems[_bidId].auctionId, _bidId);
        }else{
            acceptBidOffAuction(_bidId);
        }
    }

    /**
    * @notice Accept Bid for Auction
    *
    * @param _bidId Bid Item Id
    *
    */
    function acceptBidOffAuction(uint256 _bidId)
    IsValidBid(_bidId)
    private {

        BidData memory auctionBidItem	=	auctionBidItems[_bidId];

        if(keccak256(bytes(auctionBidItem.schema)) == keccak256(bytes("ERC721"))){
            IERC721Upgradeable nftContract     =   IERC721Upgradeable(auctionBidItem.tokenAddress);
            require(nftContract.ownerOf(auctionBidItem.tokenId) == _msgSender(), "Not owner of asset.");
        }else{
            IERC1155Upgradeable nftContract     =   IERC1155Upgradeable(auctionBidItem.tokenAddress);
            require(nftContract.balanceOf(_msgSender(),  auctionBidItem.tokenId) > 0, "Owner does not have enough balance.");
        }

        uint256 itemPrice   =   auctionBidItem.bidAmount;

        SaleData memory saleItem   =   SaleData({
        id: 0,
        tokenAddress: auctionBidItem.tokenAddress,
        tokenId: auctionBidItem.tokenId,
        seller: payable(_msgSender()),
        price: auctionBidItem.bidAmount,
        quantity: 1,
        schema: auctionBidItem.schema,
        isAuction: false
        });

        //Distribute Royalty
        uint256 royaltyAmount   =   transferRoyaltyFeesWrapped(auctionBidItem.tokenAddress, auctionBidItem.tokenId, itemPrice, auctionBidItem.user);

        //Distribute Payment
        uint256 sellerAmount = itemPrice.sub(royaltyAmount.add(itemPrice.mul(listingFee[0].div(10)).div(100)));
        transferWrapped(auctionBidItem.user, _msgSender(), sellerAmount);

        //send All remaining balance to owner
        uint256 remainingAmount = itemPrice.sub(sellerAmount.add(royaltyAmount));
        transferWrapped(auctionBidItem.user, owner(), remainingAmount);

        tokenTransferFrom(saleItem, _msgSender(), auctionBidItem.user, 1);

        emit itemSold(auctionBidItem.tokenAddress, auctionBidItem.auctionId, auctionBidItem.tokenId, _msgSender(), auctionBidItem.user, itemPrice);
    }

    /**
    * @notice Accept Bid for Auction
    *
    * @param _auctionId Auction Item Id
    * @param _bidId Bid Item Id
    *
    */
    function acceptBidOnAuction(uint256 _auctionId, uint256 _bidId)
    IsOnAuction(_auctionId)
    IsValidBid(_bidId)
    private {

        BidData memory auctionBidItem	=	auctionBidItems[_bidId];

        // Only check if seller is set
        if(auctionBidItem.seller != address(0))
            require(_msgSender() == auctionBidItem.seller, "Can not accept other bids.");

        uint256 itemPrice   =   auctionBidItem.bidAmount;
        //Distribute Royalty
        uint256 royaltyAmount   =   transferRoyaltyFeesWrapped(saleItems[_auctionId].tokenAddress, saleItems[_auctionId].tokenId, itemPrice, auctionBidItem.user);

        //Distribute Payment
        uint256 sellerAmount = itemPrice.sub(royaltyAmount.add(itemPrice.mul(listingFee[0].div(10)).div(100)));
        transferWrapped(auctionBidItem.user, auctionBidItem.seller, sellerAmount);
        //send All remaining balance to owner
        uint256 remainingAmount = itemPrice.sub(sellerAmount.add(royaltyAmount));
        transferWrapped(auctionBidItem.user, owner(), remainingAmount);

        tokenTransferFrom(saleItems[_auctionId], auctionBidItem.seller, auctionBidItem.user, 1);

        emit itemSold(saleItems[_auctionId].tokenAddress, _auctionId, saleItems[_auctionId].tokenId, auctionBidItem.seller, auctionBidItem.user, itemPrice);
        // remove the sale if all sold
        saleItems[_auctionId].quantity  =   saleItems[_auctionId].quantity.sub(1);
        itemsForSale[saleItems[_auctionId].seller][saleItems[_auctionId].tokenAddress][saleItems[_auctionId].tokenId]  =   itemsForSale[saleItems[_auctionId].seller][saleItems[_auctionId].tokenAddress][saleItems[_auctionId].tokenId].sub(1);
        if(saleItems[_auctionId].quantity<1){
            removeItemFromSale(_auctionId);
        }
    }

    function transferWrapped(address _from, address _to, uint256 _quantity) private{

        require(wrappedBNBContract.balanceOf(_from)>=_quantity,"Insufficent Balance");

        SafeERC20Upgradeable.safeTransferFrom(wrappedBNBContract,_from,_to,_quantity);
    }

    /**
    * @notice Transfer Token on purchase
    *
    * @param saleItem Sale Item
    * @param _from To Address
    * @param _to To Address
    *
    * @return Bool
    */
    function tokenTransferFrom(SaleData memory saleItem, address _from, address _to, uint256 _quantity) internal returns(bool) {

        if(keccak256(bytes(saleItem.schema)) == keccak256(bytes("ERC721"))){
            return nftERC721TransferFrom(saleItem.tokenAddress, _from, _to, saleItem.tokenId);
        }else{
            return nftERC1155TransferFrom(saleItem.tokenAddress, _from, _to, saleItem.tokenId, _quantity);
        }
    }

    /**
    * @notice Transfer ERC721 Token on purchase
    *
    * @param tokenAddress Token Contract Address
    * @param _from From Address
    * @param _to To Address
    * @param _tokenId Token Id
    *
    * @return Bool
    */
    function nftERC721TransferFrom(address tokenAddress, address _from, address _to, uint256 _tokenId) internal returns(bool){
        IERC721Upgradeable nftContract     =   IERC721Upgradeable(tokenAddress);
        require(nftContract.ownerOf(_tokenId) == _from, "does not own the item");
        require(nftContract.balanceOf(_from) >0, "Does not have enough balance.");
        nftContract.safeTransferFrom(_from, _to, _tokenId);

        return true;
    }

    /**
    * @notice Transfer ERC1155 Token on purchase
    *
    * @param tokenAddress Token Contract Address
    * @param _from From Address
    * @param _to To Address
    * @param _tokenId Token Id
    *
    * @return Bool
    */
    function nftERC1155TransferFrom(address tokenAddress, address _from, address _to, uint256 _tokenId, uint256 _quantity) internal returns(bool){
        IERC1155Upgradeable nftContract     =   IERC1155Upgradeable(tokenAddress);
        require(nftContract.balanceOf(_from, _tokenId) >0, "Does not have enough balance.");
        nftContract.safeTransferFrom(_from, _to, _tokenId, _quantity, "");
        return true;
    }

    /**
    * @notice View Sale Details
    * @param _saleID Token Id
	*
    * @return SaleData
    */
    function getSaleDetails(uint256 _saleID) public view returns(SaleData memory) {
        return saleItems[_saleID];
    }

    /**
    * @notice View Balance of contract
    *
    * @return Balance
    */
    function getBalance() public view returns(uint) {
        return address(this).balance;
    }

    /**
     * @dev Update BLH Tier contract address
     * Can only be called by the current owner.
     */
    function setBLHTierContract(address _contractAddress) public virtual onlyOwner {
        require(_contractAddress != address(0), "Ownable: is the zero address");
        require(_contractAddress.isContract(), "Only Contract Address Allowed");

        blhTierPoolContract    =    BlueHorizonTierPool(_contractAddress);
    }

    /**
     * @dev get User Tier based on their balance from Tier contract
     * @param _user User address
     * @return _tier
    */
    function getUserTierFee(address _user) public returns (uint256){

        uint256   _tier   =   0;

        (bool success, bytes memory result) = address(blhTierPoolContract).call(abi.encodeWithSignature("getTierOf(address)", _user));
        if(success){
            _tier = abi.decode(result, (uint256));
        }

        return listingFee[_tier];
    }

    /**
     * @dev Update listingFee
     * Can only be called by the current owner.
    */

    function setlistingFee(uint256[4] memory _listingFee) public virtual onlyOwner {

        listingFee = _listingFee;
    }
}

