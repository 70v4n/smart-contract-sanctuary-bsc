pragma solidity >=0.8.1;

import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "../common/OnChPayable.sol";
import "../common/OnChOwnableWithWhitelist.sol";
import "../stake/IOnChStakingOrchestrator.sol";
import "../kyc/IOnChKycOrchestrator.sol";

import "hardhat/console.sol";

contract OnChSeedPool is OnChPayable, OnChOwnableWithWhitelist {

    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    event StakedBasedDeposit(string poolName, address indexed depositer, uint256 collectedCoinAmount, address indexed collectedCoinAddress);

    event CoinBasedDeposit(string poolName, address indexed depositer, uint256 collectedCoinAmount, address indexed collectedCoinAddress, uint256 seedCoinAmount, address indexed seedCoinAddress);

    event RewardCoinsDeposit(string poolName, address indexed depositer, uint256 rewardCoinAmount, address indexed rewardCoinAddress);

    event RewardCoinsWithdrawn(string poolName, address indexed withdrawer, uint256 rewardCoinAmount, address indexed rewardCoinAddress);

    event SeedCoinsWithdrawn(string poolName, address indexed withdrawer, uint256 seedCoinAmount, address indexed seedCoinAddress);

    event CollectedCoinsWithdrawn(
        string poolName,
        address indexed seedPlatformWalletAddress1, uint256 seedPlatformWallet1Fee,
        address indexed seedPlatformWalletAddress2, uint256 seedPlatformWallet2Fee,
        address indexed seedPoolWalletAddress, uint256 seedPoolWalletAmount,
        address collectedCoinAddress
    );

    struct OnChSeedPoolWithdrawConfig {
        address seedPlatformWalletAddress1;
        address seedPlatformWalletAddress2;
        address seedPoolWalletAddress;
        uint256 seedPlatformWalletAddress1FeePercent;
        uint256 seedPlatformWalletAddress2FeePercent;
    }

    struct OnChSeedPoolConfig {
        string name;
        address collectedCoinAddress;
        address seedCoinAddress;
        address rewardCoinAddress;
        address kycOrchestratorAddress;
        address stakingOrchestratorAddress;
    }

    struct OnChSeedPoolState {
        uint256 seedCoinToCollectedCoinRatio;
        uint256 rewardCoinToCollectedCoinRatio;
        uint256 stakeBasedDepositStarts;
        uint256 stakeBasedDepositEnds;
        uint256 coinBasedDepositStarts;
        uint256 coinBasedDepositEnds;
        uint256 depositSoftCap;
        uint256 depositHardCap;
    }

    bool withdrawConfigReady;
    bool configReady;
    bool stateReady;

    OnChSeedPoolWithdrawConfig _withdrawConfig;
    OnChSeedPoolConfig _config;
    OnChSeedPoolState _state;

    mapping(address => uint256) internal _stakeBasedDeposits;
    mapping(address => uint256) internal _coinBasedDeposits;
    mapping(address => uint256) internal _seedCoinDeposits;

    uint256 public stakeBasedDepositTotal;
    uint256 public coinBasedDepositTotal;
    uint256 public depositTotal;
    uint256 public seedCoinDepositTotal;
    uint256 public rewardCoinTotal;

    constructor() {
        withdrawConfigReady = false;
        configReady = false;
        stateReady = false;
    }

    function name() external view returns (string memory) {
        return _config.name;
    }

    function withdrawConfig() external view returns (OnChSeedPoolWithdrawConfig memory) {
        return _withdrawConfig;
    }

    function config() external view returns (OnChSeedPoolConfig memory) {
        return _config;
    }

    function state() external view returns (OnChSeedPoolState memory) {
        return _state;
    }

    function changeWithdrawConfig(
        address seedPlatformWalletAddress1_,
        address seedPlatformWalletAddress2_,
        address seedPoolWalletAddress_,
        uint256 seedPlatformWalletAddress1FeePercent_,
        uint256 seedPlatformWalletAddress2FeePercent_
    )
    external
    onlyOwner {
        require(seedPlatformWalletAddress1_ != address(0), "OnChSeedPool: Missing seed platform wallet 1 address");
        require(seedPlatformWalletAddress2_ != address(0), "OnChSeedPool: Missing seed platform wallet 2 address");
        require(seedPoolWalletAddress_ != address(0), "OnChSeedPool: Missing pool wallet address");
        require(seedPlatformWalletAddress1FeePercent_ > 0, "OnChSeedPool: Wrong platform percent 1");
        require(seedPlatformWalletAddress1FeePercent_ <= 100, "OnChSeedPool: Wrong platform percent 1");
        require(seedPlatformWalletAddress2FeePercent_ > 0, "OnChSeedPool: Wrong platform percent 2");
        require(seedPlatformWalletAddress2FeePercent_ <= 100, "OnChSeedPool: Wrong platform percent 2");

        _withdrawConfig.seedPlatformWalletAddress1 = seedPlatformWalletAddress1_;
        _withdrawConfig.seedPlatformWalletAddress2 = seedPlatformWalletAddress2_;
        _withdrawConfig.seedPoolWalletAddress = seedPoolWalletAddress_;
        _withdrawConfig.seedPlatformWalletAddress1FeePercent = seedPlatformWalletAddress1FeePercent_;
        _withdrawConfig.seedPlatformWalletAddress2FeePercent = seedPlatformWalletAddress2FeePercent_;

        withdrawConfigReady = true;
    }

    function changeConfig(
        string memory name_,
        address collectedCoinAddress_,
        address seedCoinAddress_,
        address rewardCoinAddress_,
        address kycOrchestratorAddress_,
        address stakingOrchestratorAddress_
    )
    external
    onlyOwner {
        require(collectedCoinAddress_ != address(0), "OnChSeedPool: Missing collected coin address");
        require(seedCoinAddress_ != address(0), "OnChSeedPool: Missing seed coin address");
        require(rewardCoinAddress_ != address(0), "OnChSeedPool: Missing coin address");
        require(kycOrchestratorAddress_ != address(0), "OnChSeedPool: Missing kyc orchestrator address");
        require(stakingOrchestratorAddress_ != address(0), "OnChSeedPool: Missing staking orchestrator address");

        _config.name = name_;
        _config.collectedCoinAddress = collectedCoinAddress_;
        _config.seedCoinAddress = seedCoinAddress_;
        _config.rewardCoinAddress = rewardCoinAddress_;
        _config.kycOrchestratorAddress = kycOrchestratorAddress_;
        _config.stakingOrchestratorAddress = stakingOrchestratorAddress_;

        configReady = true;
    }

    function changeState(
        uint256 stakeBasedDepositStarts_,
        uint256 stakeBasedDepositEnds_,
        uint256 coinBasedDepositStarts_,
        uint256 coinBasedDepositEnds_,
        uint256 depositSoftCap_,
        uint256 depositHardCap_,
        uint256 seedCoinToCollectedCoinRatio_,
        uint256 rewardCoinToCollectedCoinRatio_
    )
    external
    onlyOwner {
        require(stakeBasedDepositStarts_ > 0, "OnChSeedPool: zero stake based deposit start time");
        require(stakeBasedDepositEnds_ >= stakeBasedDepositStarts_, "OnChSeedPool: stake based deposit end must be after stake based deposit starts");
        require(coinBasedDepositStarts_ >= stakeBasedDepositEnds_, "OnChSeedPool: coin based deposit end must be after stake based deposit ends");
        require(coinBasedDepositEnds_ >= coinBasedDepositStarts_, "OnChSeedPool: coin based deposit end must be after coin based deposit starts");
        require(depositSoftCap_ >= 0, "OnChSeedPool: deposit soft cap cannot be negative");
        require(depositHardCap_ >= depositSoftCap_, "OnChSeedPool: deposit hard cap cannot be lower than the soft cap");
        require(seedCoinToCollectedCoinRatio_ > 0, "OnChSeedPool: seed coin to collected coin (BUSD) ratio cannot be negative");
        require(rewardCoinToCollectedCoinRatio_ > 0, "OnChSeedPool: reward coin to collected coin (BUSD) ratio cannot be negative");

        _state.stakeBasedDepositStarts = stakeBasedDepositStarts_;
        _state.stakeBasedDepositEnds = stakeBasedDepositEnds_;
        _state.coinBasedDepositStarts = coinBasedDepositStarts_;
        _state.coinBasedDepositEnds = coinBasedDepositEnds_;
        _state.depositSoftCap = depositSoftCap_;
        _state.depositHardCap = depositHardCap_;
        _state.seedCoinToCollectedCoinRatio = seedCoinToCollectedCoinRatio_;
        _state.rewardCoinToCollectedCoinRatio = rewardCoinToCollectedCoinRatio_;

        stateReady = true;
    }

    function ready() external view returns (bool) {
        return _ready();
    }

    function _ready() internal view returns (bool) {
        return withdrawConfigReady && configReady && stateReady;
    }

    function stakeBasedMaxDepositFor(address account) external view returns (uint256) {
        require(_ready(), "OnChSeedPool: Pool not ready");
        if (block.timestamp > _state.stakeBasedDepositStarts && block.timestamp < _state.stakeBasedDepositEnds ||
            block.timestamp > _state.coinBasedDepositStarts && block.timestamp < _state.coinBasedDepositEnds) {
            return _stakeBasedMaxDepositFor(account);
        }
        return 0;
    }

    function _stakeBasedMaxDepositFor(address account) internal view returns (uint256) {
        if (!IOnChKycOrchestrator(_config.kycOrchestratorAddress).checkKyc(account)) {
            return 0;
        }
        uint256 stakedCoin = IOnChStakingOrchestrator(_config.stakingOrchestratorAddress).getRealtimeStakedAmount(account);
        uint256 maxAmount = stakedCoin * _state.seedCoinToCollectedCoinRatio - _stakeBasedDeposits[account];
        if (depositTotal + maxAmount > _state.depositHardCap) {
            maxAmount = _state.depositHardCap - depositTotal;
        }
        return maxAmount;
    }

    function coinBasedMaxDepositFor(address account) external view returns (uint256) {
        require(_ready(), "OnChSeedPool: Pool not ready");
        if (block.timestamp > _state.coinBasedDepositStarts && block.timestamp < _state.coinBasedDepositEnds) {
            return _coinBasedMaxDepositFor(account);
        }
        return 0;
    }

    function _coinBasedMaxDepositFor(address account) internal view returns (uint256) {
        if (!IOnChKycOrchestrator(_config.kycOrchestratorAddress).checkKyc(account)) {
            return 0;
        }
        uint256 seedCoinBalance = IERC20(_config.seedCoinAddress).balanceOf(account);
        uint256 maxAmount = (seedCoinBalance * _state.seedCoinToCollectedCoinRatio) + _stakeBasedMaxDepositFor(account);
        if (depositTotal + maxAmount > _state.depositHardCap) {
            maxAmount = _state.depositHardCap - depositTotal;
        }
        return maxAmount;
    }

    function stakeBasedDepositOf(address account) external view returns (uint256) {
        require(_ready(), "OnChSeedPool: Pool not ready");
        return _stakeBasedDeposits[account];
    }

    function stakeBasedDeposit(uint256 collectedCoinAmount) external returns (bool) {
        require(_ready(), "OnChSeedPool: Pool not ready");
        require(depositTotal < _state.depositHardCap, "OnChSeedPool: Pool is full");
        require(IOnChKycOrchestrator(_config.kycOrchestratorAddress).checkKyc(msg.sender), "OnChSeedPool: KYC must be completed first");
        if (depositTotal + collectedCoinAmount > _state.depositHardCap) {
            collectedCoinAmount = _state.depositHardCap - depositTotal;
        }

        require(block.timestamp > _state.stakeBasedDepositStarts && block.timestamp < _state.stakeBasedDepositEnds ||
            block.timestamp > _state.coinBasedDepositStarts && block.timestamp < _state.coinBasedDepositEnds, "OnChSeedPool: Outside time interval");

        uint256 maxAmount = _stakeBasedMaxDepositFor(msg.sender);
        require(collectedCoinAmount <= maxAmount, "Cannot deposit more than max amount: please stake more HER");

        require(IERC20(_config.collectedCoinAddress).allowance(msg.sender, address(this)) >= collectedCoinAmount, "OnChSeedPool: Insufficient allowance");
        _payMe(msg.sender, collectedCoinAmount, _config.collectedCoinAddress);

        _stakeBasedDeposits[msg.sender] = _stakeBasedDeposits[msg.sender] + collectedCoinAmount;
        stakeBasedDepositTotal = stakeBasedDepositTotal + collectedCoinAmount;
        depositTotal = depositTotal + collectedCoinAmount;

        emit StakedBasedDeposit(_config.name, msg.sender, collectedCoinAmount, _config.collectedCoinAddress);

        return true;
    }

    function coinBasedDepositOf(address account) external view returns (uint256) {
        require(_ready(), "OnChSeedPool: Pool not ready");
        return _coinBasedDeposits[account];
    }

    function coinBasedDeposit(uint256 collectedCoinAmount) external returns (bool) {
        require(_ready(), "OnChSeedPool: Pool not ready");
        require(depositTotal < _state.depositHardCap, "OnChSeedPool: Pool is full");
        require(IOnChKycOrchestrator(_config.kycOrchestratorAddress).checkKyc(msg.sender), "OnChSeedPool: KYC must be completed first");
        if (depositTotal + collectedCoinAmount > _state.depositHardCap) {
            collectedCoinAmount = _state.depositHardCap - depositTotal;
        }

        require(block.timestamp > _state.coinBasedDepositStarts && block.timestamp < _state.coinBasedDepositEnds, "OnChSeedPool: Outside time interval");

        uint256 maxAmount = _coinBasedMaxDepositFor(msg.sender);
        require(collectedCoinAmount <= maxAmount, "Cannot deposit more than max amount: please buy more HER");
        uint256 seedCoinAmount = collectedCoinAmount.div(_state.seedCoinToCollectedCoinRatio);

        require(IERC20(_config.collectedCoinAddress).allowance(msg.sender, address(this)) >= collectedCoinAmount, "OnChSeedPool: Insufficient allowance");
        require(IERC20(_config.seedCoinAddress).allowance(msg.sender, address(this)) >= seedCoinAmount, "OnChSeedPool: Insufficient allowance");

        _payMe(msg.sender, collectedCoinAmount, _config.collectedCoinAddress);
        _payMe(msg.sender, seedCoinAmount, _config.seedCoinAddress);

        _coinBasedDeposits[msg.sender] = _coinBasedDeposits[msg.sender] + collectedCoinAmount;
        coinBasedDepositTotal = coinBasedDepositTotal + collectedCoinAmount;
        depositTotal = depositTotal + collectedCoinAmount;

        _seedCoinDeposits[msg.sender] = _seedCoinDeposits[msg.sender] + seedCoinAmount;
        seedCoinDepositTotal = seedCoinDepositTotal + seedCoinAmount;

        emit CoinBasedDeposit(_config.name, msg.sender, collectedCoinAmount, _config.collectedCoinAddress, seedCoinAmount, _config.seedCoinAddress);

        return true;
    }

    function addRewardCoins(uint256 rewardCoinsAmount) external whitelistedOnly returns (bool) {
        require(_ready(), "OnChSeedPool: Pool not ready");
        require(rewardCoinsAmount > 0, "Cannot deposit less than 0");

        require(IERC20(_config.rewardCoinAddress).allowance(msg.sender, address(this)) >= rewardCoinsAmount, "OnChSeedPool: Insufficient allowance");
        _payMe(msg.sender, rewardCoinsAmount, _config.rewardCoinAddress);

        emit RewardCoinsDeposit(_config.name, msg.sender, rewardCoinsAmount, _config.rewardCoinAddress);

        return true;
    }

    function withdrawCoins() external returns (bool) {
        require(_ready(), "OnChSeedPool: Pool not ready");
        require(block.timestamp > _state.coinBasedDepositEnds, "Cannot withdraw until pool ends");

        uint256 rewardAmount = 0;

        if (_stakeBasedDeposits[msg.sender] > 0) {
            rewardAmount = rewardAmount + _state.rewardCoinToCollectedCoinRatio.mul(_stakeBasedDeposits[msg.sender]);
            _stakeBasedDeposits[msg.sender] = 0;
        }
        if (_coinBasedDeposits[msg.sender] > 0) {
            rewardAmount = rewardAmount + _state.rewardCoinToCollectedCoinRatio.mul(_coinBasedDeposits[msg.sender]);
            _coinBasedDeposits[msg.sender] = 0;
        }
        if (rewardAmount > 0) {
            _payDirect(msg.sender, rewardAmount, _config.rewardCoinAddress);
            emit RewardCoinsWithdrawn(_config.name, msg.sender, rewardAmount, _config.rewardCoinAddress);
        }

        if (_seedCoinDeposits[msg.sender] > 0) {
            _payDirect(msg.sender, _seedCoinDeposits[msg.sender], _config.seedCoinAddress);
            emit SeedCoinsWithdrawn(_config.name, msg.sender, _seedCoinDeposits[msg.sender], _config.seedCoinAddress);
            seedCoinDepositTotal = seedCoinDepositTotal - _seedCoinDeposits[msg.sender];
            _seedCoinDeposits[msg.sender] = 0;
        }

        return true;
    }

    function withdrawCollectedCoins() external whitelistedOnly returns (bool) {
        require(_ready(), "OnChSeedPool: Pool not ready");
        require(block.timestamp > _state.coinBasedDepositEnds, "Cannot withdraw until pool ends");
        require(depositTotal > 0, "Cannot withdraw from empty pool");

        uint256 seedPlatformWallet1Fee = _withdrawConfig.seedPlatformWalletAddress1FeePercent.mul(depositTotal).div(100);
        uint256 seedPlatformWallet2Fee = _withdrawConfig.seedPlatformWalletAddress2FeePercent.mul(depositTotal).div(100);
        uint256 seedPoolWalletAmount = (100 - _withdrawConfig.seedPlatformWalletAddress1FeePercent - _withdrawConfig.seedPlatformWalletAddress2FeePercent).mul(depositTotal).div(100);

        _payDirect(_withdrawConfig.seedPlatformWalletAddress1, seedPlatformWallet1Fee, _config.collectedCoinAddress);
        _payDirect(_withdrawConfig.seedPlatformWalletAddress2, seedPlatformWallet2Fee, _config.collectedCoinAddress);
        _payDirect(_withdrawConfig.seedPoolWalletAddress, seedPoolWalletAmount, _config.collectedCoinAddress);

        depositTotal = 0;

        emit CollectedCoinsWithdrawn(
            _config.name,
            _withdrawConfig.seedPlatformWalletAddress1, seedPlatformWallet1Fee,
            _withdrawConfig.seedPlatformWalletAddress2, seedPlatformWallet2Fee,
            _withdrawConfig.seedPoolWalletAddress, seedPoolWalletAmount,
            _config.collectedCoinAddress
        );

        return true;
    }

}

