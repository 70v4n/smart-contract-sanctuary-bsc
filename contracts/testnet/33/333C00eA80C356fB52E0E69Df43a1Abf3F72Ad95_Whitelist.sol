pragma solidity ^0.6.12;

import "./Crowdsale.sol";
import "./tools/Ownable.sol";


// Otium Token: 0x56A02228CE17DBBbc809fe2262B36Dd99f28E824

/**
 *  Standard de preventa con algunas customizacinones personales.
 *
 *  endsold: para la preventa y envia los tokens que no se hayan vendido al propietario
 *  TokenBalance: Indica la cantidad de tokens que hay en el contrato
 *
 *  Setrate: Cambia la cantidad de tokens a enviar por 1 BNB cuando queramos.
 *
 */


interface TokenInterface {
    // determinamos las funciones que necesitamos del ERC20. Tienen que ser iguales.
    function decimals() external view  returns(uint8);
    function balanceOf(address _address) external view returns(uint256);
    function transfer(address _to, uint256 _value) external returns (bool success);
}


contract Whitelist is Crowdsale, Ownable {
    
  uint256  public minBuy = 1000000000000000000;
  uint256  public maxBuy = 3000000000000000000;
  
  bool startReclaim = false;
  
  mapping(address => uint256) public buyers;
  mapping(address => bool) public isclaimed;
    
  TokenInterface TokenContract; // interface para manipular metodos del token.

    // tanto _token como _addresstoken son la misma direccion. La diferencia esta en su uso.
    // Debemos pasarlo como contrato y como direccion para 
    // hacer operaciones diferentes.
  constructor  (uint256 _rate, 
    BEP20 _token,
    address _addressToken
  ) Crowdsale(_rate,  msg.sender, _token)  public {
                    
        
        // p치samos el contrato del token, como direccion para ser usada por la interface
        TokenContract = TokenInterface(_addressToken);    
        //console.log("Desplegado contrato de venta de los tokens del contrato: ", _token);      
  }
    
  // Funcion que liquida el contrato para que no se pueda vender mas.
  function endSold() public  onlyOwner() {
      
      // compensacion de saldos. 
      require(TokenContract.transfer(owner(), TokenContract.balanceOf(address(this))));
      msg.sender.transfer(address(this).balance);
      //wallet.transfer(address(this).balance);
      weiRaised = 0;
  }
    
  function TokenBalance() public view returns (uint256)  {
      return TokenContract.balanceOf(address(this));
  }
  
  function setRate(uint256 _newrate) public onlyOwner() {
      
      rate = _newrate;
  }
  
  function setMintBuy(uint256 _newLimit) public onlyOwner() {
        minBuy = _newLimit;
  }
  
    function setMaxtBuy(uint256 _newLimit) public onlyOwner() {
         maxBuy = _newLimit;
  }
  
     function start() public onlyOwner() {
         startReclaim = !startReclaim;
  }
  
  /// Prueba acceder al saldo  de Otiums del usuario reservado
  function getBalanceOtium() public view returns (uint256){
      require(buyers[msg.sender] != 0, "Usted no est치 en la Whitelist");
      return _getTokenAmount(buyers[msg.sender]);
  }
  
  
   
  function claimToken() public   {
    require(startReclaim, 'La reclamacion de tokens no esta habilitada todavia');
    require(buyers[msg.sender] != 0, "Usted no est치 en la Whitelist");
    require(!isclaimed[msg.sender], "Usted ya ha reclamado sus tokens");
    
    uint256 weiAmount = buyers[msg.sender];
    uint256 balance = TokenContract.balanceOf(msg.sender);
    require(balance.add(weiAmount.mul(rate)) <= maxBuy.mul(rate), "Excede compra de otiums permitida");
    
    _preValidatePurchase(msg.sender, buyers[msg.sender]);

    // calculate token amount to be created
    uint256 tokens = _getTokenAmount(buyers[msg.sender]);

    _processPurchase(msg.sender, tokens);
    
    isclaimed[msg.sender] = true;
    
    emit TokenPurchase(
       address(this),
      msg.sender,
      weiAmount,
      tokens
    );
    
  }
  
    function reserveTokens() public payable {

    require(buyers[msg.sender] == 0, "Usted ya est치 en la Whitelist");

    uint256 weiAmount = msg.value;
    uint256 balance = TokenContract.balanceOf(msg.sender);
    require(balance.add(weiAmount.mul(rate)) <= maxBuy.mul(rate), "Excede compra de otiums permitida");
    
    buyers[msg.sender] = msg.value;
    
    // update state
    weiRaised = weiRaised.add(buyers[msg.sender]);
    
    isclaimed[msg.sender] = false;
 
    
  }

}

