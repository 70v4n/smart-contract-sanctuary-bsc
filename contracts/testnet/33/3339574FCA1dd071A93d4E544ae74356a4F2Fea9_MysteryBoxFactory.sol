pragma solidity 0.8.4;

import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/utils/ERC1155HolderUpgradeable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";

import "@chainlink/contracts/src/v0.8/VRFConsumerBase.sol";

import "./interfaces/IJNFT.sol";
import "./interfaces/IFeeManager.sol";

contract MysteryBox is VRFConsumerBase, AccessControlUpgradeable, ERC1155HolderUpgradeable
{
    using EnumerableSet for EnumerableSet.UintSet;
    using SafeMath for uint256;

    IFeeManager public feeManager;
    IJNFT public jnft;    
    
    address public creator;

    string public name;
    string public description;

    address public acceptedToken;
    uint256 public price;

    uint256 public startTokenId;
    uint256 public size;

    uint256 public startTime;
    uint256 public endTime;

    bytes32 internal keyHash;
    uint256 internal fee;
    
    uint256 public randomResult;

    // VRF request id => buyer
    mapping(bytes32 => address) public buyers;

    EnumerableSet.UintSet private unopenBoxes;
    EnumerableSet.UintSet private openBoxes;

    modifier onlyAdmin 
    {
        require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "Caller is not an admin");
        _;
    }

    constructor(
        address _admin,
        address _feeManager, 
        address _jnft,
        address _link,
        address _vrfCoordinator,
        bytes32 _keyHash,
        uint256 _fee
    ) VRFConsumerBase(_vrfCoordinator, _link) {
        _setupRole(DEFAULT_ADMIN_ROLE, _admin);
        feeManager = IFeeManager(_feeManager);
        jnft = IJNFT(_jnft);        
        keyHash = _keyHash;
        fee = _fee;
    }

    function initialize(
        address _creator, 
        string memory _name,
        string memory _description,
        uint256 _startTokenId, 
        uint256 _size,
        uint256 _startTime,
        uint256 _endTime,
        address _acceptedToken,
        uint256 _price
    ) public initializer {
        __AccessControl_init_unchained();
        __ERC1155Holder_init_unchained();
        __MysteryBox_init_unchained(
            _creator, 
            _name,
            _description,
            _startTokenId, 
            _size, 
            _startTime, 
            _endTime,
            _acceptedToken,
            _price);
    }

    function __MysteryBox_init_unchained(
        address _creator,
        string memory _name,
        string memory _description,
        uint256 _startTokenId, 
        uint256 _size,
        uint256 _startTime,
        uint256 _endTime,
        address _acceptedToken,
        uint256 _price
    ) internal initializer {        
        creator = _creator;
        name = _name;
        description = _description;
        startTokenId = _startTokenId;
        size = _size;
        startTime = _startTime;
        endTime = _endTime;
        acceptedToken = _acceptedToken;
        price = _price;

        for (uint256 i=_startTokenId; i < _startTokenId+size; i++) {
            unopenBoxes.add(i);
        }
    }

    function claim() external {
        require(currentTime() > endTime, "Not finish");
        require(unopenBoxes.length() != 0, "No box left");
        for (uint256 i; i < unopenBoxes.length(); i++) {
            uint256 _tokenId = unopenBoxes.at(i);
            if (jnft.exists(_tokenId) && jnft.balanceOf(address(this), _tokenId) != 0) {
                jnft.safeTransferFrom(address(this), creator, _tokenId, jnft.balanceOf(address(this), _tokenId), "");
            }
        }
    }

    function buy() external {
        require(LINK.balanceOf(address(this)) >= fee, "Not enough LINK - fill contract with faucet");
        require(unopenBoxes.length() != 0, "No box left");        
        require(currentTime() >= startTime, "Not started");
        require(currentTime() <= endTime, "Timeout");

        bytes32 _requestId = requestRandomness(keyHash, fee);
        buyers[_requestId] = msg.sender;
        IERC20(acceptedToken).transferFrom(msg.sender, address(this), price);        
    }

    function fulfillRandomness(bytes32 requestId, uint256 randomness) internal override {
        if (unopenBoxes.length() != 0) {
            randomResult = (randomness % unopenBoxes.length());
            uint256 _tokenId = unopenBoxes.at(randomResult);
            unopenBoxes.remove(_tokenId);
            openBoxes.add(_tokenId);

            uint256 _platformFee = feeManager.calculateFee(price);
            IERC20(acceptedToken).transfer(creator, price.sub(_platformFee));
            IERC20(acceptedToken).transfer(feeManager.getFeeCollector(), _platformFee);

            uint256 _amount = jnft.balanceOf(address(this), _tokenId);
            jnft.safeTransferFrom(address(this), buyers[requestId], _tokenId, _amount, "");

            emit Buy(buyers[requestId], creator, _tokenId, acceptedToken, price, _amount);
        } else {
            IERC20(acceptedToken).transfer(buyers[requestId], price);
        }        
    }

    function withdrawLink() external onlyAdmin {
        uint256 _amount = LINK.balanceOf(address(this));
        require(_amount != 0, "No LINK");
        LINK.transfer(msg.sender, _amount);
    }

    function openBoxesCount() external view returns (uint256) {
        return openBoxes.length();
    }

    function unopenBoxesCount() external view returns (uint256) {
        return unopenBoxes.length();
    }

    function currentTime() internal virtual view returns (uint256) {
        return block.timestamp;
    }

    function supportsInterface(bytes4 interfaceId) 
        public view virtual override(AccessControlUpgradeable, ERC1155ReceiverUpgradeable) returns (bool) 
    {
        return super.supportsInterface(interfaceId);
    }

    event Buy(
        address indexed buyer,
        address indexed creator,
        uint256 indexed tokenId,
        address acceptedToken,
        uint256 price,
        uint256 amount
    );

    uint256[50] private __gap;
}

