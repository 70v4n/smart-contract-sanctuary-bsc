pragma solidity 0.8.4;

import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

import "./MysteryBox.sol";

contract MysteryBoxFactory is AccessControlUpgradeable, PausableUpgradeable
{
    using Counters for Counters.Counter;
    using SafeMath for uint256;

    struct Params {
        address creator;
        string name;
        string description;
        uint256 startTime;
        uint256 endTime;
        address acceptedToken;
        uint256 price;
        uint256 loyaltyFeePercent; 
        uint256 collectionId;
    }

    modifier onlyAdmin 
    {
        require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "Caller is not an admin");
        _;
    }

    address private admin;
    address private link;
    address private vrfCoordinator;
    bytes32 private keyHash;
    uint256 private linkFee;

    IFeeManager public feeManager;
    IJNFT public jnft;    

    Counters.Counter private _boxIdTracker;

    // box id => box address
    mapping(uint256 => address) public boxes;

    function initialize(
        address _admin, 
        address _feeManager, 
        address _jnft,
        address _link,
        address _vrfCoordinator,
        bytes32 _keyHash,
        uint256 _linkFee
    ) public initializer {
        __AccessControl_init_unchained();
        __Pausable_init_unchained();
        __MysteryBoxFactory_init_unchained(_admin, _feeManager, _jnft, _link, _vrfCoordinator, _keyHash, _linkFee);
    }

    function __MysteryBoxFactory_init_unchained(
        address _admin, 
        address _feeManager, 
        address _jnft,
        address _link,
        address _vrfCoordinator,
        bytes32 _keyHash,
        uint256 _linkFee
    ) internal initializer {
        _setupRole(DEFAULT_ADMIN_ROLE, _admin);
        feeManager = IFeeManager(_feeManager);
        jnft = IJNFT(_jnft);        
        admin = _admin;
        link = _link;
        vrfCoordinator = _vrfCoordinator;
        keyHash = _keyHash;
        linkFee = _linkFee;
    }

    function mintWithRarities(
        Params memory params,
        uint256[] memory _amounts,
        uint256[] memory _rarities
    ) external whenNotPaused returns (uint256 _boxId) {
        require(jnft.isMaintainer(msg.sender), "Caller is not a maintainer");
        _boxIdTracker.increment(); // box id start at 1
        _boxId = _boxIdTracker.current();        
        
        MysteryBox _box = new MysteryBox(
            admin, 
            address(feeManager), 
            address(jnft), 
            link, 
            vrfCoordinator, 
            keyHash, 
            linkFee);

        _box.initialize(
            params.creator,
            params.name,
            params.description,
            jnft.currentTokenId() + 1, 
            _amounts.length, 
            params.startTime, 
            params.endTime,
            params.acceptedToken,
            params.price
        );
        boxes[_boxId] = address(_box);

        emit Mint(
            params.creator, 
            _boxId, 
            boxes[_boxId],
            params.name, 
            params.description, 
            jnft.currentTokenId() + 1,
            _amounts.length, 
            params.startTime, 
            params.endTime, 
            params.acceptedToken, 
            params.price);

        jnft.mintBatchWithRarities(
            params.creator, 
            boxes[_boxId], 
            params.loyaltyFeePercent, 
            _amounts, 
            params.collectionId, 
            _rarities);

        IERC20(link).transfer(boxes[_boxId], linkFee.mul(_amounts.length));
    }

    function mint(
        Params memory params,
        uint256[] memory _amounts
    ) external whenNotPaused returns (uint256 _boxId) {
        require(jnft.isCreator(msg.sender), "Caller is not a creator");
        _boxIdTracker.increment(); // box id start at 1
        _boxId = _boxIdTracker.current(); 

        MysteryBox _box = new MysteryBox(
            admin, 
            address(feeManager), 
            address(jnft), 
            link, 
            vrfCoordinator, 
            keyHash, 
            linkFee);

        _box.initialize(
            params.creator,
            params.name,
            params.description,
            jnft.currentTokenId() + 1, 
            _amounts.length, 
            params.startTime, 
            params.endTime,
            params.acceptedToken,
            params.price
        );
        boxes[_boxId] = address(_box);

        emit Mint(
            params.creator, 
            _boxId, 
            boxes[_boxId],
            params.name, 
            params.description, 
            jnft.currentTokenId() + 1,
            _amounts.length, 
            params.startTime, 
            params.endTime, 
            params.acceptedToken, 
            params.price);

        uint256[] memory _rarities = new uint256[](_amounts.length);
        jnft.mintBatchWithRarities(
            params.creator, 
            boxes[_boxId], 
            params.loyaltyFeePercent, 
            _amounts, 
            params.collectionId, 
            _rarities);

        IERC20(link).transfer(boxes[_boxId], linkFee.mul(_amounts.length));        
    }

    function withdrawLink() external onlyAdmin {
        uint256 _amount = IERC20(link).balanceOf(address(this));
        require(_amount != 0, "No LINK");
        IERC20(link).transfer(msg.sender, _amount);
    }

    function pause() external onlyAdmin whenNotPaused {
        _pause();
    }

    function unpause() external onlyAdmin whenPaused {
        _unpause();
    }

    function currentBoxId() external view returns (uint256) {
        return _boxIdTracker.current();
    }

    event Mint(
        address indexed creator,
        uint256 indexed boxId,
        address indexed boxAddress,
        string name,
        string description,
        uint256 startTokenId,
        uint256 size,
        uint256 startTime,
        uint256 endTime,
        address acceptedToken,
        uint256 price
    );

    uint256[50] private __gap;
}

