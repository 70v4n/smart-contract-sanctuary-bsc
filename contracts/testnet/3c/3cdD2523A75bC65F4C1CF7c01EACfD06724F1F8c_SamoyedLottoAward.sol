/**
 * THIS IS AN EXAMPLE CONTRACT WHICH USES HARDCODED VALUES FOR CLARITY.
 * PLEASE DO NOT USE THIS CODE IN PRODUCTION.
 */
contract SamoyedLottoAward is Ownable, ReentrancyGuard, VRFConsumerBase {
    using SafeMath for uint256;

    // # Chainlink Variable
    bytes32 private s_keyHash;
    uint256 private s_fee;
    uint256 public randomResult;

    // # Samoyed Variable
    address public admin;
    uint256 public currentRound;
    uint256 public currentAwardRound;
    uint256 public currentAwardTimestamp;
    mapping(uint256 => uint256) public randomResultByRound;
    mapping(uint256 => uint256) public awardTimestampByRound;
    // mapping(uint256 => bytes32) public requestIdByRound;

    // Storage
    GodfatherStorage public godfatherStorage;
    PuppyStorage public puppyStorage;

    // Constant
    uint256 public constant MAXIMUM_ROUND = 180;
    uint256 public constant DIVISOR = 16;
    uint256 public constant GODFATHER_EXPONENT = 6;
    uint256 public constant PUPPY_EXPONENT = 2;
    uint256 public constant PUPPY_PER_PACK = 256;
    uint256 public constant MOY_EXPONENT = 1;

    // # Samoyed Event
    event onAwardRequest(uint256 indexed round, bytes32 indexed requestId);
    event onAwardResponse(uint256 indexed round, bytes32 indexed requestId, uint256 indexed randomResult);
    event onSetLottoCheck(string name, address value);

    // # Temporary Variable
    uint256 jackpotNumber = 0;

    GodfatherLottoCheck public godfatherLottoCheck;
    PuppyLottoCheck public puppyLottoCheck;
    PuppyLottoCheck public moyLottoCheck;

    modifier onlyAdmin() {
        require(admin == msg.sender, "SamoyedLottoAward: caller is not the admin");
        _;
    }

    constructor(
        address vrfCoordinator,
        address link,
        bytes32 keyHash,
        uint256 fee,
        GodfatherStorage _godfatherStorage,
        PuppyStorage _puppyStorage
    ) VRFConsumerBase(vrfCoordinator, link) {
        s_keyHash = keyHash;
        s_fee = fee;
        godfatherStorage = _godfatherStorage;
        puppyStorage = _puppyStorage;
        admin = msg.sender;
    }

    function setGodfatherLottoCheck(GodfatherLottoCheck _godfatherLottoCheck) external onlyOwner {
        godfatherLottoCheck = _godfatherLottoCheck;
        emit onSetLottoCheck("Godfather", address(godfatherLottoCheck));
    }

    function setPuppyLottoCheck(PuppyLottoCheck _puppyLottoCheck) external onlyOwner {
        puppyLottoCheck = _puppyLottoCheck;
        emit onSetLottoCheck("Puppy", address(puppyLottoCheck));
    }

    function setMoyLottoCheck(PuppyLottoCheck _moyLottoCheck) external onlyOwner {
        moyLottoCheck = _moyLottoCheck;
        emit onSetLottoCheck("MOY", address(moyLottoCheck));
    }

    function setJackpotNumber(uint256 _jackpotNumber) external onlyOwner {
        jackpotNumber = _jackpotNumber;
    }

    function setAdmin(address _admin) external onlyOwner {
        admin = _admin;
    }

    function setChainLinkFee(uint256 _fee) external onlyOwner {
        s_fee = _fee;
    }

    function awardWinnerByLocalTest(bool isInjectWinner, uint256 _jackpotNumber)
        external
        nonReentrant
        returns (bytes32 requestId)
    {
        // require(
        //     block.timestamp >= currentAwardTimestamp + 22 hours,
        //     "SamoyedChainlinkVRFConsumer: block.timestamp less than timelock"
        // );
        require(currentRound < MAXIMUM_ROUND, "SamoyedChainlinkVRFConsumer: No more round lefts");
        // require(LINK.balanceOf(address(this)) >= s_fee, "SamoyedChainlinkVRFConsumer: Not enough LINK to pay fee");
        // require(currentRound == currentAwardRound, "SamoyedChainlinkVRFConsumer: Awarding in Progress");
        requestId = randomBytes32();

        emit onAwardRequest(currentRound, requestId);
        currentAwardTimestamp = block.timestamp;
        currentRound++;

        // Manual Simulate LoopBack Call From VRF
        // console.log(isInjectWinner);
        // console.log("isInjectWinner: %s",isInjectWinner);
        if (isInjectWinner) {
            // console.log(" - Inject");
            fulfillRandomness(requestId, _jackpotNumber);
        } else {
            // console.log(" - Random");
            fulfillRandomness(requestId, random());
        }
    }

    function awardWinnerByChainLink() external nonReentrant returns (bytes32 requestId) {
        // require(
        //     block.timestamp >= currentAwardTimestamp + 22 hours,
        //     "SamoyedChainlinkVRFConsumer: block.timestamp less than timelock"
        // );
        require(currentRound < MAXIMUM_ROUND, "SamoyedChainlinkVRFConsumer: No more round lefts");
        require(LINK.balanceOf(address(this)) >= s_fee, "SamoyedChainlinkVRFConsumer: Not enough LINK to pay fee");
        require(currentRound == currentAwardRound, "SamoyedChainlinkVRFConsumer: Awarding in Progress");
        requestId = requestRandomness(s_keyHash, s_fee);
        emit onAwardRequest(currentRound, requestId);
        currentAwardTimestamp = block.timestamp;
        currentRound++;
    }

    /**
     * Callback function used by VRF Coordinator
     */
    function fulfillRandomness(bytes32 requestId, uint256 randomness) internal override {
        uint256 currentTime = block.timestamp;
        randomResult = randomness;
        randomResultByRound[currentAwardRound] = randomness;
        awardTimestampByRound[currentAwardRound] = currentTime;

        // Set Godfather Results
        uint256 godfatherWonTokenIndex = _getGodfatherWinnerTokenID(randomness);
        uint256 godfatherValidTokenIndex = _getGodfatherValidTokenIndexCounter(godfatherWonTokenIndex);
        // console.log("godfatherValidTokenIndex : %d",godfatherValidTokenIndex);
        godfatherLottoCheck.fulfillLottoResult(
            currentAwardRound,
            currentTime,
            godfatherWonTokenIndex,
            godfatherValidTokenIndex,
            1
        );

        // Set Puppy Winner Address
        uint256 puppyWinnerTokenIndex = _getPuppyWinnerTokenID(randomness);
        uint256 puppyValidTokenIndex = _getPuppyValidTokenIndexCounter();
        uint256 puppySharingHolder = _getPuppySharingHolderQuantity(puppyWinnerTokenIndex);
        // console.log("puppyValidTokenIndex: %d , puppySharingHolder: %d",puppyValidTokenIndex,puppySharingHolder);
        puppyLottoCheck.fulfillLottoResult(
            currentAwardRound,
            currentTime,
            puppyWinnerTokenIndex,
            puppyValidTokenIndex,
            puppySharingHolder
        );

        // Set Moy Winner Address
        uint256 moyWinnerTokenIndex = _modWonNumber(randomness, DIVISOR, MOY_EXPONENT);
        moyLottoCheck.fulfillLottoResult(currentAwardRound, currentTime, moyWinnerTokenIndex, puppyValidTokenIndex, 0);

        currentAwardRound++;

        emit onAwardResponse(currentAwardRound, requestId, randomness);
    }

    function _getGodfatherWinnerTokenID(uint256 randomness) internal pure returns (uint256 godfatherWinner) {
        //Godfather Award
        uint256 godfatherTokenIndex = _modWonNumber(randomness, DIVISOR, GODFATHER_EXPONENT);
        // console.log("godfatherTokenURI: ",godfatherTokenURI);
        // uint256 baseTicketTokenURI = godfatherStorage.getBaseTicket(godfatherTokenURI);
        // console.log("baseTicketTokenURI: ",baseTicketTokenURI);
        // wonAddress[0]=_getERC721OwnerAddress(godfatherStorage,baseTicketTokenURI);
        return godfatherTokenIndex;
    }

    function _getGodfatherValidTokenIndexCounter(uint256 _tokenIndex) internal view returns (uint256 tokenIndex) {
        uint256 baseTicketTokenURI = godfatherStorage.getBaseTicket(_tokenIndex);
        try godfatherStorage.ownerOf(baseTicketTokenURI) returns (address) {
            return _tokenIndex;
        } catch {
            return 0;
        }
    }

    function _getPuppyWinnerTokenID(uint256 randomness) internal pure returns (uint256 puppyWinnerTokenArray) {
        //Godfather Award
        uint256 puppyWonNumber = _modWonNumber(randomness, DIVISOR, PUPPY_EXPONENT);
        // uint256 maxPackAllowed = puppyStorage.maxPackAllowed();

        // uint128 currentPack;
        // uint128 nextTicketNumber;
        // (currentPack,nextTicketNumber) = puppyStorage.puppyCounter();

        // uint256 winnerCounter=0;
        // uint256[] memory tmpWonTokenIDArray = new uint256[](maxPackAllowed+1);
        // for (uint pack = 0; pack <= maxPackAllowed; pack++) {
        //     uint256 tokenId = (pack * PUPPY_PER_PACK) + puppyWonNumber;
        //     try puppyStorage.ownerOf(tokenId) returns (address winner) {
        //         // you can use variable foo here
        //         tmpWonTokenIDArray[winnerCounter] = tokenId;
        //         winnerCounter++;
        //     } catch{ }
        //     //console.log("i: %d, tokenId: %d", i, tokenId);
        // }
        // console.log("WinnerCounter : ",winnerCounter);
        // uint256[] memory wonTokenIDArray = new uint256[](winnerCounter);
        // for (uint i=0;i<winnerCounter;i++){
        //     wonTokenIDArray[i]=tmpWonTokenIDArray[i];
        //     console.log("Puppy Winner Token Index : %d",wonTokenIDArray[i]);
        // }

        // uint256 erc721TokenURI=_modWonNumber(randomness,DIVISOR,PUPPY_EXPONENT);
        return puppyWonNumber;
    }

    function _getPuppyValidTokenIndexCounter() internal view returns (uint256 tokenIndex) {
        uint128 currentPack;
        uint128 nextTicketNumber;
        (currentPack, nextTicketNumber) = puppyStorage.puppyCounter();
        uint256 tokenId = (currentPack * PUPPY_PER_PACK) + nextTicketNumber;
        // console.log("currentPack : %d, nextTicketNumber",currentPack ,nextTicketNumber);
        // console.log("valid puppy tokenID : %d",tokenId);
        if (tokenId == 0) {
            return 0;
        } else {
            return tokenId - 1;
        }
    }

    function _getPuppySharingHolderQuantity(uint256 puppyNumber) internal view returns (uint256 holderQuantity) {
        uint128 currentPack;
        uint128 nextTicketNumber;
        (currentPack, nextTicketNumber) = puppyStorage.puppyCounter();
        uint256 _holderQuantity = currentPack + 1;
        if (puppyNumber >= nextTicketNumber) {
            _holderQuantity--;
        }
        return _holderQuantity;
    }

    // function getERC721OwnerAddress(IERC721 erc721ContractInterface, uint256 tokenURI) public view returns(address){
    //     address wonAddress;
    //     try erc721ContractInterface.ownerOf(tokenURI) returns (address winner) {
    //         // you can use variable foo here
    //         wonAddress = winner;
    //     } catch{
    //         wonAddress = address(0x0);
    //     }
    //     return wonAddress;
    // }

    // You may want a way to handle the LINK returned when a request is cancelled.
    //   withdrawLINK is an example of a method that allows the owner to withdraw the LINK.
    function withdrawLINK() external nonReentrant onlyOwner {
        require(LINK.transfer(admin, LINK.balanceOf(address(this))), "SamoyedChainlinkVRFConsumer: Unable to transfer");
    }

    function random() private view returns (uint256) {
        return uint256(keccak256(abi.encodePacked(block.timestamp)));
    }

    function randomBytes32() private view returns (bytes32) {
        return keccak256(abi.encodePacked(block.timestamp));
    }

    // function getWinnerFromJackpotNumber(uint256 round, uint256 exponentNumber) public view returns (uint256 wonNumber) {
    //     require(exponentNumber > 0, "SamoyedChainlinkVRFConsumer: exponentNumber must greater than 0");
    //     require(round <= currentAwardRound, "SamoyedChainlinkVRFConsumer: round greter than currentAwardRound");
    //     uint256 _wonNumber = jackpotNumber % (divisor**exponentNumber);
    //     return _wonNumber;
    // }

    // function getWinnerByRound(uint256 round, uint256 exponentNumber) public view returns (uint256 wonNumber) {
    //     require(exponentNumber > 0, "SamoyedChainlinkVRFConsumer: exponentNumber must greater than 0");
    //     require(round <= currentAwardRound, "SamoyedChainlinkVRFConsumer: round greter than currentAwardRound");
    //     uint256 _wonNumber = randomResultByRound[round] % (divisor**exponentNumber);
    //     return _wonNumber;
    // }
    function _modWonNumber(
        uint256 _inputNumber,
        uint256 _divisor,
        uint256 _exponentNumber
    ) internal pure returns (uint256 wonNumber) {
        require(_exponentNumber > 0, "SamoyedChainlinkVRFConsumer: exponentNumber must greater than 0");
        uint256 tmpDivisor = _divisor**_exponentNumber;
        require(tmpDivisor >= _divisor, "SamoyedChainlinkVRFConsumer: divisor overflow");
        uint256 _wonNumber = _inputNumber.mod(tmpDivisor);
        return _wonNumber;
    }

    function getAwardInformationByRound(uint256 _round)
        external
        view
        returns (
            uint256 round,
            uint256 awardTimestampUTC,
            uint256 chainLinkRandomResult
        )
    {
        // console.log("XROUND : %d, currentAwardRound : %d",_round,currentAwardRound);
        require(_round < currentAwardRound, "SamoyedChainlinkVRFConsumer: round exceed currentAwardRound");
        return (_round, awardTimestampByRound[_round], randomResultByRound[_round]);
    }
}

