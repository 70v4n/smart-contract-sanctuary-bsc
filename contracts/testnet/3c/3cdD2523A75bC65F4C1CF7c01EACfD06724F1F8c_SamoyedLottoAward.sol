import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "./SamoyedLottoCheck.sol";
import "./LottoResultFulfill.sol";
import "../tokens/PuppyStorage.sol";

import "hardhat/console.sol";

contract PuppyLottoCheck is SamoyedLottoCheck, LottoResultFulfill {
    using SafeMath for uint256;
    PuppyStorage public puppyStorage;
    uint256 public constant BNB_FINAL_PRIZE = 10 ether;

    constructor(
        PuppyStorage _puppyStorage,
        address _samoyedLottoAward,
        IBEP20 _smoyAddress,
        IBEP20 _busdAddress
    ) LottoResultFulfill(_samoyedLottoAward) {
        puppyStorage = _puppyStorage;
        smoyToken = _smoyAddress;
        busdToken = _busdAddress;
    } 

    function claimMultipleReward(uint256 _round, uint256[] memory _tokenIDArray) external nonReentrant {
        require(_round < MAXIMUM_ROUND, "PuppyLottoCheck: RoundNumber overflow");

        uint256 round;
        uint256 timestamp;
        uint256 wonNumber;
        uint256 validTokenIndexCounter;
        uint256 winnerSharingQuantity;
        (round, timestamp, wonNumber, validTokenIndexCounter, winnerSharingQuantity) = getLottoResultByRound(_round);

        for (uint i = 0; i < _tokenIDArray.length; i++) {
            uint256 _tokenID = _tokenIDArray[i];
            // console.log("_tokenID: %d , validTokenIndexCounter : %d",_tokenID,validTokenIndexCounter);
            require(_tokenID <= validTokenIndexCounter, "PuppyLottoCheck: No Owner Found when Awarding");
            require(_tokenID.mod(256) == wonNumber, "PuppyLottoCheck: you didn't win");

            address winnerAddress = _getERC721OwnerAddress(puppyStorage, _tokenID);
            require(msg.sender == winnerAddress, "PuppyLottoCheck: You are not the owner of the token");

            require(!isTokenAlreadyClaimedReward[_round][_tokenID],"PuppyLottoCheck: You've got the reward before");

            // console.log("tokenID : %d , address : %s , Claim Sucess",_tokenID,winnerAddress);
            // console.log("Sharing Winner:",winnerSharingQuantity);
            isTokenAlreadyClaimedReward[_round][_tokenID] = true;

            if (_round == MAXIMUM_ROUND - 1) {
                uint256 finalPrize = BNB_FINAL_PRIZE.div(winnerSharingQuantity);
