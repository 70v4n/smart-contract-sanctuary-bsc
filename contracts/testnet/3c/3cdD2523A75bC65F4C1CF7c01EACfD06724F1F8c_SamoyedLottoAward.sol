pragma solidity ^0.8.4;

import "./SamoyedLottoCheck.sol";
import "../tokens/GodfatherStorage.sol";
import "./LottoResultFulfill.sol";

import "hardhat/console.sol";

contract GodfatherLottoCheck is SamoyedLottoCheck, LottoResultFulfill {
    GodfatherStorage public godfatherStorage;
    uint256 public constant SMOY_PRIZE = 100_000 ether;
    uint256 public constant BNB_PRIZE = 0.1 ether;

    constructor(
        GodfatherStorage _godfatherStorage,
        address _samoyedLottoAward,
        IBEP20 _smoyAddress,
        IBEP20 _busdAddress
    ) LottoResultFulfill(_samoyedLottoAward) {
        godfatherStorage = _godfatherStorage;
        smoyToken = _smoyAddress;
        busdToken = _busdAddress;
    }

    function claimMultipleReward(uint256 _round, uint256[] memory _tokenIDArray) external nonReentrant {
        uint256 round;
        uint256 timestamp;
        uint256 wonNumber;
        uint256 validTokenIndexCounter;
        uint256 winnerSharingQuantity;
        (round, timestamp, wonNumber, validTokenIndexCounter, winnerSharingQuantity) = getLottoResultByRound(_round);
        require(validTokenIndexCounter == wonNumber, "GodfatherLottoCheck: No Winner Found when Awarding");

        
        // console.log("round : %d",round);
        // console.log("wonNumber : %d",wonNumber);
        // console.log("validTokenIndexCounter : %d",validTokenIndexCounter);
        require(_tokenIDArray[0] == wonNumber,"GodfatherLottoCheck: You didn't win");
        uint256 baseTicketTokenURI = godfatherStorage.getBaseTicket(wonNumber);
        address winnerAddress = _getERC721OwnerAddress(godfatherStorage, baseTicketTokenURI);
        require(msg.sender == winnerAddress, "GodfatherLottoCheck: You are not the owner of the token");

        uint256 _tokenID = baseTicketTokenURI;
        if (!isTokenAlreadyClaimedReward[_round][_tokenID]) {
            // console.log("You Won a JACKPOT");
            // console.log("tokenID : %d , address : %s , Claim Sucess",_tokenID,winnerAddress);
            isTokenAlreadyClaimedReward[_round][_tokenID] = true;
            _transferSMOYTo(msg.sender, SMOY_PRIZE);
            _sendETHViaCall(payable(msg.sender), BNB_PRIZE);

            emit onClaimReward(_round, _tokenID, msg.sender, SMOY_PRIZE, BNB_PRIZE);
        } else {
            // console.log("tokenID : %d , address : %s , You've got the reward before",_tokenID,winnerAddress);
            revert("GodfatherLottoCheck: You've got the reward before");
        }            
        

    }
}

