contract EagleMoon  {
    
    using SafeMath for uint256;
    using EnumerableSet for EnumerableSet.UintSet;

    address payable ownerAddress;
    BEP20Interface public USDTaddress;
    bool public isRunning;
    mapping(uint256=>uint256) public packages;
    mapping(uint256=>uint256) public eaglePackages;
    
    // uint256 index = 0 ;
    
    mapping(address => EnumerableSet.UintSet) private userPackage;

   event buyMmebershipPlan(address indexed user,uint256 plan,uint256 eaglecount,uint256 amount,uint256 date);

   constructor(address payable _ownerAddress, address _tokenAddress) public {

        isRunning=true;
        ownerAddress=_ownerAddress;
        USDTaddress=BEP20Interface(_tokenAddress);
        packages[1]=5;
        packages[2]=25;
        packages[3]=50;
        packages[4]=100;

        eaglePackages[1]=21000;
        eaglePackages[2]=101000;
        eaglePackages[3]=201000;
        eaglePackages[4]=501000;
    }

    modifier onlyOwner() {
      require(msg.sender==ownerAddress,"Invalid user");
      _;
    }

    function setPrice(uint256 plan,uint256 amount,uint256 eagleAmount) public onlyOwner
    {
        //require(msg.sender==ownerAddress,"Invalid user");
        require(plan > 0 && amount > 0,"Invalid amount or plan");
        packages[plan] = amount;
        eaglePackages[plan] = eagleAmount;
    }

    function setStatusToContract(bool status) public onlyOwner
    {
        //require(msg.sender==ownerAddress,"Invalid user");
        isRunning = status;
    }
    
    function userPackageAtIndex(address owner, uint256 index) public view returns (uint256) {
        return userPackage[owner].at(index);
    }

    function buyMembership(uint256 plan) public
    {
        require(packages[plan] > 0, "Invalid plan");
        require(isRunning, "Contract stopped");
        //require(BEP20Interface(address(USDTaddress)).transferFrom(msg.sender, address(this), packages[plan]*1e18),"Token transfer failed");
        BEP20Interface(address(USDTaddress)).transferFrom(msg.sender, address(ownerAddress), packages[plan]*1e18);
        
        //userPackage[msg.sender][index] = plan;
        
        userPackage[msg.sender].add(plan);
        
        emit buyMmebershipPlan(msg.sender,plan,eaglePackages[plan],packages[plan],now);

    }
}