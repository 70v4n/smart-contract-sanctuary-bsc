pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
import "hardhat/console.sol";



/**@title Presale BitHotel smart contract
 * @author BitHotel Team
 */
contract PresaleBitHotel is Ownable,ERC721Holder{
 
   // collection struct
   struct GameNFT{
       uint256 index;
       uint256 cost;
       address contractAddress;
       address ownerAddress;
       uint256 tokenId;
   }


  struct UserNFT{
       address userAddress;
       address nftAddress;
   }
   // whitelist struct
   struct WhitelistUser{
       uint256 index;
       UserNFT userNFT;
   }


   mapping(address => mapping(address => WhitelistUser)) private whiteListUsers;
   address[] private whitelistedIndex;
   mapping(address =>mapping(uint256 => GameNFT)) private gameNFTs;
   address[] private gameIndex;

   mapping(address => uint256) private solds; 

   bool public _initialized = false;
   IERC20 public busdContract;
   address public wallet;

   event WhiteListUser(
       address userAddress,
       address nftAddress);

   event AddGameNFT(
       address contractAddress,
       uint256 cost,
       address ownerAddress,
       uint256 id);

    event Claim(
        address sender,
        address receiver,
        uint256 tokenId
    );

   constructor(address _wallet,address _busd){
       wallet = _wallet;
       busdContract = IERC20(_busd);
   }


   modifier notInitialized(){
       require(!_initialized,"Initialized");
       _;
   }

   modifier onlyInitialized(){
       require(_initialized,"Not Initialized");
       _;
   }
   

     function onERC721Received(
        address,
        address,
        uint256,
        bytes memory
    ) public virtual override returns (bytes4) {
        return this.onERC721Received.selector;
    }
          
   /**@notice initializer will run after deploy the contract immediately 
    * @param _gameNFTs set up the game collection NFTs 
    */
   function initializer(
      GameNFT[] memory _gameNFTs
   ) external notInitialized onlyOwner{
        
        for(uint256 i = 0; i< _gameNFTs.length; i++){
          
                addGameNft(
                    _gameNFTs[i].index,
                    _gameNFTs[i].contractAddress,
                    _gameNFTs[i].cost,
                    _gameNFTs[i].ownerAddress,
                    _gameNFTs[i].tokenId
                    );

                
                IERC721(_gameNFTs[i].contractAddress).safeTransferFrom(msg.sender, address(this),_gameNFTs[i].tokenId);

        
        }

        _initialized = true;

   }

   /**@notice add the nft in the list 
    * @param _index set up the index
    * @param _contractAddress set up the nft contract address
    * @param _cost set up the cost 
    * @param _ownerAddress set up the owner address 
    * @param _id set up the id 
    */

   function addGameNft(
       uint256 _index,
       address _contractAddress,
       uint256 _cost,
       address _ownerAddress,
       uint256 _id
       ) public onlyOwner{
      
      if(!isListNFT(_contractAddress,_id)){
          gameNFTs[_contractAddress][_id].cost = _cost;
          gameNFTs[_contractAddress][_id].index = _index;
          gameNFTs[_contractAddress][_id].ownerAddress = _ownerAddress;
          gameNFTs[_contractAddress][_id].contractAddress = _contractAddress;
          gameNFTs[_contractAddress][_id].tokenId = _id;
          gameIndex.push(_contractAddress);
          emit AddGameNFT(_contractAddress, _cost, _ownerAddress, _id);
      }
   }


   /**@notice add the users in the whitelist
    * @param _userAddresses set up the user addresses
    * @param _userNFTs set up the nft list
    */

    function addWhiteListUsers(
        address[] memory _userAddresses,
        UserNFT[] memory _userNFTs
    ) external onlyOwner {
        
       for(uint256 i = 0; i < _userAddresses.length; i++){
           for(uint256 j = 0; j < _userNFTs.length; j++){
               if(_userAddresses[i] == _userNFTs[j].userAddress){
                 addWhiteListUser(_userAddresses[i], _userNFTs[j].nftAddress);

               }
           }
       }
    }

    /**@notice add the user in the whitelist
     * @param _userAddress set up the user address to get NFT 
     * @param _nftAddress set up the nft contract address
     */
    function addWhiteListUser(
        address _userAddress,
        address _nftAddress) public onlyOwner {
        
        if(!isWhitelisted(_userAddress,_nftAddress)){
            whiteListUsers[_userAddress][_nftAddress].userNFT.userAddress = _userAddress;
            whiteListUsers[_userAddress][_nftAddress].userNFT.nftAddress = _nftAddress;
            whitelistedIndex.push(_userAddress);
            unchecked {
                whiteListUsers[_userAddress][_nftAddress].index = whitelistedIndex.length - 1;
            }

            emit WhiteListUser(_userAddress, _nftAddress);
        }

    }
    
    /**@notice check the whitelisted user 's status
     * @param _userAddress set up the user address to check in the whitelist
     * @return isIndeed true or false 
     */

    function isWhitelisted(address _userAddress,address _nftContract) public view returns(bool isIndeed){
        
        if(whitelistedIndex.length == 0) return false;
        return (whitelistedIndex[whiteListUsers[_userAddress][_nftContract].index] == _userAddress);
    }

    /**@notice check the listed nft contract status
     * @param _contractAddress set up the nft contract in list
     * @return isIndeed true of false
     */
    function isListNFT(address _contractAddress,uint256 _tokenId) public view returns(bool isIndeed){
        if(gameIndex.length == 0)  return false;
        return (gameIndex[gameNFTs[_contractAddress][_tokenId].index] == _contractAddress);
    }

    /**@notice set up the new cost 
     * @param _contractAddress set up the nft contract 
     * @param _newCost set up the new cost 
     */
    function setCostToNft(address _contractAddress,uint256 _newCost,uint256 _tokenId) external onlyOwner{
        if(isListNFT(_contractAddress,_tokenId)){
           gameNFTs[_contractAddress][_tokenId].cost = _newCost;
        }
    }


    /**@notice set up the new owner address
     * @param _contractAddress set up the nft contract
     * @param _newOwnerAddress set up the new owner address
     */

    function setOwnerAddress(address _contractAddress,address _newOwnerAddress,uint256 _tokenId) external onlyOwner{
        if(isListNFT(_contractAddress,_tokenId)){
            gameNFTs[_contractAddress][_tokenId].ownerAddress = _newOwnerAddress;
        }
    }
    
    /**@notice delete the game nft when only the sale ends 
     * @param _contractAddress set up the nft contract
     */
    function deleteGameNft(address _contractAddress,uint256 _tokenId) internal{
        if(isListNFT(_contractAddress,_tokenId)){
            delete gameNFTs[_contractAddress][_tokenId];
        }
    }

    function getNftCount(address _contractAddress) internal view returns(uint256) {

        return IERC721(_contractAddress).balanceOf(address(this));

    }

    function getSoldNft(address _contractAddress) external view  returns(uint256){
         return solds[_contractAddress];
    }


    function buy(address _contractAddress,uint256 amount) external onlyInitialized {
       require(_contractAddress != address(this),"The contract address can not be zero.");
       require(amount > 0,"The amount can not be");
       require(isWhitelisted(msg.sender, _contractAddress),"The user is not whitelisted");
       require(getNftCount(_contractAddress) > 0,"The token was sold out.");
       unchecked {
            solds[_contractAddress] = solds[_contractAddress] + 1;
       }
      
       uint256 newTokenId = solds[_contractAddress];
       require(isListNFT(_contractAddress, newTokenId),"The token is not exist.");
       require(gameNFTs[_contractAddress][newTokenId].cost == amount,"The price is not exact.");

       require(
          busdContract.transferFrom(msg.sender, wallet, amount),
        "Insufficient amount");

        IERC721(_contractAddress).safeTransferFrom(address(this), msg.sender, newTokenId);
        deleteGameNft(_contractAddress, newTokenId);

        emit Claim(address(this),msg.sender,newTokenId);
    
    }
    

}

