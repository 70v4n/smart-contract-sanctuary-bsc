contract NftStaking is Ownable {
  using SafeMath for uint256;
    bytes4 private constant INTERFACE_ID_ERC721 = 0x80ac58cd;
    bytes4 private constant INTERFACE_ID_ERC1155 = 0xd9b67a26;

    event Distribution(uint256 tokenId, uint256 rarity, uint256 balance, uint256 value);

/* 
  /// @notice Events for the contract
    event ItemListed(
        address indexed owner,
        address indexed nft,
        uint256 tokenId,
        uint256 quantity,
        uint256 pricePerItem,
        uint256 startingTime,
        bool isPrivate,
        address allowedAddress
    ); */

      /// @notice Structure for listed items
    struct StakedNft {
        address nftContractAddress;
        uint256 tokenId;
        address owner;
    }


    StakedNft[] public stakedNfts;

    mapping(uint256 => uint256) public nftRarities;

    address public airdropContractAddress;

    constructor() public {

    }

    function setAirdropContractAddress(address _airdropContractAddress) external {
        airdropContractAddress = _airdropContractAddress;
    }

    function stake(
        address _nftAddress,
        uint256 _tokenId
    ) external {
        if (_supportERC721(_nftAddress)) {
            IERC721 nft = IERC721(_nftAddress);
            require(
                nft.ownerOf(_tokenId) == _msgSender(),
                "Must be owner of NFT."
            );
            require(
                nft.isApprovedForAll(_msgSender(), address(this)),
                "Must be approved before list."
            );

            IERC721(_nftAddress).transferFrom(_msgSender(), address(this), _tokenId);
        } else if (_supportERC1155(_nftAddress)) {
            IERC1155 nft = IERC1155(_nftAddress);
            require(
                nft.balanceOf(_msgSender(), _tokenId) >= 1,
                "Must hold enough NFTs."
            );
            require(
                nft.isApprovedForAll(_msgSender(), address(this)),
                "Must be approved before list."
            );

             IERC1155(_nftAddress).safeTransferFrom(_msgSender(), address(this), _tokenId,1,'');
        } else {
            revert("Invalid NFT address.");
        }

         StakedNft memory stakedNft  =  StakedNft({
            nftContractAddress: _nftAddress,
            tokenId: _tokenId,
            owner: _msgSender()
        });
  
        stakedNfts.push(stakedNft);
    }

    function   getTotalRarity() public view returns(uint256)  {
         uint256 totalRarity = 0;
        for (uint256 i = 0; i < stakedNfts.length; i++) {  
            totalRarity += nftRarities[stakedNfts[i].tokenId];
        } 
        return totalRarity;
    }

    function pay(address tokenContactAddress) external onlyOwner {
         address[] memory recipients = new address[](stakedNfts.length);
        uint256[] memory values = new uint256[](stakedNfts.length);
        uint256 totalRarity = getTotalRarity();
         uint256 balance = IERC20(tokenContactAddress).balanceOf(address(this));

        for (uint256 i = 0; i < stakedNfts.length; i++) {  
             recipients[i] = stakedNfts[i].owner;
             values[i] = nftRarities[stakedNfts[i].tokenId].div(totalRarity).mul(balance);

             emit Distribution(nftRarities[stakedNfts[i].tokenId], totalRarity, balance, values[i]);
        }

        CampaignAirdrop(airdropContractAddress).addAllocations(IERC20(tokenContactAddress), recipients, values);

        IERC20(tokenContactAddress).approve(airdropContractAddress, balance);
 

        IERC20(tokenContactAddress).transfer(
            airdropContractAddress,
            balance
        );
    }

