import "@openzeppelin/contracts/access/Ownable.sol";
// import "@openzeppelin/contracts/utils/Address.sol";
// import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol"; 

import "./utils/TransferHelper.sol";

import "./utils/IDEX/IDEXFactory.sol";
import "./utils/IDEX/IDEXRouter.sol";
import "./utils/DividendDistributor/IDividendDistributor.sol";
import "./utils/DividendDistributor/DividendDistributor.sol";

import "hardhat/console.sol";

contract DefiNFT is ERC721Enumerable, ERC721URIStorage, Ownable {
    using SafeMath for uint256;
    uint256 constant _wei = (10 ** 18);
    uint8 constant _decimals = 9;

    // Contract Addresses
    address WBNB = 0xae13d989daC2f0dEbFf460aC112a837C89BAa7cd;
    // address BTC = 0x6ce8da28e2f864420840cf74474eff5fd80e65b8; //not on testnet
    address ETH = 0x8BaBbB98678facC7342735486C851ABD7A0d17Ca;
    address CAKE = 0xF9f93cF501BFaDB6494589Cb4b4C15dE49E85D0e;
    address USDT = 0x7ef95a0FEE0Dd31b22626fA2e10Ee6A223F8a684;
    address DAI = 0x8a9424745056Eb399FD19a0EC26A14316684e274;

    address ROUTER = 0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3;
    address mintFeeAddr = 0x544e5A8D2740E84a2fEb53D8857460C449dB24a6;
    address[] assetContracts = [DAI, USDT]; // Asset backing portfolio (even for now)

    IDEXRouter public router;
    DividendDistributor distributor;
    uint256 distributorGas = 500000;
    
    
    uint256 investmentPoolBalance = 0;
    uint256 profitBalance = 0;

    // Shares for dividend tracking, not really needed if we set prices 
    uint256 public initalShareSupply = 1000000000; 
    uint256 public remainingShareSupply = initalShareSupply; 
    // Mint Rounds
    uint256 public rounds = 10;
    uint256 public sharesPerRound = initalShareSupply.div(rounds);
    // NFT
    uint256 public initalSharePrice = 0.0001 ether;
    uint256 public initalEvaluation = initalSharePrice.mul(remainingShareSupply);
    uint256 public sharesPerNFT = 1000;
    uint256 public mintFeeAmount = sharesPerNFT.mul(initalSharePrice);

    struct Account {
        uint256 amount;
        uint256 shares;
        uint256[] tokenBalances; // 30% of mint cost will be stored as other currencies
        uint256 lockEndTime;
    }
    // @dev Each NFT repersents a single account (NFT 1 : 1 ACCOUNT)
    Account[] public accounts;

    event Locked(address indexed address_, uint amount_, uint durration_, uint account_);
    event Liquidated(address indexed address_, uint account_);

    constructor(string memory name, string memory symbol) ERC721(name, symbol) {
        router = IDEXRouter(ROUTER);
        distributor = new DividendDistributor(address(router));
    }

    function calcSharePrice(uint256 round) public view returns (uint256) {
        console.log("FUNCTION: calcSharePrice.round", round); // TODO: remove log
        uint256 nftTotalBalance = address(this).balance; //all money locked in all NFT's, repersents MCAP
        console.log("FUNCTION: calcSharePrice.nftTotalBalance", nftTotalBalance); // TODO: remove log

        //calculate 
        uint256 newEvulation = initalEvaluation.add(nftTotalBalance);
        console.log("FUNCTION: calcSharePrice.newEvulation", newEvulation); // TODO: remove log
        
        uint256 circSupply = initalShareSupply.sub(sharesPerRound.mul(round));
        console.log("FUNCTION: calcSharePrice.circSupply", circSupply); // TODO: remove log
        uint256 sharePrice = newEvulation / circSupply;
        console.log("FUNCTION: calcSharePrice.sharePrice", sharePrice); // TODO: remove log
        return sharePrice;
    }

    function getAccountTokens(uint256 _id) public view returns (uint256[] memory){
        return accounts[_id].tokenBalances;
    }

    function mintAccount(address _nftOwner, string memory _tokenURI, uint _lockDurration) public onlyOwner payable{
        /*
            1.  Check all conditions are met
            2.  Allocate funds
                - Take 20% (1/5) Fee
                - Add 50% (1/2) to investment pool balance
                - Trade 30% (3/10) for NFT portfolioth as asset backing in NFT
            3.  Reduce total share

        Create NFT
            5.  Create Account (keep track of portfolio balance)
            6.  Set shares
            7.  Mint Accounts on NFT
        */
        console.log("FUNCTION: mintAccount.mintFeeAmount", mintFeeAmount); // TODO: remove log

        // 1.  Check all conditions are met
        uint paidAmount = msg.value;
        require(_lockDurration > 0, "BLNFT: Lock should be in the future.");
        require(msg.value > mintFeeAmount, "BLNFT: Invalid BNB amount.");  // Todo: Make sure price is correct for the round
        require(block.timestamp.add(_lockDurration) <= block.timestamp + 315360000, "BLNFT: Too optimistic. Only less than 10 years allowed.");

        // 2. Allocate Funds
        uint256 feeAmount = paidAmount.div(5); //  20% (1/5) Fee
        uint256 poolAmount = paidAmount.div(2); //  50% (1/2) for investment pool
        uint256 tradeAmount = paidAmount.mul(3).div(10); //  30% (3/10) Fee
        console.log("FUNCTION: mintAccount.feeAmount", feeAmount); // TODO: remove log
        console.log("FUNCTION: mintAccount.poolAmount", poolAmount); // TODO: remove log
        console.log("FUNCTION: mintAccount.tradeAmount", tradeAmount); // TODO: remove log

        //  - Take 20% (1/5) Fee
        TransferHelper.safeTransferETH(mintFeeAddr, feeAmount); 

        //  - Add 50% (1/2) to investment pool balance
        investmentPoolBalance = investmentPoolBalance.add(poolAmount); // add balance to pool
        console.log("FUNCTION: mintAccount.investmentPoolBalance", investmentPoolBalance); // TODO: remove log

        uint256 balanceBefore = address(this).balance; // TODO: remove log
        console.log("FUNCTION: mintAccount.balanceBefore", balanceBefore); // TODO: remove log

        //  - Trade 30% (3/10) for NFT portfolioth as asset backing in NFT
        uint256[] memory tokenBalances = new uint256[](assetContracts.length);
        // buy tokens IN assetContracts
        for (uint i = 0; i < assetContracts.length ; i++) {
                
            address[] memory path = new address[](2);
            path[0] = WBNB;
            path[1] = assetContracts[i];

            uint256 amountToBuy = tradeAmount.div(2);
            console.log("FUNCTION: mintAccount.amountToBuy", amountToBuy); // TODO: remove log

            router.swapExactETHForTokensSupportingFeeOnTransferTokens{value:amountToBuy}(
                0,
                path,
                address(this),
                block.timestamp
            );

            uint256 boughtTokenBalance = getTokenBalance(assetContracts[i]);
            console.log("FUNCTION: mintAccount.boughtTokenBalance", boughtTokenBalance); // TODO: remove log
            console.log("FUNCTION: mintAccount.boughtTokenBalance - ADDRESS", assetContracts[i]); // TODO: remove log

            tokenBalances[i] = boughtTokenBalance;
        }
    
        uint256 balanceAfter = address(this).balance; // TODO: remove log
        console.log("FUNCTION: mintAccount.balanceAfter", balanceAfter); // TODO: remove log

        // 3.  Reduce total share
        remainingShareSupply = remainingShareSupply.sub(sharesPerNFT); //reduce from total shares
        console.log("FUNCTION: mintAccount.remainingShareSupply", remainingShareSupply); // TODO: remove log

        // 4. Create Account
        accounts.push(Account(msg.value, sharesPerNFT, tokenBalances, block.timestamp.add(_lockDurration)));

        //  5. Set shares
        distributor.setShare(_nftOwner, sharesPerNFT);
        
        //  6. Mint Account
        uint256 newItemId = accounts.length - 1;
        _mint(_nftOwner, newItemId);
        _setTokenURI(newItemId, _tokenURI);

        // @dev Emit {Locked} event.
        emit Locked(_nftOwner, msg.value, _lockDurration, newItemId);
    }

    
    function liquidate(uint account_, address _to) external {
        require(_exists(account_), "DEFI-NFT: Lock does not exist.");
        address owner = ownerOf(account_);
        // require(owner == msg.sender, "DEFI-NFT: Lock is not yours."); // add back for deployment
        bool locked = accounts[account_].lockEndTime >= block.timestamp; //if false, take 20% (1/5) fee
        console.log("FUNCTION: liquidate.locked", locked); // TODO: remove log

        // @dev Withdraw tokens
        for (uint i = 0; i < assetContracts.length ; i++) {
            uint256 balance = accounts[account_].tokenBalances[i];
            console.log("FUNCTION: liquidate.balance", balance); // TODO: remove log
            
            uint256 earlyFeeAmount;
            //take fee if locked
            if(locked){
                // take 20% (1/5) fee
                earlyFeeAmount = balance.div(5); 
                balance = balance.sub(earlyFeeAmount); 
                // leave in contract as profit to be shared among everyone
                profitBalance = profitBalance.add(earlyFeeAmount); 
                console.log("FUNCTION: liquidate.balance AFTERTAKE FEE ", balance); // TODO: remove log
            }

            IERC20 token = IERC20(assetContracts[i]); 
            token.transfer(_to, balance);
        }

        // @dev delete account
        _burn(account_);
        delete accounts[account_];
        // distributor.setShare(_nftOwner, 0);

        // @dev Emit {Unlocked} event.
        emit Liquidated(_to, account_);
    }

    // Helpers
    function getTokenBalance(address tokenContractAddress) public view returns(uint) {
        IERC20 token = IERC20(tokenContractAddress); // token is cast as type IERC20, so it's a contract
        return token.balanceOf(address(this));
    }

    function distributeProfit() public {
        try distributor.deposit{value: profitBalance}() {} catch {}
    }

    function addProfit() public payable {
        require(msg.value > 0, "BLNFT: Invalid BNB amount.");  // Todo: Make sure price is correct for the round
        profitBalance = profitBalance.add(msg.value);
    }

    // Setters
    function transferMintFeeAddress(address _mintFeeAddr) public {
        require(_msgSender() == mintFeeAddr, 'FORBIDDEN');
        mintFeeAddr = _mintFeeAddr;
        // emit MintFeeAddressTransferred(_msgSender(), mintFeeAddr);
    }

    function setMintFeeAmount(uint256 _mintFeeAmount) public onlyOwner {
        require(mintFeeAmount != _mintFeeAmount, 'Not need update');
        // emit SetMintFeeAmount(_msgSender(), mintFeeAmount, _mintFeeAmount);
        mintFeeAmount = _mintFeeAmount;
    }

    // Override functions
    function _beforeTokenTransfer(address from, address to, uint256 tokenId) internal override(ERC721, ERC721Enumerable) {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function _burn(uint256 tokenId) internal override(ERC721, ERC721URIStorage) {
        super._burn(tokenId);
    }

    function tokenURI(uint256 tokenId) public view override(ERC721, ERC721URIStorage) returns (string memory) {
        return super.tokenURI(tokenId);
    }

    function supportsInterface(bytes4 interfaceId) public view override(ERC721, ERC721Enumerable) returns (bool) {
        return super.supportsInterface(interfaceId);
    }
}

