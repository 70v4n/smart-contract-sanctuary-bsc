        uint256 end = page * 10;
        if (length > 0 && length > start) {
            start = length - start;
            end = length > end ? length - end : 0;
            uint256 j;
            for (uint256 i = start; i > end; i--) {
                uint256 drawnEpoch = userDrawnEpochs[user][i - 1];
                Round storage round = rounds[drawnEpoch];
                drawn[j] = round.drawn;
                _boxPrice[j] = round.boxPrice;
                endTime[j] = round.endTime;
                userBoxesLength[j] = round.userBoxesLength[msg.sender];
                userWonPlanetsLength[j] = round.userWonPlanetsLength[
                    msg.sender
                ];
                userRewardedPlanets[j] = round.userRewardedPlanets[msg.sender];
                j++;
            }
        }
    }
}