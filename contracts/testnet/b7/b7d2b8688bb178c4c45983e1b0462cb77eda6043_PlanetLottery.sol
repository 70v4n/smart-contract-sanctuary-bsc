        require(
            block.timestamp > round.endTime,
            "This Round has Not Ended yet"
        );
        if (round.drawn == true) revert("This Round has Already been Drawn");

        round.drawn = true;
        if (round.boxesLength > 0) {
            if (round.planetsLength > 0) {
                uint256 minLength = round.boxesLength < round.planetsLength
                    ? round.boxesLength
                    : round.planetsLength;
                uint256 boxesLength = round.boxesLength;
                address[] memory boxOwners = round.boxOwners;
                for (uint256 i = 0; i < minLength; i++) {
                    uint256 random = uint256(
                        keccak256(
                            abi.encodePacked(
                                msg.sender,
                                block.timestamp,
                                randomSeed,
                                currentEpoch,
                                round.planetsLength,
                                boxesLength,
                                round.planetIds[i],
                                i
                            )
                        )
                    ) % boxesLength;
                    address winner = boxOwners[random];
                    round.winners.push(winner);
                    round.userWonPlanetsLength[winner]++;
                    round.userRewardedPlanets[winner].push(round.planetIds[i]);
                    userPlanetWithdrawStatus[winner][round.planetIds[i]] = true;

                    boxOwners[random] = boxOwners[boxesLength - 1];
                    boxesLength--;
                }
                totalSupply += round.supply;
                totalPlanetsLength += round.planetsLength;
            }
            totalBoxesLength += round.boxesLength;
        } else if (round.planetsLength > 0) {
            totalSupply += round.supply;
            totalPlanetsLength += round.planetsLength;
        }

        emit CalcWinners(currentEpoch, round.winners);

        currentEpoch++;
        Round storage nextRound = rounds[currentEpoch];
        nextRound.boxPrice = boxPrice;
        nextRound.startTime = round.startTime + intervalTime;
        nextRound.endTime = nextRound.startTime + durationTime;
    }

    /**
