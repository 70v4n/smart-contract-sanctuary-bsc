    function buyBoxes(uint256 boxesLength) external {
        require(boxesLength > 0, "The Boxes Length must > 0");
        require(boxesLength <= 100, "The Boxes Length must <= 100");
        require(currentEpoch > 0, "This Activity has Not Started yet");
        Round storage round = rounds[currentEpoch];
        require(
            block.timestamp > round.startTime,
            "This Round has Not Started yet"
        );
        require(
            block.timestamp <= round.endTime,
            "This Round has Already Ended"
        );

        uint256 drawAmount = boxesLength * boxPrice;
        ticketToken.transferFrom(msg.sender, address(this), drawAmount);

        uint256 userDrawnEpochsLength = userDrawnEpochs[msg.sender].length;
        if (userDrawnEpochsLength > 0) {
            if (
                userDrawnEpochs[msg.sender][userDrawnEpochsLength - 1] <
                currentEpoch
            ) {
                userDrawnEpochs[msg.sender].push(currentEpoch);
            }
        } else {
            userDrawnEpochs[msg.sender].push(currentEpoch);
        }
        round.boxesLength += boxesLength;
        round.userBoxesLength[msg.sender] += boxesLength;
        for (uint256 i = 0; i < boxesLength; i++) {
            round.boxOwners.push(msg.sender);
        }

        emit BuyBoxes(currentEpoch, msg.sender, boxesLength);
    }

    /**
     * @dev Withdraw Planet
     */
    function withdrawPlanet(uint256 planetId) external {
        require(
            userPlanetWithdrawStatus[msg.sender][planetId],
            "This Planet is Not Your Reward"
        );

        userPlanetWithdrawStatus[msg.sender][planetId] = false;

        planetNFT.safeTransferFrom(address(this), msg.sender, planetId);

        emit WithdrawPlanet(msg.sender, planetId);
