        address tokenAddr,
        address to,
        uint256 amount
    ) external onlyRole(MANAGER_ROLE) {
        IERC20 token = IERC20(tokenAddr);
        token.transfer(to, amount);
    }

    /**
     * @dev Withdraw NFT
     */
    function withdrawNFT(
        address nftAddr,
        address to,
        uint256 amount
    ) external onlyRole(MANAGER_ROLE) {
        IERC721Enumerable nft = IERC721Enumerable(nftAddr);
        nft.safeTransferFrom(address(this), to, amount);
    }

    /**
     * @dev Set Box Price
     */
    function setBoxPrice(uint256 _boxPrice) external onlyRole(MANAGER_ROLE) {
        boxPrice = _boxPrice;
    }

    /**
     * @dev Set Duration Time
     */
    function setDurationTime(uint256 _durationTime)
        external
        onlyRole(MANAGER_ROLE)
    {
        durationTime = _durationTime;
    }

    /**
     * @dev Set Interval Time
     */
    function setIntervalTime(uint256 _intervalTime)
        external
        onlyRole(MANAGER_ROLE)
    {
        intervalTime = _intervalTime;
    }

    /**
     * @dev Start First Round
     */
    function startFirstRound(uint256 startTime)
        external
        onlyRole(MANAGER_ROLE)
    {
        currentEpoch++;
        Round storage round = rounds[currentEpoch];
        round.boxPrice = boxPrice;
        round.startTime = startTime;
        round.endTime = round.startTime + durationTime;
    }

    /**
     * @dev Set Round Info
     */
    function setRoundInfo(
        uint256 epoch,
        uint256 _boxPrice,
        uint256 startTime,
        uint256 endTime
    ) external onlyRole(MANAGER_ROLE) {
        Round storage round = rounds[epoch];
        round.boxPrice = _boxPrice;
        round.startTime = startTime;
        round.endTime = endTime;
    }

    /**
     * @dev Transfer Planets
     */
    function transferPlanets(uint256 epoch, uint256[] calldata planetIds)
        external
        onlyRole(MANAGER_ROLE)
    {
        require(planetIds.length > 0, "The Planet Ids Length must > 0");
        require(epoch > 0, "The Epoch must > 0");
        require(epoch >= currentEpoch, "This Round has Already Ended");
        Round storage round = rounds[epoch];
        if (epoch == currentEpoch && block.timestamp > round.endTime)
            revert("This Round has Already Ended");

        uint256 supply;
        for (uint256 i = 0; i < planetIds.length; i++) {
            planetNFT.safeTransferFrom(msg.sender, address(this), planetIds[i]);
            supply += planetNFT.getPlanetSupply(planetIds[i]);
            round.planetIds.push(planetIds[i]);
        }

        round.supply += supply;
        round.planetsLength += planetIds.length;

        emit TransferPlanets(epoch, supply, planetIds.length);
    }

    /**
     * @dev Calc Winners
     */
    function calcWinners(uint256 randomSeed) external onlyRole(MANAGER_ROLE) {
