pragma solidity ^0.8.9;
contract FYToken is FYTokenCover {
    using SafeMath for uint256;

    uint256 private rechargeGamePoolPercentage = 50;
    uint256 private rechargeCommunityPercentage = 40;
    uint256 private rechargeBurnPercentage = 10;

    constructor() ERC20("FYToken", "FYT") FYTokenCover() {}

    modifier validBalance(uint256 _amount) {
        require(balanceOf(msg.sender) >= _amount, "insufficient balance");
        _;
    }

    // function transferAccount(
    //     address from,
    //     address to,
    //     uint256 amount
    // ) external override onlyLandTokenAddress returns (bool) {
    //     require(validBalanceWithTax(from, amount, 0), "insufficient balance");
    //     _transfer(from, to, amount);
    //     return true;
    // }

    function transferToAddress(
        address from,
        uint256 amount,
        address playerAddress,
        uint256 commission
    ) external override onlyLandTokenAddress returns (bool) {
        require(balanceOf(from) >= amount, "insufficient balance");
        toAddress(from, amount, playerAddress, commission);
        return true;
    }

    function transferAccountToAddress(address from, uint256 amount)
        external
        override
        onlyLandTokenAddress
        returns (bool)
    {
        require(balanceOf(from) >= amount, "insufficient balance");
        toAddress(from, amount, gamePoolAddress, amount);
        return true;
    }

    function toAddress(
        address from,
        uint256 _amount,
        address to,
        uint256 commission
    ) internal {
        _transfer(from, to, _amount);
        if (to != gamePoolAddress) {
            _transfer(
                to,
                gamePoolAddress,
                commission.mul(rechargeGamePoolPercentage).div(100)
            );
        }
        _transfer(
            to,
            communityAddress,
            commission.mul(rechargeCommunityPercentage).div(100)
        );
        _transfer(
            to,
            burnAddress,
            commission.mul(rechargeBurnPercentage).div(100)
        );
    }

    function recharge(uint256 _amount)
        public
        minAmount(_amount)
        validBalance(_amount)
    {
        transfer(gamePoolAddress, _amount);
        emit Recharge(msg.sender, gamePoolAddress, _amount);
    }

    function drawCoin(
        uint256 toUserAmount,
        address userAddress,
        uint256 toSelfAmount
    ) public onlyGamePoolAddress validBalance(toUserAmount.sub(toSelfAmount)) {
        transfer(userAddress, toUserAmount);
        _transfer(
            userAddress,
            communityAddress,
            toSelfAmount.mul(rechargeCommunityPercentage).div(100)
        );
        _transfer(
            userAddress,
            burnAddress,
            toSelfAmount.mul(rechargeBurnPercentage).div(100)
        );
        emit DrawCoin(userAddress, toUserAmount, toSelfAmount);
    }
}