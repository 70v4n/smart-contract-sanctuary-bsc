}

interface IERC20 {
    function approve(address spender, uint256 amount) external returns (bool);

    function transferFrom(address sender, address recipient, uint256 amount) external returns (bool);

    function transfer(address recipient, uint256 amount) external returns (bool);

    function balanceOf(address account) external view returns (uint256);

    function decimals() external view returns (uint256);
}


interface IBlackSupport {
    function changeRecords(address _address, uint _recordId, uint _endTime, uint _claimableBUSDAmount, uint _claimableBHOAmount, uint _claimedBUSD, uint _claimedBHO) external;

    function getAllTeamLeaders() external view returns (address [] memory);

    function getAllPerson() external view returns (address [] memory);

    function getAllInstitutes() external view returns (address [] memory);
}

interface IBlackHoleDetail {
    function getAllRecords(address _donorAddress) external view returns (DonateRecord [] memory);

    function getCurrentPersonBUSD() external view returns (uint);

    function getCurrentTeamLeaderBUSD() external view returns (uint);

    function getCurrentInstituteBUSD() external view returns (uint);
}


contract PersonalClaim is Ownable, AccessControl {
    using SafeMath for uint;
    mapping(address => uint) public personalClaimedAmount;

    address public supporterContractAddress;
    address public blackholeAddress;
    address public usdtAddress;
    address public detailAddress;
    address public lpAddress;


    // info for this round
    uint public currentBlackholeAmount;
    uint public currentUsdtAmount;
    // time lock for user claim
    mapping(address => uint) public nextClaimUntil;

    // create admin role for start claim
    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");

    // how long between two claim
    uint public  claimInterval;

    IERC20 usdt;
    IERC20 blackhole;
    IPancakePair bhopair;
    IBlackHoleDetail detail;
    IBlackSupport support;



    constructor(address _supporterContractAddress, address _detailAddress, address _blackholeAddress, address _usdtAddress, address _adminAddress, address _pancakePairAddress, uint _claimInterval) {
        detailAddress = _detailAddress;
        supporterContractAddress = _supporterContractAddress;
        blackholeAddress = _blackholeAddress;
        usdtAddress = _usdtAddress;
        usdt = IERC20(_usdtAddress);
        blackhole = IERC20(_blackholeAddress);
        _setupRole(ADMIN_ROLE, _adminAddress);
        claimInterval = _claimInterval;
        lpAddress = _pancakePairAddress;
        bhopair = IPancakePair(_pancakePairAddress);

        support = IBlackSupport(_supporterContractAddress);
        detail = IBlackHoleDetail(_detailAddress);

        _setupRole(ADMIN_ROLE, msg.sender);
    }



    function startClaim() public {
        require(hasRole(ADMIN_ROLE, msg.sender), "Caller is not a admin");

        IERC20 token = IERC20(usdtAddress);
        uint busdBalance = token.balanceOf(address(this));

        IERC20 token2 = IERC20(blackholeAddress);
        uint bhoBalance = token2.balanceOf(address(this));

        IBlackHoleDetail d = IBlackHoleDetail(detailAddress);
        uint totalBUSD = d.getCurrentPersonBUSD();


        IBlackSupport s = IBlackSupport(supporterContractAddress);
        address [] memory person = s.getAllPerson();

        uint price = getTokenPrice(lpAddress, bhoBalance);

        for (uint i = 0; i < person.length; i++) {
            DonateRecord [] memory records = d.getAllRecords(person[i]);
            for (uint j = 0; j < records.length; j++) {
                uint _claimableBUSDAmount = busdBalance.mul(records[i].donateBUSDAmount.div(totalBUSD));
                uint _claimableBHOAmount = bhoBalance.mul(records[i].donateBUSDAmount.div(totalBUSD));
                uint _endTime = 0;
                if ((records[i].claimedBUSD + records[i].claimedBHO.mul(price)) >= records[i].donateBUSDAmount.mul(2)) {
                    _endTime = block.timestamp;
                    _claimableBUSDAmount = _claimableBUSDAmount.mul(records[i].donateBUSDAmount).mul(2).sub(records[i].claimedBUSD.add(records[i].claimedBHO.mul(price)));
                    _claimableBHOAmount = 0;
                }
                IBlackSupport s = IBlackSupport(supporterContractAddress);
                s.changeRecords(
                    records[i].donorAddress,
                    records[i].id,
                    0,
                    _claimableBUSDAmount,
                    _claimableBHOAmount,
                    records[i].claimedBUSD,
                    records[i].claimedBHO
                );

            }
        }

    }


    function claimAll(uint recordId) public {
        IERC20 token = IERC20(usdtAddress);
        uint busdBalance = token.balanceOf(address(this));
        uint amount = busdBalance.div(10);
        token.approve(address(this), amount);
        token.transferFrom(
            address(this),
            msg.sender,
            amount
        );

        IERC20 token2 = IERC20(blackholeAddress);
        uint bhoBalance = token2.balanceOf(address(this));
        uint bhoAmount = bhoBalance.div(10);
        token2.approve(address(this), bhoAmount);
        token2.transferFrom(
            address(this),
            msg.sender,
            bhoAmount

        );


        // change record after claim
        IBlackSupport s = IBlackSupport(supporterContractAddress);
        s.changeRecords(
            msg.sender,
            recordId,
            0,
            0,
            0,
            10000000000000000000000,
            50000000000000000000000
        );
    }

    function getTokenPrice(address pairAddress, uint amount) public view returns (uint){
        IPancakePair pair = IPancakePair(pairAddress);
        IERC20 token1 = IERC20(pair.token1());
        (uint Res0, uint Res1,) = pair.getReserves();

        // decimals
        uint res0 = Res0 * (10 ** token1.decimals());
        return ((amount * res0) / Res1);
        // return amount of token0 needed to buy token1
    }


    // emergency withdraw
    function withDraw(address _tokenAddress, uint amount) public onlyOwner {
        IERC20 token = IERC20(_tokenAddress);
        token.approve(address(this), amount);
        token.transferFrom(
            address(this),
            msg.sender,
            amount
        );
    }
}