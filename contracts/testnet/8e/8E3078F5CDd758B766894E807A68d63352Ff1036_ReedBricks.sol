        swapAndLiquifyEnabled = _enabled;
        emit SwapAndLiquifyEnabledUpdated(_enabled);
    }

    function setBuyBackEnabled(bool _enabled) public onlyOwner {
        buyBackEnabled = _enabled;
        emit BuyBackEnabledUpdated(_enabled);
    }

    function transferToAddressBNB(address payable recipient, uint256 amount)
        private
    {
        recipient.transfer(amount);
    }

    function removeStuckToken(address _token) external onlyOwner {
        IBEP20(_token).transfer(
            owner(),
            IBEP20(_token).balanceOf(address(this))
        );
    }

    function freeze(
        address _account,
        uint256 _startsDays,
        uint256 _endsDays,
        uint256 _percentage,
        uint256 _intervalDays
    ) external onlyAdmin {
        _freeze(_account, _startsDays, _endsDays, _percentage, _intervalDays);
    }

    function unfreez(address _account) external onlyAdmin {
        _unfreeze(_account);
    }

    function getFreeFund(address _account) private view returns (uint256) {
        Frozen memory frozen = _frozen[_account];
        uint256 _now = block.timestamp;
        uint256 balance = _balanceOf(_account);
        //Check if user if frozen
        if (onFreezer(_account) == false) {
            return balance;
        }
        // Check if freez still Enabled for the account //
        if (frozen.status == false) {
            return balance;
        }
        // Check freez is ended
        if (frozen.endsInDays >= _now) {
            return balance;
        }
        // Get the time that has passed (start - end = time_diff)//
        uint256 _timePassed = _now.sub(frozen.startsInDays);

        // Get the number of interval in the spread (_amountSpread) //
        uint256 _intervalPassed = _timePassed.div(frozen.intervalInDays);

        // Get the amount per Interval (_intervalPassed) in the Spread
        uint256 _percentageAmountPerInterval = balance
            .mul(frozen.percentage)
            .div(10**2);

        // Get the total freeable funds from _percentageAmountPerInterval and _intervalPassed//
        uint256 _totalBurst = _percentageAmountPerInterval.mul(_intervalPassed);

        if (_totalBurst > balance) {
            return balance;
        }

        return _totalBurst;
    }

    function getCirculatingSupply() public view returns (uint256) {
        return
            _tTotal
                .sub(
                    balanceOf(
                        address(0x000000000000000000000000000000000000dEaD)
                    )
                )
                .sub(
                    balanceOf(
                        address(0x0000000000000000000000000000000000000000)
                    )
                );
    }

    function getLiquidityBacking(uint256 accuracy)
        public
        view
        returns (uint256)
    {
        return
            accuracy.mul(balanceOf(uniswapV2Pair).mul(2)).div(
                getCirculatingSupply()
            );
    }

    function isOverLiquified(uint256 target, uint256 accuracy)
        public
        view
        returns (bool)
    {
        return getLiquidityBacking(accuracy) > target;
    }

    function buyTokens(uint256 tokenAmount, address to) external lockTheSwap {
        require(tokenAmount > 0, "REED: Amount is less than 0");
        // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = uniswapV2Router.WETH();
        _approve(address(this), address(uniswapV2Router), tokenAmount);
        // make the swap
        uniswapV2Router.swapExactTokensForBNBSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            to, // The contract
            block.timestamp
        );
        emit SwapTokensForBNB(tokenAmount, path);
    }

    //to recieve ETH from uniswapV2Router when swaping
    receive() external payable {}
}

