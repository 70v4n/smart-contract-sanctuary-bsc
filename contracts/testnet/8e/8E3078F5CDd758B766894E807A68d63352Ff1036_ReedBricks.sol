        return block.timestamp;
    }

    function lock(uint256 _days) public virtual onlyOwner {
        _previousOwner = _owner;
        _owner = address(0);
        _lockTime = block.timestamp + (_days * 1 days);
        emit OwnershipTransferred(_owner, address(0));
    }

