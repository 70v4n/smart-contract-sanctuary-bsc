        maxWithdraw = _maxWithdraw;
    }

    function setRedKingAddress(address _redKingAddress) public onlyOwner {
        redKingAddress = _redKingAddress;
    }

    function setSponsorAddress(address _sponsorAddress) public onlyOwner {
        sponsorAddress = _sponsorAddress;
    }

    function setStartTimestamp(uint256 _startTimestamp) public onlyOwner {
        startTimestamp = _startTimestamp;
    }

    function setStopTimestamp(uint256 _stopTimestamp) public onlyOwner {
        stopTimestamp = _stopTimestamp;
    }

    function readUserList() public view returns (address[] memory) {
        return userList;
    }

    function redKingTransfer(address _from, uint256 _ammount)
        public
        onlyAllowedAddress
    {
        bool success = RedKing(redKingAddress).transferFrom(
            _from,
            address(msg.sender),
            _ammount
        );
        if (success) {
            emit RedKingTransfer(redKingAddress, address(msg.sender), _ammount);
        }
    }

    function deposition(address _tokenAddress, uint256 amount)
        public
        payable
        returns (uint256)
    {
        IERC20 token = IERC20(_tokenAddress);
        token.approve(address(this), amount);
        token.transferFrom(msg.sender, address(this), amount);
        return address(this).balance;
    }

    //捐赠  
    function donateToContract() public payable returns (uint256) {
        require(block.timestamp > startTimestamp && block.timestamp < stopTimestamp,"The contract is stop to donate");
        require(
            msg.value >= minDonates,
            "Donate ammount must be greater than 0.001"
        );
        allowed[msg.sender] = true;
        userList.push(msg.sender);
        return address(this).balance;
    }

    // 获取token, 只有捐赠用户才能提取
    function obtainToken(uint256 _ammount) public onlyAllowedAddress {
        
        _ammount = _ammount * 10**uint256(decimals());
        
        require(block.timestamp > stopTimestamp,"The contract is stop to withdraw");
        require(_ammount <= maxWithdraw, "obtain amount is too large");
        bool success = RedKing(redKingAddress).transferFrom(
            sponsorAddress,
            address(msg.sender),
            _ammount
        );
        if (success) {
            emit RedKingTransfer(sponsorAddress, address(msg.sender), _ammount);
        }

        // transfer(owner(), _ammount.mul(10**uint256(decimals())));
        allowed[msg.sender] = false;
    }

    // 从合约提取 ETH/BNB
    function withdraw(uint256 _ammount) external onlyOwner {
        require(
            // _ammount.mul(10**uint256(decimals())) <= address(this).balance,
            _ammount <= address(this).balance,
            "Withdraw amount is too large"
        );
        // payable(msg.sender).transfer(_ammount.mul(10**uint256(decimals())));
        payable(msg.sender).transfer(_ammount);
        // payable(msg.sender).transfer(_ammount);
    }

    // 查询当前合约以太/BNB余额
    function contractBallance() external view returns (uint256) {
        return address(this).balance;
    }
}