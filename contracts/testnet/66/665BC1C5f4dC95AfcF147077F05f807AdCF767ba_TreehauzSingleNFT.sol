pragma solidity ^0.8.10;

import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "./MainMarketplace.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/interfaces/IERC2981Upgradeable.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721URIStorageUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721BurnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";

import "hardhat/console.sol";

contract TreehauzSingleNFT is 
    Initializable, 
    OwnableUpgradeable,
    IERC2981Upgradeable,
    ERC721Upgradeable,
    ERC721EnumerableUpgradeable,
    ERC721URIStorageUpgradeable,
    ERC721BurnableUpgradeable
    {

    struct SingleItem {
        address tokenMinter; //primary owner 20b
        uint16 totalRoyalty; //map address with royalty 2b
        uint16[] royaltyPercentage; // 2b
        address[] royaltyReceiver;
        string tokenIPFSURI; //IPFS CID
    }

    struct ListingParameters {
        address assetContract;
        uint64 quantityToList;
        uint256 tokenId;
        uint256 buyoutPricePerToken;
    }

    struct royaltyInfos {
        // sequence => address
        mapping (uint16 => address) royaltyReceiver;
        // sequence => percentage 10000 = 100%
        mapping (uint16 => uint16) royaltyPercentage;
        // uint16 noOfRoyaltyReceiver;
    }

    struct AccountRoyaltyInfo {
        // tokenId
        uint256[] tokenId;
        // percentage 10000 = 100%
    }

    /**
    * @dev Declare contract admin address
    */
    address private _admin;

    /**
    * @dev Base token uri prefix.
    */
    string private _baseTokenURI;

        /**
    * @dev Declare marketplace contract 
    */
    MainMarketplace private _marketplaceContract;

        /**
    * @dev TokenID counter
    */
    using Counters for Counters.Counter;
    Counters.Counter private _singleTokenIds;

    /// @dev tokenId => single token info
    mapping(uint256 => SingleItem) private _tokenItems;

    /// @dev address => tokenId token info
    mapping(address => AccountRoyaltyInfo) private _accountRoyalty;

    /// @dev address => tokenId => approval info
    // approval for update royalty
    mapping(address => mapping(uint256 => bool)) private _tokenOwnerApproval;

    /// @dev tokenId => single token royalty info
    // mapping(uint256 => royaltyInfos) private _tokenRoyalty;

    /// @dev tokenId => single token uri info
    // mapping(uint256 => string) private _tokenURIs;

    /**
    * @dev This empty reserved space is put in place to allow future versions to add new
    * variables without shifting down storage in the inheritance chain.
    * See https://docs.openzeppelin.com/contracts/4.x/upgradeable#storage_gaps
    */
    uint256[50] private __gap;


    /// @dev Checks whether caller is admin or owner.
    modifier onlyAdminOrOwner() {
        require(_msgSender() == _admin || _msgSender() == owner(), "Treehauz: Caller is not admin or owner.");
        _;
    }

    

    /// @dev Checks whether caller is token primary owner.
    modifier onlyTokenPrimaryOwner(uint256 _tokenId) {
        require(_msgSender() == _tokenItems[_tokenId].tokenMinter, "Treehauz: Caller is not token minter or primary token owner.");
        _;
    }

    /**
    * @dev events
    */
    event SingleTokenMinted(address indexed sender, uint256 indexed tokenId, SingleItem item);
    event SetApproveMarket(address indexed owner, address indexed to, uint256 price);
    event RoyaltyInfoUpdated(uint256 tokenId, address[] sender, uint16[] royaltyPercentage);
    event SingleTokenTransferred(address indexed sender, address indexed to, address _assetContract, uint256 tokenId);

    // initialize contract
    function initialize (
        string memory name,
        string memory symbol,
        string memory baseURI
    ) 
    initializer public payable {
        __ERC721_init(name, symbol);
        __ERC721Enumerable_init();
        __ERC721URIStorage_init();
        __ERC721Burnable_init();
        __Ownable_init();

        _baseTokenURI = baseURI;
        _singleTokenIds.reset();
    }

    /**
    * @dev Approve `operator` to operate on all of `owner` tokens
    *
    * Emits a {SetApproveMarket} event.
    */
    function approveMarket(ListingParameters memory _params) external 
    returns (bool) {
        require(_exists(_params.tokenId), "Treehauz: nonexistent token");
        address ownerTest = IERC721Upgradeable(_params.assetContract).ownerOf(_params.tokenId);
        address msgSenderTest = _msgSender();
        console.log("_msgSender()=", msgSenderTest);
        console.log("owner before listing=", ownerTest);
        console.log("_assetContract=", _params.assetContract);
        console.log("address(_marketplaceContract)=", address(_marketplaceContract));
        setApprovalForAll(address(_marketplaceContract), true);
        approve(address(_marketplaceContract), _params.tokenId);        
        _marketplaceContract.createListing(_params.assetContract,
                                           _msgSender(),
                                           1,
                                           _params.tokenId,
                                           _params.buyoutPricePerToken
                                           );
                                           
        bool approved = isApprovedForAll(_msgSender(),address(_marketplaceContract));
        address getApprovedd = getApproved(_params.tokenId);
        console.log("approved=", approved);
        console.log("getApprovedd=", getApprovedd);
        ownerTest = IERC721Upgradeable(_params.assetContract).ownerOf(_params.tokenId);
        console.log("owner after listing=", ownerTest);

        // emit SetApproveMarket(_msgSender(), address(_marketplaceContract), _params.tokenId);
        return true;
    }

    // Safe mint ERC721, assign royaltyInfo
    function singleMint(uint16 _totalRoyalty,
                        uint16[] memory _royaltyPercentage,
                        address[] memory _royaltyReceiver,
                        string memory _tokenURI
                        )
    external {
        require(_royaltyReceiver[0] != address(0), "Treehauz : Royalty receiver does not exists");
        // check FE require(_royaltyPercentage[0] >= 0, "Treehauz : Royalty percentage less than 0 is not valid");
        require(_totalRoyalty < 9000, "Treehauz : Total royalty must less than 90%");

        uint256 newItemId = _singleTokenIds.current();
        // _tokenURIs[newItemId] = _tokenURI;
        
        // SingleItem memory newSingleItem =  SingleItem ({
        _tokenItems[newItemId] = SingleItem ({
            tokenMinter : _msgSender(), // token minter or primary owner
            totalRoyalty : _totalRoyalty, // total royalty combined
            royaltyPercentage : _royaltyPercentage, // list of royalty percentage
            royaltyReceiver : _royaltyReceiver, // list of royalty receiver
            tokenIPFSURI : _tokenURI // IPFS content identifier
        });        

        for (uint16 i=0; i < _royaltyReceiver.length ; i++) {
            _accountRoyalty[_royaltyReceiver[i]].tokenId.push(newItemId);
        }

        // if (_royaltyReceiver.length > 1) {
        //     bool _royalty = setRoyaltyInfoMint(newItemId, _royaltyReceiver, _royaltyPercentage, _totalRoyalty);
        //     require(_royalty, "Treehauz: Royalty info not valid");
        // }
        // else {
        //     _tokenRoyalty[newItemId].royaltyReceiver[0] = _royaltyReceiver[0];
        //     _tokenRoyalty[newItemId].royaltyPercentage[0] = _royaltyPercentage[0];
        // }
        // _tokenItems[newItemId] = newSingleItem;
        //count of total royalty receiver/percentage for a token    
        // _tokenRoyalty[newItemId].noOfRoyaltyReceiver = _royaltyReceiver.length;

        _safeMint(_msgSender(), newItemId);
        _setTokenURI(newItemId, tokenURI(newItemId));

        _singleTokenIds.increment();
    }

    /**
     * @dev See {IERC721Metadata-tokenURI}.
     */
    function tokenURI(uint256 tokenId) public view virtual 
    override (ERC721Upgradeable, ERC721URIStorageUpgradeable) 
    returns (string memory) {
        require(_exists(tokenId), "ERC721URIStorage: URI query for nonexistent token");

        // string memory _tokenURI = _tokenURIs[tokenId];
        string memory _tokenURI = _tokenItems[tokenId].tokenIPFSURI;
        string memory base = _baseURI();

        // If there is no base URI, return the token URI.
        if (bytes(base).length == 0) {
            return _tokenURI;
        }
        // If both are set, concatenate the baseURI and tokenURI (via abi.encodePacked).
        if (bytes(_tokenURI).length > 0) {
            return string(abi.encodePacked(base, _tokenURI));
        }
        return super.tokenURI(tokenId);
    }

    /// @param _tokenId - get token information
    function getTokenInfo(uint256 _tokenId) 
    public view returns (
        SingleItem memory getSingleItem) {
            getSingleItem = _tokenItems[_tokenId];
            return getSingleItem;
    }

    /// @param _tokenId - get info primary/secondary market
    function getAccountRoyaltyByAddress(address _address) 
    public view returns (uint256[] memory _tokenId, 
                        uint16[] memory _percentage) {    

        for (uint256 i=0; i < _accountRoyalty[_address].tokenId.length; i++) {
            _percentage[i] = _tokenItems[_accountRoyalty[_address].tokenId[i]].royaltyPercentage[i];
        }

        return (_accountRoyalty[_address].tokenId, _percentage);
    }
    

    /// @param _tokenId - get info primary/secondary market
    function getTokenMinter(uint256 _tokenId) 
    public view returns (address) {
        return _tokenItems[_tokenId].tokenMinter;
    }

    /// @param _tokenId - get total royalty percentage
    function getTotalRoyaltyPercentage(uint256 _tokenId) 
    public view returns (uint16) {
        return _tokenItems[_tokenId].totalRoyalty;
    }

    /// @notice Called with the sale price to determine how much royalty
    //          is owed and to whom.
    /// @param _tokenId - the NFT asset queried for royalty information
    /// @param _salePrice - the sale price of the NFT asset specified by _tokenId
    /// @return receiver - address of who should be sent the royalty payment
    /// @return royaltyAmount - the royalty payment amount for _salePrice
    function royaltyInfo(uint256 _tokenId, uint256 _salePrice) 
    external view returns (
        address receiver,
        uint256 royaltyAmount) {
        require(_exists(_tokenId), "Treehauz: nonexistent token");
        require(_salePrice > 0, "Treehauz : Sale price must be more than 0");
        // receiver = _tokenRoyalty[_tokenId].royaltyReceiver[0]; // set return only for primary receiver
        receiver = _tokenItems[_tokenId].royaltyReceiver[0]; // set return only for primary receiver
        royaltyAmount = (_salePrice / 10000) * _tokenItems[_tokenId].totalRoyalty; // set return for total royalty
    }

    /// @param _tokenId - the NFT asset queried for royalty information
    function getRoyaltyInfo(uint256 _tokenId) 
    external view returns (
        address[] memory, uint256[] memory, uint256) {
        require(_exists(_tokenId), "Treehauz: nonexistent token");
        address[] memory __receiver = new address[](_tokenItems[_tokenId].royaltyReceiver.length);
        uint256[] memory __percentage = new uint256[](_tokenItems[_tokenId].royaltyReceiver.length);
        uint64 count;

        for (uint64 i=0; i < _tokenItems[_tokenId].royaltyReceiver.length; i++ ) {
            __receiver[i] = address(_tokenItems[_tokenId].royaltyReceiver[i]);
            __percentage[i] = _tokenItems[_tokenId].royaltyPercentage[i];
            count++;
        }
        
        return (__receiver, 
                __percentage, 
                count);
    }

    /**
    * @dev Set royalty info.
    * Permission: Only contract owner.
    * @param _tokenId NFT asset.
    * @param _address Royalty receiver.
    * @param _royaltyPercentage Royalty percentage of each receiver.
    * @param _totalRoyalty Combined total royalty.
    */
    function setRoyaltyInfo(uint64 _tokenId, 
                            address[] memory _address, 
                            uint16[] memory _royaltyPercentage, 
                            uint16 _totalRoyalty) 
    external onlyTokenPrimaryOwner(_tokenId) {
        require(_exists(_tokenId), "Treehauz: nonexistent token");
        require(_totalRoyalty < 90000, "Treehauz: Total royalty must less than 90%");
        require(_address[0] != address(0), "Treehauz: invalid primary royalty receiver address");
        require(_royaltyPercentage[0] >= 0, "Treehauz: Royalty percentage less than 0 is not valid");
        console.log("debug1");
        // delete existing addresses that mapped with the token Id
        for (uint256 i=0; i < _tokenItems[_tokenId].royaltyReceiver.length; i++) {
            if (_accountRoyalty[_tokenItems[_tokenId].royaltyReceiver[i]].tokenId[i] == _tokenId) {
                delete _accountRoyalty[_tokenItems[_tokenId].royaltyReceiver[i]].tokenId[i];
            }
        }
        delete _tokenItems[_tokenId].royaltyReceiver;
        delete _tokenItems[_tokenId].royaltyPercentage;

        _tokenItems[_tokenId].totalRoyalty = 0;
        for (uint j=0; j < _address.length; j++) {
            if (_address[j] != address(0)) {
                _tokenItems[_tokenId].royaltyReceiver.push(_address[j]);
                if (_royaltyPercentage[j] > 0) {
                    _tokenItems[_tokenId].royaltyPercentage.push(_royaltyPercentage[j]);
                    _tokenItems[_tokenId].totalRoyalty += _royaltyPercentage[j];
                    _accountRoyalty[_address[j]].tokenId.push(_tokenId);
                }
            //     else {
            //         _tokenItems[_tokenId].royaltyPercentage.push(0);
            //     }
            // }
            // else {
            //     _tokenItems[_tokenId].royaltyReceiver.push(address(0));
            //     _tokenItems[_tokenId].royaltyPercentage.push(0);
            }
        }
        _tokenOwnerApproval[_tokenItems[_tokenId].tokenMinter][_tokenId] = false;
        emit RoyaltyInfoUpdated(_tokenId, _address, _royaltyPercentage);
    }

    /**
    * @dev Set new token primary owner for update royalty purpose.
    * Permission: Only admin or owner.
    * @param _tokenMinter token minter or primary owner.
    * @param _tokenId token Id.
    */
    function setPrimaryTokenOwner(address _tokenMinter, uint256 _tokenId) external onlyAdminOrOwner {
        _tokenItems[_tokenId].tokenMinter = _tokenMinter;
        _tokenOwnerApproval[_tokenMinter][_tokenId] = true;
    }

    /**
    * @dev Set marketplace contract.
    * Permission: Only owner.
    * @param _marketplaceAddress MainMarketplace contract address.
    */
    function setMarketplaceContract(address payable _marketplaceAddress) external onlyOwner {
        _marketplaceContract = MainMarketplace(_marketplaceAddress);
    }

    /**
    * @dev Set admin address.
    * Permission: Only owner.
    * @param _adminAddress admin address.
    */
    function setAdmin(address _adminAddress) external onlyOwner {
        _admin = _adminAddress;
    }

    function _beforeTokenTransfer(address from, address to, uint256 tokenId) 
    internal override (ERC721Upgradeable, ERC721EnumerableUpgradeable) {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    /**
    * @dev Set token IPFS base URI.
    * Permission: Only owner.
    * @param _base IPFS base URI.
    */
    function _setBaseURI(string memory _base) external onlyOwner {
        _baseTokenURI = _base;
    }

    /**
    * @dev Reset token IPFS base URI.
    * Permission: Only owner.
    */
    function _resetBaseURI() external onlyOwner {
        _baseTokenURI = "";
    }

    /**
    * @dev Get base URI.
    */
    function _baseURI() internal view virtual override returns (string memory) {
        return _baseTokenURI;
    }

    /**
    * @dev Destroys `tokenId`.
    * The approval is cleared when the token is burned.
    *
    * Requirements:
    *
    * - `tokenId` must exist.
    *
    * Emits a {Transfer} event.
    */
    function _burn(uint256 _tokenId) 
    internal virtual override (ERC721Upgradeable, ERC721URIStorageUpgradeable) {   
        super._burn(_tokenId);
    }

    function burnSingleToken(uint256 _tokenId) public {
        require(_exists(_tokenId), "Treehauz: nonexistent token");
        // require(_tokenItems[_tokenId].assetContract == address(this), "Treehauz: nonexistent token");
        require(_msgSender() == _tokenItems[_tokenId].tokenMinter, "Treehauz : Must be primary minter");
        require(_msgSender() == ownerOf(_tokenId), "Treehauz : Must be token owner");
        
        // delete listing on marketplace
        _marketplaceContract.burnToken(_tokenId);
        _burn(_tokenId);
        
        if (bytes(_tokenItems[_tokenId].tokenIPFSURI).length != 0) {
            // delete _tokenURIs[_tokenId];
            delete _tokenItems[_tokenId];
            // delete _tokenRoyalty[_tokenId];
        }
    }

    /**
    * @dev See {IERC165-supportsInterface}.
    */
    function supportsInterface(bytes4 interfaceId) public view virtual 
    override(IERC165Upgradeable, ERC721Upgradeable, ERC721EnumerableUpgradeable) 
    returns (bool) {
        bytes4 _INTERFACE_ID_ERC2981 = 0x2a55205a;
        return
        // interfaceId == type(IERC721Upgradeable).interfaceId ||
        // interfaceId == type(IERC721MetadataUpgradeable).interfaceId ||
        // interfaceId == type(IERC721EnumerableUpgradeable).interfaceId ||
        // interfaceId == type(IERC165Upgradeable).interfaceId ||
        // interfaceId == _INTERFACE_ID_ERC2981 ||
        interfaceId == type(IERC2981Upgradeable).interfaceId ||
        super.supportsInterface(interfaceId);
    }

    // transfer single item
    function singleTransfer(address _assetContract,
                            address _recepient,
                            uint256 _tokenId) 
    external {
        require(_msgSender() != address(0), "Treehauz: Address not valid");
        IERC721(_assetContract).setApprovalForAll(address(_marketplaceContract), true);
        IERC721(_assetContract).approve(address(_marketplaceContract), _tokenId);
        _marketplaceContract.transferTokensUtil(_msgSender(), _recepient, _assetContract, _tokenId, 1);

        // IERC721(_assetContract).safeTransferFrom(_msgSender(), _recepient, _tokenId, "");

        emit SingleTokenTransferred(_msgSender(), _recepient, _assetContract, _tokenId);
    }

}

