    // pointer aliasing, and it cannot be disabled.

    // The values being non-zero value makes deployment a bit more expensive,
    // but in exchange the refund on every call to nonReentrant will be lower in
    // amount. Since refunds are capped to a percentage of the total
    // transaction's gas, it is best to keep them low in cases like this one, to
    // increase the likelihood of the full refund coming into effect.
    uint256 private constant _NOT_ENTERED = 1;
    uint256 private constant _ENTERED = 2;

    uint256 private _status;

    constructor () internal {
        _status = _NOT_ENTERED;
    }

    /**
     * @dev Prevents a contract from calling itself, directly or indirectly.
     * Calling a `nonReentrant` function from another `nonReentrant`
     * function is not supported. It is possible to prevent this from happening
     * by making the `nonReentrant` function external, and make it call a
     * `private` function that does the actual work.
     */
    modifier nonReentrant() {
        // On the first call to nonReentrant, _notEntered will be true
        require(_status != _ENTERED, "ReentrancyGuard: reentrant call");

        // Any calls to nonReentrant after this point will fail
        _status = _ENTERED;

        _;

        // By storing the original value once again, a refund is triggered (see
        // https://eips.ethereum.org/EIPS/eip-2200)
        _status = _NOT_ENTERED;
    }
}

// File: contracts\MasterChef.sol

// https://RewardToken.com


interface IStdReference {
    /// A structure returned whenever someone requests for standard reference data.
    struct ReferenceData {
        uint256 rate; // base/quote exchange rate, multiplied by 1e18.
        uint256 lastUpdatedBase; // UNIX epoch of the last time when base price gets updated.
        uint256 lastUpdatedQuote; // UNIX epoch of the last time when quote price gets updated.
    }

    /// Returns the price data for the given base/quote pair. Revert if not available.
    function getReferenceData(string memory _base, string memory _quote)
        external
        view
        returns (ReferenceData memory);

    /// Similar to getReferenceData, but with multiple base/quote pairs at once.
    function getReferenceDataBulk(string[] memory _bases, string[] memory _quotes)
        external
        view
        returns (ReferenceData[] memory);
}


// MasterChef is the master of Token. He can make Token and he is a fair guy.
//
// Note that it's ownable and the owner wields tremendous power. The ownership
// will be transferred to a governance smart contract once RewardToken is sufficiently
// distributed and the community can show to govern itself.
//
// Have fun reading it. Hopefully it's bug-free. God bless.
contract CLPStaking is Ownable, ReentrancyGuard {
    using SafeMath for uint256;
    using SafeBEP20 for IBEP20;
    IStdReference ref = IStdReference(0xDA7a001b254CD22e46d3eAB04d937489c93174C3);

    // Info of each user.
    struct UserInfo {
        uint256 amount;         // How many LP tokens the user has provided.
        uint256 rewardDebt;     // Reward debt. See explanation below.
        uint256 rewardLockedUp;  // Reward locked up.
        uint256 lastDepositTime; // when user deposited
        uint256 lastClaimTime;   // when user claim reward
    }

    // Info of each pool.
    struct LpPoolInfo {
        IPancakePair lpToken;             // Address of LP token contract.
        string baseTokenSymbol;
        string quoteTokenSymbol;
        uint256 lastRewardBlock;    // Last block number that RewardToken distribution occurs.
        uint256 balance;            // pool token balance, allow multiple pools with same token
    }

    address public devAddress;
    uint256 public constant BONUS_MULTIPLIER = 1;

    uint256 public startBlock;
    uint256 public endBlock;
    // Total locked up rewards
    uint256 public totalLockedUpRewards;

    event Deposit(address indexed user, address lpToken, uint256 amount);
    event Withdraw(address indexed user, address indexed lpToken, uint256 amount);
    event RewardLockedUp(address indexed user, uint256 indexed pid, uint256 amountLockedUp);


    IBEP20 public rewardToken;
    uint256 public rewardAmountPerMonth = 500 * 1000 * 10 ** 18;   // example..
    mapping(address => LpPoolInfo) public lpPoolInfo;
    mapping(address => mapping(address => UserInfo)) public userInfo;
    address public rank1Lp;
    uint256 public rank1TVL;
    uint256 public rank1Balance;
    uint256 public lockDay = 3 days;

    uint256 round = 1;
    uint256 public snapShot;
    address[] public allLpTokens;
    constructor (
        IBEP20 _rewardToken,
        uint256 _startBlock
    ) public {
        rewardToken = _rewardToken;
        startBlock = _startBlock;
        rewardToken.balanceOf( address(this) );
        devAddress = msg.sender;
    }

    function getPrice2() external view returns (uint256){
        IStdReference.ReferenceData memory data = ref.getReferenceData("BTC","USD");
        return data.rate;
    }

    function getPrice(string memory _baseTokenSymbol, string memory quoteTokenSymbol) public view returns (uint256){
        IStdReference.ReferenceData memory data = ref.getReferenceData(_baseTokenSymbol,quoteTokenSymbol);
        return data.rate;
    }
    
    function deposit(address lpToken, uint256 _amount, string memory _baseTokenSymbol, string memory _quoteTokenSymbol) external nonReentrant {

        require(rewardToken.balanceOf(address(msg.sender)) >= 100 , 'CRANK token balance is too lower');
        require(keccak256(abi.encodePacked('DXTC3')) == keccak256(abi.encodePacked(_quoteTokenSymbol)) , 'quote token symbol incorrect ');

        address token0 = address(IPancakePair(lpToken).token0());
        address token1 = address(IPancakePair(lpToken).token1());
        require(token0 != address(0x0) && token1 != address(0x0) , 'LP token is not correct');
        require(keccak256(abi.encodePacked(IBEP20(token0).symbol())) == keccak256(abi.encodePacked(_quoteTokenSymbol)) ||
            keccak256(abi.encodePacked(IBEP20(token1).symbol())) == keccak256(abi.encodePacked(_quoteTokenSymbol)) , 'quote token symbol no match ');

        LpPoolInfo storage pool = lpPoolInfo[lpToken];
        LpPoolInfo memory poolRank1 = lpPoolInfo[rank1Lp];
        UserInfo storage user = userInfo[lpToken][_msgSender()];
        IBEP20(lpToken).transferFrom(_msgSender(), address(this), _amount);
        if (lpToken == rank1Lp) {
            claimReward();
        }

        if (pool.balance == 0) {
            pool.baseTokenSymbol = _baseTokenSymbol;
            pool.quoteTokenSymbol = _quoteTokenSymbol;
        }
        pool.balance = pool.balance + _amount;
        uint256 tvl = pool.balance * getPrice(_baseTokenSymbol, _quoteTokenSymbol);
        
        if (tvl > rank1TVL && block.timestamp < snapShot) {
            rank1TVL = tvl;
            rank1Lp = lpToken;
            rank1Balance = pool.balance;
        }

        user.amount = user.amount + _amount;
        user.lastDepositTime = block.timestamp;
    }

    function claimReward() public {
        require(block.timestamp > snapShot, "This contest did not finished!");
        LpPoolInfo memory pool = lpPoolInfo[rank1Lp];
        UserInfo memory user = userInfo[rank1Lp][_msgSender()];

        require(user.amount > 0, "Sorry, your pool did not win in this contest");
        require(user.lastDepositTime < snapShot, "You deposited after this contest");
        require(user.lastClaimTime < snapShot, "You have already claimed reward");
        
        uint256 rewards = rewardAmountPerMonth.mul(user.amount).div(pool.balance);
        if (rewards > 0) {
            rewardToken.transfer(_msgSender(), rewards);
            user.lastClaimTime = block.timestamp;
        }
    }

    // Withdraw LP tokens from MasterChef.
    function withdraw(address _lpToken, uint256 _amount) external nonReentrant {
        LpPoolInfo storage pool = lpPoolInfo[_lpToken];
        UserInfo storage user = userInfo[_lpToken][msg.sender];
        require(user.amount >= _amount, "withdraw: not good");
        require(user.lastClaimTime + lockDay < block.timestamp, "You could not withdraw in 30 days from last reward claim time");

        // updatePool(_pid);
        // payOrLockupPendingToken(_pid);
        if (_amount > 0) {
            user.amount = user.amount.sub(_amount);
            pool.balance = pool.balance.sub(_amount);
            IBEP20(_lpToken).transfer(address(msg.sender), _amount);
            user.lastDepositTime = block.timestamp;
        }

        // user.rewardDebt = user.amount.mul(pool.accTokenPerShare).div(1e12);
        emit Withdraw(msg.sender, _lpToken, _amount);
    }

    function random(uint256 seed) public view returns (uint256) {
        return uint256(keccak256(abi.encodePacked(
            tx.origin,
            blockhash(block.number - 1),
            block.timestamp,
            seed
        )));
    }

    // 1 day = 86,400s, 15 days = 1,296,000s, 30 days = 2,592,000s, 45 days = 3,888,000
    uint256 private constant DATE_INTERVAL = 3 days; //current 3day
    function selectNextDate(uint256 seed) internal view returns (uint256 date) {
        uint256 res = random(seed);
        date = res % DATE_INTERVAL;
        if (date > DATE_INTERVAL / 2) {
            return date;
        }

        return selectNextDate(random(date));
    }

    function setSnapShot() external onlyOwner {
        require(block.timestamp > snapShot);
        if (snapShot == 0) {
            snapShot = block.timestamp;
        } else {
            snapShot = snapShot + selectNextDate(block.number);
        }
    }

    function getTreasury() external view returns (uint256){
        return rewardToken.balanceOf(address(this));
    }

    function setLockDay(uint256 _seconds) external onlyOwner {
        lockDay = _seconds;
    }

    function getCurrentTime() external view returns (uint256) {
        return block.timestamp;
    }

    function getWinner() external view returns (address) {
        require(block.timestamp > snapShot, "Contest didn't start yet.");
        return rank1Lp;
    }
}