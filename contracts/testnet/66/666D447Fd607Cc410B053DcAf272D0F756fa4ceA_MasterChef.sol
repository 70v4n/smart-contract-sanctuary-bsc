                DELEGATION_TYPEHASH,
                delegatee,
                nonce,
                expiry
            )
        );

        bytes32 digest = keccak256(
            abi.encodePacked(
                "\x19\x01",
                domainSeparator,
                structHash
            )
        );

        address signatory = ecrecover(digest, v, r, s);
        require(signatory != address(0), "Ranch::delegateBySig: invalid signature");
        require(nonce == nonces[signatory]++, "Ranch::delegateBySig: invalid nonce");
        require(block.timestamp <= expiry, "Ranch::delegateBySig: signature expired");
        return _delegate(signatory, delegatee);
    }

    /**
     * @notice Gets the current votes balance for `account`
     * @param account The address to get votes balance
     * @return The number of current votes for `account`
     */
    function getCurrentVotes(address account)
        external
        view
        returns (uint256)
    {
        uint32 nCheckpoints = numCheckpoints[account];
        return nCheckpoints > 0 ? checkpoints[account][nCheckpoints - 1].votes : 0;
    }

    /**
     * @notice Determine the prior number of votes for an account as of a block number
     * @dev Block number must be a finalized block or else this function will revert to prevent misinformation.
     * @param account The address of the account to check
     * @param blockNumber The block number to get the vote balance at
     * @return The number of votes the account had as of the given block
     */
    function getPriorVotes(address account, uint blockNumber)
        external
        view
        returns (uint256)
    {
        require(blockNumber < block.number, "Ranch::getPriorVotes: not yet determined");

        uint32 nCheckpoints = numCheckpoints[account];
        if (nCheckpoints == 0) {
            return 0;
        }

        // First check most recent balance
        if (checkpoints[account][nCheckpoints - 1].fromBlock <= blockNumber) {
            return checkpoints[account][nCheckpoints - 1].votes;
        }

        // Next check implicit zero balance
        if (checkpoints[account][0].fromBlock > blockNumber) {
            return 0;
        }

        uint32 lower = 0;
        uint32 upper = nCheckpoints - 1;
        while (upper > lower) {
            uint32 center = upper - (upper - lower) / 2; // ceil, avoiding overflow
            Checkpoint memory cp = checkpoints[account][center];
            if (cp.fromBlock == blockNumber) {
                return cp.votes;
            } else if (cp.fromBlock < blockNumber) {
                lower = center;
            } else {
                upper = center - 1;
            }
        }
        return checkpoints[account][lower].votes;
    }

    function _delegate(address delegator, address delegatee)
        internal
    {
        address currentDelegate = _delegates[delegator];
        uint256 delegatorBalance = balanceOf(delegator); // balance of underlying Ranchs (not scaled);
        _delegates[delegator] = delegatee;

        emit DelegateChanged(delegator, currentDelegate, delegatee);

        _moveDelegates(currentDelegate, delegatee, delegatorBalance);
    }

    function _moveDelegates(address srcRep, address dstRep, uint256 amount) internal {
        if (srcRep != dstRep && amount > 0) {
            if (srcRep != address(0)) {
                // decrease old representative
                uint32 srcRepNum = numCheckpoints[srcRep];
                uint256 srcRepOld = srcRepNum > 0 ? checkpoints[srcRep][srcRepNum - 1].votes : 0;
                uint256 srcRepNew = srcRepOld - amount;
                _writeCheckpoint(srcRep, srcRepNum, srcRepOld, srcRepNew);
            }

            if (dstRep != address(0)) {
                // increase new representative
                uint32 dstRepNum = numCheckpoints[dstRep];
                uint256 dstRepOld = dstRepNum > 0 ? checkpoints[dstRep][dstRepNum - 1].votes : 0;
                uint256 dstRepNew = dstRepOld + amount;
                _writeCheckpoint(dstRep, dstRepNum, dstRepOld, dstRepNew);
            }
        }
    }

    function _writeCheckpoint(
        address delegatee,
        uint32 nCheckpoints,
        uint256 oldVotes,
        uint256 newVotes
    )
        internal
    {
        uint32 blockNumber = safe32(block.number, "Ranch::_writeCheckpoint: block number exceeds 32 bits");

        if (nCheckpoints > 0 && checkpoints[delegatee][nCheckpoints - 1].fromBlock == blockNumber) {
            checkpoints[delegatee][nCheckpoints - 1].votes = newVotes;
        } else {
            checkpoints[delegatee][nCheckpoints] = Checkpoint(blockNumber, newVotes);
            numCheckpoints[delegatee] = nCheckpoints + 1;
        }

        emit DelegateVotesChanged(delegatee, oldVotes, newVotes);
    }

    function safe32(uint n, string memory errorMessage) internal pure returns (uint32) {
        require(n < 2**32, errorMessage);
        return uint32(n);
    }

    function getChainId() internal view returns (uint) {
        uint256 chainId;
        assembly { chainId := chainid() }
        return chainId;
    }
}


// MasterChef is the master of Ranch. He can make Ranch and he is a fair guy.
//
// Note that it's ownable and the owner wields tremendous power. The ownership
// will be transferred to a governance smart contract once Ranch is sufficiently
// distributed and the community can show to govern itself.
//
// Have fun reading it. Hopefully it's bug-free. God bless.
contract MasterChef is Ownable {
    using SafeMath for uint256;
    using SafeBEP20 for IBEP20;

    // Info of each user.
    struct UserInfo {
        uint256 amount;     // How many LP tokens the user has provided.
        uint256 rewardDebt; // Reward debt. See explanation below.
        // uint256 nftBoost;
        //
        // We do some fancy math here. Basically, any point in time, the amount of Ranchs
        // entitled to a user but is pending to be distributed is:
        //
        //   pending reward = (user.amount * pool.accRanchPerShare) - user.rewardDebt
        //
        // Whenever a user deposits or withdraws LP tokens to a pool. Here's what happens:
        //   1. The pool's `accRanchPerShare` (and `lastRewardBlock`) gets updated.
        //   2. User receives the pending reward sent to his/her address.
        //   3. User's `amount` gets updated.
        //   4. User's `rewardDebt` gets updated.
    }
   struct NftUserInfo {
       uint256 amount;
       uint256[] tokenIds;
       mapping (uint256 => uint256) tokenIndex;
   }

   struct NftInfo{
       address nftContract;
       uint256 boostPerToken;
       // uint256 fPID;
   }
    
    mapping (uint256 => address) public tokenOwner;
    
    // Info of each pool.
    struct PoolInfo {
        IBEP20 lpToken;           // Address of LP token contract.
        uint256 allocPoint;       // How many allocation points assigned to this pool. Ranchs to distribute per block.
        uint256 lastRewardBlock;  // Last block number that Ranchs distribution occurs.
        uint256 accRanchPerShare; // Accumulated Ranchs per share, times 1e12. See below.
        uint256[] Boosters;
    }

     bytes4 private constant _ERC721_RECEIVED = 0x150b7a02;
    // The Ranch TOKEN!
    RanchToken public Ranch;
    // The Cattle TOKEN!
    CattleFarm public Cattle;
    // pair to get price from
    address public pricePair;
    // Dev address.
    address public devaddr;
    // Ranch tokens created per block.
    uint256 public RanchPerBlock;
    // Bonus muliplier for early Ranch makers.
    uint256 public BONUS_MULTIPLIER = 1;
    // BNB PAIR for Ranch!
    address[] public ranchPair;
    mapping (address => bool) private _isListed;
    // LOCK THE TOKEN TO MASTERCHEF FOREVER
    bool public tokenLOCKforever = false;
    
    // Set price Point of Ranch in BNB
    uint256 public RanchPricePoint = 1000000000;
    // Set modify liquidity mode
    bool public _modifyLiquidity = false;

    
        uint256 percentBurnLevel1 = 120;    // 120% of set price
        uint256 percentBurnLevel2 = 110;
        uint256 percentBurnLevel3 = 90;     // 90% =  of set price
        uint256 percentBurnLevel4 = 80;
        uint256 percentBurnLevel5 = 70;
        uint256 multiplierBurnLevel1 = 25;  // 25 = 25% of rewards burned in liquidity
        uint256 multiplierBurnLevel2 = 50;
        uint256 multiplierBurnLevel3 = 100;
        uint256 multiplierBurnLevel4 = 200; // 200 = 200% of rewards burned in liquidity
        uint256 multiplierBurnLevel5 = 300;
    
    
    // Info of each pool.
    PoolInfo[] public poolInfo;
    // Info of each user that stakes LP tokens.
    mapping (uint256 => mapping (address => UserInfo)) public userInfo;
    // Info of each pool.
    NftInfo[] public nftInfo;
    // Info of each user that stakes LP tokens.
    mapping (uint256 => mapping (address => NftUserInfo)) public nftuserInfo;
    // Total allocation poitns. Must be the sum of all allocation points in all pools.
    uint256 public totalAllocPoint = 0;
    // The block number when Ranch mining starts.
    uint256 public startBlock;

    event Deposit(address indexed user, uint256 indexed pid, uint256 amount);
    event Withdraw(address indexed user, uint256 indexed pid, uint256 amount);
    event EmergencyWithdraw(address indexed user, uint256 indexed pid, uint256 amount);
    event _Stake(uint256 _nid, uint256 _tokenId);
    event _Unstake(uint256 _nid, uint256 _tokenId);

    constructor(
        RanchToken _Ranch,
        CattleFarm _Cattle,
        address _devaddr,
        uint256 _RanchPerBlock,
        uint256 _startBlock
    ) {
        Ranch = _Ranch;
        Cattle = _Cattle;
        devaddr = _devaddr;
        RanchPerBlock = _RanchPerBlock;
        startBlock = _startBlock;
        uint256[] memory db = new uint256[](1);
        db[0] = 0;

        // blank or NO BOOST uses this.
        nftInfo.push(NftInfo({
            nftContract: address(0),
            boostPerToken: 0
            // fPID: 0
        }));

        // staking pool
        poolInfo.push(PoolInfo({
            lpToken: _Ranch,
            allocPoint: 1000,
            lastRewardBlock: startBlock,
            accRanchPerShare: 0,
            Boosters: db
        }));

        

        totalAllocPoint = 1000;

    }

    function setModifyLiquidity(bool _ModifyLiquidity) public onlyOwner{
        _modifyLiquidity = _ModifyLiquidity;
    }

    function setRanchPricePointBNB(uint _newRanchPricePoint) public onlyOwner {
        RanchPricePoint = _newRanchPricePoint;
    }

    function setModifiers(
                uint256 _percentBurnLevel1,
                uint256 _percentBurnLevel2,
                uint256 _percentBurnLevel3,
                uint256 _percentBurnLevel4,
                uint256 _percentBurnLevel5,
                uint256 _multiplierBurnLevel1,
                uint256 _multiplierBurnLevel2,
                uint256 _multiplierBurnLevel3,
                uint256 _multiplierBurnLevel4,
                uint256 _multiplierBurnLevel5
                ) public onlyOwner{
        percentBurnLevel1 = _percentBurnLevel1;
        percentBurnLevel2 = _percentBurnLevel2;        
        percentBurnLevel3 = _percentBurnLevel3;
        percentBurnLevel4 = _percentBurnLevel4;
        percentBurnLevel5 = _percentBurnLevel5;
        multiplierBurnLevel1 = _multiplierBurnLevel1;
        multiplierBurnLevel2 = _multiplierBurnLevel2;
        multiplierBurnLevel3 = _multiplierBurnLevel3;
        multiplierBurnLevel4 = _multiplierBurnLevel4;
        multiplierBurnLevel5 = _multiplierBurnLevel5;
    }

    function updateRanchPerBlock(uint256 newRanchPerBlock) public onlyOwner {
        RanchPerBlock = newRanchPerBlock;
    }
    
    function setPricePair(address _pricePair) public onlyOwner {
        pricePair = _pricePair;
    }
    
    function isPairListed(address account) public view returns (bool) {
        return _isListed[account];
    }
    
    function addRanchPair(address _LPPair) public onlyOwner {
        require(!_isListed[_LPPair], "Account: PAIR already listed" );
                _isListed[_LPPair] = true;
                ranchPair.push(_LPPair);
    }
    function removeRanchPair(address _LPPair) public onlyOwner {
        require(_isListed[_LPPair], "PAIR not in the List");
        for (uint256 i = 0; i < ranchPair.length; i++) {
            if (ranchPair[i] == _LPPair) {
                ranchPair[i] = ranchPair[ranchPair.length - 1];
                delete _isListed[_LPPair];
                ranchPair.pop();
            }
        }
    }

    function updateMultiplier(uint256 multiplierNumber) public onlyOwner {
        BONUS_MULTIPLIER = multiplierNumber;
    }

    function poolLength() external view returns (uint256) {
        return poolInfo.length;
    }

    function nftTotalBoosterLength() external view returns (uint256) {
        return nftInfo.length;
    }
    
    function getPoolBoostersLength(uint256 _pid) public view returns (uint256) {
        return poolInfo[_pid].Boosters.length;
    }
    
    function getPoolBoosters(uint256 _pid) external view returns (uint256[] memory _boosters) {
        return poolInfo[_pid].Boosters;
    }

    function ChangeOwnerShipOfToken(address _newMasterChef) public onlyOwner{
    require(!tokenLOCKforever, "LOCKED :  Token is locked to MasterChef Forever");
        Ranch.transferOwnership(_newMasterChef);
    }
    
    function LockTokenToMasterChef() external onlyOwner {
        tokenLOCKforever = true;
    }

    function calculateRanchPrice() public view returns (uint _RanchPriceBNB) {
        (uint reserve0, uint reserve1,) = LPTOKEN(pricePair).getReserves();
        address token0 = LPTOKEN(pricePair).token0();
        if (token0 == address(Ranch)) return ((reserve1 * 10**18) / reserve0);
        else return((reserve0 * 10**18) / reserve1);
        
    }

    // Add a new lp to the pool. Can only be called by the owner.
    // XXX DO NOT add the same LP token more than once. Rewards will be messed up if you do.
    function add(uint256 _allocPoint, IBEP20 _lpToken, bool _withUpdate, uint256[] memory _boosters) public onlyOwner {
        
        if (_withUpdate) {
            massUpdatePools();
        }
        uint256 lastRewardBlock = block.number > startBlock ? block.number : startBlock;
        totalAllocPoint = totalAllocPoint.add(_allocPoint);
        poolInfo.push(PoolInfo({
            lpToken: _lpToken,
            allocPoint: _allocPoint,
            lastRewardBlock: lastRewardBlock,
            accRanchPerShare: 0,
            Boosters: _boosters
        }));
    
    }

    function addNFTBooster(address _nftContract, uint256 _fPID, uint256 _boostPerToken) public onlyOwner {
        nftInfo.push(NftInfo({
            nftContract: _nftContract,
            boostPerToken: _boostPerToken
        }));
        poolInfo[_fPID].Boosters.push(nftInfo.length - 1);
    
    }

    function setBooster(uint256 _nid, uint256 _boostPerToken) public onlyOwner{
        nftInfo[_nid].boostPerToken = _boostPerToken;
    }

    function addBoosterToFarm(uint256 _nid, uint256 _pid) public onlyOwner{
        require(_nid != 0, "Cannot remove 0");
        PoolInfo storage pool = poolInfo[_pid];
        for (uint i = 0; i < pool.Boosters.length; i++) {
            require(pool.Boosters[i] != _nid, "Booster already added to FARM");
        }
        pool.Boosters.push(_nid);
        
    }

    function removeBoosterFromFarm(uint256 _pid, uint256 _nid) external {
        PoolInfo storage pool = poolInfo[_pid];
        for (uint256 i = 0; i < pool.Boosters.length; i++) {
            if (pool.Boosters[i] == _nid) {
                pool.Boosters[i] = pool.Boosters[pool.Boosters.length - 1];
                pool.Boosters.pop();
            }
        }
    }

    // Update the given pool's Ranch allocation point. Can only be called by the owner.
    function set(uint256 _pid, uint256 _allocPoint, bool _withUpdate) public onlyOwner {
        if (_withUpdate) {
            updatePool(_pid);
        }
        totalAllocPoint = totalAllocPoint.sub(poolInfo[_pid].allocPoint).add(_allocPoint);
        poolInfo[_pid].allocPoint = _allocPoint; 
    }
    
    // Change a current PID to a NEW LP token address.. only do this if there is a bad or duplicate LP token!!!!
    function changePIDtoNewToken(uint256 _pid, IBEP20 _lpToken, uint256 _allocPoint) public onlyOwner {
        totalAllocPoint = totalAllocPoint.sub(poolInfo[_pid].allocPoint).add(_allocPoint);
        poolInfo[_pid].lpToken = _lpToken;
        poolInfo[_pid].allocPoint = _allocPoint;
        poolInfo[_pid].accRanchPerShare = 0;
    }


    // Return reward multiplier over the given _from to _to block.
    function getMultiplier(uint256 _from, uint256 _to) public view returns (uint256) {
        return _to.sub(_from).mul(BONUS_MULTIPLIER);
    }

    // View function to see pending Ranchs on frontend.
    function pendingRanch(uint256 _pid, address _user) external view returns (uint256) {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_user];
        uint256 accRanchPerShare = pool.accRanchPerShare;
        uint256 lpSupply = pool.lpToken.balanceOf(address(this));
        if (block.number > pool.lastRewardBlock && lpSupply != 0) {
            uint256 multiplier = getMultiplier(pool.lastRewardBlock, block.number);
            uint256 RanchReward = multiplier.mul(RanchPerBlock).mul(pool.allocPoint).div(totalAllocPoint);
            accRanchPerShare = accRanchPerShare.add(RanchReward.mul(1e12).div(lpSupply));
        }
        return user.amount.mul(accRanchPerShare).div(1e12).sub(user.rewardDebt);
    }

    // Update reward variables for all pools. Be careful of gas spending!
    function massUpdatePools() public {
        uint256 length = poolInfo.length;
        for (uint256 pid = 0; pid < length; ++pid) {
            updatePool(pid);
        }
    }


    // Update reward variables of the given pool to be up-to-date.
    function updatePool(uint256 _pid) public {
        PoolInfo storage pool = poolInfo[_pid];
        if (block.number <= pool.lastRewardBlock) {
            return;
        }
        uint256 lpSupply = pool.lpToken.balanceOf(address(this));
        if (lpSupply == 0) {
            pool.lastRewardBlock = block.number;
            return;
        }
        uint256 multiplier = getMultiplier(pool.lastRewardBlock, block.number);
        uint256 RanchReward = multiplier.mul(RanchPerBlock).mul(pool.allocPoint).div(totalAllocPoint);
        Ranch.mint(devaddr, RanchReward.div(10));
        Ranch.mint(address(Cattle), RanchReward);
        if (_modifyLiquidity) modifyLiquidity(RanchReward);
        pool.accRanchPerShare = pool.accRanchPerShare.add(RanchReward.mul(1e12).div(lpSupply));
        pool.lastRewardBlock = block.number;
    }

    function modifyLiquidity (uint256 _RanchReward) internal {
        uint256 burnAmount = 0;
                            
            if (calculateRanchPrice() < RanchPricePoint * percentBurnLevel5 / 100) burnAmount = _RanchReward * multiplierBurnLevel5 / 100;
            if (calculateRanchPrice() < RanchPricePoint * percentBurnLevel4 / 100) burnAmount = _RanchReward * multiplierBurnLevel4 / 100;
            if (calculateRanchPrice() < RanchPricePoint * percentBurnLevel3 / 100) burnAmount = _RanchReward * multiplierBurnLevel3 / 100;
            if (calculateRanchPrice() < RanchPricePoint * percentBurnLevel2 / 100) burnAmount = _RanchReward * multiplierBurnLevel2 / 100;
            if (calculateRanchPrice() < RanchPricePoint * percentBurnLevel1 / 100) burnAmount = _RanchReward * multiplierBurnLevel1 / 100;
            
            if (burnAmount > 0) {
            for (uint i=0; i < ranchPair.length; i++){
                Ranch.burnLiq(address(ranchPair[i]), (burnAmount / ranchPair.length));
                LPTOKEN(ranchPair[i]).sync();
            }}
            
    }
    // GETS USERS TOTAL BOOST FOR GIVEN FARM (pid)
    function getUsersBoost(uint256 _pid, address _user)public view returns (uint256 _totalBoost) {
        PoolInfo storage pool = poolInfo[_pid];
        uint256 totalBoost = 0;
        uint256 boosterLength = getPoolBoostersLength(_pid);
        if (boosterLength > 1){
            for (uint i = 1; i < boosterLength; i++) {
                uint256 booster = pool.Boosters[i];
                NftInfo storage nftBooster = nftInfo[booster];
                NftUserInfo storage nftuser = nftuserInfo[booster][_user];
                if( nftuser.amount > 0 && nftBooster.boostPerToken > 0){
                    uint256 addedBoost = nftBooster.boostPerToken * nftuser.amount;
                    totalBoost = totalBoost + addedBoost;
                }
            }
        }
        return totalBoost;
    }

    function sendBoostReward(uint256 _pendingAmount, uint256 _boost) internal {
        uint256 boostedReward = _pendingAmount * _boost / 10000;
        Ranch.mint(msg.sender, boostedReward);
    }

    // Deposit LP tokens to MasterChef for Ranch allocation.
    function deposit(uint256 _pid, uint256 _amount) public {

        require (_pid != 0, 'deposit Ranch by staking');

        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        updatePool(_pid);
        
        if (user.amount > 0) {
            uint256 pending = user.amount.mul(pool.accRanchPerShare).div(1e12).sub(user.rewardDebt);
            if(pending > 0) {
                safeRanchTransfer(msg.sender, pending);
                if ( getUsersBoost(_pid,msg.sender) > 0) sendBoostReward(pending, getUsersBoost(_pid, msg.sender));
            }
        }
        if (_amount > 0) {
            pool.lpToken.safeTransferFrom(address(msg.sender), address(this), _amount);
            user.amount = user.amount.add(_amount);
        }
        user.rewardDebt = user.amount.mul(pool.accRanchPerShare).div(1e12);
        emit Deposit(msg.sender, _pid, _amount);
    }

    // Withdraw LP tokens from MasterChef.
    function withdraw(uint256 _pid, uint256 _amount) public {

        require (_pid != 0, 'withdraw Ranch by unstaking');

        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        require(user.amount >= _amount, "withdraw: not good");
        updatePool(_pid);
        uint256 pending = user.amount.mul(pool.accRanchPerShare).div(1e12).sub(user.rewardDebt);
        if(pending > 0) {
            safeRanchTransfer(msg.sender, pending);
            if ( getUsersBoost(_pid, msg.sender) > 0) sendBoostReward(pending, getUsersBoost(_pid, msg.sender));
        }
        if(_amount > 0) {
            user.amount = user.amount.sub(_amount);
            pool.lpToken.safeTransfer(address(msg.sender), _amount);
        }
        user.rewardDebt = user.amount.mul(pool.accRanchPerShare).div(1e12);
        emit Withdraw(msg.sender, _pid, _amount);
    }

    // Stake Ranch tokens to MasterChef
    function enterStaking(uint256 _amount) public {
        PoolInfo storage pool = poolInfo[0];
        UserInfo storage user = userInfo[0][msg.sender];
        updatePool(0);
        if (user.amount > 0) {
            uint256 pending = user.amount.mul(pool.accRanchPerShare).div(1e12).sub(user.rewardDebt);
            if(pending > 0) {
                safeRanchTransfer(msg.sender, pending);
            }
        }
        if(_amount > 0) {
            pool.lpToken.safeTransferFrom(address(msg.sender), address(this), _amount);
            user.amount = user.amount.add(_amount);
        }
        user.rewardDebt = user.amount.mul(pool.accRanchPerShare).div(1e12);

        Cattle.mint(msg.sender, _amount);
        emit Deposit(msg.sender, 0, _amount);
    }

    // Withdraw Ranch tokens from STAKING.
    function leaveStaking(uint256 _amount) public {
        PoolInfo storage pool = poolInfo[0];
        UserInfo storage user = userInfo[0][msg.sender];
        require(user.amount >= _amount, "withdraw: not good");
        updatePool(0);
        uint256 pending = user.amount.mul(pool.accRanchPerShare).div(1e12).sub(user.rewardDebt);
        if(pending > 0) {
            safeRanchTransfer(msg.sender, pending);
        }
        if(_amount > 0) {
            user.amount = user.amount.sub(_amount);
            pool.lpToken.safeTransfer(address(msg.sender), _amount);
        }
        user.rewardDebt = user.amount.mul(pool.accRanchPerShare).div(1e12);

        Cattle.burn(msg.sender, _amount);
        emit Withdraw(msg.sender, 0, _amount);
    }

    // Withdraw without caring about rewards. EMERGENCY ONLY.
    function emergencyWithdraw(uint256 _pid) public {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        pool.lpToken.safeTransfer(address(msg.sender), user.amount);
        emit EmergencyWithdraw(msg.sender, _pid, user.amount);
        user.amount = 0;
        user.rewardDebt = 0;
    }

    // Safe Ranch transfer function, just in case if rounding error causes pool to not have enough Ranch.
    function safeRanchTransfer(address _to, uint256 _amount) internal {
        Cattle.safeRanchTransfer(_to, _amount);
    }

    // Update dev address by the previous dev.
    function dev(address _devaddr) public {
        require(msg.sender == devaddr, "dev: wut?");
        devaddr = _devaddr;
    }

     function onERC721Received(
        address,
        address,
        uint256,
        bytes calldata data
    )
        public returns(bytes4)
    {
        return _ERC721_RECEIVED;
    }

    function getTokenIdsForNID(uint256 _nid, address _user) external view returns (uint256[] memory) {
         NftUserInfo storage nftuser = nftuserInfo[_nid][_user];
         return (nftuser.tokenIds);
    }


    // STAKE NFT BOOSTERS

    function stakeBooster(uint256 _nid, uint256[] memory tokenIds)
        public
    {
        deposit(_nid, 0);
        for (uint i = 0; i < tokenIds.length; i++) {
            _stakeBooster(_nid, tokenIds[i]);
        }
    }

    /// @notice Stake all your NFTs and earn reward tokens. 
    function stakeBoosterAll(uint256 _nid)
        external
    {
        NftInfo storage nftBooster = nftInfo[_nid];
        address nftAddress = nftBooster.nftContract;
        uint256[] memory TokenIDs = FarmageddonNFT(nftAddress).walletOfOwner(msg.sender);
        require(TokenIDs.length > 0, "You have no NFTs of this kind");
        stakeBooster(_nid, TokenIDs);
    }

    function _stakeBooster(uint256 _nid, uint256 _tokenId) internal {
        NftUserInfo storage nftuser = nftuserInfo[_nid][msg.sender];
        NftInfo storage nftBooster = nftInfo[_nid];
        // uint256 _pid = nftBooster.fPID;

                address nftAddress = nftBooster.nftContract;
                nftuser.tokenIds.push(_tokenId);
                nftuser.tokenIndex[nftuser.tokenIds.length - 1];
                tokenOwner[_tokenId] = msg.sender;

                FarmageddonNFT(nftAddress).safeTransferFrom(
                msg.sender,
                address(this),
                _tokenId
                );

            nftuser.amount = nftuser.amount.add(1);

        emit _Stake( _nid, _tokenId);
    }

// unstake NFT BOOSTERS

     function unstakeBooster (uint256 _nid, uint256[] memory tokenIds)
        public
    {
            deposit(_nid, 0);
        for (uint i = 0; i < tokenIds.length; i++) {
            uint TokenId = tokenIds[i];
            if (tokenOwner[TokenId] == msg.sender) {
                _unstakeBooster(_nid, TokenId);
            }
        }
    }

    /*
    * @notice unstake all TokenId's listed in the userInfo memory.
    */
    function unstakeBoosterAll(uint256 _nid) external {
        NftUserInfo storage nftuser = nftuserInfo[_nid][msg.sender];
        require(nftuser.tokenIds.length > 0, "You have no NFTs of this kind");
        unstakeBooster(_nid, nftuser.tokenIds);
    }


    function _unstakeBooster(uint256 _nid, uint256 _tokenId) internal {
        NftUserInfo storage nftuser = nftuserInfo[_nid][msg.sender];
        NftInfo storage nftBooster = nftInfo[_nid];
        // uint256 _pid = nftBooster.fPID;
        
        require(tokenOwner[_tokenId] == msg.sender, "You are Not the Owner of this NFT");
        require(nftuser.tokenIndex[_tokenId] >=0, "Not staked in this Farm");

        address nftAddress = nftBooster.nftContract;
        uint256 lastIndex = nftuser.tokenIds.length - 1;
        uint256 lastIndexKey = nftuser.tokenIds[lastIndex];
        uint256 tokenIdIndex = nftuser.tokenIndex[_tokenId];

        nftuser.tokenIds[nftuser.tokenIndex[_tokenId]] = lastIndexKey;
        nftuser.tokenIndex[lastIndexKey] = tokenIdIndex;
        if (nftuser.tokenIds.length > 0) {
            nftuser.tokenIds.pop();
            delete nftuser.tokenIndex[_tokenId];
        }
       
        delete tokenOwner[_tokenId];
        
            

            FarmageddonNFT(nftAddress).safeTransferFrom(
            address(this),
            msg.sender,
            _tokenId
        );

        nftuser.amount = nftuser.amount.sub(1);
        
        emit _Unstake(_nid, _tokenId);
    }

