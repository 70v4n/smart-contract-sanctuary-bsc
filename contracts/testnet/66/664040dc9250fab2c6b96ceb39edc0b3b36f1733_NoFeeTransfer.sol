contract NoFeeTransfer is Context, Ownable {
    using SafeMath for uint256;
    using Address for address;
    



    
    //Set the Artists Wallet Address
    address payable private Artist_Wallet = payable(0x23ADAE3696621f58eF8D846423A15B0002FAEeBA);

    // Set the Native Tips Token (MUSO)
    IERC20 TipsToken = IERC20(0x5e3D1c13c764a7C0C27B8985FC715BfBdF6B7174);


    // Send all Accumulated Tips to Artist
    function _1_Send_Tips() public onlyOwner {

        uint256 _contractBalance = IERC20(TipsToken).balanceOf(address(this));
        IERC20(TipsToken).transfer(Artist_Wallet, _contractBalance);
    }
   

    // Change the Artist Wallet
    function _2_Set_Artist_Wallet(address payable wallet) public onlyOwner() {
        Artist_Wallet = wallet;
    }
    

    // Change the Tips Token
    function _3_Set_Tips_Token(address newTipsToken) external onlyOwner(){
        TipsToken = IERC20(newTipsToken);
    }

    // Send percent of BNB from Contract to Wallet
    function _4_Send_BNB(address payable send_to_wallet,uint256 percent_of_BNB) public onlyOwner {
        if (percent_of_BNB > 100){percent_of_BNB = 100;}
        uint256 contractBNB = address(this).balance;
        if (contractBNB > 0) {
        uint256 sendBNB = contractBNB*percent_of_BNB/100;
        send_to_wallet.transfer(sendBNB);
    }
    }

    // Send percent  Accumulated Tips to Artist
    function _5_Send_ALT_Tokens(address alt_token_address, address send_to_wallet, uint256 percent_of_tokens) public onlyOwner returns(bool _sent){
        if (percent_of_tokens > 100){percent_of_tokens = 100;}
        uint256 _contractBalance = IERC20(alt_token_address).balanceOf(address(this));
        if (_contractBalance > 0) {
        uint256 sendTokens = _contractBalance*percent_of_tokens/100;
        _sent = IERC20(alt_token_address).transfer(send_to_wallet, sendTokens);
    }
    }
   
    

}