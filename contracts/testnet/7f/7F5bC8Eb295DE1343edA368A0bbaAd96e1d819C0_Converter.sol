pragma solidity 0.8.9;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";

import "./external/IFactory01.sol";
import "./external/IPair01.sol";
import "./external/IRouter01.sol";

interface IConverter {
    function convertLp2Lp(
        uint fromAmount,
        address fromLp,
        bytes32 fromRouterName,
        bytes32 toRouterName
    ) external;
}

contract Converter is IConverter, Initializable, OwnableUpgradeable, PausableUpgradeable {
    using SafeERC20Upgradeable for IERC20Upgradeable;

    uint constant UINT_MAX = type(uint256).max;
    
    address constant ROUTER_PCS2 = 0x10ED43C718714eb63d5aA57B78B54704E256024E;
    address constant FACTORY_PCS2 = 0xcA143Ce32Fe78f1f7019d7d551a6402fC5350c73;
    address constant ROUTER_WAULT = 0xD48745E39BbED146eEC15b79cBF964884F9877c2;
    address constant FACTORY_WAULT = 0xB42E3FE71b7E0673335b3331B3e1053BD9822570;
    address constant ROUTER_MDEX = 0x7DAe51BD3E3376B8c7c4900E9107f12Be3AF1bA8;
    address constant FACTORY_MDEX = 0x3CD1C46068dAEa5Ebb0d3f55F6915B10648062B8;
    address constant ROUTER_APE = 0xcF0feBd3f17CEf5b47b0cD257aCf6025c5BFf3b7;
    address constant FACTORY_APE = 0x0841BD0B734E4F5853f0dD8d7Ea041c241fb0Da6;
    address constant ROUTER_BAKERY = 0xCDe540d7eAFE93aC5fE6233Bee57E1270D3E330F;
    address constant FACTORY_BAKERY = 0x01bF7C66c6BD861915CdaaE475042d3c4BaE16A7;
    address constant ROUTER_JET = 0xBe65b8f75B9F20f4C522e0067a3887FADa714800;
    address constant FACTORY_JET = 0x0eb58E5c8aA63314ff5547289185cC4583DfCBD5;
    address constant ROUTER_KEBAB = 0x05fF2B0DB69458A0750badebc4f9e13aDd608C7F;
    address constant FACTORY_KEBAB = 0xBCfCcbde45cE874adCB698cC183deBcF17952812;
    address constant ROUTER_PLANET = 0x9F088377BcdC220CB0E1Ad15aE6Bc75074beE9F6;
    address constant FACTORY_PLANET = 0xa053582601214FEb3778031a002135cbBB7DBa18;
    address constant ADDR_ZERO = address(0);

    // 10e5 = 100%
    uint constant MAX_FEE = 10e5 / 10e2; // 1%
    uint mLp2LpFee;

    event Lp2LpFeeChanged(uint fee);
    event FeeWithdrawn(address indexed token, uint amount);
    event Lp2LpExecuted(
        address who, 
        address indexed fromLp, 
        uint fromAmount, 
        address indexed fromRouterAddr, 
        address indexed toRouterAddr
    );

    struct LpToken {
        address base;
        address token0;
        address token1;
    }

    function initialize() public initializer {
        __Pausable_init();
        __Ownable_init();
    }

    function convertLp2Lp(
        uint fromAmount,
        address fromLp,
        bytes32 fromRouterName,
        bytes32 toRouterName
    ) external whenNotPaused {
        require(fromAmount > 0, "FromAmount must be > 0");
        require(fromRouterName != toRouterName, "Same routers");

        (address fromRouterAddr, address fromFactoryAddr) = getRouter(fromRouterName);
        require(fromRouterAddr != ADDR_ZERO, "FromRouter not found");

        (address toRouterAddr, ) = getRouter(toRouterName);
        require(toRouterAddr != ADDR_ZERO, "ToRouter not found");

        LpToken memory lpInfo = LpToken(
            fromLp,
            IPair01(fromLp).token0(),
            IPair01(fromLp).token1()
        );

        require(
            IFactory01(fromFactoryAddr)
                .getPair(lpInfo.token0, lpInfo.token1) == fromLp,
            "Wrong Lp token"
        );

        IERC20Upgradeable(fromLp).safeTransferFrom(msg.sender, address(this), fromAmount);

        convertLp2Lp(fromAmount, lpInfo, fromRouterAddr, toRouterAddr, mLp2LpFee, msg.sender);

        emit Lp2LpExecuted(msg.sender, fromLp, fromAmount, fromRouterAddr, toRouterAddr);
    }

    function setLp2LpFee(uint fee) external onlyOwner {
        require(fee <= MAX_FEE, "Fee is too high");
        mLp2LpFee = fee;
        
        emit Lp2LpFeeChanged(fee);
    }
    
    function withdrawFee(address token) external onlyOwner {
        uint amount = IERC20Upgradeable(token).balanceOf(address(this));
        if (amount > 0) {
            IERC20Upgradeable(token).safeTransfer(msg.sender, amount);
            emit FeeWithdrawn(token, amount);
        }
    }

    function pause() external onlyOwner {
        _pause();
    }
    
    function resume() external onlyOwner {
        _unpause();
    }
    
    function convertLp2Lp(
        uint fromAmount,
        LpToken memory fromLpInfo,
        address fromRouterAddr,
        address toRouterAddr,
        uint fee,
        address targetAddress
    ) internal {
        uint deadline = block.timestamp + 300;
        
        approveTokenIfNeeded(fromLpInfo.base, fromRouterAddr, fromAmount);
        (uint amount0, uint amount1) = IRouter01(fromRouterAddr).removeLiquidity(
            fromLpInfo.token0, 
            fromLpInfo.token1, 
            fromAmount, 
            0, 
            0, 
            address(this), 
            deadline
        );
        
        if (fee > 0) {
            amount0 = amount0 * (10e5 - mLp2LpFee) / 10e5;
            amount1 = amount1 * (10e5 - mLp2LpFee) / 10e5;
        }
        
        approveTokenIfNeeded(fromLpInfo.token0, toRouterAddr, amount0);
        approveTokenIfNeeded(fromLpInfo.token1, toRouterAddr, amount1);
        IRouter01(toRouterAddr).addLiquidity(
            fromLpInfo.token0, 
            fromLpInfo.token1, 
            amount0, 
            amount1, 
            0, 
            0, 
            targetAddress, 
            deadline
        );
    }

    function approveTokenIfNeeded(address token, address spender, uint amount) internal {
        uint current = IERC20Upgradeable(token).allowance(address(this), spender);
        if (current < amount) {
            IERC20Upgradeable(token).safeIncreaseAllowance(spender, UINT_MAX - current);
        }
    }

    function getRouter(
        bytes32 name
    ) internal pure returns (address router, address factory) {
        if      (name == "PCS2")    return (ROUTER_PCS2,    FACTORY_PCS2);
        else if (name == "WAULT")   return (ROUTER_WAULT,   FACTORY_WAULT);
        else if (name == "MDEX")    return (ROUTER_MDEX,    FACTORY_MDEX);
        else if (name == "APE")     return (ROUTER_APE,     FACTORY_APE);
        else if (name == "BAKERY")  return (ROUTER_BAKERY,  FACTORY_BAKERY);
        else if (name == "PLANET")  return (ROUTER_PLANET,  FACTORY_PLANET);
        else if (name == "JET")     return (ROUTER_JET,     FACTORY_JET);
        else if (name == "KEBAB")   return (ROUTER_KEBAB,   FACTORY_KEBAB);
        else return (ADDR_ZERO, ADDR_ZERO);
    }
}

