 * This module is used through inheritance. It will make available the modifier
 * `onlyOwner`, which can be applied to your functions to restrict their use to
 * the owner.
 */
contract Ownable is Context {
    address private _owner;

    event OwnershipTransferred(address indexed previousOwner, address indexed newOwner);

    /**
     * @dev Initializes the contract setting the deployer as the initial owner.
     */
    constructor() {
        _setOwner(_msgSender()); 
    }

    /**
     * @dev Returns the address of the current owner.
     */
    function owner() public view virtual returns (address) {
        return _owner;
    }

    /**
     * @dev Throws if called by any account other than the owner.
     */
    modifier onlyOwner() {
        require(owner() == _msgSender(), "Ownable: caller is not the owner");
        _;
    }

    /**
     * @dev Leaves the contract without owner. It will not be possible to call
     * `onlyOwner` functions anymore. Can only be called by the current owner.
     *
     * NOTE: Renouncing ownership will leave the contract without an owner,
     * thereby removing any functionality that is only available to the owner.
     */
    function renounceOwnership() public virtual onlyOwner {
        _setOwner(address(0));
    }

    /**
     * @dev Transfers ownership of the contract to a new account (`newOwner`).
     * Can only be called by the current owner.
     */
    function transferOwnership(address newOwner) public virtual onlyOwner {
        require(newOwner != address(0), "Ownable: new owner is the zero address");
        _setOwner(newOwner);
    }

    function _setOwner(address newOwner) internal {
        address oldOwner = _owner;
        _owner = newOwner;
        emit OwnershipTransferred(oldOwner, newOwner);
    }
}

interface IbnbItchesGame {
    function collectionInfo(uint256) external view returns (uint256);
    function multiplierInfo(uint256) external view returns (uint256);
}

contract bnbItchesStake is Ownable,ERC1155Holder {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    struct UserInfo {
        uint256 supply;
        uint256 amount;   
        uint256 rewardDebt; 
    }

    struct PoolInfo {          
        uint256 allocPoint;      
        uint256 poolStartBlock;
        mapping (uint256 => RewardStore) rewardsInfo;       
    }
    
    struct RewardStore {
        uint256 lastRewardBlock; 
        uint256 accbnbItchesPerShare; 
    }

    IERC20 public bnbItchesToken;
    IERC1155 public bnbItchesNft;
    IbnbItchesGame public bnbItchesGame;

    address public devaddr;
    uint256 public bnbItchesPerBlock;
    uint256 public BONUS_MULTIPLIER = 10;
    uint256 public precision = 10 ** 18;

    PoolInfo public poolInfo;
    mapping (address => mapping (uint256 => UserInfo)) private userInfo;  
    uint256 public totalAllocPoint = 0;
    uint256 public startBlock;

    event Deposit(address indexed user,uint256 id,uint256 amount);
    event Withdraw(address indexed user,uint256 id,uint256 amount);
    event EmergencyWithdraw(address indexed user,uint256 id,uint256 amount);

    constructor(
        IERC20 _bnbItchesToken,
        IERC1155 _bnbItchesNft,
        IbnbItchesGame _bnbItchesGame,
        address _devaddr,
        uint256 _bnbItchesPerBlock,
        uint256 _allocPoint,
        uint256 _startBlock
    ) {
        bnbItchesToken = _bnbItchesToken;
        bnbItchesNft = _bnbItchesNft;
        bnbItchesGame = _bnbItchesGame;
        devaddr = _devaddr;
        bnbItchesPerBlock = _bnbItchesPerBlock;
        startBlock = _startBlock;

        poolInfo.poolStartBlock = block.number > startBlock ? block.number : startBlock;
        totalAllocPoint = totalAllocPoint.add(_allocPoint);
        poolInfo.allocPoint = _allocPoint;
    }

    function set(uint256 _allocPoint) public onlyOwner {
        totalAllocPoint = totalAllocPoint.add(_allocPoint);
        poolInfo.allocPoint = _allocPoint;
    }

    function setStartBlock(uint256 _startBlock) public onlyOwner {
        startBlock = _startBlock;
    }

    function setbnbItchesPerBlock(uint256 _bnbItchesPerBlock) public onlyOwner {
        bnbItchesPerBlock = _bnbItchesPerBlock;
    }

    function setBonusMultiplier(uint256 _BONUS_MULTIPLIER) public onlyOwner {
        BONUS_MULTIPLIER = _BONUS_MULTIPLIER;
    }
    
    function getMultiplier(uint256 _from, uint256 _to) public view returns (uint256) {
        return _to.sub(_from).mul(BONUS_MULTIPLIER);
    }

    function multiplierStore(uint256 nft) public view returns (uint256) {
        uint256 getCollect = bnbItchesGame.collectionInfo(nft);
        return bnbItchesGame.multiplierInfo(getCollect);
    }

    function pendingbnbItchesToken(address _user,uint256 _tokenId) external view returns (uint256) {
        UserInfo storage user = userInfo[_user][_tokenId];
        RewardStore storage reward = poolInfo.rewardsInfo[_tokenId];
        uint256 accbnbItchesPerShare = reward.accbnbItchesPerShare;
        uint256 lpSupply = bnbItchesNft.balanceOf(address(this),_tokenId).mul(precision);
        if (block.number > reward.lastRewardBlock && lpSupply != 0) {
            uint256 multiplier = getMultiplier(reward.lastRewardBlock, block.number);
            uint256 bnbItchesTokenReward = multiplier.mul(bnbItchesPerBlock).mul(poolInfo.allocPoint).div(totalAllocPoint);
            accbnbItchesPerShare = accbnbItchesPerShare.add(bnbItchesTokenReward.mul(1e12).div(lpSupply));
        }
        return user.amount.mul(accbnbItchesPerShare).div(1e12).sub(user.rewardDebt);
    }

    function updatePool(uint256 _tokenId) public returns (uint256){
        RewardStore storage reward = poolInfo.rewardsInfo[_tokenId];
        if(reward.lastRewardBlock == 0) {
            reward.lastRewardBlock = poolInfo.poolStartBlock;
        }
        if (block.number <= reward.lastRewardBlock) {
            return 0;
        }
        uint256 lpSupply = bnbItchesNft.balanceOf(address(this),_tokenId).mul(precision);
        if (lpSupply == 0) {
            reward.lastRewardBlock = block.number;
            return 0;
        }
        uint256 multiplier = getMultiplier(reward.lastRewardBlock, block.number);
        uint256 bnbItchesTokenReward = multiplier.mul(bnbItchesPerBlock).mul(poolInfo.allocPoint).div(totalAllocPoint);
        bnbItchesToken.mint(devaddr, bnbItchesTokenReward.div(10));
        bnbItchesToken.mint(address(this), bnbItchesTokenReward);
        reward.accbnbItchesPerShare = reward.accbnbItchesPerShare.add(bnbItchesTokenReward.mul(1e12).div(lpSupply));
        reward.lastRewardBlock = block.number;
        return reward.accbnbItchesPerShare;
    }

    function deposit(uint256 _tokenId,uint256 _amount) public {
        UserInfo storage user = userInfo[msg.sender][_tokenId];
        uint256 accbnbItchesPerShare = updatePool(_tokenId);
        if(accbnbItchesPerShare == 0) {
            accbnbItchesPerShare = poolInfo.rewardsInfo[_tokenId].accbnbItchesPerShare;
        }
        if (user.amount > 0) {
            uint256 pending = user.amount.mul(accbnbItchesPerShare).div(1e12).sub(user.rewardDebt);
            uint256 bonus = pending.mul(multiplierStore(_tokenId));
            if(bonus > 0) {
                safebnbItchesTokenTransfer(msg.sender,bonus);
            }
        }

        if(_amount > 0) {
            bnbItchesNft.safeTransferFrom(address(msg.sender), address(this),_tokenId,_amount,"");
            user.amount = user.amount.add(_amount.mul(precision));
            user.supply = user.supply.add(_amount);
        }
        user.rewardDebt = user.amount.mul(accbnbItchesPerShare).div(1e12);
        emit Deposit(msg.sender, _tokenId, _amount);
    }

    function withdraw(uint256 _tokenId, uint256 _amount) public {
        UserInfo storage user = userInfo[msg.sender][_tokenId];
        require(user.supply >= _amount, "withdraw: not good");
        uint256 accbnbItchesPerShare = updatePool(_tokenId);
        if(accbnbItchesPerShare == 0) {
            accbnbItchesPerShare = poolInfo.rewardsInfo[_tokenId].accbnbItchesPerShare;
        }
        
        uint256 pending = user.amount.mul(accbnbItchesPerShare).div(1e12).sub(user.rewardDebt);
        uint256 bonus = pending.mul(multiplierStore(_tokenId));
        if(bonus > 0) {
            safebnbItchesTokenTransfer(msg.sender, bonus);
        }

        if(_amount > 0) {
            user.amount = user.amount.sub(_amount * precision);
            user.supply = user.supply.sub(_amount);
            bnbItchesNft.safeTransferFrom(address(this), address(msg.sender),_tokenId,_amount,"");        
        }      
        
        user.rewardDebt = user.amount.mul(accbnbItchesPerShare).div(1e12);
        emit Withdraw(msg.sender,_tokenId, _amount);
    }

    function emergencyWithdraw(uint256 _tokenId) public {
        UserInfo storage user = userInfo[msg.sender][_tokenId];
        uint256 reward = user.supply;
        user.amount = 0;
        user.rewardDebt = 0;
        user.supply = 0;
        bnbItchesNft.safeTransferFrom(address(this), address(msg.sender),_tokenId,reward,"");
        emit EmergencyWithdraw(msg.sender,_tokenId, reward);
    }

    function safebnbItchesTokenTransfer(address _to, uint256 _amount) private {
        uint256 bnbItchesTokenBal = bnbItchesToken.balanceOf(address(this));
        if (_amount > bnbItchesTokenBal) {
            bnbItchesToken.transfer(_to, bnbItchesTokenBal);
        } else {
            bnbItchesToken.transfer(_to, _amount);
        }
    }

    function bnbEmergencySafe(uint256 amount) public onlyOwner {
       (payable(owner())).transfer(amount);
    }
    
    function tokenEmergencySafe(IERC20 token,uint256 amount) public onlyOwner {
       token.safeTransfer(owner(),amount);
    }

    function dev(address _devaddr) public {
        require(msg.sender == devaddr, "dev: wut?");
        devaddr = _devaddr;
    }    
        
    function userDetails(address user,uint256 tokenId) public view returns(UserInfo memory res) {
        return userInfo[user][tokenId];
    }

    function rewardDetails(uint256 tokenId) public view returns (RewardStore memory res){
        return poolInfo.rewardsInfo[tokenId];
    }
}