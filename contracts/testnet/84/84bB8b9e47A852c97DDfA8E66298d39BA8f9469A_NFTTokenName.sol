        ), "ERC721: transfer to non ERC721Receiver implementer");
        bytes4 retval = abi.decode(returndata, (bytes4));
        return (retval == _ERC721_RECEIVED);
    }

    function _approve(address to, uint256 tokenId) private {
        _tokenApprovals[tokenId] = to;
        emit Approval(ERC721.ownerOf(tokenId), to, tokenId); // internal owner
    }

    /**
     * @dev Hook that is called before any token transfer. This includes minting
     * and burning.
     *
     * Calling conditions:
     *
     * - When `from` and `to` are both non-zero, ``from``'s `tokenId` will be
     * transferred to `to`.
     * - When `from` is zero, `tokenId` will be minted for `to`.
     * - When `to` is zero, ``from``'s `tokenId` will be burned.
     * - `from` cannot be the zero address.
     * - `to` cannot be the zero address.
     *
     * To learn more about hooks, head to xref:ROOT:extending-contracts.adoc#using-hooks[Using Hooks].
     */
    function _beforeTokenTransfer(address from, address to, uint256 tokenId) internal virtual { }
}

contract NFTTokenName is ERC721, Ownable {
    using SafeMath for uint256;
    using BytesLib for bytes;

    uint256 public MAX_QUESTION_TOKENS;
    uint256 public MAX_ANSWER_TOKENS;

    uint256 public MINTED_QUESTION_TOKENS = 0;
    uint256 public MINTED_ANSWER_TOKENS = 0;

    uint256 public QUESTION_TOKEN_TYPE = 1;
    uint256 public ANSWER_TOKEN_TYPE = 2;

    uint256 public QUESTION_CARD_PRICE = 400000;
    uint256 public ANSWER_CARD_PRICE = 200000;

    //uint256 public constant MAX_CARDS_PURCHASE = 10;

    bool public isSaleActive = false;
    bool public isCardsActive = false;
    
    uint256 [][] private metadataIdQuestionCardStorage;
    uint256 [][] private metadataIdAnswerCardStorage;

    // If we want to store metadata in the contract
    mapping(uint256 => bytes) private metadata;

    constructor(string memory name, string memory symbol, 
        uint256 initialQuestionTokenSupply, uint256 initialAnswerTokenSupply) ERC721(name, symbol){
        MAX_QUESTION_TOKENS = initialQuestionTokenSupply;
        MAX_ANSWER_TOKENS = initialAnswerTokenSupply;
    }

    function setBaseURI(string memory baseURI) public onlyOwner { 
        _setBaseURI(baseURI);
    }

    function changeMaxQuestionTokens(uint256 maxTokens) public onlyOwner { 
        MAX_QUESTION_TOKENS = maxTokens;
    }

    function changeMaxAnswerTokens(uint256 maxTokens) public onlyOwner { 
        MAX_ANSWER_TOKENS = maxTokens;
    }

    function setSaleState() public onlyOwner { 
        isSaleActive = !isSaleActive;
    }
    
    function setCardsState() public onlyOwner { 
        isCardsActive = !isCardsActive;
    }

    function withdraw() public payable onlyOwner{
        uint balance = address(this).balance;
        payable(msg.sender).transfer(balance);
    }
    
    function getMetadataForId(uint256 tokenId) public view returns(bytes memory){
        require(isCardsActive, "Cards must be active to view the content of a Card!");
        return metadata[tokenId];
    }
    
    function getAllQuestionMetadata() public view returns(bytes[] memory){
        bytes [] memory questionCollection; 
        
        for (uint256 i = 0; i < MINTED_QUESTION_TOKENS; i++){
            questionCollection[i] = metadata[i];
        }
        
        return questionCollection;
    }
    
    function getAllAnswerMetadata() public view returns(bytes[] memory){
        bytes [] memory answerCollection;
        
        for (uint256 i = MAX_QUESTION_TOKENS; i < MINTED_ANSWER_TOKENS.add(MAX_QUESTION_TOKENS); i++){
            answerCollection[i-MAX_QUESTION_TOKENS] = metadata[i];
        }
        
        return answerCollection;
    }
    
    function getMetadataForLength(uint256 length, uint256 tokenType) private view returns(uint256 [] memory) {
        if (tokenType == QUESTION_TOKEN_TYPE)
            return metadataIdQuestionCardStorage[length];
        else
            return metadataIdAnswerCardStorage[length];
    }

    function checkIfMetadataExist(bytes memory blueprint, uint256 tokenType) public view returns(bool) {
        uint256 [] memory metadataIds = getMetadataForLength(blueprint.length, tokenType);
        
        if(metadataIds.length < 1)
            return false;
        else
            for(uint256 i = 0; i < metadataIds.length; i++){
                if(metadata[metadataIds[i]].equal(blueprint)){
                    return true;
                }
            }

        return false;
    }

    function mintCard(uint256 tokenType, bytes memory blueprint) public payable {
        require(isSaleActive, "Sale must be active to mint a Card!");
        require(tokenType == QUESTION_TOKEN_TYPE || 
            tokenType == ANSWER_TOKEN_TYPE, "Wrong token type!");
        
        // If number of tokens added we should require a check for how many and edit the total supply .add(numberOfTokens)
        if (tokenType == QUESTION_TOKEN_TYPE){
            require(QUESTION_CARD_PRICE <= msg.value, "Bnb value sent is not correct!");
            require(totalSupply().sub(MINTED_ANSWER_TOKENS).add(1) <= MAX_QUESTION_TOKENS, "Purchase would exceed the max supply of question cards!");
            
            if(checkIfMetadataExist(blueprint, tokenType))
                revert("Already minted such a card!");

            uint mintIndex = totalSupply().sub(MINTED_ANSWER_TOKENS);
            //_safeMint(msg.sender, mintIndex); // Without blueprint
            _safeMint(msg.sender, mintIndex, blueprint);
            
            // Save metadata to contract aka text on the card
            metadata[mintIndex] = blueprint;
            metadataIdQuestionCardStorage.push([blueprint.length, mintIndex]);
            
            MINTED_QUESTION_TOKENS += 1;
        } else {
            require(ANSWER_CARD_PRICE <= msg.value, "Bnb value sent is not correct!");
            require(totalSupply().sub(MINTED_QUESTION_TOKENS).add(1) <= MAX_ANSWER_TOKENS, "Purchase would exceed the max supply of answer cards!");
            
            if(checkIfMetadataExist(blueprint, tokenType))
                revert("Already minted such a card!");

            uint mintIndex = totalSupply().sub(MINTED_QUESTION_TOKENS).add(MAX_QUESTION_TOKENS);
            //_safeMint(msg.sender, mintIndex);// Without blueprint
            _safeMint(msg.sender, mintIndex, blueprint);
            
            // Save metadata to contract aka text on the card
            metadata[mintIndex] = blueprint;
            metadataIdAnswerCardStorage.push([blueprint.length, mintIndex]);
            
            MINTED_ANSWER_TOKENS += 1;
        }
    }

    function burnReportedCard(uint256 tokenId) public onlyOwner{
        require(ownerOf(tokenId) != address(0), "Token is already burned!");
        _burn(tokenId);
        metadata[tokenId] = "";
    }
}