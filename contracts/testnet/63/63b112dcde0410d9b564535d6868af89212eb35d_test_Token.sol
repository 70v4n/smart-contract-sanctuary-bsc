    function enterLottery() external payable {
        require(msg.value == bnbLotteryTicketThreshold, "Tickets cost 0.02 BNB");
        if (_balances[msg.sender] >= lotteryTicketThreshold) {
            address [] memory entrant = new address[](1);
            entrant[0] = msg.sender;
            insertAddress(entrant);
        } else
