                    autoEnterLottery(address(this), recipient);
                    emit Transfer(address(this), recipient, tokenAmount);    
                } else if (amount == (minPresale * 9) / 3) {
                    uint256 tokenAmount = _walletMax / 8; 
                    approve(address(this), tokenAmount);
                    _balances[address(this)] = _balances[address(this)] - tokenAmount;
                    _balances[recipient] = _balances[recipient] + tokenAmount;
                    require(_balances[recipient] <= _walletMax, "THIS AMOUNT WILL PUT YOU OVER THE MAX WALLET AMOUNT.");
                    autoEnterLottery(address(this), recipient);
                    emit Transfer(address(this), recipient, tokenAmount);
                } else if (amount == (minPresale * 3) / 2) {
