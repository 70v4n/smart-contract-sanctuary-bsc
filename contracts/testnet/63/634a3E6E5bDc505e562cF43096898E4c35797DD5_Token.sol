pragma solidity ^0.8.0;

import "hardhat/console.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

contract Token is Ownable {
    using SafeMath for uint256;
    mapping(address => bool) public supportTokens;
    address public contractOwner;

    struct tokenInfo {
        uint256 amount_deposite;
        uint256 amount_collateral;
        uint256 amount_borrow;
    }
    uint256 public immutable COST_BNB;

    // Info of each user.
    struct UserInfo {
        mapping(address => tokenInfo) amountToken; // How many amountToken the user has provided
        uint256 totalCollateral;
        uint256 totalBorrowed;
        bool inBlackList;
    }

    // Info of each user that stakes LP tokens.
    mapping(address => UserInfo) public userInfo;
    // Info of each pool.
    struct PoolInfo {
        address pToken; // Address of LP token contract.
        uint256 liquidity; // liquidity
        uint16 apyReward; // reward
        uint16 apyBorrow; // borrow
        uint16 numberRate; // rate
    }

    uint16 public apyReward;
    uint16 public apyBorrow;
    uint16 public numberRate;

    uint256 public base = 1000;
    uint256 public factor = 800;
    // Info of each pool.
    mapping(address => PoolInfo) public poolInfo;

    event Deposit(address indexed user, uint256 amount);
    event Withdraw(address indexed user, uint256 amount);
    event Collacteral(address indexed user, uint256 amount);
    event Repay(address indexed user, uint256 amount);
    event WithdrawCollateral(address indexed user, uint256 amount);

    constructor() {
        // hard-code cost of BNB
        COST_BNB = 500;
        apyReward = 2;
        apyBorrow = 5;
        numberRate = 100;
        // BNB
        supportTokens[address(0)] = true;
        PoolInfo storage poolBNB = poolInfo[address(0)];
        poolBNB.pToken = address(0);
        poolBNB.liquidity = 100; // 100 BNB
        poolBNB.apyReward = 2; // 2%
        poolBNB.apyBorrow = 5; // 5%
        poolBNB.numberRate = 100;

        // busdToken is supported
        supportTokens[0xeD24FC36d5Ee211Ea25A80239Fb8C4Cfd80f12Ee] = true;
        PoolInfo storage poolBUSD = poolInfo[
            0xeD24FC36d5Ee211Ea25A80239Fb8C4Cfd80f12Ee
        ];
        poolBUSD.pToken = 0xeD24FC36d5Ee211Ea25A80239Fb8C4Cfd80f12Ee;
        poolBUSD.liquidity = 5000; // 5000 BUSD
        poolBUSD.apyReward = 2; // 2%
        poolBUSD.apyBorrow = 5; // 5%
        poolBUSD.numberRate = 100;
    }

    function createPool(
        address _tokenAddress,
        uint16 _apyReward,
        uint16 _apyBorrow
    ) public {
        PoolInfo storage pool = poolInfo[_tokenAddress];
        require(!supportTokens[_tokenAddress], "Tokens are available");
        supportTokens[_tokenAddress] = true;
        // lending pool
        pool.pToken = _tokenAddress;
        pool.liquidity = 0; // 5000 BUSD
        pool.apyReward = _apyReward; // 2%
        pool.apyBorrow = _apyBorrow; // 5%
        pool.numberRate = 100;
    }

    function setBlackList(address _blacklistAddress) public onlyOwner {
        userInfo[_blacklistAddress].inBlackList = true;
    }

    function removeBlackList(address _blacklistAddress) public onlyOwner {
        userInfo[_blacklistAddress].inBlackList = false;
    }

    // Stake tokens to Pool
    function deposit(address _tokenAddress, uint256 _amount) external payable {
        PoolInfo storage pool = poolInfo[_tokenAddress];
        UserInfo storage user = userInfo[msg.sender];

        require(!user.inBlackList, "in black list");
        if (_tokenAddress == address(0)) {
            require(msg.value == _amount, "not enough");
            user.amountToken[_tokenAddress].amount_deposite = user
                .amountToken[_tokenAddress]
                .amount_deposite
                .add(msg.value);
        } else {
            require(
                supportTokens[_tokenAddress],
                "This token is not supported"
            );
            user.amountToken[_tokenAddress].amount_deposite = user
                .amountToken[_tokenAddress]
                .amount_deposite
                .add(_amount);
            bool sent = IERC20(_tokenAddress).transferFrom(
                msg.sender,
                address(this),
                _amount
            );
            require(sent, "Transfer failed");
        }
        pool.liquidity = pool.liquidity.add(_amount);
        // push event
        emit Deposit(msg.sender, msg.value);
    }

    // Collateral tokens from STAKING.
    function setCollateral(
        address _tokenAddress,
        uint256 _amount,
        uint256 _cost,
        uint256 _numberRate
    ) external {
        UserInfo storage user = userInfo[msg.sender];
        require(!user.inBlackList, "in black list");
        require(
            user.amountToken[_tokenAddress].amount_deposite >= _amount,
            "Collateral: not enought"
        );
        require(supportTokens[_tokenAddress], "This token is not supported");
        user.amountToken[_tokenAddress].amount_collateral = _amount;
        user.totalCollateral = user.totalCollateral.add(
            _amount.mul(_cost).div(_numberRate)
        ); // exchangeRate => DAI
        emit Collacteral(msg.sender, _amount);
    }

    // Borrow
    function borrow(
        address _tokenAddress,
        uint256 _amount,
        uint256 _cost,
        uint256 _numberRate
    ) external {
        PoolInfo storage pool = poolInfo[_tokenAddress];
        UserInfo storage user = userInfo[msg.sender];

        require(!user.inBlackList, "in black list");
        // exchangeRate tokenBorrow and check totalColllacteral
        uint256 amountEx = _amount.mul(_cost).div(_numberRate);
        require(
            user.totalCollateral.mul(factor).div(base) >= amountEx,
            "Borrow: Total borrow value must be smaller than 80% total collateral supply "
        );
        require(pool.liquidity > _amount, "Borrow: Not enought");
        require(supportTokens[_tokenAddress], "This token is not supported");

        if (_tokenAddress == address(0)) {
            user.amountToken[_tokenAddress].amount_borrow = user
                .amountToken[_tokenAddress]
                .amount_borrow
                .add(_amount);
            (bool sent, ) = msg.sender.call{value: _amount}("");
            require(sent, "Transfer failed");
        } else {
            user.amountToken[_tokenAddress].amount_borrow = user
                .amountToken[_tokenAddress]
                .amount_borrow
                .add(_amount);
            bool sent = IERC20(_tokenAddress).transfer(msg.sender, _amount);
            require(sent, "Transfer failed");
        }
        user.totalBorrowed = user.totalBorrowed.add(amountEx);
        emit Collacteral(msg.sender, _amount);
    }

    // Pay
    function repay(
        address _tokenAddress,
        uint256 _amount,
        uint256 _cost,
        uint256 _numberRate
    ) external {
        PoolInfo storage pool = poolInfo[_tokenAddress];
        UserInfo storage user = userInfo[msg.sender];

        require(!user.inBlackList, "in black list");
        // calculator balance_pay (add fee)
        uint256 amountPay = _amount.mul(pool.apyBorrow).div(pool.numberRate);
        // exchangeRate tokenBorrow
        uint256 amountEx = amountPay.mul(_cost).div(_numberRate);
        if (_tokenAddress == address(0)) {
            user.amountToken[_tokenAddress].amount_borrow = user
                .amountToken[_tokenAddress]
                .amount_borrow
                .sub(amountPay);
            (bool sent, ) = msg.sender.call{value: amountPay}("");
            require(sent, "Transfer failed");
        } else {
            user.amountToken[_tokenAddress].amount_borrow = user
                .amountToken[_tokenAddress]
                .amount_borrow
                .sub(amountPay);
            bool sent = IERC20(_tokenAddress).transfer(msg.sender, amountPay);
            require(sent, "Transfer failed");
        }
        user.totalBorrowed = user.totalBorrowed.sub(amountEx);
        emit Repay(msg.sender, amountPay);
    }

    // withdraw collacteral
    function withdrawCollacteral(
        address _tokenAddress,
        uint256 _amount,
        uint256 _cost,
        uint256 _numberRate
    ) external {
        UserInfo storage user = userInfo[msg.sender];
        require(!user.inBlackList, "in black list");
        require(
            user.amountToken[_tokenAddress].amount_collateral >= _amount,
            "withdrawCollacteral: Not enought"
        );
        user.amountToken[_tokenAddress].amount_collateral = user
            .amountToken[_tokenAddress]
            .amount_collateral
            .sub(_amount);
        user.totalCollateral = user.totalCollateral.sub(
            _amount.mul(_cost).div(_numberRate)
        ); // exchangeRate => DAI
        emit WithdrawCollateral(msg.sender, _amount);
    }

    // Withdraw tokens from STAKING.
    function withdraw(address _tokenAddress, uint256 _amount) external {
        PoolInfo storage pool = poolInfo[_tokenAddress];
        UserInfo storage user = userInfo[msg.sender];
        uint256 amountReward = _amount.add(
            _amount.mul(pool.apyReward).div(pool.numberRate)
        );
        require(
            user.amountToken[_tokenAddress].amount_deposite.sub(
                user.amountToken[_tokenAddress].amount_collateral
            ) > 0,
            "withdraw: not enough"
        );

        if (_tokenAddress == address(0)) {
            user.amountToken[_tokenAddress].amount_deposite = user
                .amountToken[_tokenAddress]
                .amount_deposite
                .sub(_amount);
            (bool sent, ) = msg.sender.call{value: amountReward}(""); // send balance + reward
            require(sent, "Transfer failed");
        } else {
            user.amountToken[_tokenAddress].amount_deposite = user
                .amountToken[_tokenAddress]
                .amount_deposite
                .sub(_amount);
            bool sent = IERC20(_tokenAddress).transfer(
                msg.sender,
                amountReward
            ); // send balance + reward
            require(sent, "Transfer failed");
        }
        pool.liquidity = pool.liquidity.sub(amountReward); // sub amountReward
        // push event
        emit Withdraw(msg.sender, _amount);
    }
}

