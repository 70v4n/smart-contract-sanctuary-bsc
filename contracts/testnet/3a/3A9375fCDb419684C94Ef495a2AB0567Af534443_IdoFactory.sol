pragma solidity ^0.8.0;

import "@openzeppelin/contracts/utils/Create2.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./ProjectFactory.sol";
import "./TicketFactory.sol";

contract IdoFactory is Ownable {

    mapping(uint => address) public projects;
    mapping(uint => address) public nftProjects;
    mapping(address => bool) public operators;
    ProjectFactory public projectFactory;
    TicketFactory public ticketFactory;

    modifier onlyOperator() {
        require(operators[msg.sender], "IdoFactory: only operator");
        _;
    }
    event ProjectCreated(uint indexed projectId, address indexed contractAddress);
    event ProjectNFTCreated(uint indexed projectId, address indexed contractAddress);
    event ChangeOperator(address indexed _newOperator, bool indexed _status);

    constructor() {
        operators[msg.sender] = true;
    }

    function setFactory(ProjectFactory _projectFactory, TicketFactory _ticketFactory) external onlyOwner {
        require(address(_projectFactory)  != address(0), "IdoFactory: input zero _projectFactory");
        require(address(_ticketFactory)  != address(0), "IdoFactory: input zero _ticketFactory");
        projectFactory = _projectFactory;
        ticketFactory = _ticketFactory;
    }

    function setOperator(address _newOperator, bool _status) external onlyOwner {
        require(_newOperator != address(0), "IdoFactory: input zero");
        operators[_newOperator] = true;
        emit ChangeOperator(_newOperator, _status);
    }

    function createProject(uint _projectId, address _operator) external onlyOperator {
        require(_operator != address(0), "!zero");
        require(projects[_projectId] == address(0), "Duplicate project id");
        address _projectAddress = projectFactory.createProject(_projectId, _operator);
        projects[_projectId] = _projectAddress;
        emit ProjectCreated(_projectId, _projectAddress);
    }

    function createNFTTicket(uint _projectId, address _operator, string memory uri_, uint256 _numberOfTypes, uint256[] memory maxSupply_, uint256 maxBuyForUser_) external onlyOperator {
        require(_operator != address(0), "!zero");
        require(projects[_projectId] != address(0), "Need to have project id");
        require(nftProjects[_projectId] == address(0), "Already deployed nftproject with id");
        // address _project, address _operator, string memory uri_, uint256 _numberOfTypes, uint256[] memory maxSupply_, uint256 maxBuyForUser_
        address ticketAddress = ticketFactory.createNFTTicket( projects[_projectId], _operator, uri_, _numberOfTypes, maxSupply_, maxBuyForUser_);
        nftProjects[_projectId] = ticketAddress;
        emit ProjectNFTCreated(_projectId, ticketAddress);
    }

    receive() external payable {
        revert("Nothing send to here");
    }
}

