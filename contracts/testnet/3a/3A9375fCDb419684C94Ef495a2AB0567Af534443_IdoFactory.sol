contract IdoProject is ReentrancyGuard, Initializable {
    using EnumerableSet for EnumerableSet.AddressSet;
    using EnumerableSet for EnumerableSet.UintSet;
    using SafeERC20 for IERC20;
    using ECDSA for bytes32;
    using ECDSA for bytes;


    struct Cliff {
        uint timeStamp;
        uint percentage;
    }

    struct ClaimInfo {
        uint8 lastClaim;
        uint totalToken;
        uint totalTokenClaimed;
        bool initialize;
        bool finish;
    }

    //constant
    uint public constant ONE_HUNDRED_PERCENT = 10000;

    // address verify
    address public admin;

    //setting
    uint public projectId;
    uint public startTimeCommunity;
    uint public endTimeCommunity;
    uint public communityRate; // multiple with decimals token
    uint public communitySlot;

    //vesting
    Cliff[] public vestingInfoCommunity;

    //info
    IERC20 public currency;
    IERC20 public tokenSale;

    // storage
    mapping(address=> ClaimInfo) public claimInfoCommunity; // user address => ClaimInfo data
    mapping(address => uint) public commitCommunity;  // user address => committed amount

    //list user
    EnumerableSet.AddressSet community; // Danh sách những người đã tham gia trong pool xổ số
    EnumerableSet.AddressSet winnerCommunity;

    // allocation
    uint public maxAllocationCommunity;


    //control
    bool public communityEnd = false;
    uint public currentCheck = 0;
    bool public withdrawCommunity = false;
    bool public settingProject = false;
    uint public randomSeed = 0;
    uint private _entropy =0;

    //operator
    address public operator;

