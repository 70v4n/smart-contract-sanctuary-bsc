    address public nftTicket;
    uint public timeStart;
    uint public timeEnd;
    uint public totalReceive;
    mapping(uint => uint) public prices;


    //===================EVENTS===================//
    modifier onlyOperator() {
        require(msg.sender == operator, "!Operator");
        _;
    }

    modifier onCommunityPoolSale() {
        require(block.timestamp >= startTimeCommunity && block.timestamp <= endTimeCommunity, "!Time community pool");
        require(!community.contains(msg.sender), "User has registered");
        _;
    }

    modifier afterCommunitySale() {
        require(block.timestamp > endTimeCommunity, "!After community pool");
        require(!communityEnd, "!communityEnd");
        _;
    }

    modifier checkSigner(bytes memory _signature) {
        bytes32 _hash = keccak256(abi.encode(projectId, msg.sender, address(this)));
        require(_hash.recover(_signature) == admin, "!verify");
        _;
    }

    modifier isWithdrawable(uint8 _pool) {
        bool checkCommunity = block.timestamp > endTimeCommunity && communityEnd && !winnerCommunity.contains(msg.sender);
        require(_pool <=2 , "_pool not support");
        require(checkCommunity , "!checkCommunity");
        _;
    }

    modifier isClaimable(uint8 _pool) {
        bool checkCommunity = block.timestamp > endTimeCommunity && communityEnd && winnerCommunity.contains(msg.sender);
        require(checkCommunity , "!checkCommunity");
        _;
    }

    modifier isWithdrawableAdmin() {
        bool checkCommunity = block.timestamp > endTimeCommunity && communityEnd && !withdrawCommunity;
        require(checkCommunity , "!checkCommunity");
        _;
    }

    modifier onSaleTicket() {
        require(block.timestamp > timeStart && block.timestamp <= timeEnd, "Not in time sale nft");
        _;
    }
    //===================EVENTS===================
    event Setting(uint indexed projectId);
    event SettingINO(address indexed ticketAddress);
    event BuyNFTSuccess(address indexed user, uint256 indexed nftType, uint256 indexed amount);
    event ProjectCreated(uint indexed projectId, address indexed contractAddress);
    event RegisterCommunity(address indexed user, uint indexed projectId);
    event Withdraw(address indexed user, uint indexed amount, uint indexed projectId);
    event Claim(address indexed user, uint8 indexed pool, uint indexed projectId, uint amount);
    event WinnerCommunity(address indexed user,uint indexed projectId);

    // initialize
    function initialize(uint _projectId, address _operator) public initializer returns(bool) {
        require(_operator != address(0), "!zero");
        projectId = _projectId;
        operator = _operator;
        emit ProjectCreated(projectId, address(this));
        return true;
    }


    //====================== Internal =====================//

    function _initializeClaimInfo() internal {
        ClaimInfo storage _data = claimInfoCommunity[msg.sender];
        require(!_data.initialize && winnerCommunity.contains(msg.sender), "initialized");
        _data.totalToken = maxAllocationCommunity / communityRate;
        _data.initialize = true;
    }

    function _min(uint256 a, uint256 b) internal pure returns (uint256) {
        return a < b ? a : b;
    }

    //=================== External ===================== //
    function registerCommunity(bytes memory _signature) external onCommunityPoolSale checkSigner(_signature) {
        currency.safeTransferFrom(msg.sender, address(this), maxAllocationCommunity);
        commitCommunity[msg.sender] = maxAllocationCommunity;
        community.add(msg.sender);
        emit RegisterCommunity(msg.sender, projectId);
    }


    //TODO: implement random
    function _addRandomIndex(uint256 _range, uint256 _count) internal returns (uint256, bool _finish){
        uint i = 0;
        uint256 _index = 0;
        _finish = true;
        do {
            bytes32 _hash = keccak256(abi.encode(projectId, _entropy, randomSeed));
            _index = uint(_hash) % _range;
            _entropy ++;
            ++i;
            if(i > _count) {
               _finish = false;
                break;
            }
        } while (!winnerCommunity.add(community.at(_index)));
        return (i, _finish);
    }

    function announceCommunity(uint _count) external onlyOperator afterCommunitySale{
        if(randomSeed == 0) {
            randomSeed = Random.computerSeed(block.number);

        }
        if(_entropy == 0) {
            _entropy = block.timestamp;
        }
        uint256 i = 0;
        uint256 _winnerLength = winnerCommunity.length();
        uint256 _poolLength = community.length();
        while( i < _count || _winnerLength <= communitySlot) {
            (uint _step, bool _finish)= _addRandomIndex(_poolLength, _count - i);
            i += _step;
            if(!_finish) {
                break;
            }
            _winnerLength ++;
        }
        if(_winnerLength >= _min(communitySlot, _poolLength)) {
            communityEnd = true;
            emit WinnerCommunity(msg.sender ,projectId);
        }
    }

    //_pool: 0 - all, 1 - pioneer, 2 - community
    function withdraw(uint8 _pool) external isWithdrawable(_pool) nonReentrant {
        uint _amount = 0;
        _amount = commitCommunity[msg.sender];
        commitCommunity[msg.sender] = 0;
        currency.safeTransfer(msg.sender, _amount);
        emit Withdraw(msg.sender, _amount, projectId);
    }

    function claim(uint8 _pool) external isClaimable(_pool) nonReentrant {
        uint _amount = 0;
        if(!claimInfoCommunity[msg.sender].initialize) {
            _initializeClaimInfo();
        }
        ClaimInfo storage _community = claimInfoCommunity[msg.sender];
        (,bool _finish,bool _claimable,uint _totalClaim,uint8 _lastClaim) = getClaimInfo(_community.totalToken, _community.lastClaim);
        require(_claimable, "!Claimable");
        _amount = _totalClaim;
        if(_finish) {
            _community.finish = _finish;
            _amount = _community.totalToken - _community.totalTokenClaimed;
        }
        _community.lastClaim = _lastClaim;
        _community.totalTokenClaimed += _amount;
        require(_community.totalTokenClaimed <= _community.totalToken, "Overflow claimable");
        tokenSale.safeTransfer(msg.sender, _amount);
        emit Claim(msg.sender, _pool, projectId, _amount);
    }

    function withdrawAdmin() external onlyOperator isWithdrawableAdmin() nonReentrant{
        uint _amount = 0;
        _amount = winnerCommunity.length() * maxAllocationCommunity;
        withdrawCommunity = true;
        currency.safeTransfer(msg.sender, _amount);
        emit Withdraw(msg.sender, _amount, projectId);
    }

    function withdrawINO() external onlyOperator nonReentrant {
        uint _amount = totalReceive;
        totalReceive = 0;
        currency.safeTransfer(msg.sender, _amount);
        emit Withdraw(msg.sender, _amount, projectId);
    }

    function setting(
        uint[] memory _dataUint,
        IERC20[] memory _dataErc20,
        uint[] memory _communityTimestamps,
        uint[] memory _communityPercentages,
        address _admin
    ) external onlyOperator {
        require(!settingProject, "Project setting");
        require(_dataUint.length == 5, "Not enough ");
        require(_dataUint[0] > block.timestamp, "_startTimeCommunity in pass");
        require(_dataUint[0] < _dataUint[1], "_startTimeCommunity < _endTimeCommunity");
        require(_dataUint[2] > 0 , "Rate can not zero");
        require(_dataUint[3] > 0 , "slot can not zero");
        require(_dataUint[4] > 0 , "allocation can not zero");


        require(address(_dataErc20[0]) != address(0), "!zero");
        require(address(_dataErc20[1]) != address(0), "!zero");
        require(address(_admin) != address(0), "!zero");

        startTimeCommunity = _dataUint[0];
        endTimeCommunity = _dataUint[1];
        communityRate = _dataUint[2];
        communitySlot = _dataUint[3];
        maxAllocationCommunity = _dataUint[4];

        currency = _dataErc20[0];
        tokenSale = _dataErc20[1];
        admin = _admin;
        _setCliffInfo(_communityTimestamps, _communityPercentages);
        settingProject = true;
        emit Setting(projectId);
    }

    function settingINO(address _nftTicket, uint _timeStart, uint _timeEnd, uint[] memory _prices) external onlyOperator {
        require(timeStart > 0 && timeEnd > timeStart, "!Time");
        require(IIdoTicket(_nftTicket).totalType() == _prices.length, "!Prices");
        nftTicket = _nftTicket;
        timeStart = _timeStart;
        timeEnd = _timeEnd;
        for(uint i = 1; i <= _prices.length; i++ ) {
            require(_prices[i-1] > 0, "price can not zero");
            prices[i] = _prices[i-1];
        }
        emit SettingINO(_nftTicket);
    }

    function buyNFTTicket(uint256 _type, uint256 _number) public onSaleTicket {
        require(_type > 0 && _type <= IIdoTicket(nftTicket).totalType(), "Type not support");
        require(_number > 0, "Number at least 1");
        uint _price = prices[_type];
        uint totalSend = _price * _number;
        //receive amount buy
        currency.safeTransferFrom(msg.sender, address(this), totalSend);
        totalReceive += totalSend;

        //mint ticket
        IIdoTicket(nftTicket).mint(msg.sender, _type, _number, "");

        emit BuyNFTSuccess(msg.sender, _type, _number);
    }

    function setOperator(address _newOperator) external onlyOperator {
        require(_newOperator != address(0), "!zero");
        operator = _newOperator;
    }

    function _setCliffInfo(uint[] memory _timestamps, uint[] memory _percentages) internal onlyOperator {
        require(_timestamps.length == _percentages.length , "length must be equal");
        uint256 sum;
        for(uint256 i = 0; i < _timestamps.length; i ++) {
            require(_percentages[i] <= ONE_HUNDRED_PERCENT, "percentage over 100 %");
            Cliff memory _cliffInfo;
            _cliffInfo.percentage = _percentages[i];
            _cliffInfo.timeStamp = _timestamps[i];
            vestingInfoCommunity.push(_cliffInfo);
            sum += _percentages[i];
        }
        require(sum == ONE_HUNDRED_PERCENT, "total percentage is not 100%");
    }

    function updateCliffInfo(uint[] memory _timestamps, uint[] memory _percentages, bool _deleted) external onlyOperator {
        require(_timestamps.length == _percentages.length , "length must be equal");
        require(block.timestamp < startTimeCommunity, "Can not change after _pool start");
        if(_deleted) {
            delete vestingInfoCommunity;
            _setCliffInfo(_timestamps, _percentages);
        } else {
            require(_timestamps.length == vestingInfoCommunity.length, "!length");
            for(uint i =0; i < vestingInfoCommunity.length; i++) {
                vestingInfoCommunity[i].timeStamp = _timestamps[i];
                vestingInfoCommunity[i].percentage = _percentages[i];
            }
        }
    }

    //=============== External ==================//
    function getClaimInfo(uint256 _totalToken, uint8 _claimTimes) public view returns (uint amountClaim, bool finish, bool claimable, uint totalClaim, uint8 lastCliff) {
        (uint _timestamp,) = getCliffInfo(_claimTimes);
        claimable = _timestamp <= block.timestamp;
        lastCliff = _claimTimes;
        uint totalPercentage = 0;
        finish = false;
        amountClaim = 0;
        totalClaim = 0;
        for(uint i = _claimTimes;i < vestingInfoCommunity.length; i++) {
            if(vestingInfoCommunity[i].timeStamp <= block.timestamp) {
                totalPercentage += vestingInfoCommunity[i].percentage;
                lastCliff += 1;
            }
        }
        amountClaim = vestingInfoCommunity[_claimTimes].percentage * _totalToken / ONE_HUNDRED_PERCENT;
        totalClaim = _totalToken * totalPercentage / ONE_HUNDRED_PERCENT;
        if(lastCliff == vestingInfoCommunity.length) {
            finish = true;
        }
    }

    function getCliffInfo(uint256 _index) public view returns (uint256 _percentage, uint256 _timestamp) {
        if(_index < vestingInfoCommunity.length) {
            Cliff memory _cliffInfo = vestingInfoCommunity[_index];
            _percentage = _cliffInfo.percentage;
            _timestamp = _cliffInfo.timeStamp;
        }
    }


    function getCommunity(uint256 _page, uint256 _limit) external view returns (address[] memory, uint _length) {
        uint _from = _page * _limit;
        _length = community.length();
        uint _to = _min((_page + 1) * _limit, community.length());
        address[] memory _result = new address[](_to - _from);
        for(uint i = 0; _from < _to; i++){
            _result[i] = community.at(_from);
            ++_from;
        }
        return (_result, _length);
    }


    function getWinnerCommunity(uint256 _page, uint256 _limit) external view returns (address[] memory, uint _length) {
        uint _from = _page * _limit;
        _length = winnerCommunity.length();
        uint _to = _min((_page + 1) * _limit, winnerCommunity.length());
        address[] memory _result = new address[](_to - _from);
        for(uint i = 0; _from < _to; i++){
            _result[i] = winnerCommunity.at(_from);
            ++_from;
        }
        return (_result, _length);
    }

    function checkUserInPool(address _user) external view returns(bool, bool) {
        return (community.contains(_user), winnerCommunity.contains(_user));
    }

    function getProgress() external view returns(uint _numerator, uint _denominator) {
        _numerator = 0;
        _denominator = 1;
        if(communityRate > 0) {
            _numerator = winnerCommunity.length() * maxAllocationCommunity / communityRate;
            _denominator = communitySlot * maxAllocationCommunity / communityRate;
        }
    }

    function verifySignature(bytes memory _signature) external view returns (bool) {
        bytes32 _hash = keccak256(abi.encode(projectId, msg.sender, address(this)));
        return _hash.recover(_signature) == admin;
    }
}

