library Levels {
    function getLevel(uint256 a) public pure returns (uint256) {
        if (a > 750 && a < 1250) {
            return 1;
        } else if (a > 1250 && a < 2500) {
            return 2;
        } else if (a > 2500 && a < 5000) {
            return 3;
        } else {
            return 0;
        }
    }
}

