pragma solidity ^0.8.9;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "hardhat/console.sol";

// Internal Imports
import "./interface/IRevoStaking.sol";
import "./interface/IRevoLauncher.sol";
import "./interface/IChainlink.sol";
import "./helpers/Error.sol";
import "./helpers/Levels.sol";
import {SaleInfo, StakingInfo, TierInfo, WhiteListInfo} from "./types/Type.sol";

/// @title RevoLauncher
/// @author Nodeberry (P) Ltd.,
/// @dev inherits from {IRevoLauncher}, implements the launchpad functionality.

contract RevoLauncher is IRevoLauncher, Ownable, ReentrancyGuard {
    /// @dev represents the total sales created
    uint256 public sales;

    /// @dev represents the staking address
    IRevoStaking public staking;

    /// @dev represents the sale info mapped to saleId
    mapping(uint256 => SaleInfo) public sale;

    /// @dev maps the saleId to whitelisted amount
    mapping(uint256 => mapping(address => uint256)) public allocation;

    /// @dev maps the saleId to whitelisted amount
    mapping(address => mapping(uint256 => uint256)) public unclaimed;

    /// @dev maps the purchase of user tokens during staking sale
    mapping(uint256 => mapping(address => uint256)) public stakeSalePurchase;

    /// @dev maps the purchase of user tokens in public pool
    mapping(address => mapping(uint256 => uint256)) public publicPurchase;

    /// @dev represents TierLevels info mapped to project id/sale id

    mapping(uint256 => TierInfo[5]) public TierLevelPerProject;

    /// @dev represents the oracle of BNB
    IChainlink public oracle;

    /// @dev is the address of BUSD
    address public busd;

    constructor(address _busd) {
        busd = _busd;
    }

    /// @dev see {IRevoLauncher-createSale}
    function createSale(
        address saleAddress,
        uint256 tokenDecimal,
        uint256 saleAmount,
        uint256 publicAllocation,
        uint256 costPerToken,
        uint256[3] memory time,
        uint256 stakePoolId,
        uint256 publicPerWallet,
        bool vested,
        bool whitelist
    ) public virtual override onlyOwner nonReentrant returns (bool) {
        sales += 1;
        require(
            saleAmount - publicAllocation > 0,
            Error.VE_ERROR_INPUT_VALIDATION
        );
        require(
            IERC20(saleAddress).balanceOf(_msgSender()) >= saleAmount,
            Error.VE_INSUFFICIENT_BALANCE
        );
        require(
            IERC20(saleAddress).allowance(_msgSender(), address(this)) >=
                saleAmount,
            Error.VE_INSUFFICIENT_ALLOWANCE
        );

        SaleInfo storage s = sale[sales];
        s.tokenAddress = saleAddress;
        s.totalAllocated = saleAmount;
        s.publicAllocated = publicAllocation;
        s.costPerToken = costPerToken;
        s.time = time;
        s.stakePoolId = stakePoolId;
        s.whitelist = whitelist;
        s.tokenDecimal = tokenDecimal;
        s.vested = vested;
        s.publicPerWallet = publicPerWallet;

        IERC20(saleAddress).transferFrom(
            _msgSender(),
            address(this),
            saleAmount
        );

        emit SaleCreated(
            saleAddress,
            tokenDecimal,
            saleAmount,
            publicAllocation,
            costPerToken,
            time[0],
            time[1],
            stakePoolId,
            whitelist
        );
        return true;
    }

    /// @dev see {RevolLauncher-publicPurchaseWithBNB}
    function publicPurchaseWithBNB(uint256 saleId)
        public
        payable
        virtual
        override
        nonReentrant
        returns (bool)
    {
        uint256 purchaseValue = usdValue(msg.value) * 10**18;
        SaleInfo storage s = sale[saleId];
        require(
            block.timestamp >= s.time[0] && block.timestamp <= s.time[1],
            Error.VE_SALE_OUT_OF_BOUND
        );
        uint256 purchaseTokens = purchaseValue / s.costPerToken;
        purchaseTokens = purchaseTokens / 10**(18 - s.tokenDecimal);
        require(
            publicPurchase[_msgSender()][saleId] + purchaseTokens <=
                s.publicPerWallet,
            Error.VE_UNBOUNDED_PURCHASE_LIMIT
        );
        if (s.time[1] - block.timestamp < 3600 seconds) {
            require(
                s.totalAllocated - s.totalSold >= purchaseTokens,
                Error.VE_INSUFFICIENT_TOKENS_LEFT_FOR_SALE
            );
            s.totalSold += purchaseTokens;
            publicPurchase[_msgSender()][saleId] += purchaseTokens;
            return
                settleTokens(
                    s.vested,
                    s.time[2],
                    _msgSender(),
                    s.tokenAddress,
                    purchaseTokens,
                    saleId
                );
        } else {
            require(
                s.publicAllocated >= purchaseTokens,
                Error.VE_INSUFFICIENT_TOKENS_LEFT_FOR_SALE
            );
            s.publicAllocated -= purchaseTokens;
            s.totalSold += purchaseTokens;
            publicPurchase[_msgSender()][saleId] += purchaseTokens;
            return
                settleTokens(
                    s.vested,
                    s.time[2],
                    _msgSender(),
                    s.tokenAddress,
                    purchaseTokens,
                    saleId
                );
        }
    }

    /// @dev see {RevolLauncher-allocatedPurchaseWithBNB} //
    /// If no whitelist and no stake pool created, function throw divide by zero.
    function allocatedPurchaseWithBNB(uint256 saleId)
        public
        payable
        virtual
        override
        nonReentrant
        returns (bool)
    {
        uint256 purchaseValue = usdValue(msg.value) * 10**18;
        SaleInfo storage s = sale[saleId];
        require(
            block.timestamp >= s.time[0] && block.timestamp <= s.time[1],
            Error.VE_SALE_OUT_OF_BOUND
        );

        uint256 purchaseTokens = purchaseValue / s.costPerToken;
        purchaseTokens = purchaseTokens / 10**(18 - s.tokenDecimal);

        if (s.whitelist) {
            require(
                allocation[saleId][_msgSender()] >= purchaseTokens,
                Error.VE_INSUFFICIENT_ALLOCATION
            );
            s.totalSold += purchaseTokens;
            allocation[saleId][_msgSender()] -= purchaseTokens;
            return
                settleTokens(
                    s.vested,
                    s.time[2],
                    _msgSender(),
                    s.tokenAddress,
                    purchaseTokens,
                    saleId
                );
        } else {
            StakingInfo memory info = staking.stakeInfo(
                _msgSender(),
                s.stakePoolId
            );
            /*
            PoolInfo memory pinfo = staking.fetchPool(s.stakePoolId);

            //uint256 percent = (pinfo.totalStaked * 10**18) / info.amount;
            uint256 percent = (info.amount * 10**6) / pinfo.totalStaked;
            uint256 allocated = (s.totalAllocated - s.publicAllocated) *
                percent;
            allocated = allocated / 10**6;
            */
            uint256 allocatedUsd = getAllocationPerStake(saleId, info.amount);
            uint256 allocatedPurchaseValue = usdValue(allocatedUsd) * 10**18;
            uint256 allocatedTokens = allocatedPurchaseValue / s.costPerToken;

            //uint256 allocated = getAllocationPerStake(saleId, info.amount);
            require(
                allocatedTokens >=
                    purchaseTokens + stakeSalePurchase[saleId][_msgSender()],
                Error.VE_INSUFFICIENT_ALLOCATION
            );

            s.totalSold += purchaseTokens;
            stakeSalePurchase[saleId][_msgSender()] += purchaseTokens;
            return
                settleTokens(
                    s.vested,
                    s.time[2],
                    _msgSender(),
                    s.tokenAddress,
                    purchaseTokens,
                    saleId
                );
        }
    }

    /// @dev see {RevolLauncher-publicPurchaseWithBUSD}
    function publicPurchaseWithBUSD(uint256 saleId, uint256 purchaseValue)
        public
        virtual
        override
        nonReentrant
        returns (bool)
    {
        uint256 busdValue = purchaseValue * 10**10;

        require(
            IERC20(busd).balanceOf(_msgSender()) >= busdValue,
            Error.VE_INSUFFICIENT_BALANCE
        );
        require(
            IERC20(busd).allowance(_msgSender(), address(this)) >= busdValue,
            Error.VE_INSUFFICIENT_ALLOWANCE
        );

