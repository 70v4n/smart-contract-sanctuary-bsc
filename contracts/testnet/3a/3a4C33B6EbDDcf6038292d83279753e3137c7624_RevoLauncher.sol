    /// @dev settles the tokens to the users.
    function settleTokens(
        bool vested,
        uint256 claimTime,
        address user,
        address token,
        uint256 purchased,
        uint256 saleId
    ) private returns (bool) {
        if (!vested) {
            if (block.timestamp > claimTime) {
                IERC20(token).transfer(user, purchased);
            } else {
                unclaimed[user][saleId] += purchased;
            }
        }
        emit Purchase(user, saleId, purchased);
        return true;
    }

    /// @dev returns the USD equivalent value of BNB sent via tx
    function usdValue(uint256 bnbValue) private view returns (uint256) {
        (, int256 b, , , ) = oracle.latestRoundData();
        uint256 price = uint256(b);
        return ((price * bnbValue) / 10**18);
    }

    /// @dev see {RevolLauncher-withdrawTokens}
    function withdrawTokens(uint256 saleId)
        public
        virtual
        override
        onlyOwner
        returns (bool)
    {
        SaleInfo storage s = sale[saleId];
        require(s.time[1] < block.timestamp, Error.VE_SALE_NOT_ENDED);

        require(s.totalAllocated > s.totalSold, Error.VE_NO_VALUE_TO_CLAIM);
        IERC20(s.tokenAddress).transfer(
            owner(),
            s.totalAllocated - s.totalSold
        );

        return true;
    }

    /// @dev see {RevoLauncher-setStakingContract}
    function setStakingContract(address stakingAddress)
        public
        virtual
        override
        onlyOwner
        returns (bool)
    {
        require(stakingAddress != address(0), Error.VE_ZERO_ADDRESS);
        staking = IRevoStaking(stakingAddress);

        return true;
    }

    /// @dev see {RevoLauncher-setStakingContract}
    function setOracleContract(address oracleAddress)
        public
        virtual
        override
        onlyOwner
        returns (bool)
    {
        require(oracleAddress != address(0), Error.VE_ZERO_ADDRESS);
        oracle = IChainlink(oracleAddress);

        return true;
    }

    /// @dev see {RevoLauncher-whitelistUser}
    function whitelistUser(uint256 saleId, WhiteListInfo[] memory _usrAddrAmt)
        public
        onlyOwner
        returns (bool)
    {
        for (uint256 i = 0; i < _usrAddrAmt.length; i++)
            allocation[saleId][_usrAddrAmt[i].userAddress] = _usrAddrAmt[i]
                .purchaseAmount;
        return true;
    }

    /// @dev setTierLevel ;
    //[[5,10],[10,100],[15,150],[20,250],[50,1000]]
    function setTierLevel(uint256 _pId, TierInfo[5] memory _tierInfo)
        external
        onlyOwner
    {
        require(_tierInfo.length == 5, "Invalid Tier Level Inputs");
        for (uint256 i = 0; i < _tierInfo.length; i++)
            TierLevelPerProject[_pId][i] = (_tierInfo[i]);
    }

    /// @dev compute Allocation as per Stake, array of max token allocated per Tier and not usd
    function getAllocationPerStake(uint256 _pId, uint256 _stkAmt)
        public
        view
        returns (uint256)
    {
        if (_stkAmt >= TierLevelPerProject[_pId][4].tierMinStakeAmount)
            return TierLevelPerProject[_pId][4].tierAllocation;
        //tier 5
        else if (
            _stkAmt >= TierLevelPerProject[_pId][3].tierMinStakeAmount &&
            _stkAmt < TierLevelPerProject[_pId][4].tierMinStakeAmount
        ) return TierLevelPerProject[_pId][3].tierAllocation;
        //tier 4
        else if (
            _stkAmt >= TierLevelPerProject[_pId][2].tierMinStakeAmount &&
            _stkAmt < TierLevelPerProject[_pId][3].tierMinStakeAmount
        ) return TierLevelPerProject[_pId][2].tierAllocation;
        //tier 3
        else if (
            _stkAmt >= TierLevelPerProject[_pId][1].tierMinStakeAmount &&
            _stkAmt < TierLevelPerProject[_pId][2].tierMinStakeAmount
        ) return TierLevelPerProject[_pId][1].tierAllocation;
        //tier 2
        else return TierLevelPerProject[_pId][0].tierAllocation; //tier 1
    }
}

