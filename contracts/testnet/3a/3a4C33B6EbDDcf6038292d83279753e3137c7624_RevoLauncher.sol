                s.publicPerWallet,
            Error.VE_UNBOUNDED_PURCHASE_LIMIT
        );

        if (s.time[1] - block.timestamp < 3600 seconds) {
            require(
                s.totalAllocated - s.totalSold >= purchaseTokens,
                Error.VE_INSUFFICIENT_TOKENS_LEFT_FOR_SALE
            );
            s.totalSold += purchaseTokens;
            publicPurchase[_msgSender()][saleId] += purchaseTokens;
            return
                settleTokens(
                    s.vested,
                    s.time[2],
                    _msgSender(),
                    s.tokenAddress,
                    purchaseTokens,
                    saleId
                );
        } else {
            require(
                s.publicAllocated >= purchaseTokens,
                Error.VE_INSUFFICIENT_TOKENS_LEFT_FOR_SALE
            );
            s.publicAllocated -= purchaseTokens;
            s.totalSold += purchaseTokens;
            publicPurchase[_msgSender()][saleId] += purchaseTokens;
            return
                settleTokens(
                    s.vested,
                    s.time[2],
                    _msgSender(),
                    s.tokenAddress,
                    purchaseTokens,
                    saleId
                );
        }
    }

    function getAllocatedTokens(uint256 saleId)
        public
        returns (uint256, uint256)
    {
        SaleInfo storage s = sale[saleId];
        StakingInfo memory info = staking.stakeInfo(
            _msgSender(),
            s.stakePoolId
        );
        uint256 allocatedUsd = getAllocationPerStake(saleId, info.amount);
        uint256 allocatedPurchaseValue = usdValue(allocatedUsd) * 10**18;
        uint256 allocatedTokens = allocatedPurchaseValue / s.costPerToken;
        return (allocatedUsd, allocatedTokens);
    }

