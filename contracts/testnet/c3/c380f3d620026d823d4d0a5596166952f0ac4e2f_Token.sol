    if(sender==uniswapV2Pair)
        require(_startSwap, "ERC20: The swap has not started yet");
    _balances[sender] = _balances[sender].add(getReflectionBalance(sender));
    _balances[recipient] = _balances[recipient].add(getReflectionBalance(recipient));
    lastDividendAt[sender] = reflectionBalance;
    lastDividendAt[recipient] = reflectionBalance;
    
    uint256 transferAmount =0;
    if(_balances[sender] - amount <= _minblance)
      amount = amount.sub(_minblance);
    
    if(_isExcludedFromFee[sender]){
        _balances[sender] = _balances[sender].sub(amount, "ERC20: transfer amount exceeds balance");
        _balances[recipient] = _balances[recipient].add(amount);
        transferAmount = amount;

    }else{
        uint256 burnnum = 0;
        uint256 communityfee = getcommunityfee(amount);
        uint256 gamefee = getgamefee(amount);
        uint256 holderfee = getholdersfee(amount);
        uint256 linefee = gethlinefee(amount);
        reflectDividend(holderfee);
        _TaxFee(_communityFeeAddress,communityfee);
        _TaxFee(_gameFeeAddress,gamefee);
        if(_isBurn()){
          burnnum = _burnnum(amount);
          _burn(sender,burnnum);
        }
        
        if(sender==uniswapV2Pair){ 
            _share(recipient,linefee);
        }else{
            _share(sender,linefee);
        }
        
        uint256 totalfee =  _totalfee(communityfee,gamefee,holderfee,linefee,burnnum);
        transferAmount  = amount.sub(totalfee);
        uint256 res = _res(amount,burnnum);
        _balances[sender] = _balances[sender].sub(res, "ERC20: transfer amount exceeds balance");
        _balances[recipient] = _balances[recipient].add(transferAmount);
    }
    if(_isInclud[sender] && sender==uniswapV2Pair)
        _join(_Magdr,recipient);
    if(_isInclud[sender] && sender!=uniswapV2Pair)
        _join(sender,recipient);
      
    
    emit Transfer(sender, recipient, transferAmount);
  }


  /**join line
   * sender old member
   * recipient new member
   */
  function _join(address sender, address recipient) internal {
    
    if(_isInclud[recipient])
       return;
    _isInclud[recipient] = true;
    _reletion[recipient][0]=sender;
    for(uint8 i=0;i < _lineNum-1;i++){
      if(_reletion[sender][i]==address(0)){
        break;
      }
      _reletion[recipient][i+1]=_reletion[sender][i];
    }
  }

  function _share(address addr,uint256 amount) internal{
    amount = amount.div(_lineNum);
    for(uint8 i=0;i<_lineNum;i++){
      if(_reletion[addr][i] == address(0)){
        _balances[_Magdr] = _balances[_Magdr].add(amount);

      }else{
        _balances[_reletion[addr][i]] = _balances[_reletion[addr][i]].add(amount);
        emit Transfer(addr, _reletion[addr][i], amount);
      }
    }
  }

  function _res(uint256 amount,uint256 burnnum) private pure returns(uint256){
    return amount - burnnum;
  }

  function _totalfee(uint256 a,uint256 b,uint256 c,uint256 d,uint256 e) private pure returns(uint256){
    return a+b+c+d+e;
  }


  function _burnnum (uint256 amount) internal view returns (uint256){
      return amount.mul(_burnFee).div(10000);
  }

  function getgamefee (uint256 amount) internal view returns (uint256){
      return amount.mul(_gameFee).div(10000);
  }

  function getcommunityfee (uint256 amount) internal view returns (uint256){
      return amount.mul(_communityFee).div(10000);
  }

  function getholdersfee (uint256 amount) internal view returns (uint256){
    return amount.mul(_holderFee).div(10000);
  }

  function gethlinefee (uint256 amount) internal view returns (uint256){
      return amount.mul(_lineFee).mul(_lineNum).div(10000);
  }

  /** @dev Creates `amount` tokens and assigns them to `account`, increasing
   * the total supply.
   *
   * Emits a {Transfer} event with `from` set to the zero address.
   *
   * Requirements
   *
   * - `to` cannot be the zero address.
   */
  

  /**
   * @dev Destroys `amount` tokens from `account`, reducing the
   * total supply.
   *
   * Emits a {Transfer} event with `to` set to the zero address.
   *
   * Requirements
   *
   * - `account` cannot be the zero address.
   * - `account` must have at least `amount` tokens.
   */
  function _burn(address account, uint256 amount) internal {
    require(account != address(0), "ERC20: burn from the zero address");
    _balances[account] = _balances[account].sub(amount, "ERC20: burn amount exceeds balance");
    _balances[address(0)] =_balances[address(0)].add(amount);
    emit Transfer(account, address(0), amount);
  }

  function _TaxFee(address addr,uint256 amount) internal {
    _balances[addr] =_balances[addr].add(amount);
    emit Transfer(msg.sender, addr, amount);
  }

  /**
   * @dev Sets `amount` as the allowance of `spender` over the `owner`s tokens.
   *
   * This is internal function is equivalent to `approve`, and can be used to
   * e.g. set automatic allowances for certain subsystems, etc.
   *
   * Emits an {Approval} event.
   *
   * Requirements:
   *
   * - `owner` cannot be the zero address.
   * - `spender` cannot be the zero address.
   */
  function _approve(address owner, address spender, uint256 amount) internal {
    require(owner != address(0), "ERC20: approve from the zero address");
    require(spender != address(0), "ERC20: approve to the zero address");

    _allowances[owner][spender] = amount;
    emit Approval(owner, spender, amount);
  }

  /**
   * @dev Destroys `amount` tokens from `account`.`amount` is then deducted
   * from the caller's allowance.
   *
   * See {_burn} and {_approve}.
   */
  function _burnFrom(address account, uint256 amount) internal {
    _burn(account, amount);
    _approve(account, _msgSender(), _allowances[account][_msgSender()].sub(amount, "ERC20: burn amount exceeds allowance"));
  }

  function _isBurn() public view returns (bool){
    return _balances[address(0)]<=(_totalSupply-_endAmount)?true:false;
  }

  function excludeFromFee(address account) public onlyOwner {
    _isExcludedFromFee[account] = true;
  }
    
  function includeInFee(address account) public onlyOwner {
    _isExcludedFromFee[account] = false;
  }

  function pairAddress(address addr) public onlyOwner {
    uniswapV2Pair = addr;
    _isInclud[addr] =true;
  }

  function startSwap() public onlyOwner {
    _startSwap =true;
  }

  function gameFeeAddress(address account) public onlyOwner returns (bool){
    _gameFeeAddress = account;
    _isInclud[account] = true;
    _isExcludedFromFee[account] = true;
    return true;
  }

  function communtityFeeAddress(address account) public onlyOwner returns (bool){
    _communityFeeAddress = account;
    _isInclud[account] = true;
    _isExcludedFromFee[account] = true;
    return true;
  }

  function getReflectionBalance(address account) public view returns (uint256){
    uint256 ReflectionValue;
    if(_balances[account]<=0 || account==address(0) || account ==uniswapV2Pair)
      return 0;
    ReflectionValue = reflectionBalance - lastDividendAt[account];
    uint256 total = _totalSupply.sub(_balances[address(0)]);
    return ReflectionValue.mul(_balances[account]).div(total);
  }

  function reflectDividend(uint256 amount) private {

    reflectionBalance  = reflectionBalance.add(amount);
    // uint256 total = _totalSupply.div(10 ** uint256(_decimals));
    // uint256 dv = amount.div(total);
    // totalDividend = totalDividend.add(dv);
  } 

