        override(IERC165, ERC721)
        returns (bool)
    {
        return
            interfaceId == type(IERC721Enumerable).interfaceId ||
            super.supportsInterface(interfaceId);
    }

    function tokenOfOwnerByIndex(address owner, uint256 index)
        public
        view
        virtual
        override
        returns (uint256)
    {
        require(
            index < ERC721.balanceOf(owner),
            "ERC721Enumerable: owner index out of bounds"
        );
        return _ownedTokens[owner][index];
    }

    function totalSupply() public view virtual override returns (uint256) {
        return _allTokens.length;
    }

    function tokenByIndex(uint256 index)
        public
        view
        virtual
        override
        returns (uint256)
    {
        require(
            index < ERC721Enumerable.totalSupply(),
            "ERC721Enumerable: global index out of bounds"
        );
        return _allTokens[index];
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual override {
        super._beforeTokenTransfer(from, to, tokenId);

        if (from == address(0)) {
            _addTokenToAllTokensEnumeration(tokenId);
        } else if (from != to) {
            _removeTokenFromOwnerEnumeration(from, tokenId);
        }
        if (to == address(0)) {
            _removeTokenFromAllTokensEnumeration(tokenId);
        } else if (to != from) {
            _addTokenToOwnerEnumeration(to, tokenId);
        }
    }

    function _addTokenToOwnerEnumeration(address to, uint256 tokenId) private {
        uint256 length = ERC721.balanceOf(to);
        _ownedTokens[to][length] = tokenId;
        _ownedTokensIndex[tokenId] = length;
    }

    function _addTokenToAllTokensEnumeration(uint256 tokenId) private {
        _allTokensIndex[tokenId] = _allTokens.length;
        _allTokens.push(tokenId);
    }

    function _removeTokenFromOwnerEnumeration(address from, uint256 tokenId)
        private
    {


        uint256 lastTokenIndex = ERC721.balanceOf(from) - 1;
        uint256 tokenIndex = _ownedTokensIndex[tokenId];

        if (tokenIndex != lastTokenIndex) {
            uint256 lastTokenId = _ownedTokens[from][lastTokenIndex];

            _ownedTokens[from][tokenIndex] = lastTokenId; // Move the last token to the slot of the to-delete token
            _ownedTokensIndex[lastTokenId] = tokenIndex; // Update the moved token's index
        }

        delete _ownedTokensIndex[tokenId];
        delete _ownedTokens[from][lastTokenIndex];
    }

    function _removeTokenFromAllTokensEnumeration(uint256 tokenId) private {
        uint256 lastTokenIndex = _allTokens.length - 1;
        uint256 tokenIndex = _allTokensIndex[tokenId];

        uint256 lastTokenId = _allTokens[lastTokenIndex];

        _allTokens[tokenIndex] = lastTokenId; // Move the last token to the slot of the to-delete token
        _allTokensIndex[lastTokenId] = tokenIndex; // Update the moved token's index

        delete _allTokensIndex[tokenId];
        _allTokens.pop();
    }
}

abstract contract ReentrancyGuard {
    uint256 private constant _NOT_ENTERED = 1;
    uint256 private constant _ENTERED = 2;

    uint256 private _status;

    constructor() {
        _status = _NOT_ENTERED;
    }

    modifier nonReentrant() {
        require(_status != _ENTERED, "ReentrancyGuard: reentrant call");
        _status = _ENTERED;
        _;
        _status = _NOT_ENTERED;
    }
}

contract Plato is ERC721Enumerable, ReentrancyGuard, Ownable {
    using SafeMath for uint256;

    struct castingPriceInfo {
        uint256 maxTotalSupply;
        uint256 price;
    }

    mapping(uint256 => castingPriceInfo) public castingPriceMap;
    uint256 castingPriceMapLength;
    mapping(address => address) public leader;

    address public platoToken;

    mapping(address => uint256) public debt;
    uint256 public income;

    event Grant(address user, uint256 accincome);
    event Withdraw(address user, uint256 pending);

    constructor() ERC721("PLATO", "PLATO", "") Ownable() {
        _setCastingPriceMap(1, 500, 1 * 10**17);
        _setCastingPriceMap(2, 1000, 2 * 10**17);
        _setCastingPriceMap(3, 2000, 4 * 10**17);
        _setCastingPriceMap(4, 5000, 8 * 10**17);
        castingPriceMapLength = 4;
        platoToken = 0xFb37c5531657Bf92796C4b41270149C15Cf336a4;
    }

    function getPrice() public view returns (uint256) {
        for (uint256 i = 1; i <= castingPriceMapLength; i++) {
            if (totalSupply() <= castingPriceMap[i].maxTotalSupply) {
                return castingPriceMap[i].price;
            }
        }
        return 0;
    }

    function casting(address _leader) public payable {
        require(msg.value == getPrice(), "The price is wrong");
        require(totalSupply() < 5000, "no more cards");
        require(leader[msg.sender] == address(0), "already casted");
        if (
            leader[msg.sender] == address(0) &&
            _leader != address(0) &&
            _leader != msg.sender
        ) {
            leader[msg.sender] = _leader;
            _mint(msg.sender, totalSupply() + 500);
        }
    }

    function getIncome() public view returns (uint256) {
        return income.div(5000);
    }

    function withdraw() public {
        require(income > 0, "no income");
        uint256 pending = getIncome();
        IERC20(platoToken).transfer(msg.sender, pending);
        income -= pending;
        emit Withdraw(msg.sender, pending);
    }

    function setPlatoToken(address _platoToken) public onlyOwner {
        platoToken = _platoToken;
    }

    function setCastingPriceMapLength(uint256 _castingPriceMapLength)
        public
        onlyOwner
    {
        castingPriceMapLength = _castingPriceMapLength;
    }

    function setCastingPriceMap(
        uint256 _mapId,
        uint256 _maxTotalSupply,
        uint256 _price
    ) public onlyOwner {
        _setCastingPriceMap(_mapId, _maxTotalSupply, _price);
    }

    function _setCastingPriceMap(
        uint256 _mapId,
        uint256 _maxTotalSupply,
        uint256 _price
    ) internal {
        castingPriceMap[_mapId].maxTotalSupply = _maxTotalSupply;
        castingPriceMap[_mapId].price = _price;
    }

    function grant(uint256 accincome) public onlyOwner {
        income = income.add(accincome);
        emit Grant(msg.sender, accincome);
    }
}