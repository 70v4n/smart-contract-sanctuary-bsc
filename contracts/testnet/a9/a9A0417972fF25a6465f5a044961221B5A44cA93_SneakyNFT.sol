                            if (seed < 2) {
                                sneakyId = 4;
                            }
                        }
                    }
                }
            } else {
                if (seed < 95) {
                    sneakyId = 1;
                    if (seed < 85) {
                        sneakyId = 2;
                        if (seed < 65) {
                            sneakyId = 3;
                            if (seed < 35) {
                                sneakyId = 4;
                            }
                        }
                    }
                }
            }
            if (classSneakySupply[sneakyId].add(1) < MAX_CLASS_SNEAKEYS[sneakyId]) {
                state = true;
            }
            seed = uint8(seed * 2 % 100);
            if (seed == seed_old) seed = random();
        }

        return sneakyId;
    }

    function tokensOfOwner(address _owner) external view returns(uint256[] memory ) {
        uint256 tokenCount = balanceOf(_owner);
        if (tokenCount == 0) {
            // Return an empty array
            return new uint256[](0);
        } else {
            uint256[] memory result = new uint256[](tokenCount);
            uint256 index;
            for (index = 0; index < tokenCount; index++) {
                result[index] = tokenOfOwnerByIndex(_owner, index);
            }
            return result;
        }
    }

    function testrandom() public view returns (uint8[] memory arr){
        uint8[] memory c = new uint8[](10);
        uint8 seed = random();
        uint8 seed_old = uint8(1000);
        for (uint i = 0; i < 10; i++) {
            seed = uint8((seed ** (i + 1))%100);
            if (seed == seed_old) seed = random();
            c[i] = seed;
            seed_old = seed;
        }

        return c;
    }

    function presaleMint(uint256 numberOfTokens, uint256 eggId) public payable nonReentrant {
        require(totalSupply().add(numberOfTokens) < MAX_COLLECTION_COUNT, "Could not mint");
        require(classEggsSupply[eggId].add(numberOfTokens) < MAX_CLASS_EGGS[eggId], "Could not mint");
        require(msg.value >= eggsPresalePrice[eggId].mul(numberOfTokens), "Ether value sent is not correct");

        uint8 seed = random();
        for (uint i = 0; i < numberOfTokens; i++) {
            seed = uint8((seed ** (i + 1))%100);
            uint256 mintIndex = totalSupply();
            uint256 sneakyId = getSneakyIdFromEggId(eggId, seed);
            uint256 classSneakyIndex = classSneakySupply[sneakyId];
            Sneaky memory sneaky;
            if (totalSupply() < MAX_COLLECTION_COUNT) {
                _safeMint(msg.sender, mintIndex);
                classSneakySupply[sneakyId]++;
                classEggsSupply[eggId]++;
                sneaky.classIndex = classSneakyIndex;
                sneaky.classId = sneakyId;
                collects[mintIndex] = sneaky;
            }
        }
    }
    
    /// params eggId: 0~4: snake, 5~7: egg
    function pubsaleMint(uint256 numberOfTokens, uint256 eggId) public payable nonReentrant {
        require(totalSupply().add(numberOfTokens) < MAX_COLLECTION_COUNT, "Could not mint");
        require(classEggsSupply[eggId].add(numberOfTokens) < MAX_CLASS_EGGS[eggId], "Could not mint");
