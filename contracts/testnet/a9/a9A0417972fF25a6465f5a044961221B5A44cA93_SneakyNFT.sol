            uint256 classSneakyIndex = classSneakySupply[sneakyId];
            Sneaky memory sneaky;
            if (totalSupply() < MAX_COLLECTION_COUNT) {
                _safeMint(msg.sender, mintIndex);
                classSneakySupply[sneakyId]++;
                classEggsSupply[eggId]++;
                sneaky.classIndex = classSneakyIndex;
                sneaky.classId = sneakyId;
                collects[mintIndex] = sneaky;
            }
        }
    }

    function random() public view returns (uint8) {
        return uint8(uint256(keccak256(abi.encodePacked(block.timestamp, block.difficulty)))%100);
    }

    function setRoot(bytes32 _root) external onlyOwner {
        root = _root;
    }

    function _leaf(address account) internal pure returns (bytes32)
    {
        return keccak256(abi.encodePacked(account));
    }

    function _verify(bytes32 leaf, bytes32[] memory proof) internal view returns (bool)
    {
        return MerkleProof.verify(proof, root, leaf);
    }
}