pragma solidity ^0.8.10;
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "@openzeppelin/contracts/proxy/utils/Initializable.sol";
import "./interfaces/IStaking.sol";
import "./interfaces/IAdmin.sol";
import "./interfaces/IAirdrops.sol";
import "./interfaces/IERC20D.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "hardhat/console.sol";

// TODO Write function for Emergency withdraw for all types of funds

contract Airdrops is Initializable {
    using SafeERC20 for IERC20D;
    using SafeMath for uint;
    bytes32 public constant OPERATOR = keccak256("OPERATOR");
    bytes32 public constant STAKING = keccak256("STAKING");
    struct Airdrop {
        uint256 totalsupply;
        uint256 tokenstaked;
        uint256 time;
        address token;
        mapping(address => bool) claimed;
    }

    struct userAirDrop {
        uint256 index;
        uint256 startTime;
        uint256 endTime;
    }

    IAdmin public admin;
    IStaking public staking;
    IERC20D public EBSC;

    uint256 public lastEpoch;
    uint256 public epochDuration;

    uint256 public SBNB;
    uint256 public totalstakeBNB;
    uint256 public totalBNB;
    uint256 public valueNotDistribute;

    uint256 public SEBSC;
    uint256 public totalstakeEBSC;
    uint256 public totalEBSC;
    uint256 public pendingEBSC;
    uint256 public ebscStakeholder;

    uint256 public airdropCount;
    uint256 public tokenStakedAirdrop;
    uint256 public bnbStakeholder ;

    uint256 public airdropEligibleTier;
    uint256 public airdropEligibleLevel ;

    bool firstDistribution;

    address public marketingWallet;

    mapping(address => uint256) public s1BNB;
    mapping(address => uint256) public previousRewardBNB;

    mapping(address => uint256) public s1EBSC;
    mapping(address => uint256) public previousRewardEBSC;

    mapping(uint256 => Airdrop) public airdrops;
    mapping(address => userAirDrop) public userAirDrops;

    event ClaimedAirdrop(address token, address receiver, uint256 amount);
    event ClaimedReward(bool isReflection, address receiver, uint256 amount);

    // constructor(
    //     address _staking,
    //     address _admin,
    //     address _EBSC
    // ) {
    //     staking = IStaking(_staking);
    //     admin = IAdmin(_admin);
    //     EBSC = IERC20D(_EBSC);
    // }

    function initialize(
        address _staking,
        address _admin,
        address _EBSC
    ) public initializer {
        staking = IStaking(_staking);
        admin = IAdmin(_admin);
        EBSC = IERC20D(_EBSC);
        ebscStakeholder = 1;
        bnbStakeholder = 7;
        airdropEligibleTier = 3;
        airdropEligibleLevel = 1;
        // _setupRole(STAKING, address(staking));
    }

    receive() external payable {}

    function checkEpoch() public view returns (bool) {
        return (lastEpoch + epochDuration <= block.timestamp);
    }

    function setEpoch() public {
        require(checkEpoch(), "Cant distribute");
        lastEpoch = block.timestamp;
    }

    function setEpochDuration(uint256 _time) external onlyOperator{
        require(_time > 60, "time should be greater than 1 min");
        epochDuration = _time;
    }

    function setBnbStakeholders(uint256 _tier) external onlyOperator {
        bnbStakeholder = _tier;
    }

    function setEbscStakeholders(uint256 _level) external onlyOperator {
        ebscStakeholder = _level;
    }

    function setAirdropsStakeholders(uint256 _tier, uint256 _level)
        external
        onlyOperator
    {
        airdropEligibleLevel = _level;
        airdropEligibleTier = _tier;
    }

    function viewBalance() external view returns (uint256) {
        return address(this).balance;
    }

    function getReflection() public view returns (uint256) {
        return (EBSC.balanceOf(address(this)) - totalEBSC);
    }

    //add onlyAdmin modifier
    function withdrawEBSC(address user, uint256 _amount) public onlyStaking{
        totalstakeEBSC = totalstakeEBSC.sub(_amount,"totalstakedEBSC - amount fail");
        s1EBSC[user] = SEBSC;
    }

    function distributionEBSC(uint256 amount) public {
        if (amount != 0) {
            if (totalstakeEBSC != 0) {
                EBSC.safeTransferFrom(
                    marketingWallet,
                    address(this),
                    amount / 2
                );

                totalEBSC += (amount);
                SEBSC += ((amount * 10**18) / totalstakeEBSC);
            }
        }
    }

    //add only admin modifier
    function setShareForEBSCReward(address user, uint256 _amount) public onlyStaking{
        totalstakeEBSC += _amount;
        s1EBSC[user] = SEBSC;
    }

    function userPendingEBSC(address user) public onlyStaking{
        uint256 amount;
        (, , amount, ) = staking.getUserState(user);
        previousRewardEBSC[user] += (amount * (SEBSC - s1EBSC[user])) / 10**18;
        s1EBSC[user] = SEBSC;
    }

    function getEBSCReward(
        address user,
        uint256 amount,
        uint256 lock
    ) public view returns (uint256) {
        if (lock <= ebscStakeholder) {
            return (previousRewardEBSC[user]);
        } else {
            return (previousRewardEBSC[user] +
                (amount * (SEBSC - s1EBSC[user])) /
                10**18);
        }
    }

    function claimEBSC() public {
        uint256 amount;
        uint256 lock;
        uint256 reward;
        amount = getReflection();
        totalEBSC += amount;
        SEBSC += (amount * 10**18) / (totalstakeEBSC);
        (, lock, amount, ) = staking.getUserState(msg.sender);
        reward = getEBSCReward(msg.sender, amount, lock);
        previousRewardEBSC[msg.sender] = 0;
        totalEBSC = totalEBSC.sub(reward,"totalEBSC - reward fail");
        s1EBSC[msg.sender] = SEBSC;
        EBSC.safeTransfer(msg.sender, reward);
        emit ClaimedReward(true, msg.sender, reward);
    }

    function setTotalBNB(uint256 _amount) public onlyStaking {
        valueNotDistribute += _amount;
    }

    function setShareForBNBReward(address user, uint256 _amount) public onlyStaking {
        totalstakeBNB += _amount;
        s1BNB[user] = SBNB;
    }

    function distributionBNB() public {
        if (totalstakeBNB != 0 && valueNotDistribute != 0) {
            SBNB += ((valueNotDistribute) * 10**18) / totalstakeBNB;
            totalBNB += valueNotDistribute;
            valueNotDistribute = 0;
        }
    }

    function userPendingBNB(address user, uint256 amount) public onlyStaking {
        previousRewardBNB[user] += (amount * (SBNB - s1BNB[user])) / 10**18;
    }

    function getBNBReward(address user, uint256 amount, uint _tier)
        public
        view
        returns (uint256)
    {   if (uint8(_tier) < bnbStakeholder) {
            return(previousRewardBNB[user]);
        } else {
            return(previousRewardBNB[user] + (amount * (SBNB - s1BNB[user])) / 10**18);
        }
    }

    function setMarketingWallet(address _address) external onlyOperator {
        marketingWallet = _address;
    }

    function claimBNB() public {
        uint256 amount;
        uint256 lock;
        uint256 reward;
        uint256 tier;
        (tier, lock, amount, ) = staking.getUserState(msg.sender);
        reward = getBNBReward(msg.sender, amount, tier);
        require(reward != 0,"Nothing to claim");
        previousRewardBNB[msg.sender] = 0;
        totalBNB = totalBNB.sub(reward,"totalBNB - reward fail");
        s1BNB[msg.sender] = SBNB;
        (bool sent, ) = payable(msg.sender).call{value: reward}("");
        require(sent, "Failed to send BNB");
        emit ClaimedReward(false, msg.sender, reward);
    }

    modifier onlyOperator() {
        require(
            admin.hasRole(OPERATOR, msg.sender),
            "sender is not an operator"
        );
        _;
    }

    modifier onlyStaking(){
           require(
            admin.hasRole(STAKING, msg.sender),
            "Only Staking"
        );
        _;
    }

    modifier onlyExits(address _pool) {
        require(admin.tokenSalesM(_pool), "pool does not exist yet");
        _;
    }
    modifier validation(address _address) {
        require(_address != address(0), "zero address");
        _;
    }

    function setEBSC(address _address)
        external
        validation(_address)
        onlyOperator
    {
        EBSC = IERC20D(_address);
    }

    function setStaking(address _address)
        external
        validation(_address)
        onlyOperator
    {
        staking = IStaking(_address);
    }

    function setAdmin(address _address)
        external
        validation(_address)
        onlyOperator
    {
        admin = IAdmin(_address);
    }

    function addAirdrop(uint256 _totalsupply, address _token)
        external
        payable
        onlyOperator
        returns (uint256)
    {
        airdropCount++;
        airdrops[airdropCount].totalsupply = _totalsupply;
        airdrops[airdropCount].tokenstaked = tokenStakedAirdrop;
        airdrops[airdropCount].time = block.timestamp;
        if (_token != address(1)) {
            
            IERC20D(_token).safeTransferFrom(
                msg.sender,
                address(this),
                _totalsupply
            );
            airdrops[airdropCount].token = _token;
        } else {
            require(msg.value >= _totalsupply, "Invalid amount");
            airdrops[airdropCount].token = address(1);
        }
        return (airdropCount);
    }

    function pushEBSCAirdrop(uint256 _amount) external onlyStaking{
        tokenStakedAirdrop += _amount;
    }

    function popEBSCAirdrop(uint256 _amount) public onlyStaking{
        tokenStakedAirdrop = tokenStakedAirdrop.sub(_amount,"tokenStakedAirdrop - amount fail");
        // tokenStakedAirdrop -= _amount;
    }

    function userStakeAirdrop(
        address user,
        uint256 _startTime,
        uint256 _endTime
    ) external onlyStaking{
        userAirDrops[user].index = airdropCount;
        if (_startTime != 0) {
            userAirDrops[user].startTime = _startTime;
        }
        if (_endTime != 0) {
            userAirDrops[user].endTime = _endTime;
        }
    }

    function isAirdropClaimed(address user) external view returns (bool) {
        if(userAirDrops[user].index ==0 && userAirDrops[user].startTime == 0){
            return false;
        }else if((userAirDrops[user].endTime != 0) && (userAirDrops[user].endTime <= airdrops[userAirDrops[user].index + 1].time)){
            return false;
        }else{
            return true;
        }
    }

    function claimAirdrop(address user) external {
        uint256 id = userAirDrops[user].index + 1;
        uint256 end = (id + 100) < airdropCount ? (id + 100) : airdropCount;
        require(userAirDrops[user].index < airdropCount,"nothing to claim");
        uint256 t;
        uint256 l;
        uint256 amount;
        (t, l, amount, ) = staking.getUserState(user);
        if (t > airdropEligibleTier && l > airdropEligibleLevel) {
            uint32 i = uint32(id);
            for (; i <= end; i++) {
                if (airdrops[i].token == address(1)) {
                    (bool sent, ) = payable(user).call{
                        value: (amount * airdrops[i].totalsupply) /
                            airdrops[i].tokenstaked
                    }("");
                    require(sent, "Failed to send BNB");
                } else {
                    IERC20D(airdrops[i].token).safeTransfer(
                        user,
                        (amount * airdrops[i].totalsupply) /
                            airdrops[i].tokenstaked
                    );
                }
                airdrops[i].claimed[user] = true;
                emit ClaimedAirdrop(
                    airdrops[i].token,
                    user,
                    (amount * airdrops[i].totalsupply) / airdrops[i].tokenstaked
                );
            }
            userAirDrops[user].index = i-1;
        } else if (t > airdropEligibleTier || l > airdropEligibleLevel) {
            uint32 i = uint32(id);
            for (; i <= end; i++) {
                if(airdrops[id].time <= userAirDrops[user].endTime){
                    break;
                }
                if (airdrops[id].token != address(1)) {
                    IERC20D(airdrops[id].token).safeTransfer(
                        msg.sender,
                        (amount * airdrops[id].totalsupply) /
                            airdrops[id].tokenstaked
                    );
                } else {
                    (bool sent, ) = payable(msg.sender).call{
                        value: (amount * airdrops[id].totalsupply) /
                            airdrops[id].tokenstaked
                    }("");
                    require(sent, "Failed to send BNB");
                }
                airdrops[id].claimed[msg.sender] = true;
            }
            userAirDrops[user].index = i-1;

        }    
    }
}

