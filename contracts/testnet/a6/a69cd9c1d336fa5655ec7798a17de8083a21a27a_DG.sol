contract DividendService {
    address[] private account;
    uint256[] private amount;
    address private rewardToken;
    address private holdersToken;
    uint256 private gas = 300000;
    uint256 private claimWait = 60;
    uint256 private lastClaimTimes = 0;
    uint256 private lastProcessedIndex = 0;
    uint256 private minimumTokenBalanceForDividends = 10 * 1e18;

    constructor (address rewardToken_, address holdersToken_){
        setRewardToken(rewardToken_);
        setHoldersToken(holdersToken_);
    }

    function setRewardToken(address addr) public {
        rewardToken = addr;
    }
    function getRewardToken() public view returns(address) {
        return rewardToken;
    }
    function setHoldersToken(address addr) public {
        holdersToken = addr;
    }
    function getHoldersToken()public view returns(address) {
        return holdersToken;
    }
    function isProcessing() public view returns(bool) {
        return account.length > lastProcessedIndex ? true: false;
    }
    function getLastClaimTimes() public view returns(uint256) {
        return lastClaimTimes;
    }
    function setGas(uint256 gas_) public {
        gas = gas_;
    }
    function getGas()public view returns(uint256) {
        return gas;
    }
    function setClaimWait(uint256 claimWait_) public {
        claimWait = claimWait_;
    }
    function getClaimWait() public view returns(uint256) {
        return claimWait;
    }
    function setMinimumTokenBalanceForDividends(uint256 minimumTokenBalanceForDividends_) public {
        minimumTokenBalanceForDividends = minimumTokenBalanceForDividends_;
    }
    function getMinimumTokenBalanceForDividends() public view returns(uint256) {
        return minimumTokenBalanceForDividends;
    }

    function dividend(address[] memory holders_, uint256 rewardTotal) public {
        if(block.timestamp + claimWait > lastClaimTimes) return;
        IERC20 y = IERC20(holdersToken);
        uint256 totalSupply = 0;
        if(account.length != lastProcessedIndex){
            process();
            return;
        }
        delete account;
        delete amount;
        for(uint256 i = 0; i < holders_.length; i++) {
            uint256 balance = y.balanceOf(holders_[i]);
            if(balance >= minimumTokenBalanceForDividends){
                totalSupply = totalSupply + balance;
            }            
        }
        for(uint256 i = 0; i < holders_.length; i++) {
            uint256 balance = y.balanceOf(holders_[i]);
            if(balance >= minimumTokenBalanceForDividends){
                uint256 p = y.balanceOf(holders_[i]) / totalSupply;
                account.push(holders_[i]);
                amount.push(rewardTotal * p / 1000);
            }
        }
        lastProcessedIndex = 0;
        process();
    }

    function process() private {
        uint256 gasUsed = 0;
        uint256 gasLeft = gasleft();
        uint256 _lastProcessedIndex = lastProcessedIndex;
        IERC20 x = IERC20(rewardToken);
        for(uint256 i = lastProcessedIndex; i < account.length;  i++){
            if (gasUsed >= gas) break;
            bool success = x.transfer(account[i], amount[i]);
            if(!success) break;
            uint256 newGasLeft = gasleft();

            if (gasLeft > newGasLeft) {
                gasUsed = gasUsed + (gasLeft - newGasLeft);
            }

            gasLeft = newGasLeft;
            _lastProcessedIndex = i;
        }
        lastProcessedIndex = _lastProcessedIndex;
        lastClaimTimes = block.timestamp;
    }
}
contract DG is ERC20, Ownable {
    using SafeMath for uint256;
    using Address for address;

    address[] private holder;
    mapping(address => bool) holderIndexed;

    IUniswapV2Router02 public uniswapV2Router;
    IUniswapV2Pair public uniswapV2Pair;
    mapping(address => bool) private _isExcludedFromFees;    
    address public router = 0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3;
    address public burnWallet  = 0x000000000000000000000000000000000000dEaD;
    address public marketingWallet  = 0x4Db0C4097d582d472d699602A808953b84082eF4;
    address public buyRewardToken = 0xd792467fc7b275D5bD1c01B5Fd353F00b59511f8; // DOGE
    address public sellRewardToken = 0xA839BBe5f0d882502c7d94a705898D96A6BC39D9; // SHIB
    address public transferRewardToken = 0xA839BBe5f0d882502c7d94a705898D96A6BC39D9; // SHIB
    uint256 public startTimeForSwap;
    uint256 public oneBuyMax = 500000000000 * 1e18;
    uint256 public holdMax = 500000000000 * 1e18;    
    
    uint256 public liquidityFee  = 20;
    uint256 public marketingFee  = 10;
    uint256 public burnFee  = 10;
    uint256 public buyDividend = 60;
   
    uint256 public sellDividend = 120;

    uint256 public transferDividend = 50;
    uint256 public transferMarketingFee = 50;

    DividendService buyService;
    uint256 public buyFeeTokenAmount;    

    DividendService sellService;
    uint256 public sellFeeTokenAmount;    

    DividendService transferService;
    uint256 public transferFeeTokenAmount;

    uint256 public swapTokensAtAmount;
    bool private swapping;
    mapping(address => bool) public excludedFromDividends;

    event ExcludeFromFees(address indexed account, bool isExcluded);

    constructor() payable ERC20("DG2", "DG2") {
        _mint(msg.sender, 100000 * 10**18);

        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(router);
        address _uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
                .createPair(address(this), _uniswapV2Router.WETH());
        uniswapV2Router = _uniswapV2Router;
        uniswapV2Pair = IUniswapV2Pair(_uniswapV2Pair);

        // swapTokensAtAmount = balanceOf(msg.sender).mul(2).div(10**6); // 0.002% 
        swapTokensAtAmount = 2 * 1e18;

        buyService = new DividendService(buyRewardToken, address(this)); 
        sellService = new DividendService(sellRewardToken, address(this));
        transferService = new DividendService(transferRewardToken, address(this));
        
        _isExcludedFromFees[address(this)] = true;
        _isExcludedFromFees[msg.sender] = true;
        _isExcludedFromFees[burnWallet] = true;
        _isExcludedFromFees[marketingWallet] = true;
        _isExcludedFromFees[router] = true;  
        _isExcludedFromFees[address(buyService)] = true; 
        _isExcludedFromFees[address(sellService)] = true;
        _isExcludedFromFees[address(transferService)] = true;

        excludedFromDividends[address(this)]  = true;
        excludedFromDividends[burnWallet]  = true;
        excludedFromDividends[marketingWallet]  = true;
        excludedFromDividends[router]  = true;
        excludedFromDividends[address(uniswapV2Pair)]  = true;
        excludedFromDividends[msg.sender]  = true;
    }

    receive() external payable {}

    function setStartTimeForSwap(uint256 _timestamp) public onlyOwner {startTimeForSwap = _timestamp;}

    function excludeFromFees(address account, bool excluded) external onlyOwner {
        require(msg.sender == owner(),"error"); 
        _isExcludedFromFees[account] = excluded; 
        emit ExcludeFromFees(account, excluded);
    }
    
    function isExcludedFromFees(address account) external view returns (bool) {        
        return _isExcludedFromFees[account];
    }
    function setHolder(address account) public {        
        if(!holderIndexed[account] && !excludedFromDividends[account]){
            holder.push(account);
            holderIndexed[account] = true;
        }        
    }
    function getBuyService() public view returns(address){
        return address(buyService);
    }
    function getSellService() public view returns(address){
        return address(sellService);
    }
    function getTransferService() public view returns(address){
        return address(transferService);
    }

    function _transfer(address from, address to, uint256 amount) internal override {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");

        if (amount == 0) {
            super._transfer(from, to, 0);
            return;
        }

        // dividend
        if (from == address(uniswapV2Pair) || to == address(uniswapV2Pair)) {
            if(from == address(uniswapV2Pair)) {
                if(buyService.isProcessing()){
                    buyService.dividend(holder, 0);
                    return;
                }
                bool canSwap = buyFeeTokenAmount >= swapTokensAtAmount;
                if (canSwap && !swapping){
                    swapping = true;
                    swapTokensForCake(buyFeeTokenAmount, buyRewardToken);
                    uint256 dividends = IERC20(buyRewardToken).balanceOf(address(this));
                    bool success = IERC20(buyRewardToken).transfer(address(buyService), dividends);
                    if(success){
                       buyService.dividend(holder, dividends);
                    }
                    buyFeeTokenAmount = 0;
                    
                    swapping = false; 
                }
            }else{
                if(sellService.isProcessing()){
                    sellService.dividend(holder, 0);
                    return;
                }
                bool canSwap = sellFeeTokenAmount >= swapTokensAtAmount;
                if (canSwap && !swapping){
                    swapping = true;
                    swapTokensForCake(sellFeeTokenAmount, sellRewardToken);
                    uint256 dividends = IERC20(sellRewardToken).balanceOf(address(this));
                    bool success = IERC20(sellRewardToken).transfer(address(sellService), dividends);
                    if(success){
                        sellService.dividend(holder, dividends);
                    }
                    sellFeeTokenAmount = 0;
                    swapping = false; 
                }
            }
        }else{
                if(transferService.isProcessing()){
                    transferService.dividend(holder, 0);
                    return;
                }
                bool canSwap = transferFeeTokenAmount >= swapTokensAtAmount;
                if (canSwap && !swapping){
                    swapping = true;
                    swapTokensForCake(transferFeeTokenAmount, transferRewardToken);
                    uint256 dividends = IERC20(transferRewardToken).balanceOf(address(this));
                    bool success = IERC20(transferRewardToken).transfer(address(transferService), dividends);
                    if(success){
                        transferService.dividend(holder, dividends);
                    }                    
                    transferFeeTokenAmount = 0;
                    swapping = false; 
                }
        }
       
        // whitelist
        if (!_isExcludedFromFees[from] && !_isExcludedFromFees[to]) {            

            // if (startTimeForSwap > block.timestamp) {
            //    revert("Not open sale");
            // }

            if (from == address(uniswapV2Pair) || to == address(uniswapV2Pair)) {
                if(from == address(uniswapV2Pair)){
                    uint256 _burn = amount.mul(burnFee).div(1000);
                    super._transfer(from, burnWallet, _burn);

                    uint256 _marketing = amount.mul(marketingFee).div(1000);
                    super._transfer(from, marketingWallet, _marketing);

                    uint256 _liquidity = amount.mul(liquidityFee).div(1000);
                    super._transfer(from, address(uniswapV2Pair), _liquidity);

                    uint256 _buyDividend = amount.mul(buyDividend).div(1000);
                    buyFeeTokenAmount = buyFeeTokenAmount.add(_buyDividend);
                    super._transfer(from, address(this), buyFeeTokenAmount);

                    amount = amount - _burn - _marketing - _liquidity - _buyDividend;
                }else{
                    uint256 _marketing = amount.mul(marketingFee).div(1000);
                    super._transfer(from, marketingWallet, _marketing);

                    uint256 _liquidity = amount.mul(liquidityFee).div(1000);
                    super._transfer(from, address(uniswapV2Pair), _liquidity);

                    uint256 _sellDividend = amount.mul(sellDividend).div(1000);
                    sellFeeTokenAmount = sellFeeTokenAmount.add(_sellDividend);
                    super._transfer(from, address(this), sellFeeTokenAmount);

                    amount = amount - _marketing - _liquidity - _sellDividend;
                }
            }else{
                // transfer
                uint256 _marketing = amount.mul(transferMarketingFee).div(1000);
                super._transfer(from, marketingWallet, _marketing);

                uint256 _transferFeeTokenAmount = amount.mul(transferDividend).div(1000);
                transferFeeTokenAmount = transferFeeTokenAmount.add(_transferFeeTokenAmount);
                super._transfer(from, address(this), transferFeeTokenAmount);

                amount = amount - _marketing - _transferFeeTokenAmount;
            }
        }
        
        setHolder(from);
        setHolder(to);  
        
        super._transfer(from, to, amount);              
    }

    function swapTokensForCake(uint256 tokenAmount, address rewardToken) public {
        address[] memory path = new address[](3);
        path[0] = address(this);
        path[1] = uniswapV2Router.WETH();
        path[2] = rewardToken;

        _approve(address(this), address(uniswapV2Router), tokenAmount);

        // make the swap
        uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            tokenAmount,
            0,
            path,
            address(this),
            block.timestamp
        );
    }
    
}