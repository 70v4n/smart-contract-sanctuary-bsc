pragma solidity ^0.8.10;
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "./interfaces/IStaking.sol";
import "./interfaces/IAdmin.sol";
import "./interfaces/IAirdrops.sol";
import "./interfaces/IERC20D.sol";
import "hardhat/console.sol";

// TODO Write function for Emergency withdraw for all types of funds

contract Airdrops {
    using SafeERC20 for IERC20D;
    bytes32 public constant OPERATOR = keccak256("OPERATOR");

    struct Airdrop {
        uint256 totalsupply;
        uint256 tokenstaked;
        uint256 time;
        address token;
        mapping(address => bool) claimed;
    }

    struct userAirDrop{
        uint index;
        uint startTime;
        uint endTime;
    }

    IAdmin public admin;
    IStaking public staking;
    IERC20D public lpToken;

    uint256 public lastEpoch;
    uint256 public epochDuration;

    uint256 public SBNB;
    uint256 public totalstakeBNB;
    uint256 public totalBNB;
    uint256 public valueNotDistribute;

    uint256 public SEBSC;
    uint256 public totalstakeEBSC;
    uint256 public totalEBSC;
    uint256 public pendingEBSC;
    uint256 public ebscStakeholder = 1;

    uint256 public airdropCount;
    uint256 public tokenStakedAirdrop;
    uint256 public bnbStakeholder = 7;

    uint256 public airdropEligibleTier = 3;
    uint256 public airdropEligibleLevel = 1;

    bool firstDistribution;

    address public marketingWallet;

    mapping(address => uint256) public s1BNB;
    mapping(address => uint256) public previousRewardBNB;

    mapping(address => uint256) public s1EBSC;
    mapping(address => uint256) public previousRewardEBSC;

    mapping(uint256 => Airdrop) public airdrops;
    mapping(address => userAirDrop) public userAirDrops;

    event ClaimedAirdrop(address token, address receiver, uint256 amount);
    event ClaimedReward(bool isReflection, address receiver, uint256 amount);

    constructor(
        address _staking,
        address _admin,
        address _lpToken
    ) {
        staking = IStaking(_staking);
        admin = IAdmin(_admin);
        lpToken = IERC20D(_lpToken);
    }

    receive() external payable {}

    function checkEpoch() public view returns (bool) {
        return (lastEpoch + epochDuration <= block.timestamp);
    }

    function setEpoch() public{
        require(checkEpoch(),"Cant distribute");
        lastEpoch = block.timestamp;
    }

    function setEpochDuration(uint256 _time) external {
        require(_time > 60, "time should be greater than 1 min");
        epochDuration = _time;
    }

    function setBnbStakeholders(uint256 _tier) onlyOperator external {
        bnbStakeholder = _tier;
    }

    function setEbscStakeholders(uint256 _level) onlyOperator external {
        ebscStakeholder = _level;
    }

    function setAirdropsStakeholders(uint256 _tier , uint256 _level) onlyOperator external {
        airdropEligibleLevel = _level;
        airdropEligibleTier = _tier;
    }

    function viewBalance() external view returns (uint256) {
        return address(this).balance;
    }

    function getReflection() public view returns (uint256) {
        return (lpToken.balanceOf(address(this)) - totalEBSC);
    }

    function withdrawEBSC(address user, uint256 _amount) public {
        totalstakeEBSC -= _amount;
        pendingEBSC += previousRewardEBSC[user];
    }

    function distributionEBSC(uint amount) public {
        if(amount !=0){
            if(totalstakeEBSC != 0){
                // uint256 amount = staking.getReflection(); 
                totalEBSC += (amount);
                SEBSC += ((amount * 10**18) /totalstakeEBSC);
                //Todo calling from backend
                lpToken.safeTransferFrom(marketingWallet, address(this), amount/2);
                // todo transfer function in staking
                // lpToken.safeTransferFrom(address(staking),address(this), amount);
            }
        }
    }

    function setShareForEBSCReward(address user, uint256 _amount) public {
        totalstakeEBSC += _amount;
        s1EBSC[user] = SEBSC;
    }

    function userPendingEBSC(address user) public {
        uint256 amount;
        (, , amount, ) = staking.getUserState(user);
        previousRewardEBSC[user] += (amount * (SEBSC - s1EBSC[user])) / 10**18;
        s1EBSC[user] = SEBSC;
    }

    function getEBSCReward(address user, uint256 amount)
        external
        view
        returns (uint256)
    {
        return (previousRewardEBSC[user] +
            ((amount * (SEBSC - s1EBSC[user])) / 10**18));
    }

    function claimEBSC() public {
        if (checkEpoch()) {
            setEpoch();
            uint256 reflection = staking.getReflection();
            staking.transferReflection(reflection);
            distributionEBSC(2 * reflection);
            distributionBNB();
        }
        uint256 amount;
        uint256 lock;
        uint256 reward;
        amount = getReflection();
        totalEBSC += amount;
        SEBSC += (amount * 10**18) / (totalstakeEBSC + pendingEBSC);
        (, lock, amount, ) = staking.getUserState(msg.sender);
        if (lock <= ebscStakeholder) {
            reward = previousRewardEBSC[msg.sender];
            pendingEBSC -= reward;
        } else {
            reward =
                previousRewardEBSC[msg.sender] +
                (amount * (SEBSC - s1EBSC[msg.sender])) /
                10**18;
            pendingEBSC -= previousRewardEBSC[msg.sender];
        }
        totalEBSC -= reward;
        s1EBSC[msg.sender] = SEBSC;
        lpToken.safeTransfer(msg.sender, reward);
        emit ClaimedReward(true, msg.sender, reward);
    }

    function setTotalBNB(uint256 _amount) public {
        require(msg.sender == address(staking), "Only staking contract");
        valueNotDistribute += _amount;
    }

    function setShareForBNBReward(address user) public {
        require(msg.sender == address(staking), "Only staking contract");
        uint256 amount;
        (, , amount, ) = staking.getUserState(user);
        totalstakeBNB += amount;
        s1BNB[user] = SBNB;
    }

    function distributionBNB() public {
        if(totalstakeBNB != 0){
            if (!firstDistribution) {
                SBNB += (address(this).balance * 10**18) / totalstakeBNB;
                firstDistribution = true;
            } else {
                SBNB +=
                    ((valueNotDistribute) * 10**18) /
                    totalstakeBNB;
            }
            totalBNB += valueNotDistribute;
            //console.log("BNB",SBNB);
        }
    }

    function userPendingBNB(address user, uint256 amount) public {
        require(msg.sender == address(staking), "Only staking contract");
        previousRewardBNB[user] += (amount * (SBNB - s1BNB[user])) / 10**18;
     
    }

    function getBNBReward(address user, uint256 amount)
        external
        view
        returns (uint256)
    {
        return (previousRewardBNB[user] +
            (amount * (SBNB - s1BNB[user])) /
            10**18);
    }

    function setMarketingWallet(address _address) external onlyOperator {
        marketingWallet = _address;
    }

    function claimBNB() public {
        if (checkEpoch()) {
            setEpoch();
            uint256 reflection = staking.getReflection();
            staking.transferReflection(reflection);
            distributionEBSC(2 * reflection);
            distributionBNB();
        }
        uint256 amount;
        uint256 lock;
        uint256 reward;
        uint256 tier;
        (tier, lock, amount, ) = staking.getUserState(msg.sender);
        if (uint8(tier) < bnbStakeholder) {
            reward = previousRewardBNB[msg.sender];
        } else {
            reward =
                previousRewardBNB[msg.sender] +
                (amount * (SBNB - s1BNB[msg.sender])) /
                10**18;
        }
        totalBNB -= reward;
        s1BNB[msg.sender] = SBNB;
        (bool sent,) = payable(msg.sender).call{value: reward}("");
        require(sent, "Failed to send Ether");
        emit ClaimedReward(false, msg.sender, reward);
    }

    modifier onlyOperator() {
        require(
            admin.hasRole(OPERATOR, msg.sender),
            "sender is not an operator"
        );
        _;
    }

    modifier onlyExits(address _pool) {
        require(admin.tokenSalesM(_pool), "pool does not exist yet");
        _;
    }
    modifier validation(address _address) {
        require(_address != address(0), "zero address");
        _;
    }

    function setLptoken(address _address)
        external
        validation(_address)
        onlyOperator
    {
        lpToken = IERC20D(_address);
    }

    function setStaking(address _address)
        external
        validation(_address)
        onlyOperator
    {
        staking = IStaking(_address);
    }

    function setAdmin(address _address)
        external
        validation(_address)
        onlyOperator
    {
        admin = IAdmin(_address);
    }

    function addAirdrop(uint _totalsupply, address _token) external payable onlyOperator returns(uint){
        airdropCount++;
        airdrops[airdropCount].totalsupply = _totalsupply;
        airdrops[airdropCount].tokenstaked = tokenStakedAirdrop;
        airdrops[airdropCount].time = block.timestamp;
        if(_token != address(1)){
            airdrops[airdropCount].token = _token;
            IERC20D(_token).safeTransferFrom(msg.sender,address(this),_totalsupply);
        }else{
            airdrops[airdropCount].token = address(1);
            require(msg.value >= _totalsupply,"Invalid amount");
        }
        return(airdropCount);
    }

    function pushEBSCAirdrop(uint256 _amount) external {
        tokenStakedAirdrop += _amount;
    }

    function popEBSCAirdrop(uint256 _amount) public {
        tokenStakedAirdrop -= _amount;
    }

    function userStakeAirdrop(address user, uint _startTime, uint _endTime) external{
        userAirDrops[user].index = airdropCount;
        if(_startTime != 0){
            userAirDrops[user].startTime =  _startTime;
        }
        if (_endTime != 0) {
            userAirDrops[user].endTime = _endTime;
        }
    }

    function isAirdropClaimed(address user) external view returns (bool) {
        return ((airdrops[userAirDrops[user].index + 1].time >
            userAirDrops[msg.sender].endTime) ||
            (userAirDrops[msg.sender].index == airdropCount));
    }

    function claimAirdrop(address user) external {
        uint256 id = userAirDrops[user].index + 1;
        uint256 end = (id + 100) < airdropCount ? (id + 100) : airdropCount;
        if (checkEpoch()) {
            setEpoch();
            uint256 reflection = staking.getReflection();
            staking.transferReflection(reflection);
            distributionEBSC(2 * reflection);
            distributionBNB();
        }
        uint256 t;
        uint256 l;
        uint256 amount;
        (t, l, amount, ) = staking.getUserState(user);
        if (t > airdropEligibleTier && l > airdropEligibleLevel) {
            uint32 i = uint32(id);
            for (; i <= end; i++) {
                if (airdrops[i].token == address(1)) {
                    (bool sent, ) = payable(user).call{
                        value: (amount * airdrops[i].totalsupply) /
                            airdrops[i].tokenstaked
                    }("");
                    require(sent, "Failed to send Ether");
                } else {
                    IERC20D(airdrops[i].token).safeTransfer(
                        user,
                        (amount * airdrops[i].totalsupply) /
                            airdrops[i].tokenstaked
                    );
                }
                airdrops[i].claimed[user] = true;
                emit ClaimedAirdrop(
                    airdrops[i].token,
                    user,
                    (amount * airdrops[i].totalsupply) / airdrops[i].tokenstaked
                );
            }
        }else if(t > airdropEligibleTier && l > airdropEligibleLevel){
            if(airdrops[id].token != address(1)){
                IERC20D(airdrops[id].token).safeTransfer(msg.sender,(amount * airdrops[id].totalsupply) / airdrops[id].tokenstaked);
            }else{
                (bool sent, ) = payable(msg.sender).call{value: (amount * airdrops[id].totalsupply) / airdrops[id].tokenstaked}("");
                require(sent, "Failed to send BNB");
            }
            airdrops[id].claimed[msg.sender] = true;
        }
    }
}

