pragma solidity 0.6.12;

import "@openzeppelin/contracts-upgradeable/proxy/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/math/SafeMathUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/EnumerableSetUpgradeable.sol";
import "./library/NeedAccess.sol";

contract TalecraftIGO is Initializable, OwnableUpgradeable, ReentrancyGuardUpgradeable, NeedAccess {
    using SafeMathUpgradeable for uint256;
    using AddressUpgradeable for address payable;
    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.AddressSet;
    using SafeERC20Upgradeable for IERC20Upgradeable;

    enum Phase {NULL, SALE_PHASE, RANDOMIZE_PHASE, COMPLETED_PHASE}

    enum TierName {NULL, TIER1, TIER2, TIER3, TIER4, POOL2}
    struct TierSettings {
        uint256 memberQuantity;
        uint256 dropoutsQuantity; 
        uint256 tearPriceIsUSD;            // decimals = 18
        uint256 tokenSaleAmount;           // decimals = 18
    }
    mapping(TierName => TierSettings) public tier;

    // initial parameters
    address public tokenForSale;
    address payable public partnerWallet;
    uint256 public saleStartTime;
    uint256 public saleEndTime;

    // pre-launch flags
    bool public whitelistAdded;
    uint256 public coinPrice;

    // state of sale
    mapping(TierName => address[]) public whitelisted;
    mapping(address => TierName) public whitelistedTier;
    mapping(TierName => address[]) public investors;
    mapping(address => TierName) public investorTier;

    uint256 public totalTokensBought;

    uint256 public random;

    uint256 public totalRemainderCoins;
    mapping(address => uint256) public remainderCoins;

    event SetCoinPrice(
        address indexed owner,
        uint256 price,
        uint256 timestamp
    );
    event AppendWhitelist(
        address indexed owner,
        TierName indexed tier,
        uint256 timestamp
    );
    event BuyTierTokens(
        address indexed investor,
        TierName indexed tier,
        uint256 priceInCoins,
        uint256 boughtAmount,
        uint256 timestamp
    );
    event Randomize(
        address indexed owner,
        uint256 result,
        uint256 timestamp
    );
    event RandomKick(
        TierName indexed tier,
        address indexed investor,
        uint256 index,
        uint256 timestamp
    );
    event ClaimCoins(
        address indexed partner,
        uint256 claimedAmount,
        uint256 timestamp
    );
    event ClaimRemainderCoins(
        address indexed investor,
        uint256 claimedAmount,
        uint256 timestamp
    );

    modifier notAdmins() {
        require(msg.sender != owner() && msg.sender != partnerWallet, "TalecraftIGO: not for admins");
        _;
    }

    modifier onlyPartner() {
        require(msg.sender == partnerWallet, "TalecraftIGO: only for partner");
        _;
    }

    function initialize(
        address _accessListContract,
        address _tokenForSale,
        address payable _partnerWallet,
        uint256 _saleStartTime,
        uint256 _saleEndTime
    ) public initializer {
        __Ownable_init();
        __ReentrancyGuard_init();

        require(_accessListContract != address(0), "TalecraftIGO: zero access list address");
        require(_tokenForSale != address(0), "TalecraftIGO: zero token address");
        require(_partnerWallet != address(0), "TalecraftIGO: zero partner address");
        require(_saleStartTime > block.timestamp, "TalecraftIGO: invalid _saleStartTime parameter");
        require(_saleEndTime > _saleStartTime, "TalecraftIGO: invalid _saleEndTime parameter");

        accessListContract = _accessListContract;
        tokenForSale = _tokenForSale;
        partnerWallet = _partnerWallet;
        saleStartTime = _saleStartTime;
        saleEndTime = _saleEndTime;

        tier[TierName.TIER1] = TierSettings({
            memberQuantity: 25,
            dropoutsQuantity: 0,
            tearPriceIsUSD: 350 ether,
            tokenSaleAmount: 1344 ether
        });
        tier[TierName.TIER2] = TierSettings({
            memberQuantity: 25,
            dropoutsQuantity: 1,
            tearPriceIsUSD: 200 ether,
            tokenSaleAmount: 768 ether
        });
        tier[TierName.TIER3] = TierSettings({
            memberQuantity: 25,
            dropoutsQuantity: 2,
            tearPriceIsUSD: 150 ether,
            tokenSaleAmount: 576 ether
        });
        tier[TierName.TIER4] = TierSettings({
            memberQuantity: 25,
            dropoutsQuantity: 4,
            tearPriceIsUSD: 100 ether,
            tokenSaleAmount: 384 ether
        });
        tier[TierName.POOL2] = TierSettings({
            memberQuantity: 618,
            dropoutsQuantity: 0,
            tearPriceIsUSD: 50 ether,
            tokenSaleAmount: 192 ether
        });
    }

    function setPool2Members(uint256 _memberQuantity) external onlyOwner returns(bool success) {
        tier[TierName.POOL2].memberQuantity = _memberQuantity;

        return true;   
    }

    function setCoinPrice(uint256 _coinPrice) external onlyOwner returns(bool success) {
        require(_coinPrice != 0, "TalecraftIGO: zero price in argument");
        require(coinPrice == 0, "TalecraftIGO: the price has already been set");
        coinPrice = _coinPrice;

        emit SetCoinPrice(msg.sender, _coinPrice, block.timestamp);

        return true;
    }

    function appendWhitelist(address[] memory _wallets, TierName _tier) external onlyOwner returns(bool success) {
        require(_wallets.length != 0, "TalecraftIGO: argument array is empty");
        _checkTier(_tier);

        for (uint256 i = 0; i < _wallets.length; i++) {
            whitelisted[_tier].push(_wallets[i]);
            whitelistedTier[_wallets[i]] = _tier;
        }
        
        if (!whitelistAdded) {
            whitelistAdded = true;
        }

        emit AppendWhitelist(msg.sender, _tier, block.timestamp);

        return true;
    }

    function randomize(uint256 seed) external onlyOwner returns(bool success) {
        require(_currentPhase() == Phase.RANDOMIZE_PHASE, "TalecraftIGO: RANDOMIZE_PHASE phase required");
        require(seed > 0, "TalecraftIGO: wrong seed");
        require(random != 0, "TalecraftIGO: argument array is empty");

        uint256 randomSeed = uint256(keccak256(abi.encodePacked(
            block.difficulty,
            block.timestamp,
            block.coinbase,
            blockhash(block.number - 1),
            seed
        )));
        random = randomSeed;

        emit Randomize(msg.sender, randomSeed, block.timestamp);

        randomSeed = uint256(keccak256(abi.encodePacked(randomSeed)));
        _randomKick(TierName.TIER2, randomSeed);

        randomSeed = uint256(keccak256(abi.encodePacked(randomSeed)));
        _randomKick(TierName.TIER2, randomSeed);

        randomSeed = uint256(keccak256(abi.encodePacked(randomSeed)));
        _randomKick(TierName.TIER3, randomSeed);

        return true;   
    }

    function buyTierTokens() external payable nonReentrant notAdmins needAccess returns(bool success) {
        require(_currentPhase() == Phase.SALE_PHASE, "TalecraftIGO: SALE_PHASE phase required");
        require(investorTier[msg.sender] == TierName.NULL, "TalecraftIGO: the tier already bought");

        TierName tierInWhitelist = whitelistedTier[msg.sender];
        require(tierInWhitelist != TierName.NULL, "TalecraftIGO: not in whitelist");

        uint256 tierPriceInCoins = _tierPriceInCoins(tierInWhitelist);

        require(msg.value == tierPriceInCoins, "TalecraftIGO: wrong message value");

        investorTier[msg.sender] = tierInWhitelist;
        investors[tierInWhitelist].push(msg.sender);
        totalTokensBought = totalTokensBought.add(tier[tierInWhitelist].tokenSaleAmount);

        require(investors[tierInWhitelist].length <= tier[tierInWhitelist].memberQuantity, "TalecraftIGO: slot limit reached");

        emit BuyTierTokens(
            msg.sender,
            tierInWhitelist,
            msg.value,
            tier[tierInWhitelist].tokenSaleAmount,
            block.timestamp
        );

        return true;
    }

    function claimCoins() external nonReentrant onlyPartner needAccess returns(uint256 claimResult) {
        require(uint8(_currentPhase()) >= uint8(Phase.COMPLETED_PHASE), "TalecraftIGO: COMPLETED_PHASE phase require");

        uint256 claimedAmount = address(this).balance.sub(totalRemainderCoins);

        partnerWallet.sendValue(claimedAmount);

        emit ClaimCoins(msg.sender, claimedAmount, block.timestamp);

        return claimedAmount;
    }

    function claimRemainderCoins() external nonReentrant needAccess returns(uint256 claimResult) {
        uint256 remainder = remainderCoins[msg.sender];

        require(remainder > 0, "TalecraftIGO: no remainder tokens");

        remainderCoins[msg.sender] = 0;
        totalRemainderCoins = totalRemainderCoins.sub(remainder);

        msg.sender.sendValue(remainder);

        emit ClaimRemainderCoins(msg.sender, remainder, block.timestamp);

        return remainder;
    }

    function currentPhase() external view returns(uint8) {
        return uint8(_currentPhase());
    }

    function tierPriceInCoins(TierName _tier) external view returns(uint256) {
        _checkTier(_tier);
        return _tierPriceInCoins(_tier);
    }

    function totalWhitelisted(TierName _tier) external view returns(uint256) {
        _checkTier(_tier);
        return whitelisted[_tier].length;
    }

    function totalInvestors(TierName _tier) external view returns(uint256) {
        _checkTier(_tier);
        return investors[_tier].length;
    }

    function _currentPhase() internal view returns(Phase) {
        if (whitelistAdded && coinPrice != 0) {
            if (random > 0) {
                return Phase.COMPLETED_PHASE;
            } else if (block.timestamp >= saleEndTime) {
                return Phase.RANDOMIZE_PHASE;
            } else if (block.timestamp >= saleStartTime) {
                return Phase.SALE_PHASE;
            }
        }
        return Phase.NULL;
    }

    function _tierPriceInCoins(TierName _tier) internal view returns(uint256) {
        _checkTier(_tier);

        if (coinPrice == 0) {
            return 0;
        }
        return(
            tier[_tier].tearPriceIsUSD.mul(1 ether).div(coinPrice)
        );
    }

    function _randomKick(TierName _tier, uint256 _randomSeed) internal {
        require(
            _tier == TierName.TIER2 ||
            _tier == TierName.TIER3 ||
            _tier == TierName.TIER4,
            "TalecraftIGO: wrong kick tier"
        );

        uint256 needToDelete;
        uint256 emptySlots = tier[_tier].memberQuantity.sub(investors[_tier].length);
        if (emptySlots < tier[_tier].dropoutsQuantity) {
            needToDelete = tier[_tier].dropoutsQuantity - emptySlots;
        } else {
            needToDelete = 0;
        }

        uint256 deletePosition;
        uint256 nextRandom = _randomSeed;
        for (uint256 i = 0; i < needToDelete; i++) {
            deletePosition = nextRandom.mod(investors[_tier].length);

            address target = investors[_tier][deletePosition];
            emit RandomKick(_tier, target, deletePosition, block.timestamp);

            investors[TierName.POOL2].push(target);
            investorTier[target] = TierName.POOL2;

            uint256 remainder = _tierPriceInCoins(_tier).sub(_tierPriceInCoins(TierName.POOL2));
            remainderCoins[msg.sender] = remainderCoins[msg.sender].add(remainder);
            totalRemainderCoins = totalRemainderCoins.add(remainder);

            _deleteElem(investors[_tier], deletePosition);

            nextRandom = uint256(keccak256(abi.encodePacked(nextRandom)));
        }
    }

    function _deleteElem(address[] storage _array, uint256 _index) internal {
        _array[_index] = _array[_array.length-1];
        _array.pop();
    }
    
    function _checkTier(TierName _tier) internal pure {
        require(_tier != TierName.NULL, "TalecraftIGO: wrong tier");
    }

}

