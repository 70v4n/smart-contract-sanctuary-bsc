pragma solidity 0.6.12;

import "@openzeppelin/contracts/access/Ownable.sol";
import "./interface/IAccessList.sol";

// solhint-disable not-rely-on-time

contract AccessList is IAccessList, Ownable {

    mapping(address => bool) public access;

    event SetAccess(
        address indexed target,
        bool indexed value,
        uint256 timestamp
    );

    function checkAccess(address _target) external view override returns(bool) {
        return access[_target];
    }

    function setAccess(address _target, bool value) external onlyOwner {
        access[_target] = value;

        emit SetAccess(_target, value, block.timestamp);
    }

}

