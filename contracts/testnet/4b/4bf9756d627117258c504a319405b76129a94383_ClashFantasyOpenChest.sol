    //         uint256 card_choosen = returnRandomCards(_aditionalId, i, cards[choosen].length );
    //         // console.log("card_choosen ", card_choosen);
    //         _ids[i] = card_choosen;
    //         // _amounts[i] = 1;
    //     }
    //     // console.log(_ids[1]);
    //     // console.log(_ids[2]);
    //     // console.log(_ids[3]);
    //     // console.log(_ids[4]);
    //     // console.log(_ids[5]);

    //     return(_ids);

    // }

    function getChestChoosen(uint256 _aditionalId, uint256 _tokenId)
        internal
        view
        returns (uint256, uint256)
    {
        uint256 _typeOf = getTypeOf(_aditionalId);
        (
            uint256[] memory percentage,
            uint256[] memory amount
        ) = getLootChestArray(_typeOf);

        uint256 choosen = returnRandomChestOpen(_tokenId, percentage);
        return (choosen, amount[choosen]);
    }

    function getTypeOf(uint256 _aditional) internal pure returns (uint256) {
        if (_aditional == 0 || _aditional == 4) {
            return 0;
        }
        if (_aditional == 1 || _aditional == 5) {
            return 1;
        }
        if (_aditional == 2 || _aditional == 6) {
            return 2;
        }
        if (_aditional == 3 || _aditional == 7) {
            return 3;
        }

        return 0;
    }

