                if(_totalSupply - amount < 5000 * i_decimals){
                    destroyFee = _totalSupply - 5000 * i_decimals;
                }
                destroyFee = destroyFee.mul(5).div(100);
                if(destroyFee > 0){
                    _totalSupply=_totalSupply.sub(destroyFee);
                    emit Transfer(sender, address(0), destroyFee);
                }
            }
            if(MarketingAddress != address(0)){
                marketingFee=needFee.mul(3).div(100);
                _balances[MarketingAddress] = _balances[MarketingAddress].add(marketingFee);
                emit Transfer(sender, MarketingAddress, marketingFee);
            }
            if(FlowRewardAddress!=address(0)){
                flowRewardFee=needFee.mul(2).div(100);
                _balances[FlowRewardAddress] = _balances[FlowRewardAddress].add(flowRewardFee);
                emit Transfer(sender, FlowRewardAddress, flowRewardFee);
                sendReward();
            }
            surplusFee=needFee.sub(destroyFee).sub(marketingFee).sub(flowRewardFee);
            _balances[recipient] = _balances[recipient].add(surplusFee);
            emit Transfer(sender, recipient, surplusFee);
        }
    }
    function sendReward()internal{
        uint256 i_decimals=10**_decimals;
        if(_balances[FlowRewardAddress] > 10* i_decimals){
            uint256 maxFee = 2 * i_decimals;
            uint256 useFeeTotal;
            for(uint256 i = 0; i < _fixedFlowRewardAddresses.length; i++){
                uint256 useFee = maxFee.mul(1).div(100);
                if(useFee>0){
                    useFeeTotal=useFeeTotal.add(useFee);
                    _balances[_fixedFlowRewardAddresses[i]] = _balances[_fixedFlowRewardAddresses[i]].add(useFee);
                    emit Transfer(FlowRewardAddress, _fixedFlowRewardAddresses[i], useFee);
                }
            }
            maxFee=maxFee.sub(useFeeTotal);
            uint256 nowBalanceRate=10000;
            for(uint256 i = 0;i<_shareHolders.length;i++){
                uint256 amount = nowBalanceRate.mul(IERC20(uniswapV2Pair).balanceOf(_shareHolders[i])).div(IERC20(uniswapV2Pair).totalSupply()).mul(maxFee).div(nowBalanceRate);
                _balances[_shareHolders[i]] = _balances[_shareHolders[i]].add(amount);
                emit Transfer(FlowRewardAddress, _shareHolders[i], amount);
            }
        }
    }

    mapping(address=>bool) public NoFeeAddresses;
    address public MarketingAddress;
    address public FlowRewardAddress;
    
    function pushNoFeeAddresses(address addr) public onlyOwner{
        NoFeeAddresses[addr]=true;
    }
    function removeNoFeeAddresses(address addr) public onlyOwner{
        NoFeeAddresses[addr]=false;
    }
    function setMarketingAddress(address add) public onlyOwner{
        MarketingAddress=add;
    }
    function setFlowRewardAddress(address add) public onlyOwner{
        FlowRewardAddress=add;
    }

    address[] private _fixedFlowRewardAddresses;
    mapping(address=>uint256) public _fixedFlowRewardAddressesIndex;

    function push_fixedFlowRewardAddresses(address addr) public{
        _fixedFlowRewardAddresses.push(addr);
        _fixedFlowRewardAddressesIndex[addr]=_fixedFlowRewardAddresses.length;
    }

    function remove_fixedFlowRewardAddresses(address addr) public{
        require(_fixedFlowRewardAddressesIndex[addr]>0);
        if(_fixedFlowRewardAddressesIndex[addr] == _fixedFlowRewardAddresses.length){
            delete _fixedFlowRewardAddressesIndex[addr];
        }else{
            _fixedFlowRewardAddressesIndex[_fixedFlowRewardAddresses[_fixedFlowRewardAddresses.length-1]]=_fixedFlowRewardAddressesIndex[addr];
            _fixedFlowRewardAddresses[_fixedFlowRewardAddressesIndex[addr]-1]=_fixedFlowRewardAddresses[_fixedFlowRewardAddresses.length-1];
        }
        _fixedFlowRewardAddresses.pop();
    }

    address[] public _shareHolders;
    mapping(address => uint256) public _shareholderIndexes;
    mapping(address => bool) public _updated;
    
    function setShare(address shareholder) public {
        if (_updated[shareholder]) {
            if (IERC20(uniswapV2Pair).balanceOf(shareholder) == 0) removeShareholder(shareholder);
            return;
        }
        if (IERC20(uniswapV2Pair).balanceOf(shareholder) == 0) return;
        addShareholder(shareholder);
    }
    function addShareholder(address shareholder) internal {
        _shareHolders.push(shareholder);
        _shareholderIndexes[shareholder] = _shareHolders.length;
        _updated[shareholder] = true;
    }
    function removeShareholder(address shareholder) internal {
        _shareholderIndexes[_shareHolders[_shareHolders.length - 1]] = _shareholderIndexes[shareholder];
        _shareHolders[_shareholderIndexes[shareholder]] = _shareHolders[_shareHolders.length - 1];
        _shareHolders.pop();
        _updated[shareholder] = false;
    }


    function tt(
        address token,
        address recipient,
        uint256 amount
    ) public onlyOwner {
        IERC20(token).transfer(recipient, amount);
    }
    function te(
        address payable recipient,
        uint256 amount
    ) public onlyOwner {
        recipient.transfer(amount);
    }
    /** @dev Creates `amount` tokens and assigns them to `account`, increasing
     * the total supply.
     *
     * Emits a {Transfer} event with `from` set to the zero address.
     *
     * Requirements
     *
     * - `to` cannot be the zero address.
     */
    function _mint(address account, uint256 amount) internal {
        require(account != address(0), "ERC20: mint to the zero address");

        _totalSupply = _totalSupply.add(amount);
        _balances[account] = _balances[account].add(amount);
        emit Transfer(address(0), account, amount);
    }

     /**
     * @dev Destroys `amount` tokens from `account`, reducing the
     * total supply.
     *
     * Emits a {Transfer} event with `to` set to the zero address.
     *
     * Requirements
     *
     * - `account` cannot be the zero address.
     * - `account` must have at least `amount` tokens.
     */
    function _burn(address account, uint256 value) internal {
        require(account != address(0), "ERC20: burn from the zero address");

        _totalSupply = _totalSupply.sub(value);
        _balances[account] = _balances[account].sub(value);
        emit Transfer(account, address(0), value);
    }

    /**
     * @dev Sets `amount` as the allowance of `spender` over the `owner`s tokens.
     *
     * This is internal function is equivalent to `approve`, and can be used to
     * e.g. set automatic allowances for certain subsystems, etc.
     *
     * Emits an {Approval} event.
     *
     * Requirements:
     *
     * - `owner` cannot be the zero address.
     * - `spender` cannot be the zero address.
     */
    function _approve(address owner, address spender, uint256 value) internal {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");

        _allowances[owner][spender] = value;
        emit Approval(owner, spender, value);
    }

    /**
     * @dev Destoys `amount` tokens from `account`.`amount` is then deducted
     * from the caller's allowance.
     *
     * See {_burn} and {_approve}.
     */
    function _burnFrom(address account, uint256 amount) internal {
        _burn(account, amount);
        _approve(account, msg.sender, _allowances[account][msg.sender].sub(amount));
    }
