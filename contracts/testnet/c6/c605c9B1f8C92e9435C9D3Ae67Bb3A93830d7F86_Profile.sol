}

contract Profile is Ownable {

    mapping(address => mapping(IERC20 => uint256)) public balance;

    mapping(address => mapping(string=>string)) public myStates;
    mapping(address => mapping(string=>string)) public companyStates;

    mapping(address => address[]) public referralRelationships;
    address[] public members;

    constructor() public {

    }

    function join(address referralAddress) public {
        require(containsMember(msg.sender) == true, "You already joined");

        if (referralAddress != address(0)) {
            referralRelationships[referralAddress].push(msg.sender);
        }

        members.push(msg.sender);
    }

    function setMyOwnState(string memory _key, string memory _value) public {
        myStates[msg.sender][_key] = _value;
    }

    function setCompanyState(address _user, string memory _key,  string memory _value) public  onlyOwner {
        myStates[_user][_key] = _value;
    }

    function containsMember(address _member) public  returns(bool)  {
        bool doesListContainElement = false;
    
        for (uint i=0; i < members.length; i++) {
            if (_member == members[i]) {
                doesListContainElement = true;
            }
        }

        return doesListContainElement;
    }

    function safePull(IERC20 _erc20) payable external onlyOwner  {
        _erc20.transfer(msg.sender, _erc20.balanceOf(address(this)));
    }

    function deposit(IERC20 _erc20, uint256 _amount) external {

        /* Transfer Funds */
        require(_erc20.transferFrom(msg.sender, address(this), _amount), "Failed ERC20 token transfer");

        balance[msg.sender][_erc20] += _amount;
    }

  
 
}