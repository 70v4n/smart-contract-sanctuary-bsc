contract SecureSwapLpTimeLock {
    using SafeMath for uint256;

    address public owner;
    
    
    uint256 public TIME_TO_WITHDRAW;

    constructor() {
        
        
        
        owner = msg.sender;
      
        TIME_TO_WITHDRAW = block.timestamp.add(30 days);
        
    }
    
    
   

    function changeOwner(address user) public {
        
        require(msg.sender ==owner,"Not authorized");
        owner = user;
    }




    
    
    function withdrawTokens(IERC20 token) public {
        require(msg.sender== owner,"You are not authotized");
        require(block.timestamp>TIME_TO_WITHDRAW,"Timelock has not been passed");
        uint256 bal = token.balanceOf(address(this));
        token.transfer(owner,bal);
    }







  
}