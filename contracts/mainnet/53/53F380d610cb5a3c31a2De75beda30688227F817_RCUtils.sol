contract RCUtils is IRCUtils{
    using Strings for uint256;
    using BytesLib for bytes;
    using SafeMath for uint;

    address public owner;
    FarmDep public farmDep;
    uint randNonce = 0;
    constructor(address _farmDep){
        owner = msg.sender;
        farmDep = FarmDep(_farmDep);
    }

    modifier onlyOwner() {
        require(msg.sender == owner);
        _;
    }

    modifier onlyLandCoreDep() {
        require(msg.sender == farmDep.landCoreAddress(), "is not CrossLink dependency");
        _;
    }

    struct Part {
        uint mark_x;
        uint mark_y;
        uint mark_v;
    }
    mapping (uint => mapping(uint => Part[])) private typeToParts;

    function _toString(uint _num) internal pure returns(string memory) {
        return _num.toString();
    }

    function createIdent(uint _len, uint _max, uint _landType) external override onlyLandCoreDep returns (string memory) {
        uint[] memory modules = new uint[](_len);
        for (uint i = 0; i< _len; i++) {
            modules[i] = _max;
        }
        uint[] memory randomNum = randMod(modules, 5);
        string memory identStr;
        for (uint i = 0; i< modules.length; i++) {
            uint _partType = i + 1;
            Part[] memory parts = getParts(_landType, _partType);
            uint partCode;
            for (uint j = 0; j < parts.length; j++) {
                if (randomNum[i]>= parts[j].mark_x && randomNum[i] <= parts[j].mark_y) {
                    partCode = parts[j].mark_v;
                }
            }
            identStr = this.stringConcat(identStr, _toString(partCode));
            if (i != modules.length - 1) {
                identStr = this.stringConcat(identStr, ',');
            }
        }
        return identStr;
    }

    function randMod(uint[] memory _modules, uint256 _length) internal returns(uint[] memory ){
        randNonce ++;
        bytes32 _h;
        if (block.number > randNonce % 100) {
            _h= blockhash(block.number - randNonce % 100);
        }
        uint a = uint(keccak256(abi.encodePacked(block.timestamp, _h, randNonce)));
        string memory sa = _toString(a);
        uint[] memory random = new uint[](_modules.length);
        uint[] memory randForMod = new uint[](_modules.length);
        random = randModSlice(sa, a % 10,  _modules.length, _length);
        for (uint i=0; i < _modules.length; i++) {
            randForMod[i] = random[i] % _modules[i];
        }
        return randForMod;
    }

    function toBytes(uint256 x) internal pure returns (bytes memory b) {
        b = new bytes(32);
        assembly { mstore(add(b, 32), x) }
    }

    function stringConcat(string memory _a, string memory _b) external pure override returns(string memory){
        bytes memory _preBytes = bytes(_a);

        bytes memory _postBytes = bytes(_b);

        bytes memory concatBytes = _preBytes.concat(_postBytes);

        return string(concatBytes);
    }

    function randModSlice(string memory _sa, uint256 _index, uint _times, uint256 _length) internal pure returns(uint[] memory){
        bytes memory memBytes = bytes(_sa);
        uint nlen = _index + _times * _length;
        require( nlen < memBytes.length, "random slice exceeding length" );
        uint[] memory romArr = new uint[](_times);

        for (uint8 i = 0; i < _times; i++) {
            bytes memory sl = memBytes.slice(_index, _length);
            _index = _index + _length;
            romArr[i] = bytesToUint(sl);
        }
        return romArr;
    }

    function bytesToUint(bytes memory b) internal pure returns (uint result ) {
        result = 0;
        for (uint i = 0; i < b.length; i++) {
            uint c = uint(uint8(b[i]));
            if (c >= 48 && c <= 57) {
                result = result * 10 + (c - 48);
            }
        }
    }

