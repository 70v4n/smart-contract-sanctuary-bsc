    struct PartsBulk {
        uint8 stageId;
        uint8 landType;
        Part part;
    }

    function setPartsBulkByOwner(PartsBulk[] memory _pbs) public onlyOwner {
        for (uint i=0; i < _pbs.length; i++) {
            PartsBulk memory pb =   _pbs[i];
            typeToParts[pb.stageId][pb.landType].push(pb.part);
        }
    }

    function getParts(uint _stageId, uint _landType) public view returns (Part[] memory){
        return typeToParts[_stageId][_landType];
    }
}