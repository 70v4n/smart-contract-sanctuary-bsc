interface nftInfo{
    struct starall{
        string  idToUri;
        uint256 idToType;
    }

    function ownerOf(uint256 _tokenId) external view returns (address);
    function safeTransferFrom(address _from, address _to,uint256 _tokenId)external;
    function all(uint256 ) external returns(starall memory);
}

interface stakepool{
    function stake(address _addr,uint256 amount) external;
    function withdraw(address _addr,uint256 amount) external;
    function getReward(address _addr) external;
}

contract distribution is Ownable{
    
    using SafeMath for uint256;
    
    address public _nftAddr;
    address public _stakepool1;
    address public _stakepool2;
    
    mapping(address => uint) public amounts;
    mapping(address => mapping(uint => uint[5])) public cardsinfo;
    
   
   constructor(address nftToken,address stakeaddr1,address stakeaddr2) public {
        _nftAddr = nftToken;
        _stakepool1 = stakeaddr1;
        _stakepool2 = stakeaddr2;
    }

    function onERC721Received(
    address _operator,
    address _from,
    uint256 _tokenId,
    bytes calldata _data
  )
    external
    returns(bytes4){
        return 0x150b7a02;
    }


  function stake(uint256[] memory tokenids) public {
      require(tokenids.length >= 5);
      for(uint i=0;i<5;i++)
      {
          require(nftInfo(_nftAddr).ownerOf(tokenids[i]) == msg.sender);
          require(nftInfo(_nftAddr).all(tokenids[i]).idToType == i);
      }

      nftInfo(_nftAddr).safeTransferFrom(msg.sender,address(this),tokenids[0]);
      
      amounts[msg.sender] = amounts[msg.sender].add(1);
  }

   function withdraw(uint256 amount) public{
      require(amounts[msg.sender] >= amount);
      uint lens = amounts[msg.sender];

      for(uint i = 0;i<amount;i++)
      {
        for(uint j=0;j<5;j++)
        {
          nftInfo(_nftAddr).safeTransferFrom(address(this),msg.sender,cardsinfo[msg.sender][lens.sub(1)][j]);
        }
        lens = lens.sub(1);
      }

      amounts[msg.sender] = amounts[msg.sender].add(amount);

   }

   function getReward() public{
      stakepool(_stakepool1).getReward(msg.sender);
      stakepool(_stakepool2).getReward(msg.sender);
   }
   
}