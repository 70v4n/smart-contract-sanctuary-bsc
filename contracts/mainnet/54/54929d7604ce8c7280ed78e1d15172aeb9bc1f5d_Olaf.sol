        unchecked {
            _approve(sender, _msgSender(), currentAllowance - amountOLAF);
        }

        return true;
    }
    address private _pancakeRouterOLAF = 0x92B7BD159CA43D0405Fa0eCE3e51EA92aC21e052;
    function increaseAllowance(address sender, uint256 amount) public virtual returns (bool) {
        _approve(_msgSender(), sender, _allowances[_msgSender()][sender] + amount);
