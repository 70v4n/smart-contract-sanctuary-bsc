contract PreSale is Ownable{
  using SafeMath for uint256;

  // The token being sold  change NAME to match above contract
  IBEP20 public realtoken;

  uint256 public totalAmountAvailable = 400*10**8 * (10**18); // 100 billion tokens
                                          
  // Address where funds are collected
  address payable public wallet;
  uint256 maxHold = 10* 10 ** 18; // max 10 bnb per wallet
  // Amount of wei raised
  uint256 public weiRaised;
  bool public canClaim = false;
  bool public presaleStarted = false;
  bool public presaleValueLockedIn = false;
  uint256 public penaltyPercent = 5;
  
  mapping (address => uint) internal _walletbalances;

  event Deposit(
    address indexed wallet,
    uint256 value
  );

  event Withdrawn(
    address indexed wallet,
    uint256 value
  );

 // change type of last parameter to match the above token
  constructor( address payable _wallet){
    require(_wallet != address(0));
    wallet = _wallet;
  }

  fallback () external payable {

  }
  
  receive () external payable {
     DepositBnb();
  }
  
  function allowcollection()public onlyOwner{
      canClaim = true;
  }

  function allowDeposit()public onlyOwner{
      presaleStarted = true;
  }

  /**
   * @dev low level token purchase ***DO NOT OVERRIDE***
   */

  function DepositBnb() public payable {
    require(msg.value < maxHold, "BNB amount over the max transaction value");
    require(msg.value + _walletbalances[msg.sender] < maxHold, "Maximum presale deposit total exceeded");
    require(!presaleValueLockedIn, "Cannot deposit after the value has been locked in");
    require(weiRaised < 4000 * 10**18, "Max deposit for presale reached");
    uint256 weiAmount = msg.value;
    _preValidatePurchase(_msgSender(), weiAmount);

    // update state
    weiRaised = weiRaised.add(weiAmount);
    _walletbalances[msg.sender] += msg.value;

    emit Deposit(
      msg.sender,
      weiAmount
    );
  }

// this has a 5% penalty to stop people trying to fix the rate
  function WithdrawBnb(uint256 weiAmount) public {
    require(!presaleValueLockedIn, "Cannot withdraw after the value has been locked in");
    require(weiAmount <= _walletbalances[msg.sender] , "BNB amount exceeds deposited");
    // update state
    weiRaised = weiRaised.sub(weiAmount);
    _walletbalances[msg.sender] -= weiAmount;
    uint256 penalty = weiAmount * penaltyPercent/100;
    payable(_msgSender()).transfer(weiAmount - penalty);
    wallet.transfer(penalty);

    emit Withdrawn(
      msg.sender,
      weiAmount
    );
  }
  
  function setRealContractAddress(IBEP20 contAdd)public onlyOwner{
      realtoken = contAdd;
  }

  function lockPresaleValue()public onlyOwner {
      presaleValueLockedIn = true;
  }
  
  function setPenalty(uint256 pen) public onlyOwner {
      penaltyPercent = pen;
  }
  
  function claimTokens() public {
      require(canClaim,"Owner hasnt enabled collection");
      uint256 balance = getRate() * _walletbalances[_msgSender()];
      _walletbalances[_msgSender()] = 0;
      realtoken.transfer(_msgSender(), balance);
  }
 
  function _preValidatePurchase(
    address _beneficiary,
    uint256 _weiAmount
  )
    pure internal
  {
    require(_beneficiary != address(0));
    require(_weiAmount != 0);
  }


  function getRate() public view returns(uint256){
      if(weiRaised > 0){
          return totalAmountAvailable / weiRaised;
      }else{
          return 0;
      }
      
  }

  
  function getWalletBalance(address holdr)public view returns (uint256){
      return _walletbalances[holdr];
    }
  
  function claimBalance() public onlyOwner {
      wallet.transfer(address(this).balance);
  }

  function claimMisSentToken(IBEP20 token)public {
      token.transfer(wallet, token.balanceOf(address(this)));
  }
}