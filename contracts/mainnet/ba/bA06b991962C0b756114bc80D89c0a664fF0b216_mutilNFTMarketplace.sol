pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
import '@openzeppelin/contracts/token/ERC20/IERC20.sol';

abstract contract IMETAXIZ_ERC721 {
	function transferFrom(address from, address to, uint _tokenId) external virtual;
    function tokenHash(uint256 tokenId) virtual external view returns(string memory);
	function setApprovalForAll(address operator, bool approved) external virtual;
}

abstract contract IMETAXIZ_MARKETPLACE_V2 {
	function createAskOrder(
        address _collection,
        uint256 _tokenId,
        uint256 _askPrice,
        string memory _hash,
        address _assetAccept
    ) external virtual;
	
	function cancelAskOrder(address _collection, uint256 _tokenId, string memory _hash) external virtual;
}

contract mutilNFTMarketplace is ERC721Holder, Ownable {
	
	function wd() external onlyOwner {
        payable(_msgSender()).transfer(address(this).balance);
    }
	
	function onCaseStuckNFT(address _nft, uint[] memory _tokenIds) external onlyOwner
	{
		uint length = _tokenIds.length;

        for(uint i = 0; i < length; i++)
		{
			IMETAXIZ_ERC721(_nft).transferFrom(address(this), _msgSender(), _tokenIds[i]);
		}
	}
	
	function onCaseStuckToken(IERC20 _token) external onlyOwner
	{
		uint amount = _token.balanceOf(address(this));
		_token.transfer(_msgSender(), amount);
	}
	
	function transferMutilNFT(address _nft, uint[] memory _tokenIds) external
	{
		uint length = _tokenIds.length;

        for(uint i = 0; i < length; i++)
		{
			IMETAXIZ_ERC721(_nft).transferFrom(_msgSender(), address(this), _tokenIds[i]);
		}
	}
	
	function createOrders(address marketplace, address _nft, uint[] memory _tokenIds, uint[] memory _prices, address _token) external onlyOwner
	{
		require(_tokenIds.length == _prices.length, "length error");
		
		uint length = _tokenIds.length;
		
		string memory hash = IMETAXIZ_ERC721(_nft).tokenHash(_tokenIds[0]);
		
		for(uint i = 0; i < length; i++)
		{
			IMETAXIZ_MARKETPLACE_V2(marketplace).createAskOrder(_nft, _tokenIds[i], _prices[i], hash, _token);
		}
	}
	
	function cancelOrders(address marketplace, address _nft, uint[] memory _tokenIds) external onlyOwner
	{
		uint length = _tokenIds.length;
		
		string memory hash = IMETAXIZ_ERC721(_nft).tokenHash(_tokenIds[0]);
		
		for(uint i = 0; i < length; i++)
		{
			IMETAXIZ_MARKETPLACE_V2(marketplace).cancelAskOrder(_nft, _tokenIds[i], hash);
		}
	}
	
	function approve(address marketplace, address _nft) external onlyOwner
	{
		IMETAXIZ_ERC721(_nft).setApprovalForAll(marketplace, true);
	}
}

