    function swapBusdToToken() public payable nonReentrant{
        bool isEarly = false;
        for(uint256 i = 0; i<earlyUsers.length; i++){
            if(earlyUsers[i] == msg.sender){
                isEarly = true;
            }
        }
        
        require(isEarly, "you are not a presale early buyer");
        uint256 amount = msg.value;
        require(amount>0, "amount must be higher than 0.");
        require(amount <= maxBnbAmount[msg.sender], "amount is more than your presale amount.");
        (uint256 reserveRewardToken, uint256 reserveWeth, ) = IPancakePair(pairAddress).getReserves();
        uint256 currentTokenAmount = router.getAmountOut(amount, reserveWeth, reserveRewardToken);
        uint256 additionalTokenAmount = currentTokenAmount.mul(additionalNumerator).div(additionalDenominator);
        uint256 tokenAmountToTransfer = currentTokenAmount.add(additionalTokenAmount);
        require(tokenAmountToTransfer<rewardToken.balanceOf(address(this)), "token balance of the contrat lower than amount.");
        rewardToken.transfer(msg.sender, tokenAmountToTransfer);
        maxBnbAmount[msg.sender] = maxBnbAmount[msg.sender].sub(amount);
    }

    function withdraw(uint256 _amount) external onlyOwner nonReentrant{
        require(address(this).balance >= _amount, "not enough balance to withdraw ");
        payable(_msgSender()).transfer(_amount);
    }

    function withdrawAll() external onlyOwner nonReentrant{
        require(address(this).balance >= 0, "nothing to withdraw ");
