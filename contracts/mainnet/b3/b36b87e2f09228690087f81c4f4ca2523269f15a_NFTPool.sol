contract HeroNFT is ERC721URIStorageUpgradeable,ERC721EnumerableUpgradeable,Governable {

    uint256 public _tokenIds;


    mapping (address => bool) public admins;
    string private _baseTokenURI;

    struct Item{
        uint itemType;
        uint value;
    }


    struct Property {
        Item item1;
        Item item2;
        Item output;
        uint rare;
        uint combatpower;
        uint energy;
        uint durability;
        uint charges;




    }
    struct CurProperty {
        uint curDurability;
        uint curEnergy;
        uint curCharges;
        uint lastTime;
    }

    Property public property;
    mapping (uint => CurProperty) public curPropertys;  //token id >curProperty
    mapping(uint256 => bool) public bFreezed;
 /*    uint private constant WOW = 0;
    uint private constant WWF = 1;
    uint private constant WWG = 2;
    uint private constant WWW = 3;

    uint private constant Antonidas = 0;
    uint private constant Muradin = 1;
    uint private constant Uther = 2;
    uint private constant Maiev = 3;
    uint private constant Malfurion = 4;
    uint private constant Rokhan = 5;
    uint private constant Cairne = 6;
    uint private constant Thrall = 7;
    uint private constant Grom = 8;*/


    function initialize(address governor_,string memory name_, string memory symbol_,string memory baseURI_,
    uint[3] calldata type_,uint[3] calldata value, uint rare,uint combatpower,uint durability,uint charges,uint energy
    ) external initializer {
		__Governable_init_unchained(governor_);
        __Context_init_unchained();
        __ERC165_init_unchained();
        __ERC721URIStorage_init_unchained();
        __ERC721Enumerable_init_unchained();

        __ERC721_init_unchained(name_,symbol_);
        _baseTokenURI = baseURI_;
        _tokenIds=0;
        property.item1.itemType = type_[0];
        property.item2.itemType = type_[1];
        property.output.itemType = type_[2];
        property.item1.value = value[0];
        property.item2.value = value[1];
        property.output.value = value[2];
        property.rare = rare;
        property.combatpower = combatpower;
        property.durability = durability;
        property.charges = charges;
        property.energy = energy;
    }




    function setAdmin(address admin_,bool isAdmin_)  public governance {
            admins[admin_] = isAdmin_;
    }

    function baseURI() public view returns (string memory) {
        return _baseTokenURI;
    }

    function _baseURI() internal view override returns (string memory){
        return _baseTokenURI;
    }

    function setBaseURI(string memory baseURI_) public governance  {
        _baseTokenURI = baseURI_;
    }

    function _beforeTokenTransfer(address from, address to, uint256 tokenId)
        internal
        override(ERC721Upgradeable, ERC721EnumerableUpgradeable)
    {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function _burn(uint256 tokenId) internal override(ERC721Upgradeable, ERC721URIStorageUpgradeable) {
        super._burn(tokenId);
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override(ERC721Upgradeable, ERC721URIStorageUpgradeable)
        returns (string memory)
    {
        return super.tokenURI(tokenId);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721Upgradeable, ERC721EnumerableUpgradeable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }




        /**
     * @dev Returns whether the given spender can transfer a given token ID.
     * @param spender address of the spender to query
     * @param tokenId uint256 ID of the token to be transferred
     * @return bool whether the msg.sender is approved for the given token ID,
     * is an operator of the owner, or is the owner of the token
     */
    function _isApprovedOrOwner(address spender, uint256 tokenId) internal view override returns (bool) {
        require(_exists(tokenId), "ERC721: operator query for nonexistent token");
        address owner = ownerOf(tokenId);
        return (spender == owner || admins[spender] || getApproved(tokenId) == spender || isApprovedForAll(owner, spender));
    }

    function mint(address to,string memory uri) public returns (uint256 tokenId) {
        require((msg.sender == governor||admins[msg.sender]),"You do not have permission");
        _tokenIds++;
        tokenId = _tokenIds;
        _safeMint(to, tokenId);
        _setTokenURI(tokenId,uri);
        curPropertys[tokenId].curDurability = property.durability;
        curPropertys[tokenId].curEnergy = property.energy;
        curPropertys[tokenId].curCharges = property.energy;
    }

   function setCurProperty(uint tokenId,uint curDur,uint curEner,uint curCha,bool updateLastTime) public {
       require((msg.sender == governor||admins[msg.sender]),"You do not have permission");
       curPropertys[tokenId].curDurability = curDur;
       curPropertys[tokenId].curEnergy = curEner;
       curPropertys[tokenId].curCharges = curCha;
       if (updateLastTime)
            curPropertys[tokenId].lastTime = block.timestamp;
   }


    function burn(uint256 tokenId) public virtual {
        require(_isApprovedOrOwner(_msgSender(), tokenId), "ERC721 Burn: caller is not owner nor approved");
        _burn(tokenId);
    }

     function freezeMetadata(uint256 _tokenid,string memory _uri)  public{
        if(!bFreezed[_tokenid])
        {
            require(_isApprovedOrOwner(msg.sender, _tokenid), "caller is not owner nor approved");
            _setTokenURI(_tokenid, _uri);
            bFreezed[_tokenid] = true;
        }

    }
    uint256[50] private __gap;
}



interface IERC20{
    function totalSupply() external view returns (uint256);
    function balanceOf(address account) external view returns (uint256);
    function transfer(address recipient, uint256 amount) external returns (bool);
    function allowance(address owner, address spender) external view returns (uint256);
    function approve(address spender, uint256 amount) external returns (bool);
    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) external returns (bool);
    event Transfer(address indexed from, address indexed to, uint256 value);
    event Approval(address indexed owner, address indexed spender, uint256 value);
    function name() external view returns (string memory);
    function symbol() external view returns (string memory);
    function decimals() external view returns (uint8);
}

library TransferHelper {
    function safeApprove(address token, address to, uint value) internal {
        // bytes4(keccak256(bytes('approve(address,uint256)')));
        (bool success, bytes memory data) = token.call(abi.encodeWithSelector(0x095ea7b3, to, value));
        require(success && (data.length == 0 || abi.decode(data, (bool))), 'TransferHelper: APPROVE_FAILED');
    }

    function safeTransfer(address token, address to, uint value) internal {
        // bytes4(keccak256(bytes('transfer(address,uint256)')));
        (bool success, bytes memory data) = token.call(abi.encodeWithSelector(0xa9059cbb, to, value));
        require(success && (data.length == 0 || abi.decode(data, (bool))), 'TransferHelper: TRANSFER_FAILED');
    }

    function safeTransferFrom(address token, address from, address to, uint value) internal {
        // bytes4(keccak256(bytes('transferFrom(address,address,uint256)')));
        (bool success, bytes memory data) = token.call(abi.encodeWithSelector(0x23b872dd, from, to, value));
        require(success && (data.length == 0 || abi.decode(data, (bool))), 'TransferHelper: TRANSFER_FROM_FAILED');
    }

    function safeTransferETH(address to, uint value) internal {
        (bool success,) = to.call{value:value}(new bytes(0)); //(bool success,) = to.call.value(value)(new bytes(0));
        require(success, 'TransferHelper: ETH_TRANSFER_FAILED');
    }
}

contract NFTPool is Governable,IERC721ReceiverUpgradeable {
    uint private constant WOW = 0;
    uint private constant WWF = 1;
    uint private constant WWG = 2;
    uint private constant WWW = 3;

    uint private constant Antonidas = 0;
    uint private constant Muradin = 1;
    uint private constant Uther = 2;
    uint private constant Maiev = 3;
    uint private constant Malfurion = 4;
    uint private constant Rokhan = 5;
    uint private constant Cairne = 6;
    uint private constant Thrall = 7;
    uint private constant Grom = 8;

    address private constant BurnAddress   = 0x000000000000000000000000000000000000dEaD;
    mapping (address => bool) public admins;


    address[4] public ercList;
    address[9] public nftList;
    uint256 private constant _NOT_ENTERED = 1;
    uint256 private constant _ENTERED = 2;


    uint256 private _status;



    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.UintSet;

    mapping(address => EnumerableSetUpgradeable.UintSet) private stakeHero;

    function initialize(address governor_) external initializer {
		__Governable_init_unchained(governor_);
		 _status = _NOT_ENTERED;



	}
    modifier nonReentrant() {
        // On the first call to nonReentrant, _notEntered will be true
        require(_status != _ENTERED, "ReentrancyGuard: reentrant call");

        // Any calls to nonReentrant after this point will fail
        _status = _ENTERED;

        _;

        // By storing the original value once again, a refund is triggered (see
        // https://eips.ethereum.org/EIPS/eip-2200)
        _status = _NOT_ENTERED;
    }

    function setNftTokens(address[9] calldata tokens_) public governance{
        for (uint i=0;i<9;i++){
            nftList[i] = tokens_[i];
        }
    }

    function setERCTokens(address[4] calldata tokens_) public governance{
        for (uint i=0;i<4;i++){
            ercList[i] = tokens_[i];
        }
    }
   function setAdmin(address admin_,bool isAdmin_)  public governance {
            admins[admin_] = isAdmin_;
    }


    function getAllItemValues(address who) public view returns(uint[] memory) {
        IERC20 erc;
        HeroNFT prop;
        uint[] memory values = new uint[](12);
        for (uint i = 0; i < 4; i++) {
            erc = IERC20(ercList[i]);
            values[i] = erc.balanceOf(who);
        }
        for (uint i = 0; i < 9; i++) {
            prop = HeroNFT(nftList[i]);
            values[4+i] = prop.balanceOf(who);
        }
        return values;
    }



    function getInfo(address who) public view returns(HeroNFT.Property[] memory propertys,uint[] memory itemValues) {
        itemValues = getAllItemValues(who);
        propertys = new HeroNFT.Property[](9);
        HeroNFT prop;
        for (uint i=0;i<9;i++){
            prop = HeroNFT(nftList[i]);
            (propertys[i].item1,propertys[i].item2,propertys[i].output,,,,,) = prop.property();
            (,,,propertys[i].rare,propertys[i].combatpower,propertys[i].durability,propertys[i].charges,propertys[i].energy) = prop.property();
        }
    }


    function getPropIdsAndCurInfo(address who,uint propType) public view returns(uint[] memory tokenIds,uint[] memory currDurabilitys,uint[] memory curEnergy,uint[] memory curCharges,uint[] memory lastTimes) {
        HeroNFT prop = HeroNFT(nftList[propType]);
        uint len = prop.balanceOf(who);
        tokenIds = new uint[](len);
        currDurabilitys = new uint[](len);
        curEnergy = new uint[](len);
        curCharges = new uint[](len);
        lastTimes = new uint[](len);
        for (uint i = 0; i < len; i++) {
            tokenIds[i] = prop.tokenOfOwnerByIndex(who, i);
            (currDurabilitys[i],curEnergy[i],curCharges[i],lastTimes[i]) = prop.curPropertys(tokenIds[i]);
        }
    }

    function getPropIds(address who,uint propType) public view returns(uint[] memory) {
        HeroNFT prop = HeroNFT(nftList[propType]);
        uint len = prop.balanceOf(who);
        uint[] memory ids = new uint[](len);
        for (uint i = 0; i < len; i++) {
            ids[i] = prop.tokenOfOwnerByIndex(who, i);
        }
        return ids;
    }


    function craft(uint propId) public returns (uint tokenId) {
        HeroNFT prop = HeroNFT(nftList[propId]);
        HeroNFT.Item memory item1;
        HeroNFT.Item memory item2;

        (item1,item2,,,,,,)=prop.property();

        address erc1 = ercList[item1.itemType];
        address erc2 = ercList[item2.itemType];

        uint value1 = item1.value;
        uint value2 = item2.value;

        TransferHelper.safeTransferFrom(erc1, msg.sender, BurnAddress, value1);
        TransferHelper.safeTransferFrom(erc2, msg.sender, BurnAddress, value2);



        if(propId == Antonidas){
            tokenId = prop.mint(msg.sender,"Antonidas");
        }
        if(propId == Muradin){
            tokenId = prop.mint(msg.sender,"Muradin");
        }
        if(propId == Uther){
            tokenId = prop.mint(msg.sender,"Uther");
        }
        if(propId == Maiev){
            tokenId = prop.mint(msg.sender,"Maiev");
        }
        if(propId == Malfurion){
            tokenId = prop.mint(msg.sender,"Malfurion");
        }
        if(propId == Rokhan){
            tokenId = prop.mint(msg.sender,"Rokhan");
        }
        if(propId == Cairne){
            tokenId = prop.mint(msg.sender,"Cairne");
        }
        if(propId == Thrall){
            tokenId = prop.mint(msg.sender,"Thrall");
        }
        if(propId == Grom){
            tokenId = prop.mint(msg.sender,"Grom");
        }
        emit Craft(nftList[propId],tokenId,msg.sender,block.timestamp);
    }
    event Craft(address nft,uint id,address to,uint time);

    function stake(uint256 tokenId, uint256 propId)
        public
        nonReentrant

    {
        HeroNFT prop = HeroNFT(nftList[propId]);
        if (!HeroisAccountStake(msg.sender,propId,tokenId)){

            prop.safeTransferFrom(msg.sender,address(this),tokenId);
            addStake(msg.sender,propId,tokenId);
            emit Stake(msg.sender,nftList[propId],tokenId,block.timestamp);
        }





    }
    event Stake(address account,address nft,uint id,uint time);


    function unstake(address account,uint propId,uint tokenId) public  nonReentrant{
        require(HeroisAccountStake(account,propId,tokenId),"You not stake the nft");
        require((msg.sender == governor||admins[msg.sender]),"You do not have permission");
        HeroNFT prop = HeroNFT(nftList[propId]);
        removeStake(account,propId,tokenId);
        prop.safeTransferFrom(address(this),account,tokenId);
        emit Unstake(account,nftList[propId],tokenId,block.timestamp);
    }
    event Unstake(address account,address nft,uint id,uint time);



    function onERC721Received(address operator,address from,uint256 tokenId,bytes calldata data) public override pure returns (bytes4){
        operator;
        from;
        tokenId;
        data;
        return this.onERC721Received.selector;
    }

    function hl2u(uint hi,uint lo) public pure returns (uint){
        return hi*2**128+lo;
    }

    function u2hl(uint v) public pure returns (uint hi,uint lo){
        hi = v/2**128;
        lo = v%2**128;
    }

    function addStake(address account,uint propType,uint id) private {
           stakeHero[account].add(hl2u(propType,id));
    }

    function removeStake(address account,uint propType,uint id) private {
           stakeHero[account].remove(hl2u(propType,id));
    }

    function HeroisAccountStake(address account,uint propType,uint id) public view returns(bool){
        uint typeAndId = hl2u(propType,id);
        return stakeHero[account].contains(typeAndId);
    }

    function getStakeCount(address account) public view returns(uint){
        return stakeHero[account].length();
    }


    function getStakeHeroByIndex(address account,uint index) public view returns(uint propType,uint id){
        (propType,id) = u2hl(stakeHero[account].at(index));
    }

    function getAllStakeHero(address account) public view returns(uint[] memory propType,uint[] memory id){
        uint n = stakeHero[account].length();
        propType = new uint[](n);
        id = new uint[](n);
        for (uint i=0;i<n;i++){
            (propType[i],id[i]) = u2hl(stakeHero[account].at(i));
        }
    }


}