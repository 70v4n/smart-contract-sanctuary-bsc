interface ztoken {
    function addWhitelist( address newlist) external;
}

contract white{
     using SafeERC20 for IERC20;
    using Address for address;
    using SafeMath for uint256;
    
    
    address public controller;
	address public gov;
    address public ztokenAddr;
   
   
    constructor(address newztokenaddr) public {
        controller = msg.sender;
        gov = msg.sender;
        ztokenAddr = newztokenaddr;
    }
    
    
    modifier onlyOwner () {
        require(msg.sender == controller || msg.sender == gov, "!controller");
        _;
    }

    
    function transferController(address newController) public {
        require(msg.sender == controller || msg.sender == gov , "!Owner");
        require(newController != address(0), "Ownable: new owner is the zero address");
       controller = newController;
    }
   
    function setUsrList(address[] memory _usr)public onlyOwner{
        require(_usr.length > 0,"usr length >0 !!!!!!!");
        for(uint i;i<_usr.length;i++){
            ztoken(ztokenAddr).addWhitelist(_usr[i]);
        }
    }

    function setZtoken(address newztoken) public onlyOwner{
        ztokenAddr = newztoken; 
    }
    
}