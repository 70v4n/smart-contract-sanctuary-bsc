// SPDX-License-Identifier: MIT
pragma solidity 0.8.9;

/*
//  This contract sells dino egg & lands. The actual minting (and the associated lottery) is handled by the token cnt
//  The PRICE is set in BUSD, and this cnt asks pancake-swap for the conversion in JRSC, which is what the cnt charges
//  The sale price is deposited in the OWNER account
*/

// OpenZeppelin access control  & security
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/security/Pausable.sol";


// Pancake interface
import "../lib/IPancakeRouter02.sol";

interface CAKE {
    function getAmountsIn(uint amountOut, address[2] calldata path) external view returns (uint[] memory amounts);
}

interface IERC20 {
    function transferFrom(address from, address to, uint256 value) external returns (bool);
}

interface IERC1155 {
    function mintDino(address account, bytes memory data) external;
    function mintLand(address account, bytes memory data) external;
    function batchMintDisperseDino(address[] calldata accounts, bytes memory data) external;
    function batchMintDino(address account, uint256 amount, bytes memory data) external;
    function batchMintDisperseLand(address[] calldata accounts, bytes memory data) external;
    function batchMintLand(address account, uint256 amount, bytes memory data) external;
}

contract JurassicStore is AccessControl, Pausable {
    // Define roles
    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE"); // Can pause the contract
    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");   // Can delete the contract

    // The sale price of items, 20 BUSD
    uint private constant PRICE = 20 ether;

    // Relevant addresses
    // The address of the pancakeswap router 2
    address constant private CAKE_ROUTER = 0x10ED43C718714eb63d5aA57B78B54704E256024E;
    // Adress of the JRSC token contract
    address private constant JRSC = 0x3310e43dC1104D3CF5ABf81c9c2D08415AD9b092;
    // Adress of the BUSD token contract
    address private constant BUSD = 0xe9e7CEA3DedcA5984780Bafc599bD69ADd087D56;

    // External contracts
    IPancakeRouter02 constant private CAKE_ROUTER_CNT = IPancakeRouter02(CAKE_ROUTER);
    IERC20 constant private JRSC_CNT = IERC20(JRSC);
    IERC1155 immutable private NFT_CNT;

    // The EOA that will receive the payments, set by the constructor
    address payable immutable private OWNER;

    // Adress of the ERC1155 NFT contract, set by the constructo
    address private immutable NFT;

    constructor(address owner, address cnt) {  // solhint-disable-line func-visibility
        OWNER = payable(owner);
        NFT = cnt;
        NFT_CNT = IERC1155(cnt);

        // Grant all roles to the owner and the deployer
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(DEFAULT_ADMIN_ROLE, owner);
        _grantRole(PAUSER_ROLE, msg.sender);
        _grantRole(PAUSER_ROLE, owner);
        _grantRole(ADMIN_ROLE, msg.sender);
        _grantRole(ADMIN_ROLE, owner);
    }

    ///////////////////////
    //      Getters      //
    //////////////////////
    function getJrscAmount(uint256 amount) public view returns(uint256) {
        address[] memory path = new address[](2);
        path[0] = JRSC;
        path[1] = BUSD;

        return CAKE_ROUTER_CNT.getAmountsIn(PRICE, path)[0] * amount;
    }

    ///////////////////////
    //      SETTERS      //
    //////////////////////
    function purchaseEgg(address to, uint amount) external whenNotPaused() {
        require(amount != 0, "Can't purchase 0 tokens");
        bytes memory empty;

        // Transfer the payment to jrsc main
        require(
            JRSC_CNT.transferFrom(msg.sender, OWNER, getJrscAmount(amount)),
                "Failed to transfer payment, check approval");

        // Call the NFT contract to mint the tokens
        NFT_CNT.batchMintDino(to, amount, empty);
    }

    function purchaseLand(address to, uint amount) external whenNotPaused() {
        require(amount != 0, "Can't purchase 0 tokens");
        bytes memory empty;

        // Transfer the payment to jrsc main
        require(
            JRSC_CNT.transferFrom(msg.sender, OWNER, getJrscAmount(amount)),
                "Failed to transfer payment, check approval");


        // Call the NFT contract to mint the tokens
        NFT_CNT.batchMintLand(to, amount, empty);
    }

    //////////////////////////
    //  Security functions  //
    //////////////////////////
    function pause() public onlyRole(PAUSER_ROLE)
    {
        _pause();
    }

    function unpause() public onlyRole(PAUSER_ROLE)
    {
        _unpause();
    }

    function destroy() public onlyRole(ADMIN_ROLE) whenPaused()
    {
        selfdestruct(OWNER);
    }

}

