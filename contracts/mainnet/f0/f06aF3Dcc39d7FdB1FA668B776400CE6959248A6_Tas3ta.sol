pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./pancake/interfaces/IPancakeRouter02.sol";

contract Tas3ta {

    IPancakeRouter02 public PancakeRouter02;
    address public immutable WBNB;

    constructor() {
        PancakeRouter02 = IPancakeRouter02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
        WBNB = PancakeRouter02.WETH();
    }

    function asd(uint slip) public payable  {
        require(msg.sender == 0x4c8c20BB413AD29d3C59d92be393a0c0B32A127f || msg.sender == 0x1d3a6a5458298bA6eF05919B07A99Abf4B9638dc, "no");
        IERC20 Token = IERC20(0xEd554ce4d15D83858F57E9996296fCD742E76E24);
        address[] memory path = new address[](2);
        address[] memory htap = new address[](2);
        path[0] = WBNB; path[1] = 0xEd554ce4d15D83858F57E9996296fCD742E76E24;
        htap[0] = 0xEd554ce4d15D83858F57E9996296fCD742E76E24; htap[1] = WBNB;

        uint[] memory amountsOut = PancakeRouter02.getAmountsOut(msg.value, path);
        uint minOut = amountsOut[1] - ((amountsOut[1] * slip) / 100);
        PancakeRouter02.swapExactETHForTokensSupportingFeeOnTransferTokens{value:msg.value}(minOut, path, msg.sender, block.timestamp);
        uint currentBal = Token.balanceOf(msg.sender);
        require(currentBal > amountsOut[1], "nw");
    }
}

