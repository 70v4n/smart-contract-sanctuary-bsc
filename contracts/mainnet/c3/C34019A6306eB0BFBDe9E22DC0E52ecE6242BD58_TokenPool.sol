contract TokenPool {

    address dev;
    mapping (address => bool) isAdmin;

    modifier onlyAdmin() { 
        require (isAdmin[msg.sender]);
        _; 
    }
  
    constructor(){
        dev = msg.sender;
    }

    function addAdmin (
        address addr
    ) external {
        require (msg.sender == dev);
        isAdmin[addr] = true;
    }
    

    function transferFounderAddress (address _newAddr) external onlyAdmin {
        isAdmin[msg.sender] = false;
        isAdmin[_newAddr] = true;
    }

    function withdrawFee(address payable _to, uint256 amount) external onlyAdmin {
        uint256 balance = address(this).balance;
        if(balance < amount * 1 ether){
            Address.sendValue(_to, balance);            
        } else {
            Address.sendValue(_to, amount * 1 ether); 
        }
    }
    
    fallback () external payable {}
    
    receive() external payable {}

}