/**
 * @dev Implementation of the Chimerium Token.
 *
 * Deflationary token mechanics:
 *
 * When buy/sell from CHTN-USDT on Pancakeswap:
 *
 * 3% reflection
 * 1% marketing wallet
 * 1% auto-liquidity
 * for beginning
 */
contract ChimeriumToken is ERC20Burnable, Ownable {
    uint256 private constant _TEAM = 27000000;
    uint256 private constant _ADVISORS = 16200000;
    uint256 private constant _GAME_FARMING = 86400000;
    uint256 private constant _LIQ = 32400000;
    uint256 private constant _MARKETING = 13500000;
    uint256 private constant _TREASURE = 21600000;
    uint256 private constant _LAUNCHPAD = 64800000;
    uint256 private constant _PUB_SALE = 8100000;
    uint256 private constant _PERCENT_BASE = 1000;

    //taxes
    uint256 public constant MAX_TAX = 50;
    uint256 public reflectionTax = 30;
    uint256 public marketingTax = 10;
    uint256 public liquidityTax = 10;

    address public marketing;
    address public pair;
    address public usdt;
    IAgent public agent;
    IRouter public router;
    IFactory public factory;
    uint256 public totalDistributed;

    uint256 private constant MULTIPLIER = 10**20;

    bool private inSwap;
    bool private presaleSetted;
    uint256 private globalCoefficient;

    mapping(address => bool) public isExcludedFromFee; //excluded accounts do not pay fees
    mapping(address => bool) public isExcludedFromReward; //excluded accounts do not get reflection tokens
    mapping(address => uint256) private holdersReward;

    struct Fees {
        uint256 reflection;
        uint256 marketing;
        uint256 liquidity;
    }

    event TaxTaken(
        uint256 reflection,
        uint256 marketing,
        uint256 liquidity,
        uint256 totalFee
    );

    //put addresses in _wallets array due to allocation values order
    constructor(
        address[] memory _wallets,
        address _router,
        address _usdt,
        address _agent
    ) ERC20("Chimerium Token", "CHTN") {
        require(_wallets.length == 6, "Wrong address");

        marketing = _wallets[4];
        usdt = _usdt;
        router = IRouter(_router);
        factory = IFactory(router.factory());
        pair = factory.createPair(address(this), usdt);

        agent = IAgent(_agent);

        uint256 factor = 10**decimals();
        _mint(_wallets[0], _TEAM * factor);
        _mint(_wallets[1], _ADVISORS * factor);
        _mint(_wallets[2], _GAME_FARMING * factor);
        _mint(_wallets[3], _LIQ * factor);
        _mint(_wallets[4], _MARKETING * factor);
        _mint(_wallets[5], _TREASURE * factor);

        isExcludedFromFee[_wallets[0]] = true;
        isExcludedFromFee[_wallets[1]] = true;
        isExcludedFromFee[_wallets[2]] = true;
        isExcludedFromFee[_wallets[3]] = true;
        isExcludedFromFee[_wallets[4]] = true;
        isExcludedFromFee[_wallets[5]] = true;
        isExcludedFromFee[address(agent)] = true;

        isExcludedFromReward[pair] = true;
        isExcludedFromReward[address(this)] = true;
        isExcludedFromReward[address(agent)] = true;
        isExcludedFromReward[_wallets[0]] = true;
        isExcludedFromReward[_wallets[1]] = true;
        isExcludedFromReward[_wallets[2]] = true;
        isExcludedFromReward[_wallets[3]] = true;
        isExcludedFromReward[_wallets[4]] = true;
        isExcludedFromReward[_wallets[5]] = true;

        //no totalDistributed because all wallets are included from reflection
    }

    /** @dev Set presale address and mint tokens
     *  @param _presale an address of presale contract
     */
    function setPresaleAddress(address _presale) external onlyOwner {
        require(!presaleSetted);
        presaleSetted = true;
        isExcludedFromReward[_presale] = true;
        _mint(_presale, (_PUB_SALE + _LAUNCHPAD) * (10**decimals()));
    }

    /** @dev Set new fee percents
     *  @param _ref reflection fee perent
     *  @param _mark marketing fee perent
     *  @param _liq liquidity fee perent
     */
    function setNewFeePercents(uint256 _ref, uint256 _mark, uint256 _liq) external onlyOwner {
        require(_ref + _mark + _liq <= MAX_TAX, "More than 5%");
        reflectionTax = _ref;
        marketingTax = _mark;
        liquidityTax = _liq;
    }

    /** @dev Change isExcludedFromFee status
     *  @param _account an address of account to change
     */
    function changeExcludedFromFee(address _account) external onlyOwner {
        isExcludedFromFee[_account] = !isExcludedFromFee[_account];
    }

    /** @dev Change isExcludedFromReward status
     *  @param _account an address of account to change
     */
    function changeExcludedFromReward(address _account) external onlyOwner {
        if (isExcludedFromReward[_account]) {
            holdersReward[_account] =
                (globalCoefficient * super.balanceOf(_account)) /
                MULTIPLIER;
            totalDistributed += super.balanceOf(_account);
        } else {
            if (getReward(_account) > 0) withdraw(_account);
            totalDistributed -= super.balanceOf(_account);
        }
        isExcludedFromReward[_account] = !isExcludedFromReward[_account];
    }

    /**
     * @dev send rewards to the address.
     * @param account for withdraw
     */
    function withdraw(address account) public {
        uint256 amount = getReward(account);
        require(
            super.balanceOf(account) > 0 &&
                !isExcludedFromReward[account] &&
                amount > 0,
            "Chimerium: not holder"
        );

        holdersReward[account] =
            (globalCoefficient * (super.balanceOf(account) + amount)) /
            MULTIPLIER;
        totalDistributed = totalDistributed + amount;
        super._transfer(address(this), account, amount);
    }

    function burn(uint256 amount) public virtual override {
        address sender = _msgSender();
        require(amount > 0 && balanceOf(sender) >= amount, "Wrong amount");
        if (!isExcludedFromReward[sender]) {
            if (getReward(sender) > 0) withdraw(sender);
            holdersReward[sender] =
                (globalCoefficient * (super.balanceOf(sender) - amount)) /
                MULTIPLIER;
            totalDistributed -= amount;
        }
        _burn(sender, amount);
    }

    function burnFrom(address account, uint256 amount) public virtual override {
        address sender = _msgSender();
        require(amount > 0 && balanceOf(sender) >= amount, "Wrong amount");
        uint256 currentAllowance = allowance(account, sender);
        require(
            currentAllowance >= amount,
            "ERC20: burn amount exceeds allowance"
        );
        unchecked {
            _approve(account, sender, currentAllowance - amount);
        }
        if (!isExcludedFromReward[account]) {
            if (getReward(account) > 0) withdraw(account);
            holdersReward[account] =
                (globalCoefficient * (super.balanceOf(account) - amount)) /
                MULTIPLIER;
            totalDistributed -= amount;
        }
        _burn(account, amount);
    }

    function decimals() public view virtual override returns (uint8) {
        return 9;
    }

    /**
     * @dev overrided balanceOf; if account is excluded from fee, shows balance from _balances[account], else shows balance from _balances[account] plus rewards from taxes
     */
    function balanceOf(address account)
        public
        view
        virtual
        override
        returns (uint256)
    {
        uint256 internalBalance = super.balanceOf(account);
        if (isExcludedFromReward[account]) return internalBalance;
        else return internalBalance + getReward(account);
    }

    /**
     * @dev get amount reward for user.
     * @param account is address for user
     */
    function getReward(address account) public view returns (uint256 amount) {
        if (!isExcludedFromReward[account])
            amount = ((globalCoefficient * super.balanceOf(account)) /
                MULTIPLIER -
                holdersReward[account]);
    }

    function _transfer(
        address sender,
        address recipient,
        uint256 amount
    ) internal virtual override {
        require(sender != recipient);
        uint256 totalFee;

        bool pairSender = _pairCheck(sender);
        bool pairRecipient = _pairCheck(recipient);

        require(balanceOf(sender) >= amount, "Wrong amount");

        if (!isExcludedFromReward[sender]) {
            if (pairSender) {
                isExcludedFromReward[sender] = true;
            } else {
                if (getReward(sender) > 0) withdraw(sender);
                holdersReward[sender] =
                    (globalCoefficient * (super.balanceOf(sender) - amount)) /
                    MULTIPLIER;
            }
        }
        if (!isExcludedFromReward[recipient]) {
            if (pairRecipient) isExcludedFromReward[recipient] = true;
            else {
                if (getReward(recipient) > 0) withdraw(recipient);
                holdersReward[recipient] =
                    (globalCoefficient *
                        (super.balanceOf(recipient) + amount)) /
                    MULTIPLIER;
            }
        }

        if (isExcludedFromReward[sender]) {
            if (!isExcludedFromReward[recipient]) totalDistributed += amount;
        } else {
            if (isExcludedFromReward[recipient]) totalDistributed -= amount;
        }

        bool buy = (sender == pair);
        bool sell = (recipient == pair && address(router) == _msgSender());

        if (!inSwap) {
            if (
                (buy || sell) &&
                !isExcludedFromFee[sender] &&
                !isExcludedFromFee[recipient]
            ) {
                Fees memory fees;
                if(totalDistributed > 0)
                    fees.reflection = (amount * reflectionTax) / _PERCENT_BASE;
                fees.marketing = (amount * marketingTax) / _PERCENT_BASE;
                fees.liquidity = (amount * liquidityTax) / _PERCENT_BASE;

                totalFee = fees.liquidity + fees.marketing + fees.reflection;

                inSwap = true;

                //recalc. for buyer his reward
                if (sender == pair) {
                    if (!isExcludedFromReward[recipient]) {
                        holdersReward[recipient] =
                            (globalCoefficient *
                                (super.balanceOf(recipient) +
                                    amount -
                                    totalFee)) /
                            MULTIPLIER;

                        totalDistributed -= totalFee;
                    }
                }

                //marketing
                if(fees.marketing > 0)
                    super._transfer(sender, marketing, fees.marketing);

                //reflection
                if (fees.reflection > 0) {
                    super._transfer(sender, address(this), fees.reflection);
                    globalCoefficient +=
                        (fees.reflection * MULTIPLIER) /
                        totalDistributed;
                }

                //liquidity
                if(fees.liquidity > 0){
                    super._transfer(sender, address(agent), fees.liquidity);
                    agent.increaseStock(fees.liquidity);
                }

                inSwap = false;

                emit TaxTaken(
                    fees.reflection,
                    fees.marketing,
                    fees.liquidity,
                    totalFee
                );
            } else {
                if (
                    (agent.getStock() > agent.getThreshold()) &&
                    (sender != address(agent)) &&
                    (!sell && !buy)
                ) {
                    agent.autoLiquidity();
                }
            }
        }

        super._transfer(sender, recipient, amount - totalFee);
    }

    function _pairCheck(address _token) internal view returns (bool) {
        address token0;
        address token1;

        if (isContract(_token)) {
            try IPair(_token).token0() returns (address _token0) {
                token0 = _token0;
            } catch {
                return false;
            }

            try IPair(_token).token1() returns (address _token1) {
                token1 = _token1;
            } catch {
                return false;
            }

            address goodPair = factory.getPair(token0, token1);
            if (goodPair != _token) {
                return false;
            }

            if (token0 == address(this) || token1 == address(this)) return true;
            else return false;
        } else return false;
    }

    function isContract(address addr) internal view returns (bool) {
        uint256 size;
        assembly {
            size := extcodesize(addr)
        }
        return size > 0;
    }
}

