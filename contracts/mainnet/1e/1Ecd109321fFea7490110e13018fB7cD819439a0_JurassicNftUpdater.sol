pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "../tokens/extensions/ERC1155JrscCollectableUpgradeable.sol";


interface IERC1155 {
    function batchMintDino(address account, uint256 amount,
        Traits calldata fixedTraits, uint256 fixedId, bytes memory data)
    external;
}

contract JurassicNftUpdater is Initializable {
    IERC721 constant jcm_cnt   = IERC721(0x4FC7EA27D8d63bC75b25d2F904D0CB0aA5484e03);
    IERC721 constant jcl_cnt   = IERC721(0xC2D1431389Ee8538f80CB2408167628447f2eF76);
    IERC721 constant jce_cnt   = IERC721(0xBE71E3A456CF7D939EE5deeA8B64fC9B957573E1);
    IERC721 constant jcr_cnt   = IERC721(0x3D9710e8903EAB0f3FF2ccb21dC34FBAbCaC4Da8);
    IERC721 constant jcc_cnt   = IERC721(0x1c940072E0e8b52076c06F3037550f55fB43B87b);
    IERC1155 constant jrsc_cnt = IERC1155(0x566344f4015da9f5B3dF715B2948c97772815595);

    uint16 constant jcm_attack = 300;
    uint16 constant jcl_attack = 250;
    uint16 constant jce_attack = 200;
    uint16 constant jcr_attack = 150;
    uint16 constant jcc_attack = 100;

    uint256 nextId = 500;

    function updateNft(uint256[] calldata jcm, uint256[] calldata jcl, uint256[] calldata jce, uint256[] calldata jcr,
        uint256[] calldata jcc)
    public{
        // Vars used during execution
        uint256 i;

        // Transfer the nft to this cnt
        for(; i < jcm.length; i++){
            jcm_cnt.transferFrom(msg.sender, address(this), jcm[i]);
        }
        for(i=0; i < jcl.length; i++){
            jcl_cnt.transferFrom(msg.sender, address(this), jcl[i]);
        }
        for(i=0; i < jce.length; i++){
            jce_cnt.transferFrom(msg.sender, address(this), jce[i]);
        }
        for(i=0; i < jcr.length; i++){
            jcr_cnt.transferFrom(msg.sender, address(this), jcr[i]);
        }
        for(i=0; i < jcc.length; i++){
            jcc_cnt.transferFrom(msg.sender, address(this), jcc[i]);
        }

        // Get the next id from storage, so its not read for every rarity
        uint256 _nextId = nextId;


        // Mint dinos
        if (jcm.length > 0){

            _mint(uint8(Rarity_dino.mythical), jcm_attack, jcm.length, _nextId);
            _nextId += jcm.length;

        }

        if (jcl.length > 0){

            _mint(uint8(Rarity_dino.legendary), jcl_attack, jcl.length, _nextId);
            _nextId += jcl.length;

        }

        if (jce.length > 0){

            _mint(uint8(Rarity_dino.epic), jce_attack, jce.length, _nextId);
            _nextId += jce.length;

        }

        if (jcr.length > 0){

            _mint(uint8(Rarity_dino.rare), jcr_attack, jcr.length, _nextId);
            _nextId += jcr.length;

        }

        if (jcc.length > 0){

            _mint(uint8(Rarity_dino.common), jcc_attack, jcc.length, _nextId);
            _nextId += jcc.length;

        }

        // minting finished, update next id
        nextId = _nextId;

    }

    function _mint(uint8 rarity, uint16 attack, uint256 amount, uint256 _nextId) private{
        bytes memory empty;
        Traits memory traits;

        traits.rarity = rarity;
        traits.attack = attack;
        traits.hp = 90;

        jrsc_cnt.batchMintDino(msg.sender, amount, traits, _nextId, empty);
    }
}

