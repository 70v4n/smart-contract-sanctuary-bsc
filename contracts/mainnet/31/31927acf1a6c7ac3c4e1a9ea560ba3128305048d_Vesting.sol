// ----------------------------------------------------------------------------
// Vesting contract
// ----------------------------------------------------------------------------
contract Vesting {
    using SafeMath for uint256;
    uint256 public decimals = 18;
    
    address Founders = 0x8F871A346038A453aC4FC08e24DE29fdcc7c9A27;
    address DevelopmentTeam = 0x9C222832589cAB40A5b2557c0f5655dB19Acc10c;
    address LeewayEduReward = 0xF5d9127062ef094389fAE07DA59b55071325824C;
    address LeewayFoundation = 0x712C23d64B736fD2e016dA8E6F9455a70603448C;
    address TokenOwner = 0xF5d9127062ef094389fAE07DA59b55071325824C;
    address LEWT = 0xEE5b86e70eE40c7b002a99a9D07194455b5Ec695;
    
    mapping(address => locking) private locked;
    struct locking{
        uint256 locked;
        uint256 unlockDate;
    }
   
    // ------------------------------------------------------------------------
    // Constructor
    // ------------------------------------------------------------------------
    constructor() public {
        locked[Founders].locked = locked[Founders].locked.add(1200000 * 10 ** (decimals));
        locked[Founders].unlockDate = 1717200000; // 1st june 2024, 12 am GMT
        
        locked[Founders].unlockDate = 1633046400; // 1st OCT 2021, 12 am GMT
        locked[DevelopmentTeam].locked = locked[DevelopmentTeam].locked.add(300000 * 10 ** (decimals));
        
        locked[Founders].unlockDate = 1648771200; // 1st april 2022, 12 am GMT
        locked[LeewayEduReward].locked = locked[LeewayEduReward].locked.add(3500000 * 10 ** (decimals));
        
        locked[LeewayFoundation].unlockDate = 1659312000; // 1st aug, 2022, 12 am GMT
        locked[LeewayFoundation].locked = locked[LeewayFoundation].locked.add(500000 * 10 ** (decimals));
        
    }
    
    function ClaimTokens() external {
        uint256 toClaim = locked[msg.sender].locked;
        require(toClaim > 0, "Insufficient amount to be claimed");
        require(locked[msg.sender].unlockDate < block.timestamp, "Tokens are locked");
        locked[msg.sender].locked = 0;
        require(IBEP20(LEWT).transfer(msg.sender, toClaim), "Error sending tokens");
    }
    
}