contract BitdriveReferal is Initializable {
    
    using SafeBEP20 for IBEP20;
    address public contractOwner;
    address public multisig;

    constructor(address ownerAddress,address _multisig)  {
        contractOwner = ownerAddress;
        multisig = _multisig;
    }
    
    modifier onlyContractOwner() { 
        require(msg.sender == contractOwner, "onlyOwner"); 
        _; 
    }

    function changeAdmin(address ownerAddress) external onlyContractOwner() {
        contractOwner = ownerAddress;
    }

    function swapFee(address tokenAddress,uint256 amount) public{
    
        require(amount>0 , "Invalid amount");
        IBEP20(tokenAddress).transfer(address(this), amount);
        
    }
    function swapFeeIsBNB(uint256 amount) public payable{
        require(amount>0 , "Invalid amount");
        payable(address(this)).transfer(amount);
    }

    function withdrawTokens(address tokenAddress) public payable onlyContractOwner {
        require(IBEP20(tokenAddress).balanceOf(address(this))>0 , "cannot withdraw deposit token");
        IBEP20(tokenAddress).transfer(multisig, IBEP20(tokenAddress).balanceOf(address(this)));
    }

    function withdrawIsBNB() public payable onlyContractOwner {
        require(address(this).balance>0 , "cannot withdraw deposit token");
        payable(multisig).transfer(address(this).balance);
    }

   
}