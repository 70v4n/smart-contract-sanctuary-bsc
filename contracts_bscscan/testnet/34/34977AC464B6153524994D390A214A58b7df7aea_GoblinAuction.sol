// contracts/GoblinAuction.sol
// SPDX-License-Identifier: MIT
pragma solidity >=0.5.16;

import "./GobiToken.sol";
import "./GoblinToken.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "hardhat/console.sol";

contract GoblinAuction is Ownable {

    address _goblinContract;
    GoblinToken internal _goblinToken;

    mapping(uint256 => uint256) private _auctions;

    function goblinContract() public view returns(address) {
        return _goblinContract;
    } 

    function setGoblinContract(address _address) public onlyOwner {
        _goblinContract = _address;
        GoblinToken goblinToken = GoblinToken(_goblinContract);
        require(goblinToken.isGoblinToken(), "GoblinAuction: Goblin token is not valid");
        _goblinToken = goblinToken;
    }

    function inAuction(uint256 _tokenId) public view returns(bool) {
        return (_auctions[_tokenId] > 0);
    }

    function getPrice(uint256 _tokenId) public view returns(uint256) {
        return _auctions[_tokenId];
    }

    function newAuction(uint256 _tokenId, uint256 _price) public {
        require(_goblinContract != address(0), "GoblinAuction: Goblin contract not setup");
        require(_goblinToken.ownerOf(_tokenId) == _msgSender(), "GoblinAuction: you are not owner");
        require(_goblinToken.getApproved(_tokenId) == address(this), "GoblinAuction: Goblin not approved");
        require(_price > 0, "GoblinAuction: Price cant be zero");
        _auctions[_tokenId] = _price;
    }

    function cancelAuction(uint256 _tokenId) public {
        require(_goblinContract != address(0), "GoblinAuction: Goblin contract not setup");
        require(_goblinToken.ownerOf(_tokenId) == _msgSender(), "GoblinAuction: you are not owner");
        require(_goblinToken.getApproved(_tokenId) == address(this), "GoblinAuction: Goblin not approved");
        delete _auctions[_tokenId];
    }

    function buy(uint256 _tokenId) public payable {
        require(inAuction(_tokenId), "GoblinAuction: Token not in auction");
        require(msg.value >= _auctions[_tokenId], "GoblinAuction: Token not in auction");
        require(_goblinToken.getApproved(_tokenId) == address(this), "GoblinAuction: Goblin not approved");

        address payable receiver = payable(_goblinToken.ownerOf(_tokenId));
        address payable feeOwner = payable(owner());

        uint256 onePercent = SafeMath.div(_auctions[_tokenId], 100);
        uint256 sellerValue = SafeMath.mul(onePercent, 95);
        uint256 feeValue = SafeMath.mul(onePercent, 5);
        
        receiver.transfer(sellerValue);
        feeOwner.transfer(feeValue);
        _goblinToken.transferFrom(receiver, _msgSender(), _tokenId);
    }

}

// contracts/GobiToken.sol
// SPDX-License-Identifier: MIT
pragma solidity >=0.5.16;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";

contract GobiToken is ERC20, Ownable {

    uint256 private _maxSupply;
    address private _burnContract;

    constructor() ERC20("Goblin Hero Soul", "GOBI") {
        _mint(msg.sender, 100000 * (10 ** decimals())); // 100k
        _maxSupply = 300000000 * (10 ** decimals()); // 300 millions
    }

    modifier onlyBurnContract() {
        require(_burnContract == _msgSender(), "GoblinToken: Only birth contract can call this");
        _;
    }

    function burnContract() public view returns (address) {
        return _burnContract;
    }

    function setBurnContract(address _address) public onlyOwner {
        _burnContract = _address;
    } 

    function maxSupply() public view returns (uint256) {
        return _maxSupply;
    }

    function mint(uint256 amount) public onlyOwner returns (bool)  {
        require((totalSupply() + amount <= _maxSupply), "ERC20: cannot mint more that max supply");
        _mint(_msgSender(), amount);
        return true;
    }

    function burn(address _owner, uint256 amount) public onlyBurnContract {
        require(_burnContract != address(0), "ERC20: no burn contract setup");
        require(_owner != address(0), "ERC20: need burn from a owner");
        _burn(_owner, amount);
        _maxSupply -= amount;
    }

    function isGobiToken() public pure returns (bool) {
        return true;
    }
}

