pragma solidity ^0.8.0;

////import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";

////import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

interface IMysterBox {

    function mint(address to, uint256 id, uint256 amount, bytes memory data) external;

}

contract BoxStore is AccessControlEnumerable, ReentrancyGuard {

    bytes32 public constant OPERATOR_ROLE = keccak256("OPERATOR_ROLE");

    event AdminWalletUpdated(address wallet);
    event RoundUpdated(uint256 round, uint256 boxPrice, uint256 totalSupply, bool enableSale, bool enablePreOrder, uint256 claimOrderAt);
    event WhitelistUpdated(address[] accounts, uint256[] numBoxes);

    event BoxBought(uint256 round, address buyer, uint256 quantity, uint256 price);
    event BoxPreOrdered(uint256 round, address buyer, uint256 quantity, uint256 price);
    event BoxClaimed(uint256 round, address account, uint256 quantity);

    IMysterBox public boxContract;

    address public adminWallet;

    struct Round {
        uint256 boxPrice;
        uint256 totalSupply;
        uint256 totalSold;
        bool enableSale;
        bool enablePreOrder;
        uint256 claimOrderAt;
    }

    // round id => round information
    mapping(uint256 => Round) public rounds;

    // user address => number of boxes that user can buy
    mapping(address => uint256) public whitelist;

    // user address => number of boxes that user pre-ordered
    mapping(address => uint256) public orders;

    // round id => user address => number of boxes that user bought
    mapping(uint256 => mapping(address => uint256)) public boxBought;

    constructor(IMysterBox _boxContract, address _adminWallet) {
        boxContract = _boxContract;
        adminWallet = _adminWallet;

        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(OPERATOR_ROLE, _msgSender());
    }

    function setAdminWallet(address _adminWallet)
        public
    {
        require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "BoxStore: must have admin role to set");

        require(_adminWallet != address(0), "BoxStore: address must be not zero");

        adminWallet = _adminWallet;

        emit AdminWalletUpdated(_adminWallet);
    }

    function setRound(uint256 _round, uint256 _boxPrice, uint256 _totalSupply, bool _enableSale, bool _enablePreOrder, uint256 _claimOrderAt)
        public
    {
        require(hasRole(OPERATOR_ROLE, _msgSender()), "BoxStore: must have operator role to set");

        Round storage round = rounds[_round];

        if (round.boxPrice != _boxPrice) {
            round.boxPrice = _boxPrice;
        }

        if (round.totalSupply != _totalSupply) {
            round.totalSupply = _totalSupply;
        }

        if (round.enableSale != _enableSale) {
            round.enableSale = _enableSale;
        }

        if (round.enablePreOrder != _enablePreOrder) {
            round.enablePreOrder = _enablePreOrder;
        }

        if (round.claimOrderAt != _claimOrderAt) {
            round.claimOrderAt = _claimOrderAt;
        }

        require(round.totalSupply >= round.totalSold, "BoxStore: total supply must be greater or equal than total sold");

        emit RoundUpdated(_round, _boxPrice, _totalSupply, _enableSale, _enablePreOrder, _claimOrderAt);
    }

    function setWhitelist(address[] memory _accounts, uint256[] memory _numBoxes)
        public
    {
        require(hasRole(OPERATOR_ROLE, _msgSender()), "BoxStore: must have operator role to set");

        uint256 length = _accounts.length;

        require(length > 0 && length == _numBoxes.length, "BoxStore: array length is invalid");

        for (uint256 i = 0; i < length; i++) {
            whitelist[_accounts[i]] = _numBoxes[i];
        }

        emit WhitelistUpdated(_accounts, _numBoxes);
    }

    function _buyBox(uint256 _round, address _buyer, uint256 _quantity, uint256 _deposit)
        internal
    {
        require(_quantity > 0, "BoxStore: quantity must be not zero");

        Round storage round = rounds[_round];

        require(round.boxPrice > 0, "BoxStore: price must be not zero");

        require(round.totalSold + _quantity <= round.totalSupply, "BoxStore: the remaining quantity is not enough");

        require(round.boxPrice * _quantity == _deposit, "BoxStore: deposit amount is not enough");

        require(boxBought[_round][_buyer] + _quantity <= whitelist[_buyer], "BoxStore: can not buy more");

        boxBought[_round][_buyer] += _quantity;

        round.totalSold += _quantity;

        payable(adminWallet).transfer(_deposit);
    }

    function buyBox(uint256 _round, uint256 _quantity)
        public
        payable
        nonReentrant
    {
        require(rounds[_round].enableSale, "BoxStore: sale function was disabled");

        address msgSender = _msgSender();

        _buyBox(_round, msgSender, _quantity, msg.value);

        boxContract.mint(msgSender, 2, _quantity, "");

        emit BoxBought(_round, msgSender, _quantity, rounds[_round].boxPrice);
    }

    function preOrderBox(uint256 _round, uint256 _quantity)
        public
        payable
        nonReentrant
    {
        require(rounds[_round].enablePreOrder, "BoxStore: pre-order function was disabled");

        address msgSender = _msgSender();

        _buyBox(_round, msgSender, _quantity, msg.value);

        orders[msgSender] += _quantity;

        emit BoxPreOrdered(_round, msgSender, _quantity, rounds[_round].boxPrice);
    }
 
    function claimBox(uint256 _round)
        public
        nonReentrant
    {
        require(block.timestamp >= rounds[_round].claimOrderAt, "BoxStore: it is not time yet to claim");

        address msgSender = _msgSender();

        uint256 quantity = orders[msgSender];

        require(quantity > 0, "BoxStore: has no box to claim");

        orders[msgSender] = 0;

        boxContract.mint(msgSender, 2, quantity, "");

        emit BoxClaimed(_round, msgSender, quantity);
    }

}