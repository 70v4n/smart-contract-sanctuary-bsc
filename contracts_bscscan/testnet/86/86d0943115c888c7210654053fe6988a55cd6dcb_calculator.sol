pragma solidity ^0.8.0;

contract calculator {
   
   using SafeMath for uint; // 
    mapping (uint  =>  uint) public history;
    uint public value;
    
    function add(uint _value1, uint _value2) public{
        value = _value1.add(_value2);
        createHistory(value);
    }
    
    function sub(uint _value1, uint _value2) public{
        value = _value1.sub(_value2);
        createHistory(value);
    }
    
    function mul(uint _value1, uint _value2) public{
        value = _value1.mul(_value2);
        createHistory(value);
    }
    
    function div(uint _value1, uint _value2) public{
        value = _value1.div(_value2);
        createHistory(value);
    }
    
    function inc() public{
        value += 1; 
        createHistory(value);
    }
    
    function dec() public {
        require((value - 1) >= 0, "value must be greater than 0");
        value -= 1;
        createHistory(value);
    }
    
    
    function createHistory(uint _value1) public{
        if(history[1] == 0 ){
            history[1] = _value1;
        } else if (history[2] == 0) {
            history[2] = _value1;
        } else {
            history[1] = history[2];
            history[2] = history[3];
            history[3] = _value1;   
        }
    }
    
}