        uint amountIn,
        uint amountOutMin,
        address[] calldata path,
        address to,
        uint deadline
    ) external;
    function swapExactETHForTokensSupportingFeeOnTransferTokens(
        uint amountOutMin,
        address[] calldata path,
        address to,
        uint deadline
    ) external payable;
    function swapExactTokensForETHSupportingFeeOnTransferTokens(
        uint amountIn,
        uint amountOutMin,
        address[] calldata path,
        address to,
        uint deadline
    ) external;
}

interface IMasterChef {
    function deposit(uint256 _pid, uint256 _amount) external;

    function withdraw(uint256 _pid, uint256 _amount) external;

    function enterStaking(uint256 _amount) external;

    function leaveStaking(uint256 _amount) external;

    function pendingNature(uint256 _pid, address _user) external view returns (uint256);

    function userInfo(uint256 _pid, address _user) external view returns (uint256, uint256);

    function emergencyWithdraw(uint256 _pid) external;
}




abstract contract ERC1155Receiver is ERC165, IERC1155Receiver {
    /**
     * @dev See {IERC165-supportsInterface}.
     */
    function supportsInterface(bytes4 interfaceId) public view virtual override(ERC165, IERC165) returns (bool) {
        return interfaceId == type(IERC1155Receiver).interfaceId || super.supportsInterface(interfaceId);
    }
}



contract Referral is Ownable, Pausable, ERC1155Receiver {
     using SafeMath for uint256;
     using SafeBEP20 for IBEP20;

    struct UserInfo {
        uint256 shares; // number of shares for a user
        uint256 lastDepositedTime; // keeps track of deposited time for potential penalty
        uint256 natureAtLastUserAction; // keeps track of nature deposited at the last user action
        uint256 lastUserActionTime; // keeps track of the last user action time
    }

    IUniswapV2Router02 public uniswapV2Router;
    IUniswapV2Pair public uniswapV2Pair;
    IBEP20 public usdt;
    IBEP20 public token;
    ERC1155Token public erc1155;

    IMasterChef public immutable masterchef;

    mapping(address => UserInfo) public userInfo;

    uint256 public totalShares;
    uint256 public lastHarvestedTime;

    //The number of referral performed by a user
    mapping(address => uint256) public referralCount;
    //Referree to Referrer
    mapping(address => address) public referrer;
    //Has the Referree Deposited before;
    mapping(address => bool) public referreeHasDeposit;

    event Deposit(address indexed sender, uint256 amount, uint256 shares, uint256 lastDepositedTime);
    event Withdraw(address indexed sender, uint256 amount, uint256 shares);
    event Harvest(address indexed sender, uint256 performanceFee, uint256 callFee);
    event Pause();
    event Unpause();

    struct UserRank {
        uint noOfReferral;
        address addr;
    }

    UserRank[10] public top10UserRank;


    constructor(address _uniswapV2Pair, address _uniswapV2Router, address _usdt, address _n, address _erc1155, address _masterchef) public{
            uniswapV2Router = IUniswapV2Router02(_uniswapV2Router);
            uniswapV2Pair = IUniswapV2Pair(_uniswapV2Pair);
            usdt = IBEP20(_usdt);
            token = IBEP20(_n);
            erc1155 = ERC1155Token(_erc1155);
            masterchef = IMasterChef(_masterchef);
    }

    modifier notContract() {
        require(!_isContract(msg.sender), "contract not allowed");
        require(msg.sender == tx.origin, "proxy contract not allowed");
        _;
    }

    function infiniteApprove() public onlyOwner{
        IBEP20(token).safeApprove(address(masterchef), uint256(-1));
    }
                                                        
    function deposit(uint256 usdtAmount, address _referrer) external whenNotPaused notContract {
        require(usdtAmount >= 100e6, "Referral: usdt must be more than 100");
        require(_referrer != msg.sender, "Referral: Cannot refer self");
        require(usdt.balanceOf(msg.sender) >= usdtAmount, "Referral: Insufficient Funds");
        require(_referrer != address(0), "Referral: Cannot be address 0");

        if(referrer[msg.sender] != _referrer && referrer[msg.sender] != address(0)){
            _referrer = referrer[msg.sender];
        } 




        usdt.transferFrom(msg.sender,address(this), usdtAmount);
        // approve token transfer to cover all possible scenarios
        usdt.approve(address(uniswapV2Router), usdtAmount);

        address[] memory path = new address[](2);
        path[0] = address(usdt);
        path[1] = address(token);
        uint256[] memory amounts = uniswapV2Router.swapExactTokensForTokens(
        usdtAmount,
        0,
        path,
        msg.sender,
        block.timestamp
        );
        // deposit Nature    
        _deposit(amounts[1],msg.sender);
        uint256 nftId = 1;

        if(!referreeHasDeposit[msg.sender]){
            referrer[msg.sender] = _referrer;
            _addRefereeToReferrer(referrer[msg.sender]);
            if(referralCount[_referrer] % 10 == 0){
                nftId = 6;
            }
            
            erc1155.mint(_referrer, nftId,1);
            erc1155.mint(msg.sender, 1,1);
        }
        referreeHasDeposit[msg.sender] = true;
        
    }

    function _deposit(uint256 _amount, address _user) internal {
        require(_amount > 0, "Nothing to deposit");

        uint256 pool = balanceOf();
        token.safeTransferFrom(_user, address(this), _amount);
        uint256 currentShares = 0;
        if (totalShares != 0) {
            currentShares = (_amount.mul(totalShares)).div(pool);
        } else {
            currentShares = _amount;
        }
        UserInfo storage user = userInfo[_user];

        user.shares = user.shares.add(currentShares);
        user.lastDepositedTime = block.timestamp;

        totalShares = totalShares.add(currentShares);

        user.natureAtLastUserAction = user.shares.mul(balanceOf()).div(totalShares);
        user.lastUserActionTime = block.timestamp;

        _earn();
        emit Deposit(_user, _amount, currentShares, block.timestamp);
    }

    function _addRefereeToReferrer(address _referrer) internal {

        referralCount[_referrer] = referralCount[_referrer].add(1);
        _elaborateTopX(_referrer,referralCount[_referrer]);
        
    }

    function _earn() internal {

        uint256 bal = available();
        if (bal > 0) {
            IMasterChef(masterchef).enterStaking(bal);
        }
    }

    function withdrawAll() external notContract {
        withdraw(userInfo[msg.sender].shares);
    }




    function withdraw(uint256 _shares) public notContract {
        UserInfo storage user = userInfo[msg.sender];
        require(_shares > 0, "Nothing to withdraw");
        require(_shares <= user.shares, "Withdraw amount exceeds balance");

        uint256 currentAmount = (balanceOf().mul(_shares)).div(totalShares);
        uint256 credit = creditAvailable();
        uint256 creditAmount =  (credit.mul(_shares)).div(totalShares); 

        user.shares = user.shares.sub(_shares);
        totalShares = totalShares.sub(_shares);

        uint256 bal = available();
        if (bal < currentAmount) {
            uint256 balWithdraw = currentAmount.sub(bal);
            IMasterChef(masterchef).leaveStaking(balWithdraw);
            uint256 balAfter = available();
            uint256 diff = balAfter.sub(bal);
            if (diff < balWithdraw) {
                currentAmount = bal.add(diff);
            }
        }



        if (user.shares > 0) {
            user.natureAtLastUserAction = user.shares.mul(balanceOf()).div(totalShares);
        } else {
            user.natureAtLastUserAction = 0;
        }

        user.lastUserActionTime = block.timestamp;

        token.safeTransfer(msg.sender, currentAmount);
        erc1155.safeTransferFrom(address(this), msg.sender, 0, creditAmount, "");

        emit Withdraw(msg.sender, currentAmount, _shares);
    }



    function emergencyWithdraw() external onlyOwner {
        IMasterChef(masterchef).emergencyWithdraw(0);
    }

    function _elaborateTopX(address addr, uint currentValue) internal {
        uint i = 0;
        UserRank memory userRankToReplace;
        /** get the index of the current max element **/

        for(i; i < 10; i++) {
            if(top10UserRank[i].noOfReferral < currentValue) {
                userRankToReplace = top10UserRank[i];
                break;
            }
        }
        /** shift the array of position (getting rid of the last element) **/
        for(uint j = 9; j > i; j--) {
            top10UserRank[j]= top10UserRank[j - 1];
            top10UserRank[j]= top10UserRank[j - 1];
        }

        /** update the new max element **/
        userRankToReplace.noOfReferral = currentValue;
        userRankToReplace.addr = addr;

        top10UserRank[i] = userRankToReplace;
    }

    function viewTop10User() public view returns(address[] memory){
        address[] memory a;
        for(uint i = 0 ; i < top10UserRank.length ; i ++){
            a[i]=top10UserRank[i].addr;
        }
        return a;
    }

    function pause() external onlyOwner whenNotPaused {
        _pause();
        emit Pause();
    }

    /**
     * @notice Returns to normal state
     * @dev Only possible when contract is paused.
     */
    function unpause() external onlyOwner whenPaused {
        _unpause();
        emit Unpause();
    }

    function available() public view returns (uint256) {
        return token.balanceOf(address(this));
    }

    function creditAvailable() public view returns (uint256){
        return erc1155.balanceOf(address(this),0);
    }

    function balanceOf() public view returns (uint256) {
        (uint256 amount, ) = IMasterChef(masterchef).userInfo(0, address(this));
        return token.balanceOf(address(this)).add(amount);
    }


    function getPricePerFullShare() external view returns (uint256) {
        return totalShares == 0 ? 1e18 : balanceOf().mul(1e18).div(totalShares);
    }


    function inCaseTokensGetStuck(address _token) external onlyOwner {
        require(_token != address(token), "Token cannot be same as deposit token");
        require(_token != address(usdt), "Token cannot be same as receipt token");

        uint256 amount = IBEP20(_token).balanceOf(address(this));
        IBEP20(_token).safeTransfer(msg.sender, amount);
    }

    function pendingNatureCredit(address _user) external view returns (uint256){
        UserInfo storage user = userInfo[_user];
        uint256 amount = erc1155.balanceOf(address(this), 0);
        amount = amount.add(IMasterChef(masterchef).pendingNature(0, _user));
        return amount.mul(user.shares).div(totalShares);
    }


    function _isContract(address addr) internal view returns (bool) {
        uint256 size;
        assembly {
            size := extcodesize(addr)
        }
        return size > 0;
    }

    function onERC1155BatchReceived(
        address,
        address,
        uint256[] calldata,
        uint256[] calldata,
        bytes calldata 
    ) external override virtual returns (bytes4){
        return bytes4(keccak256("onERC1155BatchReceived(address,address,uint256[],uint256[],bytes)"));
    }

    function onERC1155Received(
        address,
        address,
        uint256,
        uint256,
        bytes calldata 
    ) external override virtual returns (bytes4){
        return bytes4(keccak256("onERC1155Received(address,address,uint256,uint256,bytes)"));
    }

    


}