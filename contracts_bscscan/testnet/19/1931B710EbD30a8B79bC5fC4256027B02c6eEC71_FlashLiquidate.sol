            //CTokenInterface(usdtAddr).approve(eUSDTAddr,0);
            isApprove=CTokenInterface(usdtAddr).approve(eUSDTAddr,maxClose);

            //判断是否有坏账 判断抵押品最大可清算金额
            (MathError err1, uint mulMantissa) =mulUInt(filAmount,100);
            (MathError err2, uint userCanRedeemCount) =divUInt(mulMantissa,108);

            emit StartLiquidate(filMulColAmount, usdtAmount,maxClose,userCanRedeemCount,isApprove);
            if(userCanRedeemCount>usdtAmount){
                _liquidateBorrowFresh(borrower,maxClose);
            }else{
                //执行清算
                _liquidateBorrowFresh(borrower,userCanRedeemCount);
            }
        }
    }

    function _liquidateBorrowFresh(address borrower,uint maxClose) internal {
        CErc20Interface(eUSDTAddr).liquidateBorrow(borrower,maxClose,CTokenInterface(eFilAddr));

        // 获取合约中efil的数量
        uint efilTokens = CTokenInterface(eFilAddr).balanceOf(address(this));
        // 将efil赎回成fil
        _redeem(efilTokens);
    }

    function _redeem(uint redeemTokens) internal {
        CErc20Interface(eFilAddr).redeem(redeemTokens);

        //获取赎回后的fil数量
        uint filCount = CTokenInterface(filAddr).balanceOf(address(this));
        emit LiquidateInfo(redeemTokens,filCount);
    }
    
    function setOracleAddr(address newOracleAddr) public {
        oracleAddr=newOracleAddr;
    }

    function setApprove(address _usdtAddr,address _eUSDTAddr,uint amount) public {
        //TetherToken(_usdtAddr).approve(_eUSDTAddr,amount);
        CTokenInterface(_usdtAddr).approve(_eUSDTAddr,amount);
    }
}