abstract contract ERC20Interface {
    function name() public virtual view returns (string memory);
    function symbol() public virtual view returns (string memory);
    function decimals() public virtual view returns (uint8);
    function totalSupply() public virtual view returns (uint256);
    function balanceOf(address tokenOwner) public virtual view returns (uint256 balance);
    function allowance(address tokenOwner, address spender) public virtual view returns (uint256 remaining);
    function transfer(address to, uint256 tokens) public virtual returns (bool success);
    function approve(address spender, uint256 tokens) public virtual returns (bool success);
    function transferFrom(address from, address to, uint256 tokens) public virtual returns (bool success);

    event Transfer(address indexed from, address indexed to, uint256 tokens);
    event Approval(address indexed tokenOwner, address indexed spender, uint256 tokens);
}


abstract contract Tokenomics {
    using SafeMath for uint;
    // --------------------- Token Settings ------------------- //

    string internal constant NAME = "Rich Tiger Token";
    string internal constant SYMBOL = "TIGER";
    uint16 internal constant FEES_DIVISOR = 10**2;
    
    uint8 internal constant DECIMALS = 18;
    uint256 internal constant TOTAL_SUPPLY = 10**30;
    
    //uint256 internal constant numberOfTokensToSwapToLiquidity = TOTAL_SUPPLY / 1000; // 0.1% of the total supply

    // --------------------- Fees Settings ------------------- //

    uint internal charityFee = 5;
    uint internal marketingFee = 3;
    uint internal burnFee = 2;

    address internal charityAddress = 0x518C417a05efc17B59Ab5dB31d8f4e3e0aBDB3A0;
    address internal marketingAddress = 0x445e93c06dD420eD60F34944debeeE8f929fD933;
    address internal burnAddress = 0x0000000000000000000000000000000299792458;

    enum FeeType { Antiwhale, Burn, Liquidity, Rfi, External, ExternalToBNB }
    struct Fee {
        FeeType name;
        uint256 value;
        address recipient;
        uint256 total;
    }

    Fee[] internal fees;
    uint256 internal sumOfFees;

    constructor() {
        _addFees();
    }

    function _addFee(FeeType name, uint256 value, address recipient) private {
        
        fees.push( Fee(name, value, recipient, 0 ) );
        sumOfFees += value;
    }

    function _addFees() private {

        _addFee(FeeType.Burn, burnFee, burnAddress );
        _addFee(FeeType.External, charityFee, charityAddress );
        _addFee(FeeType.External, marketingFee, marketingAddress );

    }

    function getSumOfFees() public view returns (uint){return sumOfFees;}

    function _getFeesCount() internal view returns (uint256){ return fees.length; }

    function _getFeeStruct(uint256 index) private view returns(Fee storage){
        require( index >= 0 && index < fees.length, "FeesSettings._getFeeStruct: Fee index out of bounds");
        return fees[index];
    }

    function _getFee(uint256 index) internal view returns (FeeType, uint256, address, uint256){
        Fee memory fee = _getFeeStruct(index);
        return ( fee.name, fee.value, fee.recipient, fee.total );
    }

    function _addFeeCollectedAmount(uint256 index, uint256 amount) internal {
        Fee storage fee = _getFeeStruct(index);
        fee.total = fee.total.add(amount);
    }

    // function getCollectedFeeTotal(uint256 index) external view returns (uint256){
    function getCollectedFeeTotal(uint256 index) internal view returns (uint256){
        Fee memory fee = _getFeeStruct(index);
        return fee.total;
    }

}




contract TigerContract is ERC20Interface, Tokenomics, Ownable, Presaleable {

    using SafeMath for uint256;

    mapping(address => uint256) balances;
    mapping(address => mapping(address => uint256)) allowed;
    mapping (address => mapping (address => uint256)) internal _allowances;

    mapping (address => bool) internal _isExcludedFromFee;
    mapping (address => bool) internal _isExcludedFromRewards;
    address[] private _excluded;

    constructor() {
        // exclude owner and this contract from fee
        //_approve(owner(),address(_router), ~uint256(0));
        _isExcludedFromFee[owner()] = true;
        _isExcludedFromFee[address(this)] = true;
        
        // exclude the owner and this contract from rewards
        _exclude(owner());
        _exclude(address(this));
        balances[msg.sender] = TOTAL_SUPPLY;

        _exclude(burnAddress);

        emit Transfer(address(0), msg.sender, TOTAL_SUPPLY);
    }

    
    function name() public view override returns (string memory) { return NAME; }
    function symbol() public view override returns (string memory) { return SYMBOL; }
    function decimals() public view override returns (uint8) { return DECIMALS; }
    
    /** Functions required by IERC20Metadat - END **/
    /** Functions required by IERC20 **/
    function totalSupply() public view override returns (uint256) {
        return TOTAL_SUPPLY;
    }

    function excludeFromReward(address account) external onlyOwner() {
        require(!_isExcludedFromRewards[account], "Account is not included");
        _exclude(account);
    }
    
    function _exclude(address account) internal {
        _isExcludedFromRewards[account] = true;
        _excluded.push(account);
    }

    function balanceOf(address tokenOwner) public override view returns (uint256 balance) {
        return balances[tokenOwner];
    }

    function allowance(address tokenOwner, address spender) public override view returns (uint256 remaining) {
        return allowed[tokenOwner][spender];
    }

    function _takeFee(uint256 amount, uint256 fee, address recipient) private {
        uint256 tAmount = amount.mul(fee).div(FEES_DIVISOR);
        balances[recipient] = balances[recipient].add(tAmount);
    }

    function _takeTransactionFees(uint256 amount) internal  {
        
        uint256 feesCount = _getFeesCount();
        for (uint256 index = 0; index < feesCount; index++ ){
            (FeeType name, uint256 value, address recipient,) = _getFee(index);
            // no need to check value < 0 as the value is uint (i.e. from 0 to 2^256-1)
            if ( value == 0 ) continue;
           
            else if ( name == FeeType.Burn ){
                burn( amount );
            }
            else {
                _takeFee( amount, value, recipient );
            }
        }
    }
    
    function _transfer(address from, address to, uint256 tokens, bool takeFee) private returns (bool success) {

        if(_isExcludedFromFee[msg.sender] || _isExcludedFromFee[to]){ takeFee = false; }
        if(isInPresale){
            takeFee = false;
        }

        //_beforeTokenTransfer(msg.sender, to, tokens, takeFee);
        _transferTokens(msg.sender, to, tokens, takeFee);


        // uint256 amountToCharity = tokens.mul(charityFee).div(FEES_DIVISOR);
        // uint256 amountToMarketing = tokens.mul(marketingFee).div(FEES_DIVISOR); // 5% of the transaction shall be burned
        // uint256 amountToBurn = tokens.mul(burnFee).div(FEES_DIVISOR); // 5% of the transaction shall be burned

        uint amountFees = 0;

        if(takeFee){
            amountFees = tokens.mul(sumOfFees).div(FEES_DIVISOR); 
        }

        uint256 amountToTransfer = tokens.sub(amountFees);
        
        balances[from] = balances[from].sub(tokens);
        balances[to] = balances[to].add(amountToTransfer);
        return true;
    }

    function _transferTokens(address sender, address recipient, uint256 amount, bool takeFee) private {
    
        
       
        if ( takeFee ){
            _takeTransactionFees(amount);
        }
        //emit Transfer(sender, recipient, tTransferAmount);
    }

    
    function transfer(address to, uint256 tokens) public override returns (bool success) {
        require(msg.sender != address(0), "TigerToken: transfer from the zero address");
        require(to != address(0), "TigerToken: transfer to the zero address");
        require(msg.sender != address(burnAddress), "TigerToken: transfer from the burn address");
        require(tokens > 0, "Transfer amount must be greater than zero");
        
        // indicates whether or not feee should be deducted from the transfer
        bool takeFee = true;
        

        // if any account belongs to _isExcludedFromFee account then remove the fee
        if(_isExcludedFromFee[msg.sender] || _isExcludedFromFee[to]){ takeFee = false; }

        //beforeTokenTransfer(msg.sender, to, amount, takeFee);
        
        _transfer(msg.sender, to, tokens, takeFee);
        emit Transfer(msg.sender, to, tokens);
        return true;
    }

    function _beforeTokenTransfer(address sender, address , uint256 , bool ) internal  {
       
    }


    function approve(address spender, uint256 tokens) public override returns (bool success) {
        allowed[msg.sender][spender] = tokens;
        emit Approval(msg.sender, spender, tokens);
        return true;
    }

    function transferFrom(address from, address to, uint256 tokens) public override returns (bool success) {
        //allowed[from][msg.sender] = allowed[from][msg.sender].sub(tokens);
        transfer(to, tokens);
        return true;
    }


    function burn(uint256 amount) public {

        address sender = _msgSender();
        require(sender != address(0), "Tiger: burn from the zero address");
        require(sender != address(burnAddress), "Tiger: burn from the burn address");

        uint256 balance = balanceOf(sender);
        require(balance >= amount, "Tiger: burn amount exceeds balance");

        // remove the amount from the sender's balance first

        balances[sender] = balances[sender].sub(amount);
        _burnTokens( sender, amount );
    }

    function increaseAllowance(address spender, uint256 addedValue) public virtual returns (bool) {
        _approve(_msgSender(), spender, _allowances[_msgSender()][spender].add(addedValue));
        return true;
    }
    
    function decreaseAllowance(address spender, uint256 subtractedValue) public virtual returns (bool) {
        _approve(_msgSender(), spender, _allowances[_msgSender()][spender].sub(subtractedValue, "ERC20: decreased allowance below zero"));
        return true;
    }

    function setExcludedFromFee(address account, bool value) external onlyOwner { _isExcludedFromFee[account] = value; }
    function isExcludedFromFee(address account) public view returns(bool) { return _isExcludedFromFee[account]; }
    
    function _approve(address owner, address spender, uint256 amount) internal {
        require(owner != address(0), "Token: approve from the zero address");
        require(spender != address(0), "Token: approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }
    
    function isExcludedFromReward(address account) external view returns (bool) {
        return _isExcludedFromRewards[account];
    }
    
    
    function _burnTokens(address sender, uint256 tBurn) internal {
        balances[burnAddress] = balances[burnAddress].add(tBurn);
        emit Transfer(sender, burnAddress, tBurn);
    }

    function _approveDelegate(address owner, address spender, uint256 amount) internal  {
        _approve(owner, spender, amount);
    }


}

