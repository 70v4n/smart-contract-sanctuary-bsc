/*
00 - Genre     25 - RACE
01 - Skin MM   24 - EYCL B
02 - Skin R1   23 - EYCL G
03 - Skin R2   22 - EYCL R
04 - Hair MM   21 - HRCL B
05 - Hair R1   20 - HRCL G
06 - Hair R2   19 - HRCL R
07 - Eyes MM   18 - SKCL B
08 - Eyes R1   17 - SKCL G
09 - Eyes R2   16 - SKCL R
10 - Ears MM   15 - Mout R2
11 - Ears R1   14 - Mout R1
12 - Ears R2   13 - Mout MM
13 - Mout MM   12 - Ears R2
14 - Mout R1   11 - Ears R1
15 - Mout R2   10 - Ears MM 
16 - SKCL R    09 - Eyes R2
17 - SKCL G    08 - Eyes R1
18 - SKCL B    07 - Eyes MM
19 - HRCL R    06 - Hair R2
20 - HRCL G    05 - Hair R1
21 - HRCL B    04 - Hair MM
22 - EYCL R    03 - Skin R2
23 - EYCL G    02 - Skin R1
24 - EYCL B    01 - Skin MM 
25 - RACE      00 - Genre  
*/

contract BasicGeneRules is IGeneRules {

    uint256 salt = 0;
    bytes1 constant FOREST = 0x31;
    bytes1 constant DESERT = 0x32;
    bytes1 constant CAVE = 0x33;
    bytes1 constant MOUNTAIN = 0x34;
    bytes1 constant DARK = 0x35;
    bytes1 constant SEA = 0x36; 
    bytes1 constant MALE = 0x6d; 
    bytes1 constant FEMALE = 0x66;

    uint8 constant SKIN = 1;
    uint8 constant HAIR = 4;
    uint8 constant EYES = 7;
    uint8 constant EAR = 10;
    uint8 constant MOUTH = 13;
    uint8 constant RACE = 25;

    uint8 constant ISKIN = 30;
    uint8 constant IHAIR = 27;
    uint8 constant IEYES = 24;
    uint8 constant IEARS = 21;
    uint8 constant IMOUTH = 18;
    uint8 constant IRACE = 6;
    uint8 constant IGENRE = 31;

    uint8 constant ICOLORSKIN = 15;
    uint8 constant ICOLORHAIR = 12;
    uint8 constant ICOLOREYES = 9;

    bytes1[] RACES = [FOREST, DESERT, CAVE, MOUNTAIN, DARK, SEA];
    bytes1[] GENRE = [MALE, FEMALE];

    function getByte(uint256 _gene, uint8 i) public pure returns (bytes1) {
        bytes32 g = bytes32(_gene);
        return g[i];
    }

    function getRByte(uint256 _gene, uint8 i) internal returns (bytes1) {
        bytes32 g = bytes32(_gene);
        bytes1 r = g[i];
        /*
        for (uint x = 0; x < g.length; x++) {
            console.log(x, ", ", uint8(g[x]));
        }
        */
        //console.log(i, ", ", uint8(r));
        if (r == FOREST || r == DESERT || r == CAVE || r == MOUNTAIN || r == DARK || r == SEA) {
            return r;
        }
        return RACES[random() % RACES.length];
    }
    
    function random() private returns (uint) {
        salt++;
        return uint256(keccak256(abi.encodePacked(block.difficulty, block.timestamp, salt)));
    } 

    function isGeneRules() external override pure returns (bool) {
        return true;
    }

    function getColorPart(uint256 genes1, uint256 genes2, uint8 i) internal pure returns(bytes1) {
        uint16 p = (uint16(uint8(getByte(genes1, i))) + uint16(uint8(getByte(genes2, i)))) / 2;
        //uint8 r = uint8(random() % 50);
        //if ((p + r - 25) < 0) p = 0;
        //if ((p + r) > 255) p = 255;
        /*
        console.log(i, "=", uint16(uint8(getByte(genes1, i))));
        console.log(i, "=", uint16(uint8(getByte(genes2, i))));
        console.log(i, "=", p);
        */
        return bytes1(uint8(p));
    }

    function getColor(uint256 genes1, uint256 genes2, uint8 i) internal pure returns(bytes3) {
        bytes memory _colors = new bytes(3);
        _colors = bytes.concat(
            getColorPart(genes1, genes2, i),
            getColorPart(genes1, genes2, i-1),
            getColorPart(genes1, genes2, i-2)
        );
        return bytes3(_colors);
    }

    function getGene(uint256 genes1, uint256 genes2, uint8 mm) internal returns(bytes3) {
        bytes1[12] memory skins = [
            getRByte(genes1, mm),
            getRByte(genes1, mm),
            getRByte(genes1, mm),
            getRByte(genes2, mm),
            getRByte(genes2, mm),
            getRByte(genes2, mm),
            getRByte(genes1, mm - 1),
            getRByte(genes1, mm - 1),
            getRByte(genes2, mm - 1),
            getRByte(genes2, mm - 1),
            getRByte(genes1, mm - 2),
            getRByte(genes2, mm - 2)
        ];
        bytes1[3] memory skinR1 = [
            getRByte(genes1, mm),
            getRByte(genes1, mm - 1),
            getRByte(genes1, mm - 2)
        ];
        bytes1[3] memory skinR2 = [
            getRByte(genes2, mm),
            getRByte(genes2, mm - 1),
            getRByte(genes2, mm - 2)
        ];
        bytes memory _genes = new bytes(3);
        _genes = bytes.concat(
            (random() % 100 == 50) ? 
                RACES[random() % RACES.length] : 
                skins[random() % skins.length],
            skinR1[random() % skinR1.length],
            skinR2[random() % skinR2.length]
        );
        return bytes3(_genes);
    }

    function getRace(uint256 genes1, uint256 genes2, uint256 genes) internal returns(bytes1) {
        bytes1 r1 = getRByte(genes1, IRACE);
        uint8 r1Count = 0;
        r1Count += (r1 == getRByte(genes, SKIN))  ? 1 : 0; 
        r1Count += (r1 == getRByte(genes, HAIR))  ? 1 : 0; 
        r1Count += (r1 == getRByte(genes, EYES))  ? 1 : 0; 
        r1Count += (r1 == getRByte(genes, EAR))   ? 1 : 0; 
        r1Count += (r1 == getRByte(genes, MOUTH)) ? 1 : 0; 

        bytes1 r2 = getRByte(genes2, IRACE);
        uint8 r2Count = 0;
        r2Count += (r2 == getRByte(genes, SKIN))  ? 1 : 0; 
        r2Count += (r2 == getRByte(genes, HAIR))  ? 1 : 0; 
        r2Count += (r2 == getRByte(genes, EYES))  ? 1 : 0; 
        r2Count += (r2 == getRByte(genes, EAR))   ? 1 : 0; 
        r2Count += (r2 == getRByte(genes, MOUTH)) ? 1 : 0; 

        if (r1Count > r2Count) {
            return r1;
        }
        else if (r1Count < r2Count) {
            return r2;
        }
        else {
            bytes1[2] memory rs = [r1, r2];
            return rs[random() % 2];
        }
    }

    function reverse(uint256 input) internal pure returns (uint256 v) {
        v = input;

        // swap bytes
        v = ((v & 0xFF00FF00FF00FF00FF00FF00FF00FF00FF00FF00FF00FF00FF00FF00FF00FF00) >> 8) |
            ((v & 0x00FF00FF00FF00FF00FF00FF00FF00FF00FF00FF00FF00FF00FF00FF00FF00FF) << 8);

        // swap 2-byte long pairs
        v = ((v & 0xFFFF0000FFFF0000FFFF0000FFFF0000FFFF0000FFFF0000FFFF0000FFFF0000) >> 16) |
            ((v & 0x0000FFFF0000FFFF0000FFFF0000FFFF0000FFFF0000FFFF0000FFFF0000FFFF) << 16);

        // swap 4-byte long pairs
        v = ((v & 0xFFFFFFFF00000000FFFFFFFF00000000FFFFFFFF00000000FFFFFFFF00000000) >> 32) |
            ((v & 0x00000000FFFFFFFF00000000FFFFFFFF00000000FFFFFFFF00000000FFFFFFFF) << 32);

        // swap 8-byte long pairs
        v = ((v & 0xFFFFFFFFFFFFFFFF0000000000000000FFFFFFFFFFFFFFFF0000000000000000) >> 64) |
            ((v & 0x0000000000000000FFFFFFFFFFFFFFFF0000000000000000FFFFFFFFFFFFFFFF) << 64);

        // swap 16-byte long pairs
        v = (v >> 128) | (v << 128);
    }

    function getGenre(uint256 genes) external pure returns (uint8) {
        bytes32 g = bytes32(genes);
        //console.log("genes=", genes);
        //console.log("genre=", uint8(g[IGENRE]));
        return uint8(g[IGENRE]);
    }

    function mixGenes(uint256 genes1, uint256 genes2) external override returns (uint256) {

        salt = 1;

        bytes memory _genes = new bytes(26);
        _genes = bytes.concat(
            GENRE[random() % GENRE.length],
            getGene(genes1, genes2, ISKIN),
            getGene(genes1, genes2, IHAIR),
            getGene(genes1, genes2, IEYES),
            getGene(genes1, genes2, IEARS),
            getGene(genes1, genes2, IMOUTH)
        );
        _genes = bytes.concat(
            _genes,
            getColor(genes1, genes2, ICOLORSKIN),
            getColor(genes1, genes2, ICOLORHAIR),
            getColor(genes1, genes2, ICOLOREYES),
            getRace(genes1, genes2, uint256(bytes32(_genes)))
        );
        /*
        _genes = bytes.concat(
            _genes,
            bytes3(0xffffff),
            bytes3(0xffffff),
            bytes3(0xffffff),
            DESERT
        );
        */
        /*
        _genes = bytes.concat(
            _genes,
            bytes1(0),
            bytes1(0),
            bytes1(0),
            bytes1(0),
            bytes1(0),
            bytes1(0x20)
        );
        */
        console.log(reverse(uint256(bytes32(_genes))));
        return reverse(uint256(bytes32(_genes)));
    }
}

