    struct NFT {
        address nftAddr;
        uint256 ercType;
    }

    NFT[] public allNFT;
    
    function allLength() external view returns (uint) {
        return allNFT.length;
    }

    function setGameMerchant(address _gameMerchant) external  onlyOwner()  {
        gameMerchant = _gameMerchant;
    }

