pragma solidity >=0.7.0 <0.9.0;

import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

import "./interfaces/IBEP20.sol";
// import "./interfaces/IPancakeRouter.sol";
// import "./interfaces/IPresaleCreater.sol";

import "hardhat/console.sol";

contract Presale {

    using Address for address;
    using EnumerableSet for EnumerableSet.AddressSet;
    using SafeMath for uint256;

    struct PresaleInfo {
        bool isFinalized;
        bool isBnb;
        bool isLive;
        bool isWhitelisted;
        uint256 startTime;
        uint256 endTime;
        uint256 softcap;
        uint256 hardcap;
        uint256 rate;
        uint256 fundRaised;
        uint256 liquidityPercentage;
        uint256 presaleTokens;
        uint256 soldTokens;
        uint256 minBuyLimit;
        uint256 maxBuyLimit;
        uint256 participants;
    }

    address public wallet;
    address public creater;

    IBEP20 private busd;

    address public tokenAddr;
    address public presaleOwner;
    address public presaleCreater;

    bool public isFinalized;

    uint256 public softcap;
    uint256 public hardcap;
    uint256 public rate;
    uint256 public fundRaised;
    uint256 public liquidityPercentage;
    uint256 public presaleTokens;
    uint256 public soldTokens;

    uint256 public minBuyLimit;
    uint256 public maxBuyLimit;

    uint256 public startTime;
    uint256 public endTime;

    bool public isFinalised = false; 

    bool public isWhitelisted;
    EnumerableSet.AddressSet private whitelist;
    EnumerableSet.AddressSet private buyers;

    mapping(address => uint256) public balance; 

    bool public isBnb;

    // IPancakeRouter02 public router;

    uint256 public preWithdrawFee = 10;


    modifier purchase {
        require(isLive() == true, "presale is not openned");
        require(block.timestamp >= startTime, "presale has not started");
        require(block.timestamp <= endTime, "presale has ended");
        if(isWhitelisted) {
            require(whitelist.contains(msg.sender), "puchaser is not allowed for this buyer");  
        }
        _;   
    }

    modifier onlyOwner {
        require(msg.sender == presaleOwner, "only presale owner is available");
        _;
    }

    modifier onlyCreater {
        require(msg.sender == creater, "only creater is avaible");
        _;
    }

    /*
        address tokenAddr, addrs[0]
        address presaleOwner, addrs[1]

        uint256 softcap, uints[0]
        uint256 hardcap, uints[1]
        uint256 rate, uints[2]
        uint256 minBuyLimit, uints[3]
        uint256 maxBuyLimit, uints[4]
        uint256 liquidityPercentage, uints[5]
        uint256 startTime, uints[6]
        uint256 endTime, uints[7] 
        uint256 totalTokens, uints[8]

        bool isWhitelisted, bools[0]
        bool isBnb, bools[1]

        address[] memory whitelist
    */
    constructor(
        address wallet_,
        address[] memory addrs,
        uint256[] memory uints,
        bool[] memory bools,
        address[] memory whitelist_
    ) {
        require(addrs.length == 2 && uints.length == 9 && bools.length == 2, "not matching parameters");
        require(wallet_ != address(0), "wallet address must not be zero");
        require(addrs[0] != address(0), "token address must not be zero");
        require(uints[1] >= uints[0] && uints[1] <= uints[0] * 2, "hardcap must bigger than twice of softcap");
        require(uints[2] > 0, "rate must over 0");
        require(uints[4] >= uints[3], "max limit must be bigger than min limit");
        require(uints[7] > uints[6], "end time must be after start time");

        if(bools[0] == true) {
            for(uint256 i = 0; i < whitelist_.length; i++) {
                require(whitelist_[i] == address(0), "zero address shouldn't be whitelisted");
                whitelist.add(whitelist_[i]);
            }
        }

        wallet = wallet_;
        tokenAddr = addrs[0];
        presaleOwner = addrs[1];
        softcap = uints[0];
        hardcap = uints[1];
        rate = uints[2];
        minBuyLimit = uints[3];
        maxBuyLimit = uints[4];
        isWhitelisted = bools[0];
        isBnb = bools[1];
        liquidityPercentage = uints[5];
        startTime = uints[6];
        endTime = uints[7];
        presaleTokens = uints[8];

        creater = msg.sender;

        if(isBnb) {
            busd = IBEP20(0xe9e7CEA3DedcA5984780Bafc599bD69ADd087D56);
        }
    }

    function purchaseWithBnb() external payable purchase {
        require(isBnb == true, "BNB is not allowed for this presale");

        _purchase(msg.value, msg.sender);

        payable(address(this)).transfer(msg.value);
    }

    function purchaseWithBusd(uint256 amount_) external purchase {
        require(isBnb == false, "BNB is not allowed for this presale");
        require(busd.allowance(msg.sender, address(this)) >= amount_, "didn't approved to transfer");

        _purchase(amount_, msg.sender);
        
        busd.transfer(address(this), amount_);
    }

    function _purchase(uint256 amount_, address buyer_) internal {
        require(amount_ >= minBuyLimit, "value is lower than min limit");
        require(amount_ <= maxBuyLimit, "value is higher than max limit");
        require(balance[msg.sender] + amount_ <= maxBuyLimit, "limited max value");
        require(fundRaised + amount_ <= hardcap, "fund raised is over hardcap");
        require(presaleTokens.sub(soldTokens) >= amount_.mul(rate), "exceed available tokens");

        if(balance[buyer_] == 0) {
            buyers.add(buyer_);
        }
        balance[buyer_] += amount_;
        fundRaised += amount_;
        soldTokens += (amount_ * rate);
    }

    function setWhitelisted(bool state) external onlyOwner {
        require(isWhitelisted != state, "whitelisted is current setting");
        isWhitelisted = state;
    }

    function getWhitelist() external onlyOwner view returns (address[] memory){
        require(isWhitelisted == true, "whitelisted is not enabled");
        return whitelist.values();
    }

    function addWhitelist(address[] memory list_) external onlyOwner {
        require(isWhitelisted == true, "whitelisted is not enabled");

        for(uint256 i = 0; i < list_.length; i++) {
            whitelist.add(list_[i]);
        }
    }

    function removeWhitelist(address[] memory list_) external onlyOwner {
        require(isWhitelisted == true, "whitelisted is not enabled");

        for(uint256 i = 0; i < list_.length; i++) {
            if(whitelist.contains(list_[i])) {
                whitelist.remove(list_[i]);
            }
        }
    }

    function isInWhitelist(address add_) external view returns (bool) {
        return whitelist.contains(add_);
    }

    function getBuyers() external view returns (address[] memory) {
        return buyers.values();
    }

    function emergencyWithdraw() external payable {
        require(balance[msg.sender] > 0 && buyers.contains(msg.sender), "no balance to withdraw");
        require(isFinalised == false, "presale has been finalised");
        require(block.timestamp > startTime && block.timestamp + 3600 < endTime, "not available in current time");

        buyers.remove(msg.sender);
        
        if(isBnb) {
            payable(msg.sender).transfer(balance[msg.sender].mul(9).div(10));
            payable(wallet).transfer(balance[msg.sender].div(10));
        }else {
            busd.transfer(msg.sender, balance[msg.sender].mul(9).div(10));
            busd.transfer(wallet, balance[msg.sender].div(1)); 
        }

        fundRaised -= balance[msg.sender];
        balance[msg.sender] = 0; 
    }

    function cutFee(uint256 amount_) internal pure returns (uint256) {
        require(amount_ > 0, "amount should be over zero");
        return amount_.mul(9).div(10);
    }

    function finalise() external onlyOwner payable {
        require(isFinalised == false, "presale has been finalised");
        require(fundRaised > softcap || block.timestamp > endTime, "not available in current time");

        // TODO: add % of raised funds into liquidity
        // TODO: transfer unsold tokens to owner back
        
        if(isBnb) {
            payable(wallet).transfer(1 ether);
        }else {
            // getting rate of bnb from chainlink or set static value of busd
        }

        isFinalised = true;
    }

    function getNumberOfBuyers() external view returns (uint256) {
        return buyers.length();
    }
    

    function joined(address addr) external view returns (bool) {
        return buyers.contains(addr);
    }

    function isLive() public view returns (bool) {
        if(isFinalized) return false;
        if(block.timestamp < startTime && block.timestamp > endTime) return false;
        return true;
    }

    function getInfo() external view returns (PresaleInfo memory) {
        console.log("getInfo function called by:", msg.sender);
        return PresaleInfo(
            isFinalized,
            isBnb,
            isLive(),
            isWhitelisted,
            startTime,
            endTime,
            softcap,
            hardcap,
            rate,
            fundRaised,
            liquidityPercentage,
            presaleTokens,
            soldTokens,
            minBuyLimit,
            maxBuyLimit,
            buyers.length()
        );
    }
}

