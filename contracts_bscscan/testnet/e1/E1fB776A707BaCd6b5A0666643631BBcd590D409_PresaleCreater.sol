contract PresaleCreater is Auth {
    // using Address for address;

    struct PresaleList {
        string tokenName;
        address tokenAddr;
        address presaleAddr;
        address owner;
        bool isWhitelisted;
        uint256 startTime;
        uint256 endTime;
    }

    address public wallet;

    PresaleList[] private presaleList;

    constructor(address wallet_) Auth(msg.sender) {
        require(wallet_ != address(0), "wallet address must not be zero");
        wallet = wallet_;
    }

    function setWallet(address wallet_) external onlyOwner {
        wallet = wallet_;
    }

    /*
        address tokenAddr_, addrs[0]
        address presaleOwner_, addrs[1]

        uint256 softcap_, uints[0]
        uint256 hardcap_, uints[1]
        uint256 rate_, uints[2]
        uint256 minBuyLimit_, uints[3]
        uint256 maxBuyLimit_, uints[4]
        uint256 liquidityPercentage_, uints[5]
        uint256 startTime_, uints[6]
        uint256 endTime_, uints[7]
        uint256 availableTokens_, uints[8]

        bool isWhitelisted_, bools[0]
        bool isBnb_, bools[1]
        
        address[] memory whitelist_
    */

    function createPresale(
        address[] memory addrs,
        uint256[] memory uints, 
        bool[] memory bools,
        address[] memory whitelist_
    ) external {
        require(addrs.length == 2 && uints.length == 9 && bools.length == 2, "not matching parameters");
        require(addrs[0] != address(0), "token address must not be zero");
        require(addrs[1] != address(0), "presaleOwner address must not be zero");
        require(uints[1] >= uints[0] && uints[1] <= uints[0] * 2, "hardcap must bigger than twice of softcap");
        require(uints[2] > 0, "rate must be over 0");
        require(uints[4] >= uints[3], "max limit must be bigger than min limit");
        require(uints[5] > 50 && uints[5] <= 100 , "liquidityPercentage should be between 51% ~ 100%");
        require(uints[7] > uints[6], "end time must be after start time");
        require(uints[8] > 0, "availableTokens_ should be over 0");

        Presale presale = new Presale(
            wallet,
            addrs,
            uints,
            bools,
            whitelist_
        );

        console.log("availableTokens:", uints[8], msg.sender, IBEP20(addrs[0]).balanceOf(msg.sender));
        IBEP20(addrs[0]).transferFrom(msg.sender, address(presale), uints[8]);

        presaleList.push(PresaleList(
            IBEP20(addrs[0]).name(),
            addrs[0],
            address(presale),
            msg.sender,
            bools[0],
            uints[6],
            uints[7]
        ));
    }

    function getPresales() external view authorized returns (PresaleList[] memory) {
        return presaleList;
    }
}

