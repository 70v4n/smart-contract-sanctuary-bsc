   require(_exists(_swatId), "");
   require(_exists(_targetId), "");
   require(ownerOf(_swatId) == _msgSender(), "");
   require(ownerOf(_targetId) == _msgSender(), "");
  Swat storage mySwat = swats[_swatId];
  require(mySwat.upgraded == false, "");
