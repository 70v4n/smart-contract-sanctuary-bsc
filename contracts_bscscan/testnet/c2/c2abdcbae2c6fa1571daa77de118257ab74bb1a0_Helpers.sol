// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

import "./buynft.sol";

contract Redeemer is BuyNFT{

event VoucherRedeemed(address indexed _receivingPlayer, string _name);



function redeemReward(address player, string memory name, uint tokenId, bytes memory signature)
   external
   {
     require(msg.sender == player, "This isn't yours!");
     require(isValidSignature(player, name, tokenId, signature), "You have no power here!");
     require(_exists(tokenId) == false);
        _mintSwat(name, player);

        emit VoucherRedeemed(player, name);

   }

   function getChainID() external view returns (uint256) {
     uint256 id;
     assembly {
         id := chainid()
     }
     return id;
   }

   // helper function that returns a boolean
     function isValidSignature(address player, string memory name, uint tokenId, bytes memory signature)
         internal
         view
         returns (bool)
     {
         // convert the payload to a 32 byte hash
         bytes32 hash = ECDSAUpgradeable.toEthSignedMessageHash(keccak256(abi.encodePacked(player, name, tokenId)));

         // check that the signature is from MINTER_ROLE
         return hasRole(MINTER_ROLE, ECDSAUpgradeable.recover(hash, signature));
     }
 }

