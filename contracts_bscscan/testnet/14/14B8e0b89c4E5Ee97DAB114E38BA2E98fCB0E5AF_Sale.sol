pragma solidity 0.8.4;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./libraries/Formula.sol";
import "./libraries/Config.sol";

import "hardhat/console.sol";

contract Sale is Ownable {
    struct DistributeInfo {
        uint256 amount;
        uint256 withdrawTime;
        bool isWithdrawed;
    }

    IERC20 public immutable cash;
    IERC20 public immutable token;

    /// Address receives the funds collected from the sale.
    address public fundsReceiver;
    /// Boolean variable to provide the status of sale finalization.
    bool public isSaleFinalized;

    /// Sale token buyers
    mapping(address => uint256) public boughtCashes;
    mapping(address => DistributeInfo[]) public tokenDistribute;

    uint256 public soldTokenAmount;
    uint256 public withdrawStartAt;
    uint256 internal constant ONE_MONTH = 3600;
    uint256 internal constant WITHDRAWAL_NUMBER = 12;
    // uint256 internal constant WITHDRAW_STARTING_TIME = 1643673600; /// 2022/02/01 00:00:00

    /// @dev Constructor to set initial values for the contract.
    ///
    /// @param _token Address of the token that gets distributed.
    /// @param _token Address of the cash that gets to buy.
    /// @param _fundsReceiver Address that receives the funds collected from the sale.
    constructor(address _owner, IERC20 _token, IERC20 _cash, address _fundsReceiver) {
        token = _token;
        cash = _cash;
        fundsReceiver = _fundsReceiver;
        transferOwnership(_owner);

        // Set finalize status to false.
        isSaleFinalized = false;
    }

    modifier hasSaleRunning() {
        require(block.timestamp >= Constant.PRIVATE_SALE_START_TIME, "Sale has yet to be started");
        require(!isSaleFinalized, "Sale has ended");
        _;
    }

    event SetWithdrawStartTime(uint256 oldTime, uint256 newTime);
    function setWithdrawStartTime(uint256 _startAt) external onlyOwner {
        require(_startAt > block.timestamp && _startAt != withdrawStartAt, "Invalid withdraw start time");
        withdrawStartAt = _startAt;
        emit SetWithdrawStartTime(withdrawStartAt, _startAt);
    }

    event WithdrawBoughtTokens(address indexed acc, uint256 amount, uint256 withdrawedAt);
    function withdrawTokens() external {
        require(block.timestamp >= withdrawStartAt, "It is not withdraw time yet");

        uint256 amount = _updateDistributions(_msgSender());
        require(amount > 0, "Nothing to withdraw now");

        token.transfer(_msgSender(), amount);
        emit WithdrawBoughtTokens(_msgSender(), amount, block.timestamp);
    }

    function _updateDistributions(address _acc) private returns (uint256) {
        uint256 withdrawableAmount;
        uint256 currentTime = block.timestamp;

        DistributeInfo[] storage distributeInfo = tokenDistribute[_acc];
        for (uint256 i = 0; i < distributeInfo.length; i++) {
            DistributeInfo storage item = distributeInfo[i];
            if (currentTime >= item.withdrawTime && !item.isWithdrawed) {
                withdrawableAmount += item.amount;
                item.isWithdrawed = true;
            }
        }

        return withdrawableAmount;
    }

    event PrivateSaleTokensBought(address indexed buyer, uint256 _amount, uint256 boughtAt);
    event PublicSaleTokensBought(address indexed buyer, uint256 _amount, uint256 boughtAt);
    /// @dev Used to buy tokens using USD. It is only allowed to call when sale is running.
    /// @param _amount of USD that using to buy tokens
    function buyTokens(uint256 _amount) external hasSaleRunning {
        require(_amount >= Constant.SALE_MIN_AMOUNT, "amount cash is less than 50");

        uint256 tokensToSale = _calculateTokenToSale(_amount);

        // Update this amount for referral checking
        boughtCashes[_msgSender()] += _amount;
        cash.transferFrom(_msgSender(), fundsReceiver, _amount);

        if (_hasPublicSaleRunning())
            emit PublicSaleTokensBought(_msgSender(), tokensToSale, block.timestamp);
        else
            emit PrivateSaleTokensBought(_msgSender(), tokensToSale, block.timestamp);
    }

    event PrivateSaleTokensBoughtWithRefferal(address indexed buyer, address indexed refferal, uint256 _amount, uint256 boughtAt);
    event PublicSaleTokensBoughtWithRefferal(address indexed buyer, address indexed refferal, uint256 _amount, uint256 boughtAt);
    /// @dev Used to buy tokens using USD. It is only allowed to call when sale is running.
    /// @param _amount of USD that using to buy tokens
    function buyTokensWithRefferal(uint256 _amount, address _refferal) external hasSaleRunning {
        require(boughtCashes[_refferal] >= Constant.SALE_MIN_AMOUNT, "refferal is not bought tokens yet");
        require(_amount >= Constant.SALE_MIN_AMOUNT, "amount cash is less than 50");

        uint256 tokensToSale = _calculateTokenToSale(_amount);

        // Update this amount for referral checking
        boughtCashes[_msgSender()] += _amount;

        uint256 refferalRewardAmount = Formula.mulDiv(_amount, Constant.REFERRAL_REWARD_RATE, 100);
        uint256 reaminCashAmount = _amount - refferalRewardAmount;
        cash.transferFrom(_msgSender(), _refferal, refferalRewardAmount);
        cash.transferFrom(_msgSender(), fundsReceiver, reaminCashAmount);

        if (_hasPublicSaleRunning())
            emit PublicSaleTokensBoughtWithRefferal(_msgSender(), _refferal, tokensToSale, block.timestamp);
        else
            emit PrivateSaleTokensBoughtWithRefferal(_msgSender(), _refferal, tokensToSale, block.timestamp);
    }

    function _calculateTokenToSale(uint256 _amount) private returns (uint256) {
        // Fetch the current rate as per the phases.
        uint256 rate = getCurrentRate();

        // Calculate the amount of tokens to sale.
        uint256 tokensToSale = Formula.mulDiv(_amount, Formula.SCALE, rate);
        _checkSoldOut(tokensToSale);
        soldTokenAmount += tokensToSale;

        uint256 firstReceivedAmount = Formula.mulDiv(tokensToSale, Constant.SALE_FIRST_RECEIVE_RATE, 100);
        token.transfer(_msgSender(), firstReceivedAmount);

        uint256 remainTokenAmount = tokensToSale - firstReceivedAmount;
        _addToTokenDistributions(remainTokenAmount);

        return tokensToSale;
    }

    function _addToTokenDistributions(uint256 remainTokenAmount) private {
        uint256 amountEach = remainTokenAmount / WITHDRAWAL_NUMBER;
        uint256 currentTime = block.timestamp;
        uint256 updatedCount;
        uint256 lastWithdrawTime = withdrawStartAt;

        // If already bought before
        if (tokenDistribute[_msgSender()].length > 0) {
            for (uint256 i = 0; i < tokenDistribute[_msgSender()].length; i++) {
                DistributeInfo storage distributeInfo = tokenDistribute[_msgSender()][i];
                if (currentTime < distributeInfo.withdrawTime) {
                    distributeInfo.amount += amountEach;
                    updatedCount++;
                }
            }

            lastWithdrawTime = tokenDistribute[_msgSender()][tokenDistribute[_msgSender()].length - 1].withdrawTime + ONE_MONTH;
        }

        // Add new token distribution
        if (currentTime > lastWithdrawTime) {
            lastWithdrawTime = (currentTime - lastWithdrawTime) / ONE_MONTH;
        }

        for (uint256 i = updatedCount; i < WITHDRAWAL_NUMBER; i++) {
            tokenDistribute[_msgSender()].push(
                DistributeInfo(amountEach, lastWithdrawTime, false)
            );
            lastWithdrawTime += ONE_MONTH;
        }
    }

    event SaleFinalized();
    /// @dev Finalize the sale. Only be called by the owner of the contract.
    function finalizeSale() external onlyOwner {
        // Should not already be finalized.
        require(!isSaleFinalized, "Already finalized");

        // Set finalized status to be true as it not repeatatedly called.
        isSaleFinalized = true;
        // Emit even.
        emit SaleFinalized();
    }

    /// @dev Public getter to fetch the current rate as per the running phase.
    function getCurrentRate() public view returns(uint256 _rate) {
        uint256 currentTime = block.timestamp;
        // Private sale
        if (currentTime >= Constant.PRIVATE_SALE_START_TIME && currentTime < Constant.PUBLIC_SALE_START_TIME) {
            return Constant.PRIVATE_SALE_RATE;
        }
        // Public sale
        else if (currentTime >= Constant.PUBLIC_SALE_START_TIME) {
            return Constant.PUBLIC_SALE_RATE;
        }

        // Return 0 when there is no phase running.
        return uint256(0);
    }

    function _hasPublicSaleRunning() private view returns (bool) {
        return !isSaleFinalized && block.timestamp >= Constant.PUBLIC_SALE_START_TIME;
    }

    function _checkSoldOut(uint256 _tokenAmount) private view {
        uint256 expectedSoldAmount = soldTokenAmount + _tokenAmount;
        if (block.timestamp < Constant.PUBLIC_SALE_START_TIME) {
            require(expectedSoldAmount <= Constant.PRIVATE_SALE_TOKEN_AMOUNT, "Private sale is sold out");
        }
        require(expectedSoldAmount <= Constant.SALE_MINT_TOKEN_AMOUNT, "Public sale is sold out");
    }

    function getTokenDistributtionOf(address _acc) external view returns (DistributeInfo[] memory distributeInfo) {
        distributeInfo = tokenDistribute[_acc];
    }

    function getWithdrawableAmount(address _acc) external view returns (uint256) {
        uint256 amount;
        uint256 currentTime = block.timestamp;

        DistributeInfo[] memory distributeInfo = tokenDistribute[_acc];
        for (uint256 i = 0; i < distributeInfo.length; i++) {
            DistributeInfo memory item = distributeInfo[i];
            if (currentTime >= item.withdrawTime && !item.isWithdrawed) {
                amount += item.amount;
            }
        }
        return amount;
    }

    function getRemainingTokenAmount(address _acc) external view returns (uint256) {
        uint256 amount;

        DistributeInfo[] memory distributeInfo = tokenDistribute[_acc];
        for (uint256 i = 0; i < distributeInfo.length; i++) {
            DistributeInfo memory item = distributeInfo[i];
            if (!item.isWithdrawed) {
                amount += item.amount;
            }
        }
        return amount;
    }
}

