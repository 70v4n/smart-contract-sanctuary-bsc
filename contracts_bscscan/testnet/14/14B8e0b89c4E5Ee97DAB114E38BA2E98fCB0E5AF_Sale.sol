pragma solidity 0.8.4;

library Constant {
    uint256 internal constant FIXED_POINT = 1e18;
    uint256 internal constant MAX_TOKEN_TOTAL_SUPPLY = 8e27; // 8 bilion token

    /// Sale phases timings.
    ///
    /// Phase 1 starts from Monday, 02-Nov-20 08:00:00 UTC and end at Wednesday, 04-Nov-20 07:59:59 UTC
    /// while token rate during this phase is ~ 0.000077 i.e 1 USD = 13000 tokens.
    ///
    /// Phase 2 starts from Wednesday, 04-Nov-20 08:00:00 UTC and end at Friday, 06-Nov-20 07:59:59 UTC
    /// while token rate during this phase is ~ 0.00009 i.e 1 USD = 11000 tokens.
    uint256 public constant PRIVATE_SALE_START_TIME = 1640274059; /// Saturday, 1 January 2022 00:00:00 UTC
    uint256 public constant PUBLIC_SALE_START_TIME  = 1640277000; /// Wednesday, 5 January 2022 00:00:00 UTC
    uint256 public constant PUBLIC_SALE_END_TIME    = 1642377600; /// Monday, 17 January 2022 00:00:00 UTC

    /// Rates for different phases of the sale.
    uint256 public constant PRIVATE_SALE_RATE = 100000000000000;  /// 1 tokens = 0.0001 USD
    uint256 public constant PUBLIC_SALE_RATE  = 300000000000000;  /// 1 tokens = 0.0003 USD

    uint256 public constant FIRST_MINT_TOKEN_AMOUNT = 6000000000 * FIXED_POINT;
    uint256 public constant SALE_MIN_AMOUNT         = 50 * FIXED_POINT; /// 10% of bought tokens

    uint256 public constant REFERRAL_REWARD_RATE    = 25; /// 25% USD amount of buying tokens
    uint256 public constant SALE_FIRST_RECEIVE_RATE = 10; /// 10% of bought tokens

    uint256 public constant PRIVATE_SALE_TOKEN_AMOUNT = 240000000 * FIXED_POINT; // 240M
    uint256 public constant PUBLIC_SALE_TOKEN_AMOUNT  = 960000000 * FIXED_POINT; // 960M

    /// Tokenomics's token amount
    uint256 public constant SALE_MINT_TOKEN_AMOUNT = 1200000000 * FIXED_POINT; // 1.2B tokens

    uint256 public constant AIRDROP_MINT_TOKEN_AMOUNT   = 160000000 * FIXED_POINT; // 160M token
    uint256 public constant LIQUIDITY_MINT_TOKEN_AMOUNT = 400000000 * FIXED_POINT; // 400M token
    uint256 public constant TREASURY_MINT_TOKEN_AMOUNT  = 2640000000 * FIXED_POINT; // 2B 640M token

    uint256 public constant BD_INVESTOR_TOKEN_AMOUNT    = 1120000000; // 1B 120M token
    uint256 public constant BD_TEAM_TOKEN_AMOUNT        = 480000000; // 480M token

    uint256 public constant MARKETING_TOKEN_AMOUNT      = 800000000; // 800M token
    uint256 public constant TECH_TOKEN_AMOUNT           = 240000000; // 240M token
}

