pragma solidity ^0.8.4;

import "./TokenSaleWhitelist.sol";
import "../lib/ERC20Token.sol";
import "./TokenVesting.sol"; 
import "../lib/Interfaces/AggregatorV3Interface.sol";
import "../lib/SafeMath.sol";
import "hardhat/console.sol";

contract PVTA is TokenVesting, TokenSaleWhitelist {

    using SafeMath for uint256;

     //Dev Team wallet
     address payable public ploz_wallet;

    //Minimum and maximum BNB use to buy token
    uint256 public minBNB; //minimum bnb    
    uint256 public maxBNB; //maximum;


    AggregatorV3Interface public  bnbToUSD;
    
    //Set price by USDT
    uint256 public price;

    
    constructor(
        ERC20Token _token,  
        address payable _ploz_wallet, 
        address _dataFeed,
        uint256 _startDate,
        uint256 _price,
        uint256 _minBNB,
        uint256 _maxBNB
    ) TokenVesting( 
        _token, 
        _startDate
    ) {
        price = _price;
        minBNB = _minBNB;
        maxBNB = _maxBNB;
        ploz_wallet = _ploz_wallet;
        bnbToUSD = AggregatorV3Interface(_dataFeed);
    }
   
    function setPrice(uint256 _price) public onlyOwner {
        price = _price;
    }
    
    function setBNBtoUSDPriceDataFeed(address _bnbToUSD) public onlyOwner {
        bnbToUSD = AggregatorV3Interface(_bnbToUSD);
    }

    function getBNBtoUSDPrice() public view returns (int) {
        (
            uint80 roundID, 
            int _price,
            uint startedAt,
            uint timeStamp,
            uint80 answeredInRound
        ) = bnbToUSD.latestRoundData();
        roundID; startedAt; timeStamp; answeredInRound; _price;
        return _price;
    }

    modifier buyValidate() {
        require(msg.sender != address(0), "Address zero forbidden");
        require(msg.value > 0 , "You dont have enough BNB");
        require(startDatePrivateSale() < block.timestamp, "Private sale is not yet started.");
        require(!isPrivateSaleEnd(), "Private sale is already ended.");
        require(msg.value > minBNB, "Your BNB is below minimum.");
        require(msg.value <= maxBNB, "Your BNB is above maximum.");
       
        // console.log(msg.value);
        // console.log("this is max", maxBNB);
        _;
    }

	
	function buyToken(address _account) public buyValidate payable {
        require( _account != address(0), "Address zero forbidden");

        require( (totalBNBSpend[_account] + msg.value) <= maxBNB, "Max BNB reach");
     
        uint256 token_qty = tokenQty(msg.value);
        
        require(token_qty <= tokenSupply() , "Token over purchase");
        
        //check beneficiary exist in whitelist
        require(whitelisted[_account] == true, "Not listed in whitelist");

        uint256 current_date = block.timestamp;

        investmentBalance[_account] += token_qty;

        //add and log total bnb spent
        totalBNBSpend[_account] += msg.value;
       
        createChunks(_account, investmentBalance[_account]);
		
        if (countInvestmentInput[_account] == 0) {
            listOfAllInvestor[countTotalInvestor] = _account;
            countTotalInvestor++;
            emit AddTokenVesting(current_date, _account, token_qty);
        } else {
            emit UpdateTokenVesting(current_date, _account, token_qty);
        }

        totalTokenVested += token_qty;
        
        investmentTotal[_account] += token_qty;

        countInvestmentInput[_account]++;
        
        purchase(msg.value);
    }

    
    function purchase(uint256 _price) private {
        ploz_wallet.transfer(_price);
    }
    
       //convert bnb to usdt and get the coin qty
    function tokenQty(uint _wei) public view returns (uint) {
        uint usdt_price = uint(getBNBtoUSDPrice());
        return _wei.mul(usdt_price).div(price).mul(10000000000);
    }


    function setPlozWallet(address payable _ploz_wallet) public onlyOwner {
        ploz_wallet = _ploz_wallet;
    }

    function setMaxBNB(uint256 _maxBNB) public onlyOwner {
        maxBNB = _maxBNB;
    }

    function setMinBNB(uint256 _minBNB) public onlyOwner {
        minBNB = _minBNB;
    }

    function addPrivateSaleBeneficiariesToWhitelist(address[] memory beneficiaries) public onlyOwner {
         return addToWhitelist(beneficiaries);
    }

    function removePrivateSaleBeneficiaryToWhitelist(address  beneficiary ) public onlyOwner {
        return removeToWhitelist(beneficiary);
    }
    
    

   
  
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.4;

contract TokenSaleWhitelist {
  

  uint256 public totalWhitelisted;

  event WhitelistAdded(
    address beneficiary
  );

  event WhitelistRemove(
    address beneficiary
  );


  mapping(address => bool) public whitelisted;
  
  function addToWhitelist( address[] memory beneficiaries ) internal  {
      
      for (uint i = 0; i < beneficiaries.length; i++ ) {
        require(beneficiaries[i] != address(0),"Address zero detected");

        address beneficiary = beneficiaries[i];
        
        if (!whitelisted[beneficiary]) {
          whitelisted[beneficiary] = true;
          emit WhitelistAdded(beneficiary);
        }
      }

      totalWhitelisted += beneficiaries.length;
  
    }


    function removeToWhitelist(address beneficiary) internal {
      require(beneficiary != address(0),"Address zero detected");
      require(whitelisted[beneficiary] == true, "Beneficiary not listed");
      
      delete whitelisted[beneficiary];
      
      emit WhitelistRemove(beneficiary);
      
    }

    







}

