contract ABC is IERC20, Context, Ownable{
    using SafeMath for uint256;
    using Address for address;

    string private _name = "ABC Token 2";
    string private _symbol = "ABC2";
    uint8 private _decimals = 18;

    address presaleContract;

    bool private _swapping;
    uint256 public swapEnableAmount = 100 * 10 ** 18;

    mapping (address => uint256) private _balances;
    mapping (address => mapping (address => uint256)) private _allowances;

    bool public liquidityCutEnabled = true;
    uint256 public liquidityFee = 500;     //in 10,000 -> 500 for 5% 

    uint256 public marketingFee = 200;     // in 10,000 -> 200 for 2%
    uint256 public developerFee =  300;    // in 10,000 -> 300 for 3%


    mapping(address => bool) public blacklistAddresses;

    //marketing and developer wallet 
    address public developerWallet;
    address public marketingWallet;


    uint256 public unlockTime;

    uint256 public _totalSupply = 200000000 * 10 ** 18;
    uint256 public _runningSupply = 0;

    address  pancakeswapRouterAddress = 0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3;

    address public immutable pair;
    IUniswapV2Router02 public pancakeswapRouter;

    event Mint(address minter, uint256 amount);
    event Burn(address burner, uint256 amount);
    event SwapAndLiquify(
        uint256 tokensSwapped,
        uint256 ethReceived,
        uint256 tokensIntoLiqudity
    );

    event SwapAndLiquifyEnabledUpdated(bool enabled);


    constructor(
        uint256 _initialSupply,
        address _developerWallet,
        address _marketingWallet,
        address _presaleContract, 
        bool _liquidityCutEnabled
    ){
        _balances[msg.sender] = _initialSupply;
        _runningSupply = _initialSupply;
    
        developerWallet = payable(_developerWallet);
        marketingWallet = payable(_marketingWallet);

        presaleContract = _presaleContract;

        liquidityCutEnabled = _liquidityCutEnabled;

        //router
        pancakeswapRouter =  IUniswapV2Router02(pancakeswapRouterAddress);
        // Create a uniswap pair for this new token
        pair = IUniswapV2Factory(pancakeswapRouter.factory())
        .createPair(address(this), pancakeswapRouter.WETH());

        unlockTime = block.timestamp + 1 minutes;
    }

    modifier lockSwap {
        _swapping = true;
        _;
        _swapping = false;
    }

    function name() public view returns (string memory) {
        return _name;
    }

    function symbol() public view returns (string memory) {
        return _symbol;
    }

    function decimals() public view returns (uint8) {
        return _decimals;
    }

    function totalSupply() public view override returns (uint256) {
        return _totalSupply;
    }

    function balanceOf(address account) public view override returns (uint256) {
        return _balances[account];
    }

    function mintToken(uint256 amount) external onlyOwner {
        require(_totalSupply >= amount + _runningSupply, "ERC20: Total Supply Exceed!");
        _balances[owner()] += amount;
        _runningSupply += amount;
        emit Mint(msg.sender, amount);
    }

    function burnToken(uint256 amount) external onlyOwner {
        require(_balances[owner()] >= amount, "ERC20: Not enough balance to burn!");
        _balances[owner()] -= amount;
        emit Burn(msg.sender, amount);
        emit Transfer(owner(), 0x0000000000000000000000000000000000000000, amount);
    }

    function transfer(address recipient, uint256 amount) public override returns (bool) {
        _transfer(_msgSender(), recipient, amount);
        return true;
    }

    function allowance(address owner, address spender) public view override returns (uint256) {
        return _allowances[owner][spender];
    }

    function approve(address spender, uint256 amount) public override returns (bool) {
        _approve(_msgSender(), spender, amount);
        return true;
    }

    function transferFrom(address sender, address recipient, uint256 amount) public override returns (bool) {
        require (_allowances[sender][_msgSender()] >= amount, "ERC20: transfer amount exceeds allowance");
        _transfer(sender, recipient, amount);
        _approve(sender, _msgSender(), _allowances[sender][_msgSender()] - amount);
        return true;
    }

    function _approve(address owner, address spender, uint256 amount) private {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }
    
    function _transfer(address sender, address recipient, uint256 amount) private {
        require(sender != address(0), "ERC20: transfer from the zero address");
        require(recipient != address(0), "ERC20: transfer to the zero address");
        require(amount > 0, "ERC20: cannot transfer zero");
        require(!blacklistAddresses[sender], "ERC20: Sender is Blacklisted");
        require(!blacklistAddresses[recipient], "ERC20: Recipient is Blacklisted");


        uint256 totalTax = 0;

        if (!(sender==presaleContract || sender == owner() || recipient ==owner() || sender == pair || recipient == pair || recipient == pancakeswapRouterAddress || sender == pancakeswapRouterAddress)){
            require(block.timestamp > unlockTime, "ERC20: Cannot Trade now in locked peroid");

            if(liquidityCutEnabled){
                totalTax = liquidityFee +  marketingFee + developerFee;
            }
            else{
                totalTax =  marketingFee + developerFee;
            }
        } 

        require(_balances[sender] >= amount, "ERC20: transfer exceeds balance");

        uint256 taxAmount = (amount.mul(totalTax)).div(10000);   
        uint256 taxedAmount = amount - taxAmount;

        _balances[address(this)] += taxAmount;

        _balances[recipient] += taxedAmount;
        _balances[sender] -= amount;

        uint256 swapBalance = _balances[address(this)];

        if( swapBalance >= swapEnableAmount && !_swapping && sender != pair ){
            
            uint256 lpCut = 0;
            uint256 marketingCut = 0;
            uint256 developerCut = 0;

            if (liquidityCutEnabled){
                //add to pool 
               lpCut = swapBalance.mul(5).div(10);
               marketingCut = swapBalance.mul(marketingFee).div(10000);
               developerCut = swapBalance.mul(developerFee).div(10000);

            }
            else{
                marketingCut = swapBalance.mul(marketingFee * 2).div(10000);
                developerCut = swapBalance.mul(developerFee * 2).div(10000);
            }

            _balances[pair] += lpCut;
            _balances[marketingWallet] += marketingCut;
            _balances[developerWallet] += developerCut;

            emit Transfer(address(this), pair, lpCut);
            emit Transfer(address(this), marketingWallet, marketingCut);
            emit Transfer(address(this), developerWallet, developerCut);

        }

        emit Transfer(sender, recipient, taxedAmount);
        
        if (taxAmount != 0){
            emit Transfer(sender, address(this), taxAmount);
        }

    }

    function updateSwapEnableAmount(uint256 _swapEnableAmount) external onlyOwner returns(bool){
        swapEnableAmount = _swapEnableAmount;

        emit SwapAndLiquifyEnabledUpdated(liquidityCutEnabled);
        return true;
    }

