// contracts/GoblixBox.sol
// SPDX-License-Identifier: MIT
pragma solidity >=0.5.16;

import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "./BaseGeneRules.sol";
import "./GoblinToken.sol";
import "./GoblinBox.sol";
import "./GobiToken.sol";

contract GoblinFusion is GoblinBox {

    uint8 constant IRARITY = 5;

    uint8 constant COMMON = 0;
    uint8 constant UNCOMMON = 1;
    uint8 constant RARE = 2;
    uint8 constant EPIC = 3;
    uint8 constant LEGENDARY = 4;

    address private _poolAddress;
    address private _gobiContract;
    GobiToken internal _gobiToken; 
    mapping(uint8 => uint256) private _fusionPrice;

    function poolAddress() public view returns(address) {
        return _poolAddress;
    }

    function setPoolAddress(address _address) public onlyOwner {
        _poolAddress = _address;
    } 

    function gobiContract() public view returns(address) {
        return _gobiContract;
    } 

    function setGobiContract(address _address) public onlyOwner {
        _gobiContract = _address;
        GobiToken gobiToken = GobiToken(_gobiContract);
        require(gobiToken.isGobiToken(), "GoblinBorn: Gobi Token is not valid");
        _gobiToken = gobiToken;
    }

    function addPrice(uint256 _rarity, uint256 _price) public onlyOwner {
        require(_rarity <= LEGENDARY, "GoblinFusion: Invalid rarity");
        _fusionPrice[uint8(_rarity)] = _price;
    }

    function removePrice(uint256 _rarity) public onlyOwner {
        delete _fusionPrice[uint8(_rarity)];
    }

    function getRarity(uint8 _rarity) private pure returns(uint8) {
        if (_rarity > 128 && _rarity <= 210) {
            return UNCOMMON;
        }
        else if (_rarity > 210 && _rarity <= 242) {
            return RARE;
        }
        else if (_rarity > 242 && _rarity <= 253) {
            return EPIC;
        }
        else if (_rarity > 253 && _rarity <= 255) {
            return LEGENDARY;
        }
        else {
            return COMMON;
        }
    }

    function rarityToByte(uint8 _rarity, uint32 _salt) private view returns(bytes1) {
        if (_rarity == UNCOMMON) {
            return bytes1(uint8((random(_salt) % 82) + 128));
        }
        else if (_rarity == RARE) {
            return bytes1(uint8((random(_salt) % 32) + 210));
        }
        else if (_rarity == EPIC) {
            return bytes1(uint8((random(_salt) % 11) + 242));
        }
        else if (_rarity == LEGENDARY) {
            return bytes1(uint8((random(_salt) % 2) + 253));
        }
        else {
            return bytes1(uint8(random(_salt) % 128));
        }
    }

    function updateRarity(uint8 _rarity) internal pure returns(uint8) {
        if (_rarity == UNCOMMON) {
            return _rarity = RARE;
        }
        else if (_rarity == RARE) {
            return _rarity = EPIC;
        }
        else if (_rarity == EPIC) {
            return _rarity = LEGENDARY;
        }
        else if (_rarity == LEGENDARY) {
            return _rarity = LEGENDARY;
        }
        else {
            return _rarity = UNCOMMON;
        }
    }

    function copyGene(uint256 genes1, uint8 mm) internal pure returns(bytes3) {
        bytes32 g = bytes32(genes1);
        return bytes3(bytes.concat(g[mm], g[mm - 1], g[mm - 2]));
    }

    function fuseGenes(uint256 genes1, uint256 genes2, bytes1 _rarity) private view returns (uint256) {

        bytes memory _genes = new bytes(27);
        _genes = bytes.concat(
            getByte(genes1, IGENRE),
            copyGene(genes1, ISKIN),
            copyGene(genes1, IHAIR),
            copyGene(genes1, IEYES),
            copyGene(genes1, IEARS),
            copyGene(genes1, IMOUTH)
        );

        uint32 salt = 1;
        _genes = bytes.concat(
            _genes,
            getColor(genes1, genes2, ICOLORSKIN, salt + 10),
            getColor(genes1, genes2, ICOLORHAIR, salt + 20),
            getColor(genes1, genes2, ICOLOREYES, salt + 30),
            getByte(genes1, IRACE),
            bytes1(_rarity)
        );
        return reverse(uint256(bytes32(_genes)));
    }

    function fusionCost(uint256 tokenId1) public view returns (uint256) {
        (,uint256 genes1,,,,,,,) = _goblinToken.getGoblin(tokenId1);
        return _fusionPrice[getRarity((uint8(getByte(genes1, IRARITY))))];
    }

    function fusion(uint256 tokenId1, uint256 tokenId2) public {
        require(_goblinContract != address(0), "GoblinFusion: Goblin contract is not setup");
        require(_gobiContract != address(0), "GoblinFusion: gobi contract not setup");
        require(_poolAddress != address(0), "GoblinFusion: pool contract not setup");
        require(_goblinToken.getApproved(tokenId1) == address(this), "GoblinFusion: main goblin not approved");
        require(_goblinToken.getApproved(tokenId2) == address(this), "GoblinFusion: secundary goblin not approved");

        uint256 _price = fusionCost(tokenId1);
        require(_price > 0, "GoblinFusion: price not setup");
        require(_gobiToken.allowance(_msgSender(), address(this)) >= _price, "GoblinFusion: Gobi not approved");

        (,uint256 genes1,,,,,,,) = _goblinToken.getGoblin(tokenId1);
        uint8 r1 = getRarity((uint8(getByte(genes1, IRARITY))));
        require(r1 != LEGENDARY, "GoblinFusion: Cannot fuse legendary goblin");

        (,uint256 genes2,,,,,,,) = _goblinToken.getGoblin(tokenId2);
        uint8 r2 = getRarity((uint8(getByte(genes2, IRARITY))));
        require(r1 == r2, "GoblinFusion: Goblin rarity cannot be different");

        uint32 salt = 1;
        bytes1 newRarity = rarityToByte(updateRarity(r1), salt);

        _goblinToken.transferFrom(_msgSender(), _poolAddress, tokenId1);
        _goblinToken.transferFrom(_msgSender(), _poolAddress, tokenId2);
        _gobiToken.transferFrom(_msgSender(), _poolAddress, _price);

        _goblinToken.createGenesisGoblin(_msgSender(), fuseGenes(genes1, genes2, newRarity), _defaultInventory);
    }
}

