import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";

contract GoboxToken is ERC1155, Ownable {

    address private _burnContract;

    constructor() ERC1155("https://app.goblinwars.io/Goblins/api/Box?nft={id}") {
    }

    function burnContract() public view returns (address) {
        return _burnContract;
    }

    function setBurnContract(address _address) public onlyOwner {
        _burnContract = _address;
    } 

    modifier onlyBurnContract() {
        require(_burnContract == _msgSender(), "GoboxToken: Only birth contract can call this");
