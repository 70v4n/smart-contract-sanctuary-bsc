pragma solidity >=0.5.16;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";

contract GobiToken is ERC20, Ownable {

    uint256 private _maxSupply;
    address private _burnContract;

    constructor() ERC20("Goblin Hero Soul", "GOBI") {
        _mint(msg.sender, 100000 * (10 ** decimals())); // 100k
        _maxSupply = 300000000 * (10 ** decimals()); // 300 millions
    }

    modifier onlyBurnContract() {
        require(_burnContract == _msgSender(), "GoblinToken: Only birth contract can call this");
        _;
    }

    function burnContract() public view returns (address) {
        return _burnContract;
    }

    function setBurnContract(address _address) public onlyOwner {
        _burnContract = _address;
    } 

    function maxSupply() public view returns (uint256) {
        return _maxSupply;
    }

    function mint(uint256 amount) public onlyOwner returns (bool)  {
        require((totalSupply() + amount <= _maxSupply), "ERC20: cannot mint more that max supply");
        _mint(_msgSender(), amount);
        return true;
    }

    function burn(address _owner, uint256 amount) public onlyBurnContract {
        require(_burnContract != address(0), "ERC20: no burn contract setup");
        require(_owner != address(0), "ERC20: need burn from a owner");
        _burn(_owner, amount);
        _maxSupply -= amount;
    }

    function isGobiToken() public pure returns (bool) {
        return true;
    }
}

