// SPDX-License-Identifier: MIT
pragma solidity >=0.5.16;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "./BaseGeneRules.sol";
import "./GoboxToken.sol";
import "./GoblinToken.sol";

contract GoblinBox is BaseGeneRules, Ownable {
    address _goblinContract;
    address _goboxContract;
    uint256 internal _defaultInventory = 0;
    GoblinToken internal _goblinToken;
    GoboxToken private _goboxToken;
    uint8[6] private _raceQtdy = [0, 0, 0, 0, 0, 0];

    function goblinContract() public view returns(address) {
        return _goblinContract;
    } 

    function setGoblinContract(address _address) public onlyOwner {
        _goblinContract = _address;
        GoblinToken goblinToken = GoblinToken(_goblinContract);
        require(goblinToken.isGoblinToken(), "GoblinBorn: Goblin token is not valid");
        _goblinToken = goblinToken;
    }

    function goboxContract() public view returns(address) {
        return _goboxContract;
    } 

    function setGoboxContract(address _address) public onlyOwner {
        _goboxContract = _address;
        GoboxToken goboxToken = GoboxToken(_goboxContract);
        _goboxToken = goboxToken;
    }

    function defaultInventory() public view returns (uint256) {
        return _defaultInventory;
    }

    function setDefaultInventory(uint256 value) public onlyOwner {
        _defaultInventory = value;
    } 

    function randomColor(uint32 salt) internal view returns(bytes3) {
        return bytes3(bytes.concat(
            bytes1(uint8(random(salt + 1) % 255)),
            bytes1(uint8(random(salt + 2) % 255)),
            bytes1(uint8(random(salt + 3) % 255))
        ));
    }

    function randomGene(uint32 salt) private returns(bytes3) {
        uint i1 = random(salt + 1) % RACES.length;
        uint i2 = random(salt + 2) % RACES.length;
        uint i3 = random(salt + 3) % RACES.length;
        _raceQtdy[i1]++;
        _raceQtdy[i2]++;
        _raceQtdy[i3]++;
        return bytes3(bytes.concat(RACES[i1], RACES[i2], RACES[i3]));
    }

    function randomRace() private view returns (bytes1) {
        bytes1 _race = FOREST;
        uint craceQtdy = 0;
        if (_raceQtdy[0] > craceQtdy) {
            craceQtdy = _raceQtdy[0];
            _race = FOREST;
        }
        if (_raceQtdy[1] > craceQtdy) {
            craceQtdy = _raceQtdy[1];
            _race = DESERT;
        }
        if (_raceQtdy[2] > craceQtdy) {
            craceQtdy = _raceQtdy[2];
            _race = CAVE;
        }
        if (_raceQtdy[3] > craceQtdy) {
            craceQtdy = _raceQtdy[3];
            _race = MOUNTAIN;
        }
        if (_raceQtdy[4] > craceQtdy) {
            craceQtdy = _raceQtdy[4];
            _race = DARK;
        }
        if (_raceQtdy[5] > craceQtdy) {
            craceQtdy = _raceQtdy[5];
            _race = SEA;
        }
        return _race;
    }

    function randomGenes(bytes1 rarity) private returns (uint256) {

        uint32 salt = 100;

        for (uint i = 0; i < _raceQtdy.length; i++) {
            _raceQtdy[i] = 0;
        }

        bytes memory _genes = new bytes(27);
        bytes1 _genre = GENRE[random(salt) % GENRE.length];

        salt += 30;
        _genes = bytes.concat(
            _genre,
            randomGene(salt),
            randomGene(salt + 10),
            randomGene(salt + 20),
            randomGene(salt + 30),
            randomGene(salt + 40)
        );

        salt += 30;
        _genes = bytes.concat(
            _genes,
            randomColor(salt + 10),
            randomColor(salt + 20),
            randomColor(salt + 30),
            randomRace(),
            rarity
        );
        return reverse(uint256(bytes32(_genes)));
    }

    function getRandomRarity(uint256 boxId) private view returns(bytes1) {
        uint32 salt = 1;
        bytes1 _rarity = 0x0; 
        if (boxId == 1) {
            _rarity = bytes1(uint8(random(salt) % 255));
        }
        else if (boxId == 2) {
            _rarity = bytes1(uint8((random(salt) % 127) + 128));
        }
        else if (boxId == 3) {
            _rarity = bytes1(uint8((random(salt) % 45) + 210));
        }
        else if (boxId == 4) {
            _rarity = bytes1(uint8((random(salt) % 26) + 229));
        }
        else if (boxId == 5) {
            _rarity = bytes1(uint8((random(salt) % 3) + 253));
        }
        return _rarity;
    }

    function openBox(uint256 boxId) public returns(bool) {
        require(_goblinContract != address(0), "GoboxOpen: Goblin contract is not setup");
        require(_goboxContract != address(0), "GoboxOpen: Gobox contract is not setup");
        require(_goboxToken.balanceOf(_msgSender(), boxId) > 0, "GoboxOpen: Dont have enought boxes");

        _goblinToken.createGenesisGoblin(_msgSender(), randomGenes(getRandomRarity(boxId)), _defaultInventory);
        _goboxToken.burn(_msgSender(), boxId, 1);
        return true;
    }
}

// contracts/GobiToken.sol
