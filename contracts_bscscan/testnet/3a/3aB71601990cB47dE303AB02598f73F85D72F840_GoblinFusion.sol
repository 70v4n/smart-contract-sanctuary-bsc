    }

    function burn(address _owner, uint256 id, uint256 amount) public onlyBurnContract {
        _burn(_owner, id, amount);
    }

    function mint(uint256 id, uint256 amount) public onlyOwner {
        _mint(_msgSender(), id, amount, "");
    }
}

