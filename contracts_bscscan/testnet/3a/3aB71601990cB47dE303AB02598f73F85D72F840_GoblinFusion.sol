// contracts/BaseGeneRules.sol
// SPDX-License-Identifier: MIT
pragma solidity >=0.5.16;

import "./interfaces/IGeneRules.sol";
import "hardhat/console.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

/*
00 - Genre     31 - EMPTY
01 - Skin MM   30 - EMPTY
02 - Skin R1   29 - EMPTY
03 - Skin R2   28 - EMPTY
04 - Hair MM   27 - EMPTY
05 - Hair R1   26 - RARITY
06 - Hair R2   25 - RACE
07 - Eyes MM   24 - EYCL B
08 - Eyes R1   23 - EYCL G
09 - Eyes R2   22 - EYCL R
10 - Ears MM   21 - HRCL B
11 - Ears R1   20 - HRCL G
12 - Ears R2   19 - HRCL R
13 - Mout MM   18 - SKCL B
14 - Mout R1   17 - SKCL G
15 - Mout R2   16 - SKCL R
16 - SKCL R    15 - Mout R2
17 - SKCL G    14 - Mout R1
18 - SKCL B    13 - Mout MM
19 - HRCL R    12 - Ears R2
20 - HRCL G    11 - Ears R1
21 - HRCL B    10 - Ears MM 
22 - EYCL R    09 - Eyes R2
23 - EYCL G    08 - Eyes R1
24 - EYCL B    07 - Eyes MM
25 - RACE      06 - Hair R2
26 - RARITY    05 - Hair R1
27 - EMPTY     04 - Hair MM
28 - EMPTY     03 - Skin R2
29 - EMPTY     02 - Skin R1
30 - EMPTY     01 - Skin MM 
31 - EMPTY     00 - Genre
*/

contract BaseGeneRules {

    bytes1 constant FOREST = 0x31;
    bytes1 constant DESERT = 0x32;
    bytes1 constant CAVE = 0x33;
    bytes1 constant MOUNTAIN = 0x34;
    bytes1 constant DARK = 0x35;
    bytes1 constant SEA = 0x36; 
    
    bytes1 constant MALE = 0x6d; 
    bytes1 constant FEMALE = 0x66;

    uint8 constant SKIN = 1;
    uint8 constant HAIR = 4;
    uint8 constant EYES = 7;
    uint8 constant EAR = 10;
    uint8 constant MOUTH = 13;
    uint8 constant RACE = 25;

    uint8 constant ISKIN = 30;
    uint8 constant IHAIR = 27;
    uint8 constant IEYES = 24;
    uint8 constant IEARS = 21;
    uint8 constant IMOUTH = 18;
    uint8 constant IRACE = 6;
    uint8 constant IGENRE = 31;

    uint8 constant ICOLORSKIN = 15;
    uint8 constant ICOLORHAIR = 12;
    uint8 constant ICOLOREYES = 9;

    bytes1[] RACES = [FOREST, DESERT, CAVE, MOUNTAIN, DARK, SEA];
    bytes1[] GENRE = [MALE, FEMALE];

    function getByte(uint256 _gene, uint8 i) internal pure returns (bytes1) {
        bytes32 g = bytes32(_gene);
        return g[i];
    }

    function getRByte(uint256 _gene, uint8 i, uint32 _salt) internal view returns (bytes1) {
        bytes32 g = bytes32(_gene);
        bytes1 r = g[i];
        if (r == FOREST || r == DESERT || r == CAVE || r == MOUNTAIN || r == DARK || r == SEA) {
            return r;
        }
        return RACES[random(_salt) % RACES.length];
    }
    
    function random(uint32 salt) internal view returns (uint) {
        return uint256(keccak256(abi.encodePacked(block.difficulty, block.timestamp, salt)));
    }

    function getColorPart(uint256 genes1, uint256 genes2, uint8 i, uint32 _salt) internal view returns(bytes1) {
        uint256 p = (uint256(uint8(getByte(genes1, i))) + uint256(uint8(getByte(genes2, i)))) / 2;

        uint256 _mod = 120;

        uint256 r = uint256(random(_salt) % _mod);
        uint256 m = (r / 2);
        if (r >= m) {
            p += m;
            if (p > 255) {
                p = 255;
            }
        }
        else {
            (,p) = SafeMath.trySub(p, (_mod / 2));
        }
        return bytes1(uint8(p));
    }

    function getColor(uint256 genes1, uint256 genes2, uint8 i, uint32 _salt) internal view returns(bytes3) {
        bytes memory _colors = new bytes(3);
        uint _dice = random(_salt + 1) % 100;
        if (_dice == 50 || _dice == 51) {
            _colors = bytes.concat(
                bytes1(uint8(random(_salt + 2) % 255)),
                bytes1(uint8(random(_salt + 3) % 255)),
                bytes1(uint8(random(_salt + 4) % 255))
            );
        }
        else {
            _colors = bytes.concat(
                getColorPart(genes1, genes2, i  , _salt + 5),
                getColorPart(genes1, genes2, i-1, _salt + 6),
                getColorPart(genes1, genes2, i-2, _salt + 7)
            );
        }
        return bytes3(_colors);
    }

    function getGene(uint256 genes1, uint256 genes2, uint8 mm, uint32 _salt) internal view returns(bytes3) {
        bytes1[12] memory skins = [
            getRByte(genes1, mm    , _salt     ),
            getRByte(genes1, mm    , _salt +  1),
            getRByte(genes1, mm    , _salt +  2),
            getRByte(genes2, mm    , _salt +  3),
            getRByte(genes2, mm    , _salt +  4),
            getRByte(genes2, mm    , _salt +  5),
            getRByte(genes1, mm - 1, _salt +  6),
            getRByte(genes1, mm - 1, _salt +  7),
            getRByte(genes2, mm - 1, _salt +  8),
            getRByte(genes2, mm - 1, _salt +  9),
            getRByte(genes1, mm - 2, _salt + 10),
            getRByte(genes2, mm - 2, _salt + 11)
        ];
        bytes1[3] memory skinR1 = [
            getRByte(genes1, mm    , _salt + 12),
            getRByte(genes1, mm - 1, _salt + 13),
            getRByte(genes1, mm - 2, _salt + 14)
        ];
        bytes1[3] memory skinR2 = [
            getRByte(genes2, mm    , _salt + 15),
            getRByte(genes2, mm - 1, _salt + 16),
            getRByte(genes2, mm - 2, _salt + 17)
        ];
        bytes memory _genes = new bytes(3);

        uint _g0 = random(_salt + 18) % 100;
        uint _r1 = random(_salt + 19) % 100;
        uint _r2 = random(_salt + 20) % 100;

        _genes = bytes.concat(
            (_g0 == 50 || _g0 == 51) ? 
                RACES[random(_salt + 21) % RACES.length] : 
                skins[random(_salt + 22) % skins.length],
            (_r1 == 50 || _r1 == 51) ?
                RACES[random(_salt + 23) % skinR1.length] :
                skinR1[random(_salt + 24) % skinR1.length],
            (_r2 == 50 || _r2 == 51) ?
                RACES[random(_salt + 25) % skinR1.length] :
                skinR2[random(_salt + 26) % skinR2.length]
        );
        return bytes3(_genes);
    }

    function getRace(uint256 genes1, uint256 genes2, uint256 genes, uint32 _salt) internal view returns(bytes1) {
        bytes1 r1 = getRByte(genes1, IRACE, _salt + 1);
        uint8 r1Count = 0;
        r1Count += (r1 == getRByte(genes, SKIN , _salt +  2)) ? 1 : 0; 
        r1Count += (r1 == getRByte(genes, HAIR , _salt +  3)) ? 1 : 0; 
        r1Count += (r1 == getRByte(genes, EYES , _salt +  4)) ? 1 : 0; 
        r1Count += (r1 == getRByte(genes, EAR  , _salt +  5)) ? 1 : 0; 
        r1Count += (r1 == getRByte(genes, MOUTH, _salt +  6)) ? 1 : 0; 

        bytes1 r2 = getRByte(genes2, IRACE, _salt + 7);
        uint8 r2Count = 0;
        r2Count += (r2 == getRByte(genes, SKIN , _salt +  8)) ? 1 : 0; 
        r2Count += (r2 == getRByte(genes, HAIR , _salt +  9)) ? 1 : 0; 
        r2Count += (r2 == getRByte(genes, EYES , _salt + 10)) ? 1 : 0; 
        r2Count += (r2 == getRByte(genes, EAR  , _salt + 11)) ? 1 : 0; 
        r2Count += (r2 == getRByte(genes, MOUTH, _salt + 12)) ? 1 : 0; 

        if (r1Count > r2Count) {
            return r1;
        }
        else if (r1Count < r2Count) {
            return r2;
        }
        else {
            bytes1[2] memory rs = [r1, r2];
            return rs[random(_salt + 13) % 2];
        }
    }

    function reverse(uint256 input) internal pure returns (uint256 v) {
        v = input;

        // swap bytes
        v = ((v & 0xFF00FF00FF00FF00FF00FF00FF00FF00FF00FF00FF00FF00FF00FF00FF00FF00) >> 8) |
            ((v & 0x00FF00FF00FF00FF00FF00FF00FF00FF00FF00FF00FF00FF00FF00FF00FF00FF) << 8);

        // swap 2-byte long pairs
        v = ((v & 0xFFFF0000FFFF0000FFFF0000FFFF0000FFFF0000FFFF0000FFFF0000FFFF0000) >> 16) |
            ((v & 0x0000FFFF0000FFFF0000FFFF0000FFFF0000FFFF0000FFFF0000FFFF0000FFFF) << 16);

        // swap 4-byte long pairs
        v = ((v & 0xFFFFFFFF00000000FFFFFFFF00000000FFFFFFFF00000000FFFFFFFF00000000) >> 32) |
            ((v & 0x00000000FFFFFFFF00000000FFFFFFFF00000000FFFFFFFF00000000FFFFFFFF) << 32);

        // swap 8-byte long pairs
        v = ((v & 0xFFFFFFFFFFFFFFFF0000000000000000FFFFFFFFFFFFFFFF0000000000000000) >> 64) |
            ((v & 0x0000000000000000FFFFFFFFFFFFFFFF0000000000000000FFFFFFFFFFFFFFFF) << 64);

        // swap 16-byte long pairs
        v = (v >> 128) | (v << 128);
    }
}

