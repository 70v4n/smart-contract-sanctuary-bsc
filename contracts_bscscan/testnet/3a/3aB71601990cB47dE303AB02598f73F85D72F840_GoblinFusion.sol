        for (uint i = 0; i < balanceOfSons(_parent2Id); i++) {
            //updateGoblin(tokenOfSonByIndex(_parent2Id, i));
            _parent2.lastUpdateTime = _updateTime;
        }
        */

        uint256 lastSonIndex1 = _parentSonsBalance[_parent1Id];
        //console.log("_parent1Id=", _parent1Id);
        //console.log("lastSonIndex=", lastSonIndex);
        _parentSons[_parent1Id][lastSonIndex1] = _allGoblins.length - 1;
        _parentSonsBalance[_parent1Id] = lastSonIndex1 + 1;

        uint256 lastSonIndex2 = _parentSonsBalance[_parent2Id];
        //console.log("_parent2Id=", _parent2Id);
        //console.log("lastSonIndex=", lastSonIndex);
        _parentSons[_parent2Id][lastSonIndex2] = _allGoblins.length - 1;
        _parentSonsBalance[_parent2Id] = lastSonIndex2 + 1;
        //console.log("newGoblinId=", newGoblinId);
        //console.log("_parentSons[_parent1Id][lastSonIndex]=", _parentSons[_parent1Id][lastSonIndex]);
        //console.log("_parentSons[_parent2Id][lastSonIndex]=", _parentSons[_parent2Id][lastSonIndex]);

        //updateGoblin(_parent1Id);
        //updateGoblin(_parent2Id);

        return _allGoblins.length - 1;
    }

    function _createOriginGoblin(
        address _owner,
        uint256 _genes,
        uint256 _inventory
    )  internal virtual returns (uint256) {
        uint64 _updateTime = uint64(block.timestamp);
        Goblin memory _goblin = Goblin({
            genes: _genes,
            activeMods: 0,
            mods: 0,
            inventory: _inventory,
            bag: 0,
            birthTime: _updateTime,
            lastUpdateTime: _updateTime,
            cooldownTime: _updateTime,
            fatherId: 0,
            motherId: 0, 
            spouseId: 0
        });
        _allGoblins.push(_goblin);
        uint256 newGoblinId = _allGoblins.length - 1;
        //_mint(_msgSender(), newGoblinId);
        _mint(_owner, newGoblinId);
        _ownedUpdateTime[_owner] = _updateTime;
        return newGoblinId;
    }

    function createOriginGoblin(uint256 _genes) onlyOwner public {
        _createOriginGoblin(_msgSender(), _genes, _defaultInventory);
    }

    function createGenesisGoblin(address _owner, uint256 _genes, uint256 _inventory) 
        onlyBornContract
    public {
        _createOriginGoblin(_owner, _genes, _inventory);
    }

    function createGoblin(uint32 _parent1Id, uint32 _parent2Id) onlyOwner public {
        _createBaseGoblin(_msgSender(), _parent1Id, _parent2Id);
    }

    modifier onlyBornContract() {
        require(
            _bornContract == _msgSender() || _genesisContract == _msgSender(), 
            "GoblinToken: Only born contract can call this"
        );
        _;
    }

    modifier onlyEquipContract() {
        require(_equipContract == _msgSender(), "GoblinToken: Only equip contract can call this");
        _;
    }

    modifier onlyModsContract() {
        require(_modsContract == _msgSender(), "GoblinToken: Only mod contract can call this");
        _;
    }

    function goblinFamily(uint32 _tokenId) public view returns (
        uint256 fatherId,
        uint256 motherId,
        uint256 spouseId,
        uint8 genre,
        uint256 sonsCount,
        uint256 cooldownTime
    ) {
        Goblin memory _goblin = _allGoblins[_tokenId];
        return (
            _goblin.fatherId,
            _goblin.motherId,
            _goblin.spouseId,
            _geneRules.getGenre(_goblin.genes),
            balanceOfSons(_tokenId),
            _goblin.cooldownTime
        );
    }

    function born(address _owner, uint32 _parent1Id, uint32 _parent2Id) onlyBornContract public {
        _createBaseGoblin(_owner, _parent1Id, _parent2Id);
    }

    function isGoblinToken() public pure returns (bool) {
        return true;
    }

    function changeInventory(address _owner, uint32 tokenId, uint256 inventory) onlyEquipContract public {
        require(tokenId > 0, "GoblinToken: need a token id");
        require(tokenId <= totalSupply(), "GoblinToken: goblin not exist");
        require(ownerOf(tokenId) == _owner, "GoblinToken: need to be owner to equip");
        
        uint64 _updateTime = uint64(block.timestamp);

        Goblin storage _goblin = _allGoblins[tokenId];
        _goblin.inventory = inventory;
        _goblin.lastUpdateTime = _updateTime;
        _ownedUpdateTime[_owner] = _updateTime;
    }

    function changeBag(address _owner, uint32 tokenId, uint256 _bag) onlyEquipContract public {
        require(tokenId > 0, "GoblinToken: need a token id");
        require(tokenId <= totalSupply(), "GoblinToken: goblin not exist");
        require(ownerOf(tokenId) == _owner, "GoblinToken: need to be owner to equip");
        
        uint64 _updateTime = uint64(block.timestamp);

        Goblin storage _goblin = _allGoblins[tokenId];
        _goblin.bag = _bag;
        _goblin.lastUpdateTime = _updateTime;
        _ownedUpdateTime[_owner] = _updateTime;
    }

    function changeActiveMods(address _owner, uint32 tokenId, uint256 _activeMods) onlyEquipContract public {
        require(tokenId > 0, "GoblinToken: need a token id");
        require(tokenId <= totalSupply(), "GoblinToken: goblin not exist");
        require(ownerOf(tokenId) == _owner, "GoblinToken: need to be owner to modify");
        
        uint64 _updateTime = uint64(block.timestamp);

        Goblin storage _goblin = _allGoblins[tokenId];
        _goblin.activeMods = _activeMods;
        _goblin.lastUpdateTime = _updateTime;
        _ownedUpdateTime[_owner] = _updateTime;
    }

    function changeMods(address _owner, uint32 tokenId, uint256 _mods) onlyEquipContract public {
        require(tokenId > 0, "GoblinToken: need a token id");
        require(tokenId <= totalSupply(), "GoblinToken: goblin not exist");
        require(ownerOf(tokenId) == _owner, "GoblinToken: need to be owner to modify");
        
        uint64 _updateTime = uint64(block.timestamp);

        Goblin storage _goblin = _allGoblins[tokenId];
        _goblin.mods = _mods;
        _goblin.lastUpdateTime = _updateTime;
        _ownedUpdateTime[_owner] = _updateTime;
    }

    function totalSupply() override public view returns (uint256) {
        return _allGoblins.length - 1;
    }

    function supportsInterface(bytes4 interfaceId) public view virtual override(IERC165, ERC721) returns (bool) {
        return interfaceId == type(IERC721Enumerable).interfaceId || super.supportsInterface(interfaceId);
    }

    function tokenOfOwnerByIndex(address owner, uint256 index) public view virtual override returns (uint256) {
        require(index < ERC721.balanceOf(owner), "ERC721Enumerable: owner index out of bounds");
        return _ownedTokens[owner][index];
    }

    function goblinOfOwnerByIndex(address owner, uint256 index) public view returns (
        uint256 tokenId,
        uint256 genes,
        uint256 activeMods,
        uint256 mods,
        uint256 inventory,
        uint256 bag,
        uint256 birthTime,
        uint256 lastUpdateTime,
        uint256 cooldownTime
    ) {
        uint256 _tokenId = tokenOfOwnerByIndex(owner, index);
        return getGoblin(_tokenId);
    }

    function tokenByIndex(uint256 index) public view virtual override returns (uint256) {
        require(index <= totalSupply(), "ERC721Enumerable: global index out of bounds");
        return index;
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual override {
        super._beforeTokenTransfer(from, to, tokenId);

        if (from != address(0) && from != to) {
            _removeTokenFromOwnerEnumeration(from, tokenId);
        }
        if (to != address(0) && to != from) {
            _addTokenToOwnerEnumeration(to, tokenId);
        }
    }

    function _addTokenToOwnerEnumeration(address to, uint256 tokenId) private {
        uint256 length = ERC721.balanceOf(to);
        _ownedTokens[to][length] = tokenId;
        _ownedTokensIndex[tokenId] = length;
    }

    function _removeTokenFromOwnerEnumeration(address from, uint256 tokenId) private {
        uint256 lastTokenIndex = ERC721.balanceOf(from) - 1;
        uint256 tokenIndex = _ownedTokensIndex[tokenId];

        if (tokenIndex != lastTokenIndex) {
            uint256 lastTokenId = _ownedTokens[from][lastTokenIndex];

            _ownedTokens[from][tokenIndex] = lastTokenId;
            _ownedTokensIndex[lastTokenId] = tokenIndex;
        }

        delete _ownedTokensIndex[tokenId];
        delete _ownedTokens[from][lastTokenIndex];
    }

    function balanceOfSons(uint256 tokenId) public view returns(uint256) {
        //console.log(tokenId, "=", _parentSonsBalance[tokenId]);
        return _parentSonsBalance[tokenId];
    }

    function tokenOfSonByIndex(uint256 _parentToken, uint256 _index) public view returns(uint256) {
        return _parentSons[_parentToken][_index];
    }

    function ownerLastUpdate(address _owner) public view returns (uint256) {
        return uint256(_ownedUpdateTime[_owner]);
    }

    function goblinSonByIndex(uint256 _parentId, uint256 _index) public view returns (
        uint256 tokenId,
        uint256 genes,
        uint256 activeMods,
        uint256 mods,
        uint256 inventory,
        uint256 bag,
        uint256 birthTime,
        uint256 lastUpdateTime,
        uint256 cooldownTime
    ) {
        uint256 _tokenId = tokenOfSonByIndex(_parentId, _index);
        return getGoblin(_tokenId);
    }

    function _transfer(address from, address to, uint256 tokenId) internal override {
        require(tokenId > 0 && tokenId <= totalSupply(), "GoblinToken: goblin not exist");
        Goblin storage _goblin = _allGoblins[tokenId];
        uint64 _updateTime = uint64(block.timestamp);
        require(_goblin.cooldownTime <= _updateTime, "GoblinToken: goblin in cooldown");

        super._transfer(from, to, tokenId);

        _goblin.cooldownTime = _updateTime + _cooldownDays;
        _goblin.lastUpdateTime = _updateTime;
        if (_goblin.spouseId > 0 && _goblin.spouseId <= totalSupply()) {
            Goblin storage _spouse = _allGoblins[_goblin.spouseId];
            _spouse.spouseId = 0;
            _spouse.lastUpdateTime = _updateTime;
            _goblin.spouseId = 0;
        }
        _ownedUpdateTime[from] = _updateTime;
        _ownedUpdateTime[to] = _updateTime;
    }

    function _baseURI() internal pure override returns (string memory) {
        return "https://app.goblinwars.io/Goblins/api/Goblin?nft=";
    }
}

