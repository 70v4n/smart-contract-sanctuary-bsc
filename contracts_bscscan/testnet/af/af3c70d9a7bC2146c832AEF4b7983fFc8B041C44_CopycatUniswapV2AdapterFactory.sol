import "./CopycatEmergency.sol";
import "./CopycatAdapterFactoryBase.sol";

abstract contract CopycatAdapterBase is ICopycatAdapter, CopycatEmergency {
  address public override factory;
  ICopycatLeader internal leaderContract;
  bool public initialized = false;

  constructor() {
    factory = msg.sender;
  }

  modifier notInitialized {
    require(!initialized, "Already initialized");
    _;
  }

  function contractSignature() override public virtual view returns(bytes32);

  // Initialize the adapter and bond it to leader
  function initializeLeader(ICopycatLeader _leaderContract) override public notInitialized {
    leaderContract = _leaderContract;
    initialized = true;
  }

  function getLeaderContract() override public view returns(ICopycatLeader) {
    return leaderContract;
  }

  function getLeaderAddress() override public view returns(address) {
    return Ownable(address(leaderContract)).owner();
  }

  modifier onlyLeader {
    require(msg.sender == getLeaderAddress(), "Only leader");
    _;
  }

  modifier onlyLeaderContract {
    require(msg.sender == address(getLeaderContract()), "Only leader contract");
    _;
  }

  function allowEmergencyCall(bytes32 txHash) public override view returns(bool) {
    return (
      msg.sender == address(getLeaderContract()) && 
      CopycatAdapterFactoryBase(factory).isAllowEmergency(txHash) && 
      CopycatAdapterFactoryBase(factory).allowEmergencySignature(contractSignature()) >= 1
    );
  }

  event MigrateTo(address indexed caller, address indexed leader, address indexed to);
  function migrateTo(ICopycatLeader to) public override onlyLeaderContract {
    leaderContract = to;
    emit MigrateTo(msg.sender, getLeaderAddress(), address(to));
  }
}

