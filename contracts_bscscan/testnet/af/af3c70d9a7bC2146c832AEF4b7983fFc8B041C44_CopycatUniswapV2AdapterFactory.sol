pragma solidity ^0.8.0;

import "./CopycatUniswapV2Adapter.sol";
import "../../lib/CopycatAdapterFactoryBase.sol";
import "hardhat/console.sol";

contract CopycatUniswapV2AdapterFactory is CopycatAdapterFactoryBase {
  address[] public router;
  address[] public tradingToken;
  mapping(uint256 => address[]) public tradingRouteBuy;
  mapping(uint256 => address[]) public tradingRouteSell;
  bool[] public enabled;

  uint256 public feeRate = 10;
  address public feeAddress;
  
  event SetFeeAddress(address indexed setter, address oldAddress, address newAddress);
  function setFeeAddress(address newAddress) public onlyOwner {
    emit SetFeeAddress(msg.sender, feeAddress, newAddress);
    feeAddress = newAddress;
  }

  constructor() {
    feeAddress = msg.sender;
    router.push(address(0));
    tradingToken.push(address(0));
    enabled.push(false);
  }

  event AddPair(uint256 indexed pairId, address indexed _router, address indexed _tradingToken);
  function addPair(
    address _router,
    address _tradingToken,
    address[] memory _tradingRouteBuy,
    address[] memory _tradingRouteSell
  ) public onlyOwner {
    tradingRouteBuy[tradingToken.length] = _tradingRouteBuy;
    tradingRouteSell[tradingToken.length] = _tradingRouteSell;
    router.push(_router);
    tradingToken.push(_tradingToken);
    enabled.push(true);

    emit AddPair(tradingToken.length - 1, _router, _tradingToken);
  }

  event SetPairEnabled(uint256 indexed pairId, bool _enabled);
  function setPairEnabled(uint256 pairId, bool _enabled) public onlyOwner {
    enabled[pairId] = _enabled;
    emit SetPairEnabled(pairId, _enabled);
  }

  event SetFeeRate(address indexed setter, uint256 oldFeeRate, uint256 newFeeRate);
  function setFeeRate(uint256 _feeRate) public onlyOwner {
    emit SetFeeRate(msg.sender, feeRate, _feeRate);
    feeRate = _feeRate;
  }

  function getTradingRouteBuy(uint256 pairId) public view returns (address[] memory) {
    return tradingRouteBuy[pairId];
  }

  function getTradingRouteSell(uint256 pairId) public view returns (address[] memory) {
    return tradingRouteSell[pairId];
  }

  function deployAdapter(
    address leaderContract,
    uint256 pairId
  ) external onlyLeader(leaderContract) returns(address payable adapterAddress) {
    require(pairId > 0 && pairId < tradingToken.length, "Invalid pair id");
    require(enabled[pairId], "Disabled");

    // TOEDIT: Change adapter type
    bytes memory bytecode = type(CopycatUniswapV2Adapter).creationCode;
    bytes32 salt = keccak256(abi.encodePacked(msg.sender, nextAdapterId));

    assembly {
      adapterAddress := create2(0, add(bytecode, 32), mload(bytecode), salt)
    }

    // TOEDIT: Add parameters
    CopycatUniswapV2Adapter(adapterAddress).initializeFromFactory(
      ICopycatLeader(leaderContract),
      pairId
    );

    finalize(leaderContract, adapterAddress);
  }
}

pragma solidity ^0.8.0;

import "./CopycatUniswapV2AdapterFactory.sol";
import "../../lib/CopycatAdapterBase.sol";
import "../../interfaces/IUniswapV2Router.sol";

contract CopycatUniswapV2Adapter is CopycatAdapterBase {
  IUniswapV2Router02 public uniswapV2Router;
  IERC20 public tradingToken;
  address[] public tradingRouteBuy; // end at Token
  address[] public tradingRouteSell; // end at BNB
  uint256 public pairId; // Pair ID for syncing with factory

  function initialize(
    ICopycatLeader _leaderContract,
    IUniswapV2Router02 _uniswapV2Router,
    IERC20 _tradingToken,
    address[] memory _tradingRouteBuy,
    address[] memory _tradingRouteSell
  ) public notInitialized {
    uniswapV2Router = _uniswapV2Router;
    tradingToken = _tradingToken;
    tradingRouteBuy = _tradingRouteBuy;
    tradingRouteSell = _tradingRouteSell;

    initializeLeader(_leaderContract);
  }

  function initializeFromFactory(
    ICopycatLeader _leaderContract,
    uint256 _pairId
  ) public notInitialized {
    require(msg.sender == factory, "Not factory");
    pairId = _pairId;
    CopycatUniswapV2AdapterFactory _factory = CopycatUniswapV2AdapterFactory(factory);

    initialize(
      _leaderContract,
      IUniswapV2Router02(_factory.router(pairId)),
      IERC20(_factory.tradingToken(pairId)),
      _factory.getTradingRouteBuy(pairId),
      _factory.getTradingRouteSell(pairId)
    );
  }

  event ReceiveBnb(address indexed payer, uint256 value);

  fallback() external payable {
    emit ReceiveBnb(msg.sender, msg.value);
  }

  receive() external payable {
    emit ReceiveBnb(msg.sender, msg.value);
  }

  function getTokenAmountFromBnb(uint256 amountIn) public view returns(uint256) {
    if (amountIn == 0) return 0; // Uniswap edge case
    uint256[] memory amounts = uniswapV2Router.getAmountsOut(amountIn, tradingRouteBuy);
    return amounts[amounts.length - 1];
  }

  function getBnbValueAmountIn(uint256 amountIn) public view returns(uint256) {
    if (amountIn == 0) return 0; // Uniswap edge case
    uint256[] memory amounts = uniswapV2Router.getAmountsOut(amountIn, tradingRouteSell);
    return amounts[amounts.length - 1];
  }
  
  function getBnbValuePercentage(uint256 percentage) public view returns(uint256) {
    return getBnbValueAmountIn(tradingToken.balanceOf(address(this)) * percentage / 1e18);
  }

  // Sum of BNB value in all mode
  function getBnbValue() override public view returns(uint256) {
    return getBnbValueAmountIn(tradingToken.balanceOf(address(this)));
  }

  function getTokenAmount() public view returns(uint256) {
    return tradingToken.balanceOf(address(this));
  }

  // Transfer BNB from leader to adapter and then convert BNB = msg.value to xxx
  function toAdapter(uint256 tokenMin) override public payable onlyLeaderContract returns(uint256) {
    uint256 tokenBefore = tradingToken.balanceOf(address(this));

    uint256 fee = msg.value * CopycatUniswapV2AdapterFactory(factory).feeRate() / 10000;
    payable(CopycatUniswapV2AdapterFactory(factory).feeAddress()).transfer(fee);

    uniswapV2Router.swapExactETHForTokensSupportingFeeOnTransferTokens{value: msg.value - fee}(
      tokenMin,
      tradingRouteBuy,
      address(this),
      block.timestamp
    );

    uint256 receivedToken = tradingToken.balanceOf(address(this)) - tokenBefore;

    // require(receivedToken >= minToken, "Insufficient Token");

    return receivedToken;
  }

  // Adapter sell xxx to BNB and transfer to leader (sell only ... percent (100% = 1e18)), returns BNB value
  function toLeader(uint256 percentage, uint256 bnbMin) override public onlyLeaderContract returns(uint256) {
    require(percentage <= 1e18, "Over 100%");

    uint256 tokenAmount = tradingToken.balanceOf(address(this)) * percentage / 1e18;
    uint256 bnbBefore = address(this).balance;

    uint256 fee = tokenAmount * CopycatUniswapV2AdapterFactory(factory).feeRate() / 10000;
    tradingToken.transfer(CopycatUniswapV2AdapterFactory(factory).feeAddress(), fee);

    tokenAmount -= fee;

    tradingToken.approve(address(uniswapV2Router), tokenAmount);

    // make the swap
    uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
      tokenAmount,
      bnbMin,
      tradingRouteSell,
      address(this),
      block.timestamp
    );

    uint256 receivedBnb = address(this).balance - bnbBefore;

    // require(receivedBnb >= minBnb, "Insufficient BNB");

    address(leaderContract).call{value: receivedBnb}("");

    return receivedBnb;
  }

  function contractSignature() override public view returns(bytes32) {
    return keccak256(abi.encodePacked(
      address(uniswapV2Router),
      address(tradingToken),
      pairId
    ));
  }
}

pragma solidity ^0.8.0;

import "../interfaces/ICopycatLeader.sol";
import "../interfaces/ICopycatAdapter.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./CopycatEmergencyMaster.sol";

abstract contract CopycatAdapterFactoryBase is CopycatEmergencyMaster {
  event NewAdapter(address indexed adapterContract, address indexed leaderContract, address indexed leader);
  uint256 public nextAdapterId = 0;
  address[] public adapters;

  mapping(bytes32 => uint256) public allowEmergencySignature;

  event SetAllowEmergencySignature(address indexed setter, bytes32 signature, uint256 level);
  function setAllowEmergencySignature(bytes32 signature, uint256 level) public onlyOwner {
    allowEmergencySignature[signature] = level;
    emit SetAllowEmergencySignature(msg.sender, signature, level);
  }

  modifier onlyLeader(address leaderContract) {
    require(Ownable(leaderContract).owner() == msg.sender, "Not leader");
    _;
  }

  function finalize(address leaderContract, address adapter) internal {
    adapters.push(adapter);
    ICopycatLeader(leaderContract).addAdapter(ICopycatAdapter(adapter));
    emit NewAdapter(adapter, leaderContract, Ownable(leaderContract).owner());
    nextAdapterId++;
  }
}

