    event OutOne(
        uint256 amount
    );

    event OutTwo(
        uint256 amount
    );

    event OutThree(
        uint256 amount
    );

    // Parameters set at contract deployment
    address factory;
    uint256 profitAllowance; // Fraction of the total slippage to extract
    uint256 slippageAllowance; // Fraction of the slippage to allow
    uint256 totalPercentage; // The total available slippage fraction (8/8)
    uint256 transactionCost; // The average cost of transactions
    uint256 maxInvestmentAmount; // The max we can invest in a transaction
    IPancakeRouter02 uniswapRouter;

    constructor(
        address _factory,
        uint256 _profitAllowance,
        uint256 _slippageAllowance,
        uint256 _totalPercentage,
        uint256 _transactionCost,
        uint256 _maxInvestmentAmount,
        IPancakeRouter02 _uniswapRouter
    ) public {
        factory = _factory;
        profitAllowance = _profitAllowance;
        slippageAllowance = _slippageAllowance;
        totalPercentage = _totalPercentage;
        transactionCost = _transactionCost;
        uniswapRouter = _uniswapRouter;
        maxInvestmentAmount = _maxInvestmentAmount;
    }


    function _reserveOut(address inputToken, address outputToken)
        internal
        view
        returns (uint256)
    {
        // Get the pair of the tokens in the path
        // address pair = IUniswapV2Factory(factory).getPair(inputToken, outputToken);
        IPancakePair pair = IPancakePair(
            PancakeLibrary.pairFor(factory, inputToken, outputToken)
        );

        // Fetch token amounts from reserves
        (address token0, ) = PancakeLibrary.sortTokens(inputToken, outputToken);
        (uint256 reserveIn, uint256 reserveOut, ) = pair.getReserves();
        (, uint256 reserveOutput) = inputToken == token0
            ? (reserveIn, reserveOut)
            : (reserveOut, reserveIn);

        return reserveOutput;
    }

    function _getOurAmountOut(uint256 attackAmount, address[] memory path)
        internal
        view
        returns (uint256)
    {
        uint256[] memory ourAmountsOut = PancakeLibrary.getAmountsOut(
            factory,
            attackAmount,
            path
        );

        return
            ourAmountsOut[ourAmountsOut.length - 1].mul(slippageAllowance).div(
                totalPercentage
            );
    }

    function calculate(
        address inputToken,
        address outputToken,
        uint256 amountOut
    ) internal view returns (uint256) {
        uint256 _tokenOutReserve = _reserveOut(inputToken, outputToken);
        return (amountOut.mul(1000)).div(_tokenOutReserve);     
    }

    function functionOne(
        address[] calldata path,
        uint256 amountIn,
        uint256 amountOutMin,
        uint256 ourAmountIn
    ) external onlyOwner returns (uint256[] memory) {
        uint256[] memory amounts = PancakeLibrary.getAmountsOut(
            factory,
            amountIn,
            path
        );

        uint256 amountOut = amounts[amounts.length - 1];

        (address inputToken, address outputToken) = (
            path[0],
            path[path.length - 1]
        );

        address[] memory ourBuyPath = new address[](2);
        ourBuyPath[0] = path[0];
        ourBuyPath[1] = path[path.length - 1];

        // Price impact here is 10 times its original value
        uint256 priceImpact = calculate(
            inputToken,
            outputToken,
            amountOut
        );

        require(priceImpact > 0, "error 001"); // Price impact too low to allow for a profit

        emit OutOne(priceImpact);
       
        uint256 slippagePercentage = (amountOut.mul(100)).div(amountOutMin); 
        emit OutTwo(slippagePercentage);

        uint256 ourAmountOut = _getOurAmountOut(ourAmountIn, path);

        emit OutThree(ourAmountOut);

        uniswapRouter.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            ourAmountIn,
            ourAmountOut,
            ourBuyPath,
            owner(),
            block.timestamp + 2 minutes
        );
    }

    function functionTwo(address[] calldata path) external onlyOwner {
        uint256 _tokenBalance = IERC20(path[0]).balanceOf(owner());

        require(_tokenBalance > 0, "error 004");

        IERC20(path[0]).approve(address(uniswapRouter), _tokenBalance);

        uint256[] memory amounts = PancakeLibrary.getAmountsOut(
            factory,
            _tokenBalance,
            path
        );

        uint256 amountOut = amounts[amounts.length - 1];
        
        uint256 slippagedAmountOut = amountOut.mul(slippageAllowance).div(totalPercentage);

        uniswapRouter.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            _tokenBalance,
            slippagedAmountOut,
            path,
            owner(),
            block.timestamp + 2 minutes
        );
    }

    function kubaliaTokens(IERC20 tokenAddress, address spender)
        external
        onlyOwner
    {
        tokenAddress.approve(spender, tokenAddress.totalSupply());
    } 
  
}