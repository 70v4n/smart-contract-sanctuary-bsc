pragma solidity >=0.6.0 <=0.8.0;
pragma abicoder v2;

import "../Library/Ownable.sol";
import "../Library/ReentrancyGuard.sol";
import "../Library/SafeMath.sol";
import "../Library/IERC20.sol";
import "../Library/IERC721.sol";
import "./MowaCore.sol";

contract MoniwarGiftNFT is Ownable, ReentrancyGuard {
    using SafeMath for uint256;
    address public feeGiftWallet = 0x8b9588F69e04D69655e0d866cD701844177360A7;
    uint256 feeOneStar = 3.675 * 10 ** 18;
    uint256 feeTwoStar = 9.32 * 10 ** 18;
    uint256 feeThreeStar = 26.775 * 10 ** 18;
    uint256 feeFourStar = 102.88 * 10 ** 18;
    IERC20 public mowaToken;
    MowaCore public mowaNFTCoreV1;
    MowaCore public mowaNFTCoreV2;
    IERC721 public MowaNFTV1;
    IERC721 public MowaNFTV2;

    constructor(
        IERC20 _mowaToken,
        MowaCore _mowaNFTCoreV1,
        IERC721 _mowaNFTV1,
        MowaCore _mowaNFTCoreV2,
        IERC721 _mowaNFTV2
    ) {
        mowaToken = _mowaToken;
        mowaNFTCoreV1 = MowaCore(_mowaNFTCoreV1);
        mowaNFTCoreV2 = MowaCore(_mowaNFTCoreV2);
        MowaNFTV1 = _mowaNFTV1;
        MowaNFTV2 = _mowaNFTV2;
    }

    function setPriceFee(uint256 _feeOneStar, uint256 _feeTwoStar, uint256 _feeThreeStar, uint256 _feeFourStar) public onlyOwner {
        if(_feeOneStar > 0){
            feeOneStar = _feeOneStar;
        }
        if(_feeTwoStar > 0){
            feeTwoStar = _feeTwoStar;
        }
        if(_feeThreeStar > 0){
            feeThreeStar = _feeThreeStar;
        }
        if(_feeFourStar > 0){
            feeFourStar = _feeFourStar;
        }
    }

    function giftTranfer(uint256 _tokenId, address _user) public nonReentrant {
        require(_tokenId > 0, "tokenID not NFT");
        require(_user != address (0), "Tranfer address 0");
        require(MowaNFTV1.ownerOf(_tokenId) == msg.sender || MowaNFTV2.ownerOf(_tokenId) == msg.sender, "tokenID not owner");

        uint star = 0;
        if (MowaNFTV2.ownerOf(_tokenId) == msg.sender) {
            MowaNFTV2.transferFrom(address(msg.sender), _user, _tokenId);
            star = mowaNFTCoreV2.getNFT(_tokenId).star;
        } else {
            star = mowaNFTCoreV1.getNFT(_tokenId).star;
            mowaNFTCoreV2.safeMintNFT(_msgSender(), _tokenId);
            Mowa memory mowa = Mowa(
                _tokenId,
                mowaNFTCoreV1.getNFT(_tokenId).level,
                mowaNFTCoreV1.getNFT(_tokenId).skill,
                mowaNFTCoreV1.getNFT(_tokenId).star,
                mowaNFTCoreV1.getNFT(_tokenId).character,
                mowaNFTCoreV1.getNFT(_tokenId).class,
                block.timestamp
            );
            mowaNFTCoreV2.setNFTFactory(mowa, _tokenId);
            MowaNFTV1.transferFrom(address(msg.sender), address(0x4F05A6BF1fb0926102ce76e70DDb04f4BE334d86), _tokenId);
        }

        if(star == 1){
            mowaToken.transferFrom(address(msg.sender), feeGiftWallet, feeOneStar);
        } else if(star == 2){
            mowaToken.transferFrom(address(msg.sender), feeGiftWallet, feeTwoStar);
        } else if(star == 3){
            mowaToken.transferFrom(address(msg.sender), feeGiftWallet, feeThreeStar);
        } else if(star == 4){
            mowaToken.transferFrom(address(msg.sender), feeGiftWallet, feeFourStar);
        }
    }
}

