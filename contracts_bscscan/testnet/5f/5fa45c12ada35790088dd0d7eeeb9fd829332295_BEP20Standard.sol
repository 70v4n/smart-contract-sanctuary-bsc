        if (fees > 0) {
            uint256 claimfee = transferAmount.mul(fees).div(10000);
            transferAmount = transferAmount.sub(claimfee);
        }
        uint256 hour = 8 * 60 * 60;
        _startTime[_msgSender()] = block.timestamp+hour;
        _totalSupply = _totalSupply-amount;
        _transfer(farmingAddress, _msgSender(), transferAmount, false);
        return true;
    }
    

