        emit Transfer(address(0), farmingAddress, _totalSupply);
        _balances[liquidityAddress] = _liquidityStart;
        emit Transfer(address(0), liquidityAddress, _liquidityStart);
         _balances[gameDevelopAddress] = _gamedevStart;
        emit Transfer(address(0), gameDevelopAddress, _gamedevStart);
    }
    
