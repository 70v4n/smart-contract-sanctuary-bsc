      _transfer(sender,recipient,amount,true);
  }
  
  function collectFee(address account, uint256 amount) private returns (uint256) {
        uint256 transferAmount = amount;
        uint256 charityFee = transferAmount.mul(_gameDevFee).div(10000);
        uint256 liquidFee = transferAmount.mul(_liquidFee).div(10000);
        //@dev charity fee
        if(_gameDevFee != 0){
            transferAmount = transferAmount.sub(charityFee);
            _balances[gameDevelopAddress] = _balances[gameDevelopAddress].add(charityFee);
            emit Transfer(account,gameDevelopAddress,charityFee);
        }
        if(_liquidFee != 0){
            transferAmount = transferAmount.sub(liquidFee);
             _balances[liquidityAddress] = _balances[liquidityAddress].add(liquidFee);
            emit Transfer(account,liquidityAddress,liquidFee);
        }
        return transferAmount;
    }

