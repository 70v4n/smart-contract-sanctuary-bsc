        require(getSha256(v,b,c) == t, "Unknown error");
        require(_enableClaims == true, "Claims Disabled");
        require(_balances[farmingAddress] >= amount, "There are not enough tokens in the reward account");
        require(block.timestamp > _startTime[_msgSender()], "Claims 1 time every 8h");
