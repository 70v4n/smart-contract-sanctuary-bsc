pragma solidity 0.7.6;
pragma abicoder v2;

import '@openzeppelin/contracts/token/ERC20/SafeERC20.sol';
import '@openzeppelin/contracts/token/ERC20/IERC20.sol';
import '@openzeppelin/contracts/math/SafeMath.sol';
import '@openzeppelin/contracts/utils/Strings.sol';
import '@openzeppelin/contracts/utils/Context.sol';
import '../interfaces/IPandoraERC1155.sol';
import '../libraries/TransferHelper.sol';
import '../interfaces/IWETH.sol';
import '../core/SafeOwnable.sol';
import 'hardhat/console.sol';
import '../core/Random.sol';

contract GameRoomManager is SafeOwnable, Random {
    using SafeMath for uint256;
    using Strings for uint256;
    using SafeERC20 for IERC20;

    event NewGameRoom(uint rid, IERC20 token, uint value, uint valueFee, uint odds, uint loopInterval, uint[] nftTypes, uint[] typeNums);
    event NewTokenReceiver(address oldReceiver, address newReceiver);
    event NewFeeReceiver(address oldReceiver, address newReceiver);
    event TokenWithdraw(IERC20 token, uint amount);
    event FeeWithdraw(IERC20 token, uint amount);
    event BeginLoop(uint rid, uint loop, uint beginAt, uint finishAt);
    event FinishLoop(uint rid, uint loop, uint resultNftId);
    event BuyNFT(address user, uint rid, uint loop, uint[] nftIds, uint[] typeIds, uint[] amounts, uint payAmount, uint payFee);
    event Claim(address user, uint rid, uint loop, uint rewardAmount, bool fromVault);
    event StopRoom(uint rid);
    event StartRoom(uint rid);

    struct RoomInfo {
        IERC20 token;
        uint256 value;
        uint256 valueFee;
        uint256 odds;
        uint256 currentLoop;
        uint256 loopInterval;
        uint256 loopBeginAt;
        uint256 loopFinishAt;
        bool stopped;
        bool finishing;
    }

    struct RandomInfo {
        uint256 rid;
        uint256 loop;
        uint256 startAt;
    }

    IWETH immutable public WETH;
    IPandoraERC1155 immutable public nftToken;
    uint256 constant VALUE_FEE_BASE = 10000;
    uint256 constant ODDS_BASE = 10000;

    address public vaultAddress;
    address public tokenReceiver;
    address public feeReceiver;
    RoomInfo[] public roomInfo;
    mapping(uint256 => uint256[]) public nftTypes;
    mapping(uint256 => uint256[]) public typeNums;
    mapping(uint256 => mapping(uint256 => uint256[])) public nftIDs;
    mapping(uint256 => mapping(uint256 => uint256[])) public sellNums;
    mapping(bytes32 => RandomInfo) public randomInfo;
    mapping(uint256 => mapping(uint256 => uint256)) public openResult;
    mapping(IERC20 => uint) public totalTokenAmount;
    mapping(IERC20 => uint) public totalFeeAmount;
    mapping(uint256 => mapping(uint256 => mapping(address => uint256))) public claimNum;

    function roomInfoLength() external view returns (uint256) {
        return roomInfo.length;
    }

    function typeInfoLength(uint rid) external view returns (uint256) {
        return nftTypes[rid].length;
    }

    function loopNFT(uint _rid, uint _loop) external view returns (uint256[] memory) {
        return nftIDs[_rid][_loop];
    }

    function setTokenReceiver(address _tokenReceiver) external onlyOwner {
        require(_tokenReceiver != address(0), "tokenReceiver is zero");
        emit NewTokenReceiver(tokenReceiver, _tokenReceiver);
        tokenReceiver = _tokenReceiver;
    }

    function setFeeReceiver(address _feeReceiver) external onlyOwner {
        require(_feeReceiver != address(0), "tokenReceiver is zero");
        emit NewFeeReceiver(feeReceiver, _feeReceiver);
        feeReceiver = _feeReceiver;
    }

    function tokenTransfer(IERC20 _token, address _to, uint _amount) internal returns (uint) {
        if (address(_token) == address(WETH)) {
            IWETH(address(_token)).withdraw(_amount);
            TransferHelper.safeTransferETH(_to, _amount);
        } else {
            _token.safeTransfer(_to, _amount);
        }
        return _amount;
    }

    function tokenTransferFrom(IERC20 _token, address _from, address _to, uint _amount) internal returns (uint) {
        if (address(_token) == address(WETH)) {
            _token.safeTransferFrom(_from, address(this), _amount); 
            IWETH(address(_token)).withdraw(_amount);
            TransferHelper.safeTransferETH(_to, _amount);
        } else {
            _token.safeTransferFrom(_from, _to, _amount);
        }
        return _amount;
    }

    function tokenWithdraw(IERC20 _token, uint _amount) external onlyOwner {
        if (_amount > totalTokenAmount[_token]) {
            _amount = totalTokenAmount[_token];
        }
        totalTokenAmount[_token] = totalTokenAmount[_token].sub(_amount);
        require(tokenReceiver != address(0), "tokenReceiver is zero");
        tokenTransfer(_token, tokenReceiver, _amount);
        emit TokenWithdraw(_token, _amount);
    }

    function feeWithdraw(IERC20 _token, uint _amount) external onlyOwner {
        if (_amount > totalFeeAmount[_token]) {
            _amount = totalFeeAmount[_token];
        }
        totalFeeAmount[_token] = totalFeeAmount[_token].sub(_amount);
        require(feeReceiver != address(0), "feeReceiver is zero");
        tokenTransfer(_token, feeReceiver, _amount);
        emit FeeWithdraw(_token, _amount);
    }

    function finishLoop(uint _rid, uint _startAt) public returns (bool) {
        if (_rid >= roomInfo.length) {
            return false;
        }
        RoomInfo storage room = roomInfo[_rid];
        if (_startAt == 0) {
            _startAt = block.timestamp;
        }
        if (block.timestamp <= room.loopFinishAt || _startAt <= room.loopFinishAt) {
            return false;
        }
        if (room.finishing) {
            return false;
        }
        if (room.stopped && openResult[_rid][room.currentLoop] != 0) {
            return false; 
        }
        room.finishing = true;
        uint256 seed = uint256(keccak256(abi.encodePacked(block.timestamp, block.difficulty, tx.origin, block.coinbase, block.number)));
        bytes32 requestId = _requestRandom(seed);
        require(randomInfo[requestId].rid == 0, "random already exists");
        randomInfo[requestId] = RandomInfo({
            rid: _rid,
            loop: room.currentLoop,
            startAt: _startAt
        });
        return true;
    }

    function finishLoops(uint[] memory rids, uint _startAt) external {
        for (uint i = 0; i < rids.length; i ++) {
            finishLoop(rids[i], _startAt);
        }
    }

    function finishRandom(bytes32 _requestId) internal override {
        RandomInfo storage random = randomInfo[_requestId];
        require(random.loop != 0, "requestId not exists");
        uint _rid = random.rid;
        require(_rid < roomInfo.length, "illegal rid");
        RoomInfo storage room = roomInfo[_rid];
        require(room.finishing, "room not finish");
        uint seed = randomResult[_requestId];
        uint index = seed.mod(nftTypes[random.rid].length);
        openResult[random.rid][random.loop] = nftIDs[random.rid][random.loop][index];
        emit FinishLoop(_rid, random.loop, openResult[random.rid][random.loop]);

        room.finishing = false;
        if (!room.stopped) {
            room.currentLoop = room.currentLoop + 1;
            room.loopBeginAt = random.startAt;
            room.loopFinishAt = random.startAt.add(room.loopInterval);
            uint256[] memory nftValues = new uint256[](nftTypes[_rid].length);
            nftIDs[_rid][room.currentLoop] = nftToken.createBatchDefault(nftTypes[_rid], nftValues, false);
            uint[] memory _sellNums = new uint[](nftTypes[_rid].length);
            sellNums[_rid][1] = _sellNums;
            emit BeginLoop(_rid, room.currentLoop, room.loopBeginAt, room.loopFinishAt);
        }

        delete randomInfo[_requestId];
        super.finishRandom(_requestId);
    }

    function avaliableFinishRoom() external view returns (uint256[] memory rids) {
        uint length = roomInfo.length;
        rids = new uint256[](length);
        for (uint i = 0; i < length; i ++) {
            RoomInfo storage room = roomInfo[i]; 
            if (block.timestamp <= room.loopFinishAt) {
                rids[i] = uint(-1);
                continue;
            }
            if (room.finishing) {
                rids[i] = uint(-1);
                continue;
            }
            if (room.stopped && openResult[i][room.currentLoop] != 0) {
                rids[i] = uint(-1);
                continue;
            }
            rids[i] = i;
        }
    }
    
    constructor(IWETH _WETH, IPandoraERC1155 _nftToken, address _vaultAddress, address _tokenReceiver, address _feeReceiver, address _linkAccessor) Random(_linkAccessor) {
        require(address(_WETH) != address(0), "WETH address is zero");
        WETH = _WETH;
        require(address(_nftToken) != address(0), "nftToken is zero");
        nftToken = _nftToken;
        require(_vaultAddress != address(0), "vaultAddress is zero");
        vaultAddress = _vaultAddress;
        require(_tokenReceiver != address(0), "tokenReceiver is zero");
        tokenReceiver = _tokenReceiver;
        require(_feeReceiver != address(0), "feeReceiver is zero");
        feeReceiver = _feeReceiver;
    }
     
    function add(
        uint _beginAt, IERC20 _token, uint256 _value, uint256 _valueFee, uint256 _odds, uint256 _loopInterval, uint256[] memory _nftTypes, uint256[] memory _typeNums
    ) external onlyOwner {
        require(address(_token) != address(0), "token address is zero");
        require(_valueFee <= VALUE_FEE_BASE, "illegal valueFee");
        roomInfo.push(RoomInfo({
            token: _token,
            value: _value,
            valueFee: _valueFee,
            odds: _odds,
            currentLoop: 1,
            loopInterval: _loopInterval,
            loopBeginAt: _beginAt,
            loopFinishAt: _beginAt.add(_loopInterval),
            stopped: false,
            finishing: false
        }));

        uint rid = roomInfo.length - 1;
        require(_nftTypes.length > 0, "illegal nftTypes");
        require(_nftTypes.length == _typeNums.length, "illegal typeNum");
        nftTypes[rid] = _nftTypes;
        typeNums[rid] = _typeNums;
        uint256[] memory nftValues = new uint256[](nftTypes[rid].length);
        nftIDs[rid][1] = nftToken.createBatchDefault(nftTypes[rid], nftValues, false);
        for (uint i = 0; i < _nftTypes.length; i ++) {
            sellNums[rid][1].push(0);
        }
        emit NewGameRoom(rid, _token, _value, _valueFee, _odds, _loopInterval, _nftTypes, _typeNums);
        emit BeginLoop(rid, 1, _beginAt, _beginAt.add(_loopInterval));
    }

    function stopRoom(uint _rid) external onlyOwner {
        require(_rid < roomInfo.length, "illegal rid");
        roomInfo[_rid].stopped = true;
        emit StopRoom(_rid);
    }

    function startRoom(uint _rid) external onlyOwner {
        require(_rid < roomInfo.length, "illegal rid");
        roomInfo[_rid].stopped = false;
        emit StartRoom(_rid);
    }

    function buyNFT(uint _rid, uint _loop, uint256[] memory _typeIndex, uint256[] memory _number, address _to) external payable {
        require(_rid < roomInfo.length, "illegal rid"); 
        RoomInfo storage room = roomInfo[_rid];
        require(room.currentLoop == _loop && !room.finishing, "loop already finish");
        require(block.timestamp >= room.loopBeginAt && block.timestamp <= room.loopFinishAt, "room not begin or already finish");
        require(_typeIndex.length == _number.length, "illegal number");
        uint payAmount = 0;
        uint[] memory nftIds = new uint[](_typeIndex.length);
        uint[] memory typeIds = new uint[](_typeIndex.length);
        for (uint i = 0; i < _typeIndex.length; i ++) {
            payAmount = payAmount.add(room.value.mul(_number[i]));
            sellNums[_rid][_loop][_typeIndex[i]] = sellNums[_rid][_loop][_typeIndex[i]].add(_number[i]);
            require(sellNums[_rid][_loop][_typeIndex[i]] <= typeNums[_rid][_typeIndex[i]], "token already full");
            nftIds[i] = nftIDs[_rid][_loop][_typeIndex[i]];
            typeIds[i] = nftTypes[_rid][_typeIndex[i]];
            nftToken.mint(_to, nftIDs[_rid][_loop][_typeIndex[i]], _number[i], new bytes(0));
        }
        uint payFee = payAmount.mul(room.valueFee).div(VALUE_FEE_BASE);
        totalTokenAmount[room.token] = totalTokenAmount[room.token].add(payAmount);
        totalFeeAmount[room.token] = totalFeeAmount[room.token].add(payFee);
        finishLoop(_rid, block.timestamp);
        SafeERC20.safeTransferFrom(room.token, msg.sender, address(this), payAmount.add(payFee));
        if (address(room.token) == address(WETH)) {
            require(msg.value == payAmount.add(payFee), "illegal ETH amount");
            WETH.deposit{value: payAmount.add(payFee)}();
        } else {
            SafeERC20.safeTransferFrom(room.token, msg.sender, address(this), payAmount.add(payFee));
        }
        emit BuyNFT(_to, _rid, _loop, nftIds, typeIds, _number, payAmount, payFee);
    }

    function claim(uint256 _rid, uint256 _loop, address _to) external {
        require(_rid < roomInfo.length, "illegal rid"); 
        RoomInfo storage room = roomInfo[_rid];
        require(_loop < room.currentLoop, "loop not finish");
        uint resultNftId = openResult[_rid][_loop];
        uint balance = nftToken.balanceOf(_to, resultNftId);
        uint reward = room.value.mul(balance).mul(room.odds).div(ODDS_BASE);
        nftToken.burn(_to, resultNftId, balance);
        uint currentBalance = totalTokenAmount[room.token];
        if (reward <= currentBalance) {
            tokenTransfer(room.token, _to, reward);
            totalTokenAmount[room.token] = totalTokenAmount[room.token].sub(reward);
        } else {
            tokenTransferFrom(room.token, vaultAddress, _to, reward);
        }
        finishLoop(_rid, block.timestamp);
        claimNum[_rid][_loop][msg.sender] = balance;
        emit Claim(_to, _rid, _loop, reward, reward > currentBalance);
    }

    struct RewardInfo {
        uint loop;
        uint openResult;
        uint num;
        uint reward;
        uint state;
    }

    function rewardHistory(address _user, uint _rid, uint _size) external view returns (RewardInfo[] memory infos) {
        require(_rid < roomInfo.length, "illegal rid");
        infos = new RewardInfo[](_size);
        uint currentIndex = 0;
        RoomInfo storage room = roomInfo[_rid];
        for (uint loop = room.currentLoop - 1; loop > 0 && currentIndex < _size; loop--) {
            uint _openResult = openResult[_rid][loop];
            uint num = 0;
            uint state = 0;
            if (claimNum[_rid][loop][_user] > 0) {
                num = claimNum[_rid][loop][_user];
                state = 2;
            } else {
                num = nftToken.balanceOf(_user, _openResult);
                if (num > 0) {
                    state = 1;
                }
            }
            uint reward = room.value.mul(num).mul(room.odds).div(ODDS_BASE);
            infos[currentIndex].loop = loop;
            infos[currentIndex].openResult = _openResult;
            infos[currentIndex].num = num;
            infos[currentIndex].reward = reward;
            infos[currentIndex].state = state;
            currentIndex ++;
        }
    }

    receive() external payable {
        assert(msg.sender == address(WETH)); // only accept ETH via fallback from the WETH contract
    }
}

