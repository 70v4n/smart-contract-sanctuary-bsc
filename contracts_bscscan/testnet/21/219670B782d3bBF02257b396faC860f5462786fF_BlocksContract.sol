pragma solidity ^0.8.0;

import "./BlocksRewardsPool.sol";
import "hardhat/console.sol";


contract BlocksContract {

  BlocksRewardsPool public rewardsPool;

  uint constant maxUsersToRewardPerBlock = 9; // 9 is 10. Gas optimizing at its finest for loops later on.
  uint constant treasuryFee = 10; // %
  event PosterCreated(address indexed posterOwner, uint startBlock, uint endBlock);
  event RewardClaimed(address user, uint amount);

  struct UserReward {
    address user;
    uint paid;
  }

  struct Block {
    uint price;
    address owner;
    // uint indexOfLastUserToReward; // Helper for tracking array
    // UserReward[maxUsersToRewardPerBlock + 1] usersToReward;
  }

  struct BlockView {
    uint price;
    address owner;
    uint16 blockNumber;
  }

  struct Poster {
    address owner;
    uint blockstart;
    uint blockend;
    string imghash;
    string text;
    uint zindex;
  }

  struct UserState{
    Poster posterBought;
    uint pendingReward;
  }


  address payable public owner;
  address payable public treasury;

  // Counters.Counter private _tokenIds;
  mapping(uint => Block) public blocks;
  mapping(address => UserState) public users;
  mapping(address => uint) testing;
  uint constant initialPriceOfLogoBlocks = 7 ether;

  modifier onlyOwner() {
      require(msg.sender == owner, "Not owner");
      _;
  }

  constructor(address _rewardsPoolContract) {
    owner = payable(msg.sender);
    treasury = owner;
    setPriceOfLogoBlocks(owner);
    rewardsPool = BlocksRewardsPool(_rewardsPoolContract);
  }

  function setRewardsPoolContract(address _add) public onlyOwner {
    rewardsPool = BlocksRewardsPool(_add);
  }

  function setPriceOfLogoBlocks(address initialOwner) internal {
    // 303 - 604
    blocks[303].price = initialPriceOfLogoBlocks;
    blocks[303].owner = initialOwner;
    blocks[403].price = initialPriceOfLogoBlocks;
    blocks[403].owner = initialOwner;
    blocks[503].price = initialPriceOfLogoBlocks;
    blocks[503].owner = initialOwner;
    blocks[603].price = initialPriceOfLogoBlocks;
    blocks[603].owner = initialOwner;
    blocks[304].price = initialPriceOfLogoBlocks;
    blocks[304].owner = initialOwner;
    blocks[404].price = initialPriceOfLogoBlocks;
    blocks[404].owner = initialOwner;
    blocks[504].price = initialPriceOfLogoBlocks;
    blocks[504].owner = initialOwner;
    blocks[604].price = initialPriceOfLogoBlocks;
    blocks[604].owner = initialOwner;
  }

  function rewardOf(address userAddress) external view returns (uint){
    return users[userAddress].pendingReward;
  }

  function claimReward() external {

    uint userBalanceToGrant = users[msg.sender].pendingReward;
    require(userBalanceToGrant > 0, "Nothing to withraw.");
    users[msg.sender].pendingReward = 0;
    (bool success, ) = payable(msg.sender).call{value: userBalanceToGrant}("");
    require(success, "Claiming rewards failed.");
    emit RewardClaimed(msg.sender, userBalanceToGrant);
  }

  // TODO: This shall be removed after testnet. If you see this, contact devs immediately
  // function withdrawTestOnly() external onlyOwner{
  //   uint contractBalance = address(this).balance;
  //   (bool success, ) = owner.call{value: contractBalance}("");
  //   require(success, "Transfer failed.");
  // }

  function purchasePoster(uint startBlockId, uint endBlockId, string calldata imghash, string calldata text) external payable {

    // 1. Checks
    require(isPosterValid(startBlockId, endBlockId), "Poster size not within a range");
    require(bytes(imghash).length != 0, "Image hash cannot be empty.");
    
    (uint currentPriceOfPoster, uint numberOfBlocks, bool isInitialPoster) = calculatePriceAndSize(startBlockId, endBlockId);
    uint posterPaymentReceived = msg.value;
    
    if(!isInitialPoster){
      require(currentPriceOfPoster < posterPaymentReceived, "Price you paid not enought!");
    }

    uint priceIncreasePerBlock;
    if(currentPriceOfPoster < posterPaymentReceived){
      priceIncreasePerBlock = (posterPaymentReceived - currentPriceOfPoster) / numberOfBlocks;
    }

    if(!isInitialPoster){
      require(priceIncreasePerBlock > 0, "Price increase too small.");
    }

    // 2. Storage operations
    (uint256[] memory blocksBought, address[] memory previousBlockOwners) = 
        calculateBlocksOwnershipChanges(startBlockId, endBlockId, priceIncreasePerBlock); 
    updateUserState(msg.sender, startBlockId, endBlockId, imghash, text);

    // 3. Transactions   
    // Send fresh info to RewardsPool contract, so buyer gets some sweet rewards
    rewardsPool.blockAreaBoughtOnSpace{value:msg.value}(0, msg.sender, numberOfBlocks, blocksBought, previousBlockOwners);

    emit PosterCreated(msg.sender, startBlockId, endBlockId);
  }

  function calculateBlocksOwnershipChanges(uint startBlockId, uint endBlockId, uint priceIncreasePerBlock) internal returns (uint256[] memory, address[] memory) {
    
    (uint startBlockX, uint startBlockY) = getBlockLocation(startBlockId);
    (uint endBlockX, uint endBlockY) = getBlockLocation(endBlockId);

    // Go through all blocks that were paid for
    uint256[] memory blocksBought = new uint256[](42);
    address[] memory previousBlockOwners = new address[](42);
    uint256 arrayIndex = 0;
    for(uint i = startBlockX; i <= endBlockX; i++){
      for(uint j = startBlockY; j <= endBlockY; j++){
        //Set new state of the Block
        Block storage currentBlock = blocks[i * 100 + j];      
        currentBlock.price = currentBlock.price + priceIncreasePerBlock; // Set new price that was paid for this block
        previousBlockOwners[arrayIndex] = currentBlock.owner;
        currentBlock.owner = msg.sender; // Set new owner of block
        // Prepare array of all previous owners
        blocksBought[arrayIndex] = i * 100 + j;
        arrayIndex++;
      }
    }
    return (blocksBought, previousBlockOwners);
  }

  function updateUserState(address user, uint startBlockId, uint endBlockId, string calldata imghash, string calldata text) internal {
    UserState storage userState = users[user];   
    userState.posterBought.owner = user;
    userState.posterBought.blockstart = startBlockId;
    userState.posterBought.blockend = endBlockId;
    userState.posterBought.imghash = imghash;
    userState.posterBought.text = text;
    userState.posterBought.zindex = block.number;
  }

  function getPoster(address purchaser) external view returns(Poster memory) {
    return users[purchaser].posterBought;
  }

  function calculatePrice(uint startBlockId, uint endBlockId) public view returns(BlockView[] memory){
    require(isPosterValid(startBlockId, endBlockId), "Poster size is not within a range. Haxor much? Wow!");

    (uint startBlockX, uint startBlockY) = getBlockLocation(startBlockId);
    (uint endBlockX, uint endBlockY) = getBlockLocation(endBlockId);

    BlockView[42] memory blockAreaTemp;
    uint arrayCounter;
    for(uint16 i = uint16(startBlockX); i <= endBlockX; i++){
      for(uint16 j = uint16(startBlockY); j <= endBlockY; j++){
        blockAreaTemp[arrayCounter] = BlockView(
          blocks[i * 100 + j].price, 
          blocks[i * 100 + j].owner, 
          (i * 100 + j) // block number
        );
        arrayCounter++;
      }
    }
    console.log("size of array ", arrayCounter);
    // Simply clean up unussed stuff
    BlockView[] memory blockArea = new BlockView[](arrayCounter);
    for(uint i; i < arrayCounter; i++){
      blockArea[i] = blockAreaTemp[i];
    }
    return blockArea;
  }

  function calculatePriceAndSize(uint startBlockId, uint endBlockId) internal view returns(uint, uint, bool){

    require(isPosterValid(startBlockId, endBlockId), "Poster size is not within a range. Haxor much? Wow!");

    (uint startBlockX, uint startBlockY) = getBlockLocation(startBlockId);
    (uint endBlockX, uint endBlockY) = getBlockLocation(endBlockId);

    bool isInitialPoster = true;
    uint currentPrice;
    uint numberOfBlocks;
    for(uint i = startBlockX; i <= endBlockX; i++){
      for(uint j = startBlockY; j <= endBlockY; j++){
        currentPrice += blocks[i * 100 + j].price;
        if(isInitialPoster && blocks[i * 100 + j].owner != address(0)){
          isInitialPoster = false;
        }
        numberOfBlocks++;
      }
    }
    return (currentPrice, numberOfBlocks, isInitialPoster);
  }

  function isPosterValid(uint startBlockId, uint endBlockId) internal pure returns(bool){
    (uint startBlockX, uint startBlockY) = getBlockLocation(startBlockId);
    (uint endBlockX, uint endBlockY) = getBlockLocation(endBlockId);
    // console.log("BlockY ", startBlockY, " /// ", endBlockY);
    require(startBlockX < 42 && endBlockX < 42, "X blocks out of range. Haxor much? Wow!");
    require(startBlockY < 24 && endBlockY < 24, "Y blocks out of range. Haxor much? Wow!");

    uint blockWidth = endBlockX - startBlockX;
    uint blockHeight = endBlockY - startBlockY;
    uint blockArea = blockWidth * blockHeight;

    return blockWidth <= 7 && blockHeight <= 7 && blockArea <= 42;
  }

  function getBlockLocation(uint blockId) internal pure returns(uint, uint){
    return (blockId / 100, blockId % 100);
  }

}

