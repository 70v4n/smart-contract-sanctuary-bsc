pragma solidity ^0.8.0;

// import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";

import "./interfaces/IPancakeRouter02.sol";
// import "./Oracle.sol";

// import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "hardhat/console.sol";

contract YourBank is
    Initializable,
    OwnableUpgradeable,
    ReentrancyGuardUpgradeable
{
    using SafeMath for uint256;

    uint256 public constant MAX_BORROW = 7000; // 70%
    uint256 public constant LIQUIDATE_BPS = 500; // 5%
    uint256 public loanApplicationID;
    uint256 public borrowID;
    // Oracle public oracle;
    address public dev;
    address public router;

    struct LoanApplication {
        uint256 id;
        address owner;
        address loanToken;
        uint256 loanAmount;
        address[] availableCollateralTokens;
        uint256[] interestRates;
        uint256[] durations;
        bool openNewLoan;
    }

    struct Borrow {
        uint256 appID;
        address owner;
        address collateralToken;
        uint256 collateralAmount;
        uint256 borrowAmount;
        uint256 startBlock;
        uint256 interestRate;
        uint256 duration;
        uint256 dueDateBlock;
        uint256 lastPayBlock;
        bool open;
    }

    // loanApplicationID => LoanApplication
    mapping(uint256 => LoanApplication) public loanApplications;

    // owner => LoanApplication []
    mapping(address => LoanApplication[]) public myLoanApplications;

    // borrowID = > Loan
    mapping(uint256 => Borrow) public borrows;

    // borrower address
    mapping(address => Borrow[]) public myBorrows;

    event CreateLoanApp(address indexed owner, uint256 indexed loanID);

    function initialize(address _router) public initializer {
        router = _router;
        loanApplicationID = 0;
        borrowID = 0;
        dev = 0xD842B9c175169B8E0E7339f74660406C62DE43F1;
    }

    modifier transferToBank(address token, uint256 value) {
        if (token == address(0)) {
            require(msg.value == value, "coin amount!");
        } else {
            IERC20(token).transferFrom(
                address(msg.sender),
                address(this),
                value
            );
        }
        _;
    }

    function isDuplicatedLoanApplication(address _loanToken)
        public
        view
        returns (bool)
    {
        LoanApplication[] memory myApps = myLoanApplications[msg.sender];
        uint256 length = myApps.length;
        for (uint256 i = 0; i < length; i++) {
            if (myApps[i].loanToken == _loanToken) {
                return true;
            }
        }
        return false;
    }

    function createLoanApp(
        address _loanToken,
        uint256 _loanAmount,
        address[] calldata _availableCollateralTokens,
        uint256[] calldata _interestRates,
        uint256[] calldata _durations
    ) public payable transferToBank(_loanToken, _loanAmount) {
        require(
            !isDuplicatedLoanApplication(_loanToken),
            "createLoanApp: Duplicated"
        );

        require(_interestRates.length == _durations.length, "condition!");

        loanApplicationID++;
        uint256 id = loanApplicationID;

        LoanApplication memory app = LoanApplication(
            id,
            msg.sender,
            _loanToken,
            _loanAmount,
            _availableCollateralTokens,
            _interestRates,
            _durations,
            true
        );

        loanApplications[id] = app;
        myLoanApplications[msg.sender].push(app);

        emit CreateLoanApp(msg.sender, id);
    }

    function depositLoan(
        uint256 appID,
        address loanToken,
        uint256 amount
    ) public payable transferToBank(loanToken, amount) {
        LoanApplication storage app = loanApplications[appID];
        require(app.loanToken == loanToken, "invalid loan token");

        app.loanAmount = app.loanAmount.add(amount);
    }

    function withdrawLoan(uint256 appID, uint256 amount) public payable {
        LoanApplication storage app = loanApplications[appID];

        app.loanAmount = app.loanAmount.sub(amount);
        require(
            IERC20(app.loanToken).transfer(msg.sender, amount),
            "failed to transfer"
        );
    }

    function borrow(
        uint256 _appID,
        address _collateralToken,
        uint256 _collateralAmount,
        uint256 _borrowAmount,
        uint256 _interestRate,
        uint256 _duration
    )
        public
        payable
        transferToBank(_collateralToken, _collateralAmount)
        returns (uint256)
    {
        LoanApplication storage app = loanApplications[_appID];
        require(app.loanToken != address(0), "no loan app");

        bool valid = false;
        for (uint256 i = 0; i < app.interestRates.length; i++) {
            if (
                app.interestRates[i] == _interestRate &&
                app.durations[i] == _duration
            ) {
                valid = true;
                break;
            }
        }
        require(valid, "no condition!");

        valid = false;
        for (uint256 i = 0; i < app.availableCollateralTokens.length; i++) {
            if (app.availableCollateralTokens[i] == _collateralToken) {
                valid = true;
                break;
            }
        }
        require(valid, "no CollateralToken!");

        borrowID++;
        address[] memory path = new address[](2);
        path[0] = _collateralToken;
        path[1] = app.loanToken;
        uint256 collateralPrice = price(_collateralAmount, path);
        uint256 max = collateralPrice.mul(MAX_BORROW).div(10000);
        console.log("borrow max: ", max);
        require(_borrowAmount <= max, "borrow: max");

        Borrow memory b = Borrow(
            app.id,
            msg.sender,
            _collateralToken,
            _collateralAmount,
            _borrowAmount,
            block.number,
            _interestRate,
            _duration,
            block.number + _duration,
            block.number,
            true
        );

        borrows[borrowID] = b;
        myBorrows[msg.sender].push(b);

        app.loanAmount = app.loanAmount.sub(_borrowAmount);
        // fix my loan app later

        require(
            IERC20(app.loanToken).transfer(msg.sender, _borrowAmount),
            "failed to transfer"
        );

        return borrowID;
    }

    function interestAmount(uint256 bid) public view returns (uint256) {
        Borrow memory b = borrows[bid];

        if (block.number <= b.lastPayBlock) {
            return 0;
        }

        uint256 timePast = block.number.sub(b.lastPayBlock);
        uint256 totalInterest = b.borrowAmount.mul(b.interestRate).div(10000);
        uint256 currentInterest = totalInterest.mul(timePast).div(b.duration);

        return currentInterest;
    }

    function repay(
        uint256 bid,
        address loanToken,
        uint256 amount
    ) public {
        Borrow storage b = borrows[bid];
        LoanApplication memory app = loanApplications[b.appID];
        require(app.loanToken == loanToken, "invalid loan token");
        // console.log("repay block number2: ", block.number);
        uint256 interest = interestAmount(bid);

        uint256 principal = 0;
        if (amount > interest) {
            principal = amount.sub(interest);
        } else {
            principal = interest.sub(interest);
        }

        b.borrowAmount = b.borrowAmount.sub(principal);
        loanApplications[b.appID].loanAmount = app.loanAmount.add(amount);

        // TODO add history

        require(
            IERC20(loanToken).transferFrom(
                address(msg.sender),
                address(this),
                amount
            ),
            "fail to transfer"
        );
    }

    function availableCollateralTokens(uint256 id)
        public
        view
        returns (address[] memory)
    {
        return loanApplications[id].availableCollateralTokens;
    }

    function depositCollateral(
        uint256 bid,
        address collateralToken,
        uint256 amount
    ) public payable transferToBank(collateralToken, amount) {
        Borrow storage b = borrows[bid];
        b.collateralAmount = b.collateralAmount.add(amount);
    }

    function withdrawCollateral(uint256 bid, uint256 amount) public payable {
        // TODO validate health and liquidation
        Borrow storage b = borrows[bid];
        b.collateralAmount = b.collateralAmount.sub(amount);
        require(
            IERC20(b.collateralToken).transfer(msg.sender, amount),
            "failed to transfer"
        );
    }

    function health(uint256 bid) public view returns (uint256) {
        Borrow storage b = borrows[bid];
        LoanApplication memory app = loanApplications[b.appID];
        address[] memory path = new address[](2);
        path[0] = b.collateralToken;
        path[1] = app.loanToken;
        uint256 collateralPrice = price(b.collateralAmount, path);
        console.log("healthPositon collateralPrice: ", collateralPrice);
        uint256 max = collateralPrice.mul(MAX_BORROW).div(10000);

        console.log("healthPositon max: ", max);
        return max;
    }

    function liquidate(uint256 bid) public {
        Borrow storage b = borrows[bid];
        uint256 h = health(bid);
        if (h > 40 ether) {
            return;
        }

        uint256 interest = interestAmount(bid);
        uint256 toOwner = b.borrowAmount.add(interest);
        console.log("borrowAmount: ", b.borrowAmount);
        console.log("interest: ", interest);
        console.log("toOwner: ", toOwner);

        uint256 liquidatorPrize = toOwner.mul(LIQUIDATE_BPS).div(10000);
        uint256 devFees = toOwner.mul(LIQUIDATE_BPS).div(10000);
        uint256 prize = liquidatorPrize.add(devFees);

        uint256 total = toOwner.add(prize);
        console.log("total: ", total);
        // sell to loan token in total
        LoanApplication storage app = loanApplications[b.appID];
        address[] memory path = new address[](2);
        path[0] = b.collateralToken;
        path[1] = app.loanToken;

        // uint256 allow = IERC20(b.collateralToken).allowance(
        //     address(this),
        //     address(router)
        // );
        // if (allow < total) {
        //     IERC20(b.collateralToken).approve(router, type(uint256).max);
        //     console.log("approve");
        // }
        console.log(
            "collateralToken balance: ",
            IERC20(b.collateralToken).balanceOf(address(this))
        );
        console.log("collateralToken amount: ", b.collateralAmount);
        IERC20(b.collateralToken).approve(router, type(uint256).max);
        uint256 totalSwap = swapCollateralTokenToLoanToken(total, path);
        IERC20(b.collateralToken).approve(router, 0);
        console.log("totalSwap: ", totalSwap);
        app.loanAmount = app.loanAmount.add(totalSwap);
        // TODO add history
        if (totalSwap <= toOwner) {
            return;
        }

        liquidatorPrize = totalSwap.mul(LIQUIDATE_BPS).div(10000);
        devFees = totalSwap.mul(LIQUIDATE_BPS).div(10000);

        if (liquidatorPrize > 0) {
            IERC20(app.loanToken).transfer(
                address(msg.sender),
                liquidatorPrize
            );
        }

        if (devFees > 0) {
            IERC20(app.loanToken).transfer(dev, devFees);
        }

        b.open = false;
    }

    function price(uint256 amountIn, address[] memory path)
        public
        view
        returns (uint256)
    {
        uint256[] memory amounts = IPancakeRouter02(router).getAmountsOut(
            amountIn,
            path
        );
        return amounts[1];
    }

    function swapCollateralTokenToLoanToken(
        uint256 amountLoanOut,
        address[] memory path
    ) public returns (uint256) {
        console.log("amountLoanOut: ", amountLoanOut);
        uint256[] memory amountsIn = IPancakeRouter02(router).getAmountsIn(
            amountLoanOut,
            path
        );
        console.log("amountsIn: ", amountsIn[0]);

        uint256[] memory amounts = IPancakeRouter02(router)
            .swapTokensForExactTokens(
                amountLoanOut,
                amountsIn[0],
                path,
                address(this),
                block.timestamp + 60
            );
        return amounts[1];
    }
}

