import './interfaces/IBEP20.sol';
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Context.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "./interfaces/IBTCB.sol";
import "./libs/TransferHelper.sol";
import "./base/BaseDividends.sol";
import "./base/SafeBEP20.sol";

 contract dvdmaster is BaseDividends,Context,Ownable,ReentrancyGuard {
    

    uint256 private _totalShares;
    uint256 private _totalReleased;
    using TransferHelper for address;
    using SafeMath for uint256;
    // The precision factor
    uint256 constant public PRECISION_FACTOR = 2**128;

    mapping(address => uint256) private _shares;
    mapping(address => uint256) private _released;


    event SharesAdded(address account, uint256 shares);
    event SharesRemoved(address account, uint256 shares);
    event PaymentReceived(address from, uint256 amount);
    // Info of each user that stakes tokens (stakedToken)
    mapping(address => UserInfo) public userInfo;
    struct UserInfo {
        uint256 amount; // How many staked tokens the user has provided
        uint256 rewardDebt; // Reward debt
    }
    address public immutable btcb ;
    address public immutable dvifi ;



    /**
     * @dev Creates an instance of `PaymentSplitter` where each account in `payees` is assigned the number of shares at
     * the matching position in the `shares` array.
     *
     * All addresses in `payees` must be non-zero. Both arrays must have the same non-zero length, and there must be no
     * duplicates in `payees`.
     */
    constructor(address staketoken,address rewardtoken) payable 
    BaseDividends(shares,totalShares)
    {
        btcb = rewardtoken;
        dvifi = staketoken;
    }

    /**
     * @dev The Ether received will be logged with {PaymentReceived} events. Note that these events are not fully
     * reliable: it's possible for a contract to receive Ether without triggering this function. This only affects the
     * reliability of the events, and not the actual splitting of Ether.
     *
     * To learn more about this see the Solidity documentation for
     * https://solidity.readthedocs.io/en/latest/contracts.html#fallback-function[fallback
     * functions].
     */
    receive() external payable virtual {
        rewarddeposit();
    }

    /**
     * @dev Getter for the total shares held by payees.
     */
    function totalShares() public view returns (uint256) {
        return _totalShares;
    }

    /**
     * @dev Getter for the amount of shares held by an account.
     */
    function shares(address account) public view returns (uint256) {
        return _shares[account];
    }

    /**
     * @dev Add a new payee to the contract.
     * @param shares_ The number of shares owned by the payee.
     */
    function stakeshare(uint256 shares_) external nonReentrant   {
        UserInfo storage user = userInfo[msg.sender];

        dvifi.safeTransferFrom(address(msg.sender), address(this), shares_);

        user.amount.add(shares_);
        _totalShares.add(shares_);
        emit SharesAdded(address(msg.sender), shares_);
    }

    /**
     * @dev Add a new payee to the contract.
     * @param shares_ The number of shares owned by the payee.
     */
    function removeshare(uint256 shares_) external nonReentrant   {
        UserInfo storage user = userInfo[msg.sender];

        require(user.amount >= shares_, "withdraw: Amount to withdraw too high");
        
        dvifi.safeTransfer(address(msg.sender), shares_);
        user.amount.sub(shares_);
        _totalShares.sub(shares_);
        emit SharesRemoved(address(msg.sender), shares_);
    }

    function rewarddeposit() private  {
        IBTCB(btcb).deposit{value: msg.value}();
        emit PaymentReceived(_msgSender(), msg.value);

    }
    function rewardbalance() public  view returns (uint256) {
        return IBTCB(btcb).balanceOf(address(this));
    }

  function collect() external {
    uint256 amount = _prepareCollect(msg.sender);
    btcb.safeTransfer(msg.sender, amount);
  }

  function distribute() external  {
    uint256 amountDVD = IBTCB(btcb).balanceOf(address(this));
    _distributeDividends(amountDVD);
    _totalReleased.add(amountDVD);
  }  

}

