contract Swapper{
    using SafeERC20 for IERC20;
    using Address for address;
    uint256 public allowance;
    uint256 public bnbBalance;

    IUniswapV2Router02 uniswapV2Router = IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
    address uniswapRouter = 0x10ED43C718714eb63d5aA57B78B54704E256024E;

    mapping(address => uint256) balances;

    event SwapedTokensForBnb(uint256 TokenAmount);
    event SwapedBnbForTokens(uint256 TokenAmount);

    function getAllowance() public returns (uint256) {
        return allowance;
    }

    function swapBnbForTokens(uint256 amountOutMin, address tokenToBuy, address recipient) public payable {
        address[] memory path = new address[](2);
        bnbBalance = address(this).balance;
        require(msg.value > bnbBalance, "Not enough BNB");
        path[0] = uniswapV2Router.WETH();
        path[1] = tokenToBuy;
        //@dev Make the swap
        uniswapV2Router.swapExactETHForTokensSupportingFeeOnTransferTokens{value: msg.value}(
            amountOutMin, // accept any amount of ETH
            path,
            address(this),
            block.timestamp
        );
        emit SwapedBnbForTokens(amountOutMin);
    }

    function swapTokensForBnb(uint256 amount, uint256 amountOutMin, address tokenToSell, address ethRecipient) public {


        IERC20(tokenToSell).transferFrom(msg.sender, address(this), amount);
        IERC20(tokenToSell).approve(uniswapRouter, amount);

        allowance = IERC20(tokenToSell).allowance(address(this), uniswapRouter);

        require(IERC20(tokenToSell).allowance(address(this), uniswapRouter) >= amount, "Not enough allowance");

        address[] memory path = new address[](2);
        path[0] = tokenToSell;
        path[1] = uniswapV2Router.WETH();

        uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            amount,
            amountOutMin, // accept any amount of ETH
            path,
            address(this),
            block.timestamp
        );

        emit SwapedTokensForBnb(amount);
    }

    function checkHoneyPot(address _token , uint256 buyAmountOutMin, uint256 sellAmountIn, uint256 sellAmountMin, address recipient) public payable {
        swapBnbForTokens(buyAmountOutMin, _token, recipient);
        uint256 balance = IERC20(_token).balanceOf(recipient);
        require(balance >= buyAmountOutMin, "Not enough balance");
        swapTokensForBnb(sellAmountIn, sellAmountMin, _token, recipient);
    }

    function checkHoneyPotTest(address _token , uint256 buyAmountOutMin, uint256 sellAmountIn, uint256 sellAmountMin, address recipient) public payable returns(uint256) {
        swapBnbForTokens(buyAmountOutMin, _token, recipient);
        uint256 balance = IERC20(_token).balanceOf(recipient);
        return balance;
        //swapTokensForBnb(sellAmountIn, sellAmountMin, _token, recipient);
    }
    function deposit(uint _money) public payable {}

    function getOnesBalance(address addr) public view returns (uint){
        return balances[addr];
    }

    function getMyBalance() public view returns (uint){
        return balances[msg.sender];
    }
    function getContractBalance() public view returns (uint){
        return address(this).balance;
    }

    function getContractAddress() public view returns (address){
        return address(this);
    }

    function deposit() public payable {
        balances[msg.sender]+=msg.value;
    }

    receive() external payable {
        deposit();
    }
}