  //   characters[owner][nftId] = Characters.encode(Characters.upRank(character));
  //   return true;
  // }

  function getCharacter(address owner, uint256 id) public view returns (Characters.Character memory _character) {
     uint256 details= characters[owner][id];
    _character= Characters.getCharacter(details);
  }
  
}

