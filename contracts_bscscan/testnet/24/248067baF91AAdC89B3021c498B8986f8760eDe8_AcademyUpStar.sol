pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "./KOBToken.sol";
import "./CharacterNFT.sol";
import "./Characters.sol";

contract AcademyUpStar is Ownable,Initializable  {
    
    using Characters for Characters.Character;

    // The TOKEN to upgrade NFT
    KOBToken public kob;

    CharacterNFT public characterNFT;
    
    // star star exp
    // star D star 1 exp 1000
    // 1-1-1000
    // 1-D ,2-C,3-B,4-A,5-S,6-L
    mapping(uint256 => mapping(uint256 => uint256)) stars;

    function initialize(
        KOBToken _kob,
        CharacterNFT _characterNFT
    ) public initializer {
        kob = _kob;
        characterNFT = _characterNFT;
        // RANK D
        stars[1][1]=1080;
        stars[1][2]=3840;
        stars[1][3]=5460;
        stars[1][4]=7200;
        // RANK C
        stars[2][1]=9600;
        stars[2][2]=11040;
        stars[2][3]=14300;
        stars[2][4]=18460;
        // RANK B
        stars[3][1]=24700;
        stars[3][2]=28600;
        stars[3][3]=33800;
        stars[3][4]=39000;
        // RANK A
        stars[4][1]=52500;
        stars[4][2]=58800;
        stars[4][3]=68600;
        stars[4][4]=77000;
        // RANK S
        stars[4][1]=91000;
        stars[4][2]=105000;
        stars[4][3]=120000;
        stars[4][4]=136500;
        // RANK SS
        stars[5][1]=232500;
        stars[5][2]=270000;
        stars[5][3]=300000;
        stars[5][4]=480000;
        // RANK SSS
        stars[6][1]=1088000;
        stars[6][2]=1700000;
        stars[6][3]=3060000;
        stars[6][4]=4760000;
    }

    // modifier onlyAdmin() {
    //     require(msg.sender == adminAddress, "admin: wut?");
    //     _;
    // }

  function upStar(uint256 _kobUpStar, uint256 _nftId) public returns (bool) {
        Characters.Character memory character = characterNFT.getCharacter(msg.sender,_nftId);
         uint256 kobUpStar = stars[character.rank][character.star];
        require (_kobUpStar == kobUpStar, 'Up star not correct sender KOB');
        kob.approve(msg.sender, _kobUpStar);
        kob.transferFrom(msg.sender, address(this), _kobUpStar);
        characterNFT.upStar(msg.sender, _nftId);
        return true;
    }
}

