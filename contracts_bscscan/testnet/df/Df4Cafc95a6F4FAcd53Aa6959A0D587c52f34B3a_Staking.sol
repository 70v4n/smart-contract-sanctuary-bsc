/**  @title StakingBloqueado */
contract Staking is Ownable, Pausable, Vault {
    using Counters for Counters.Counter;
    using EnumerableSet for EnumerableSet.UintSet;
    using EnumerableMap for EnumerableMap.UintToAddressMap;
    using EnumerableSet for EnumerableSet.AddressSet;
    using SafeMath for uint256;

    uint256[] public rates;
    uint256 public totalStaked;
    address public scan = 0x01a5E3620f351975e235345F826f0Da6CFb079CD; //0xCCcE542413528CB57B5761e061F4683A1247aDCB
    address public rewardWallet;
    uint256 lockTime = 10 seconds;//30 days; //Cambiar
    constructor(
        uint256[] memory _rates,
        address _rewardWallet
    ) Vault(scan){
        rates = _rates;
        rewardWallet = _rewardWallet;
    }

    function getAPR(uint256 amount) public view returns(uint256){
        //Multiplicar cantidades por 1e9
        if(amount >= 1_000_000 * 1e18) {
            return rates[4];
        }
        else if(amount >= 500_000 * 1e18) {
            return rates[3];
        }
        else if(amount >= 50_000 * 1e18) {
            return rates[2];
        }
        else if(amount >= 10_000 * 1e18) {
            return rates[1];
        }
        else{
            return rates[0];
        }
    }

    function changeRewardWallet(address _wallet) external onlyOwner {
      rewardWallet = _wallet;
    }

    function changeRate(uint256 _rateIndex, uint256 _rate) external onlyOwner {
      rates[_rateIndex] = _rate;
    }

    function calculateAPR(uint256 amount) public view returns (uint256){
        return amount.div(100).mul(getAPR(amount));
    }
    function calculateRewards(uint256 amount) public view returns (uint256) {
        //Calcular el lock time
        return calculateAPR(amount).div(365).mul(30);
    }
    function stake(uint256 amount) external {
      require(IERC20(scan).balanceOf(rewardWallet) >= calculateRewards(amount), "Insufficient liquidity");
      IERC20(scan).transferFrom(msg.sender, address(this), amount);
      _createStake(amount);
    }
    function _createStake(uint256 amount) private {
      totalStaked += amount;
      _createVault(amount, lockTime);
    }

    function harvest(uint256[] memory vaultIDs) external {
      uint256 totalAmount = 0;
      for (uint256 i = 0; i < vaultIDs.length; i++) {
        totalAmount+= getVaultByID(vaultIDs[i]).amount;
        _closeVault(vaultIDs[i]);
      }
      totalStaked -= totalAmount;
      uint256 rewards = calculateRewards(totalAmount);
      IERC20(scan).transferFrom(rewardWallet, msg.sender, rewards);
      IERC20(scan).transfer(msg.sender, totalAmount);
    }

    function reStake(uint256[] memory vaultIDs) external {
      uint256 totalAmount = 0;
      for (uint256 i = 0; i < vaultIDs.length; i++) {
        totalAmount+= getVaultByID(vaultIDs[i]).amount;
        _closeVault(vaultIDs[i]);
      }
      uint256 rewards = calculateRewards(totalAmount);
      _createStake(totalAmount += rewards);
    }

    function exit(uint256[] memory vaultIDs) external {
      uint256 totalAmount = 0;
      for (uint256 i = 0; i < vaultIDs.length; i++) {
        totalAmount+= getVaultByID(vaultIDs[i]).amount;
        _closeVault(vaultIDs[i]);
      }
      uint256 fee = (Scan(scan)._taxFee().add(Scan(scan)._liquidityFee())).mul(2);
      totalStaked -= totalAmount;
      uint256 penalty = totalAmount.div(100).mul(fee);
      totalAmount-=penalty;
      IERC20(scan).transfer(msg.sender, penalty);
      IERC20(scan).transfer(msg.sender, totalAmount);
    }

    




}