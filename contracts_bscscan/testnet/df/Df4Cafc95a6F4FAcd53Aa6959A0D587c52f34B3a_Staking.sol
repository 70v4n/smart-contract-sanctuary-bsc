/**  @title Vault */
abstract contract Vault {
    using Counters for Counters.Counter;
    using EnumerableSet for EnumerableSet.UintSet;
    using SafeMath for uint256;

    struct Lock {
        uint256 amount;
        uint256 unlockTime;
    }
    address vaultAsset;
    constructor(address _vaultAsset) {
      vaultAsset = _vaultAsset;
    }
    
    mapping(uint256 => Lock) private _locks;
    mapping(address => EnumerableSet.UintSet) private _holderLocks;
    Counters.Counter private _lockIds;

    function _createVault(uint256 amount, uint256 unlockTime) internal virtual {
      _lockIds.increment();
      _locks[_lockIds.current()] = Lock(amount, unlockTime.add(block.timestamp));
      _holderLocks[msg.sender].add(_lockIds.current());
    }

    function _closeVault(uint256 vaultID) internal virtual {
      require(_holderLocks[msg.sender].contains(vaultID), "No vault");
      Lock memory lock = _locks[vaultID];
      require(lock.unlockTime <= block.timestamp,  "Unlock time: ");
      delete _locks[vaultID];
      _holderLocks[msg.sender].remove(vaultID);
    }

    function getHolderVaultAmount(address _holder) public view returns(uint256){
      return _holderLocks[_holder].length();
    }

    function getHolderVaultIDAtIndex(address _holder, uint256 _index) public view returns(uint256) {
      return _holderLocks[_holder].at(_index);
    }

    function getVaultByID(uint256 _vaultID) public view returns(Lock memory) {
      return _locks[_vaultID];
    }

    function getAllVaultsByHolder(address _holder) public view returns(uint256[] memory, uint256[] memory, uint256[] memory) {
      uint256[] memory ids = new uint256[](_holderLocks[_holder].length());
      uint256[] memory times = new uint256[](_holderLocks[_holder].length());
      uint256[] memory amounts = new uint256[](_holderLocks[_holder].length());
      for (uint256 i = 0; i < _holderLocks[_holder].length(); i++) {
        ids[i] = _holderLocks[_holder].at(i);
        times[i] = _locks[_holderLocks[_holder].at(i)].unlockTime;
        amounts[i] = _locks[_holderLocks[_holder].at(i)].amount;
      }
      return (ids, times, amounts);
    }
}
