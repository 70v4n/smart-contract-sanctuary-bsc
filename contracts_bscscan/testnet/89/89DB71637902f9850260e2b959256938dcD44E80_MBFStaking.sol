pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/utils/ContextUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableSetUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";

import "hardhat/console.sol";

import { ABDKMath64x64 as Math } from "abdk-libraries-solidity/ABDKMath64x64.sol";

contract MBFStaking is Initializable, ContextUpgradeable, OwnableUpgradeable, ReentrancyGuardUpgradeable {
    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.UintSet;

    struct UserStake {
        address account; // address of the user staking account
        uint256 steps; // number of steps staked
        uint256 stakeId; // staking id of the stake
        uint256 startDate; // start date of the stake
        uint256 startStep; // actual step when the stake started relative to when staking contract started
        uint256 unstakeDate; // unstake date of the stake, equals to 0 when stake still in progress
        uint256 unstakeStep; // actual step when the stake was unstaked relative to when staking contract started
        uint256 amount; // amount of the stake
        uint256 shares; // the amount of shares the stake is represented by
        uint256 lastClaimedRewardStep; // the last step when the stable reward was claimed
    }

    struct StableReward {
        uint256 totalShares; // total shares in the pool
        uint256 amount; // amount of the stable reward
    }

    struct MbfReward {
        uint256 totalShares; // total shares in the pool
        uint256 amount; // amount of the MBF reward
    }

    struct Settings {
        uint256 START_DATE; // when to start staking
        uint256 MIN_STAKE_STEPS; // 30
        uint256 MAX_STAKE_STEPS; // 3333
        uint256 LATE_PENALTY_RATE_PER_STEP; // 1
        uint256 MAX_LATE_STEPS; // 28
        uint256 STEP_DURATION; // 86400 = 1 day
        uint256 NEGATIVE_INTEREST_RATE; // 5%
    }

    // constants
    uint256 public constant BASE_POINT = 10000;
    uint256 public constant ONE_YEAR = 365;

    // settings
    Settings public SETTINGS;

    // tokens
    IERC20Upgradeable public mbfToken;
    IERC20Upgradeable public busd;

    // states
    uint256 public totalStakedAmount;
    uint256 public totalUnstakedAmount;

    uint256 public totalStakedShares;
    uint256 public totalUnstakedShares;

    uint256 public currentStakeId;

    uint256 public activeStakeCount;
    uint256 public totalActiveStakeDuration;

    uint256 public totalMBFRewards;
    uint256 public totalStableRewards;

    StableReward[] public stableRewards;
    MbfReward[] public mbfRewards;

    // account => LastStableRewardId
    mapping(address => uint256) public lastStableRewardClaimStepOf;

    // stakeId => UserStake
    mapping(uint256 => UserStake) public userStakeOf;

    // account => stakeId[]
    mapping(address => EnumerableSetUpgradeable.UintSet) private stakeIdsOf;

    // events
    event Stake(
        address indexed account,
        uint256 indexed stakeId,
        uint256 steps,
        uint256 indexed startDate,
        uint256 shares,
        uint256 amount,
        uint256 totalStakedAmount,
        uint256 totalUnstakedAmount,
        uint256 totalStakedShares,
        uint256 totalUnstakedShares
    );

    event Unstake(
        address indexed account,
        uint256 indexed stakeId,
        uint256 indexed unstakeDate,
        uint256 payoutMBF,
        uint256 penaltyRate,
        uint256 penaltyMBF,
        uint256 totalStakedAmount,
        uint256 totalUnstakedAmount,
        uint256 totalStakedShares,
        uint256 totalUnstakedShares
    );

    event UpdateSettings(bytes32 indexed setting, uint256 newValue, address indexed caller);

    function initialize(
        IERC20Upgradeable _mbfToken,
        IERC20Upgradeable _busd,
        uint256 _startDate,
        uint256 _stepDuration,
        uint256 _minStakeSteps,
        uint256 _maxStakeSteps,
        uint256 _latePenaltyRatePerStep,
        uint256 _maxLateSteps,
        uint256 _negativeInterestRate
    ) public initializer {
        mbfToken = _mbfToken;
        busd = _busd;

        require(_stepDuration > 0, "Duration of a step needs to be bigger then 0");

        SETTINGS.START_DATE = _startDate;
        SETTINGS.STEP_DURATION = _stepDuration;
        SETTINGS.MIN_STAKE_STEPS = _minStakeSteps;
        SETTINGS.MAX_STAKE_STEPS = _maxStakeSteps;
        SETTINGS.LATE_PENALTY_RATE_PER_STEP = _latePenaltyRatePerStep;
        SETTINGS.MAX_LATE_STEPS = _maxLateSteps;
        SETTINGS.NEGATIVE_INTEREST_RATE = _negativeInterestRate;
    }

    /** All external only owner functions **/
    function setStartDate(uint256 _date) external onlyOwner {
        SETTINGS.START_DATE = _date;
        emit UpdateSettings("START_DATE", _date, msg.sender);
    }

    function setMinStakeSteps(uint256 _steps) external onlyOwner {
        SETTINGS.MIN_STAKE_STEPS = _steps;
        emit UpdateSettings("MIN_STAKE_STEPS", _steps, msg.sender);
    }

    function setMaxStakeSteps(uint256 _steps) external onlyOwner {
        SETTINGS.MAX_STAKE_STEPS = _steps;
        emit UpdateSettings("MAX_STAKE_STEPS", _steps, msg.sender);
    }

    function setLatePenaltyRatePerStep(uint256 _rate) external onlyOwner {
        SETTINGS.LATE_PENALTY_RATE_PER_STEP = _rate;
        emit UpdateSettings("LATE_PENALTY_RATE_PER_STEP", _rate, msg.sender);
    }

    function setMaxLateSteps(uint256 _steps) external onlyOwner {
        SETTINGS.MAX_LATE_STEPS = _steps;
        emit UpdateSettings("MAX_LATE_STEPS", _steps, msg.sender);
    }

    /** All external functions **/
    function chargeStableReward(uint256 _amount) external {
        require(_amount > 0, "MBFStaking[chargeStableReward]: _busdAmount <= 0");
        require(totalStakedShares > 0, "MBFStaking[chargeStableReward]: totalStakedShares <= 0");
        require(busd.transferFrom(msg.sender, address(this), _amount), "Token transfer failed");

        // Add missing values to the array
        _completeMissingStableRewardsSteps();

        // Add new stable reward
        StableReward storage currentStableReward = stableRewards[this.getCurrentStep()];
        currentStableReward.totalShares = totalStakedShares;
        currentStableReward.amount += _amount;

        // Total stable rewards
        totalStableRewards += _amount;
    }

    function chargeMbfReward(uint256 _amount) external {
        require(_amount > 0, "MBFStaking[chargeMbfReward]: _mbfAmount <= 0");
        require(totalStakedShares > 0, "MBFStaking[chargeMbfReward]: totalStakedShares <= 0");
        require(mbfToken.transferFrom(msg.sender, address(this), _amount), "Token transfer failed");

        // Add missing values to the array
        _completeMissingRewardSteps();

        // Add new mbf reward
        MbfReward storage currentMbfReward = mbfRewards[this.getCurrentStep()];
        currentMbfReward.totalShares = totalStakedShares;
        currentMbfReward.amount += _amount;

        // Total MBF rewards
        totalMBFRewards += _amount;
    }

    function claimStableReward() external nonReentrant {
        require(stableRewards.length > 0, "MBFStaking[claimStableReward]: stableRewards.length <= 0");

        uint256 rewardAmount = getClaimableReward(msg.sender);

        if (rewardAmount > 0) {
            require(busd.transfer(msg.sender, rewardAmount), "Token transfer failed");
            totalStableRewards -= rewardAmount;
        }

        // If transfer succeeded update last claimed step for every stake
        _updateStableRewardClaimStep(msg.sender);
    }

    function stake(uint256 _stakeSteps, uint256 _amount) external nonReentrant {
        // Stake amount should be higher than 0
        require(_amount > 0, "MBFStaking[stake]: _amount <= 0");
        // Stake length should be higher or equal to min_stake_steps
        require(_stakeSteps >= SETTINGS.MIN_STAKE_STEPS, "MBFStaking[stake]: _stakeSteps < SETTINGS.MIN_STAKE_STEPS");
        // Stake length should be lower or equal to max_stake_steps
        require(_stakeSteps <= SETTINGS.MAX_STAKE_STEPS, "MBFStaking[stake]: _stakeSteps > SETTINGS.MAX_STAKE_STEPS");
        // Stake start timstamp should be higher or equal to the timestamp staking launched
        require(block.timestamp >= SETTINGS.START_DATE, "MBFStaking[stake]: block.timestamp < SETTINGS.START_DATE");
        // Stake tokens should have been transfered to the contract
        require(mbfToken.transferFrom(msg.sender, address(this), _amount), "Token transfer failed");

        address userAccount = msg.sender;
        uint256 shares = getShares(_stakeSteps, _amount);
        uint256 startDate = block.timestamp;
        uint256 currentStep = _getCurrentStep(startDate);

        // state - update
        userStakeOf[currentStakeId] = UserStake({
            account: userAccount,
            steps: _stakeSteps,
            stakeId: currentStakeId,
            startDate: startDate,
            startStep: currentStep,
            unstakeDate: 0,
            unstakeStep: 0,
            amount: _amount,
            shares: shares,
            lastClaimedRewardStep: currentStep // Only alow claiming stable rewards from the next day you stake
        });
        stakeIdsOf[userAccount].add(currentStakeId);

        totalStakedAmount = totalStakedAmount + _amount;
        totalStakedShares = totalStakedShares + shares;

        currentStakeId++;
        activeStakeCount++;

        // The amount of shares is changed on the reward pool
        _completeMissingRewardSteps();

        totalActiveStakeDuration = totalActiveStakeDuration + _stakeSteps * SETTINGS.STEP_DURATION;

        // [event]
        emit Stake(
            userAccount,
            currentStakeId,
            _stakeSteps,
            startDate,
            shares,
            _amount,
            totalStakedAmount,
            totalUnstakedAmount,
            totalStakedShares,
            totalUnstakedShares
        );
    }

    function unstake(uint256 stakeId) external nonReentrant {
        UserStake storage userStake = userStakeOf[stakeId];
        address userAccount = msg.sender;

        require(userStake.account == userAccount, "MBFStaking[unstake]: userStake.account != userAccount");
        require(userStake.unstakeDate == 0, "MBFStaking[unstake]: userStake.unstakeDate != 0");

        // Get stake steps
        uint256 actualStakeSteps = getActualSteps(userStake.startDate);

        (bool isLatePenalty, uint256 penaltyRate) = _getPenaltyTypeAndRate(actualStakeSteps, userStake.steps);

        uint256 rateMBFToGetBack = uint256(1e18) - penaltyRate;

        _completeMissingRewardSteps();

        // Get principal
        uint256 principalMBF = userStake.amount;
        // Interest
        uint256 interest = getInterest(stakeId);

        uint256 payoutMBF;
        uint256 penaltyMBF;

        if (isLatePenalty) {
            // Late penalty
            // Get interest to payout
            uint256 payoutInterest = (interest * rateMBFToGetBack) / 1e18;
            // Payout amount of MBF (We need to divide by 1e18 because our rate is in wei.)
            payoutMBF = principalMBF + payoutInterest;
            // Penalty amount in MBF
            penaltyMBF = interest - payoutInterest;
        } else {
            // Early penalty
            // Get total amount to payout
            uint256 fullMBF = principalMBF + interest;
            // Payout amount of MBF (We need to divide by 1e18 because our rate is in wei.)
            payoutMBF = (fullMBF * rateMBFToGetBack) / 1e18;
            // Penalty amount in MBF
            penaltyMBF = fullMBF - payoutMBF;
        }

        // state - update
        uint256 unstakeDate = block.timestamp;
        userStake.unstakeDate = unstakeDate;

        // Unstaked stats
        totalUnstakedShares = totalUnstakedShares + userStake.shares;
        totalUnstakedAmount = totalUnstakedAmount + userStake.amount;

        // Staked stats
        totalStakedShares = totalStakedShares - userStake.shares;
        totalStakedAmount = totalStakedAmount - userStake.amount;

        activeStakeCount--;
        totalActiveStakeDuration = totalActiveStakeDuration - userStake.steps * SETTINGS.STEP_DURATION;

        if (payoutMBF > 0) {
            require(mbfToken.transfer(msg.sender, payoutMBF), "MBFStaking[unstake]: Token transfer failed");
        }

        if (penaltyMBF > 0) {
            // The penalty amount is added to the reward pool
            MbfReward storage currentCharge = mbfRewards[_getCurrentStep(unstakeDate)];
            currentCharge.totalShares = totalStakedShares;
            currentCharge.amount += penaltyMBF;

            if (penaltyMBF > interest) {
                totalMBFRewards += penaltyMBF - interest;
            } else {
                totalMBFRewards -= (interest - penaltyMBF);
            }
        } else {
            totalMBFRewards -= interest;
        }

        // [event]
        emit Unstake(
            userAccount,
            userStake.stakeId,
            unstakeDate,
            payoutMBF,
            penaltyRate,
            penaltyMBF,
            totalStakedAmount,
            totalUnstakedAmount,
            totalStakedShares,
            totalUnstakedShares
        );
    }

    /** Public view functions **/
    function getPenaltyRate(uint256 _startDate, uint256 expectedSteps) public view returns (uint256) {
        uint256 actualSteps = getActualSteps(_startDate);

        (, uint256 penaltyRate) = _getPenaltyTypeAndRate(actualSteps, expectedSteps);

        return penaltyRate;
    }

    function getActualSteps(uint256 _startDate) public view returns (uint256) {
        return _getActualSteps(block.timestamp, _startDate);
    }

    function getLPDMultiplier(uint256 _steps) public pure returns (uint256) {
        return (1 * BASE_POINT + ((_steps * BASE_POINT) / ONE_YEAR));
    }

    function getShareRate(uint256 _startDate) public view returns (uint256) {
        if (SETTINGS.START_DATE > _startDate) {
            return 0;
        }

        // Calculate the amount of steps
        uint256 steps = (_startDate - SETTINGS.START_DATE) / SETTINGS.STEP_DURATION;

        // declaring 64.64 float for everything
        int128 yearFloat = Math.fromUInt(ONE_YEAR);
        int128 dayFloat = Math.fromUInt(steps);
        int128 negativeInterestRate = Math.fromUInt(SETTINGS.NEGATIVE_INTEREST_RATE);
        int128 percFloat = Math.div(negativeInterestRate, Math.fromInt(100));

        // log2(1-percentage/100)
        int128 x = Math.log_2(Math.sub(Math.fromInt(1), percFloat));

        // days/365
        int128 y = Math.div(dayFloat, yearFloat);

        // 2^(days/365*log2(1-percentage/100)
        int128 result = Math.exp_2(Math.mul(y, x));

        // Convert result from a 64.64 float back to a uint256 using 4 digits
        return Math.mulu(result, BASE_POINT);
    }

    function getShares(uint256 _stakeSteps, uint256 _amount) public view returns (uint256) {
        return (_amount * getLPDMultiplier(_stakeSteps) * getShareRate(block.timestamp)) / BASE_POINT / BASE_POINT;
    }

    function getClaimableReward(address _account) public view returns (uint256) {
        uint256 rewardAmount = 0;

        for (uint256 i = 0; i < stakeIdsOf[_account].length(); i++) {
            uint256 stakeId = stakeIdsOf[_account].at(i);
            UserStake memory userStake = userStakeOf[stakeId];

            // Stake not yet unstaked?
            if (userStake.unstakeDate == 0) {
                uint256 stakeEndStep = userStake.startStep + userStake.steps;

                // Loop over the rewards untill the end of the stake
                for (uint256 stakeStep = userStake.lastClaimedRewardStep; stakeStep <= stakeEndStep; stakeStep++) {
                    // Exit the loop if no more rewards are available
                    // (Current step isn't claimable as it's not yet immutable)
                    if (stakeStep >= stableRewards.length || stakeStep == this.getCurrentStep()) {
                        break;
                    }

                    rewardAmount +=
                        (stableRewards[stakeStep].amount * userStake.shares) /
                        stableRewards[stakeStep].totalShares;
                }
            }
        }

        return rewardAmount;
    }

    function getRealTimeStableReward(address _account) public view returns (uint256) {
        uint256 rewardAmount = 0;
        uint256 currentStep = this.getCurrentStep();

        for (uint256 i = 0; i < stakeIdsOf[_account].length(); i++) {
            uint256 stakeId = stakeIdsOf[_account].at(i);
            UserStake memory userStake = userStakeOf[stakeId];

            uint256 stakeEndStep = userStake.startStep + userStake.steps;
            // Stake not yet unstaked or ended and do we have rewards for the current step?
            if (userStake.unstakeDate == 0 && currentStep <= stakeEndStep && currentStep == stableRewards.length - 1) {
                rewardAmount +=
                    (stableRewards[currentStep].amount * userStake.shares) /
                    stableRewards[currentStep].totalShares;
            }
        }

        return rewardAmount;
    }

    function getInterest(uint256 stakeId) public view returns (uint256) {
        uint256 rewardAmount = 0;

        UserStake memory userStake = userStakeOf[stakeId];

        // Stake not yet unstaked?
        if (userStake.unstakeDate == 0) {
            uint256 stakeEndStep = userStake.startStep + userStake.steps;

            // Loop over the rewards untill the end of the stake
            for (uint256 j = userStake.startStep; j < stakeEndStep; j++) {
                // Exit the loop if no more rewards are available
                if (j >= mbfRewards.length) {
                    break;
                }
                rewardAmount += (mbfRewards[j].amount * userStake.shares) / mbfRewards[j].totalShares;
            }
        }

        return rewardAmount;
    }

    /** External view functions **/
    function getAverageStakingDuration() external view returns (uint256) {
        if (activeStakeCount > 0) {
            return totalActiveStakeDuration / activeStakeCount;
        }

        return 0;
    }

    function getUserStakesCount(address _account) external view returns (uint256) {
        return stakeIdsOf[_account].length();
    }

    function getUserStakeId(address _account, uint256 idx) external view returns (uint256) {
        return stakeIdsOf[_account].at(idx);
    }

    function getCurrentStep() external view returns (uint256) {
        return _getActualSteps(block.timestamp, SETTINGS.START_DATE);
    }

    /** Internal functions **/
    function _completeMissingRewardSteps() internal {
        for (uint256 i = mbfRewards.length; i <= this.getCurrentStep(); i++) {
            mbfRewards.push(MbfReward({ totalShares: totalStakedShares, amount: 0 }));
        }
    }

    function _completeMissingStableRewardsSteps() internal {
        for (uint256 i = stableRewards.length; i <= this.getCurrentStep(); i++) {
            stableRewards.push(StableReward({ totalShares: totalStakedShares, amount: 0 }));
        }
    }

    function _updateStableRewardClaimStep(address _account) internal {
        for (uint256 i = 0; i < stakeIdsOf[_account].length(); i++) {
            uint256 stakeId = stakeIdsOf[_account].at(i);
            UserStake storage userStake = userStakeOf[stakeId];

            // Stake not yet unstaked?
            if (userStake.unstakeDate == 0) {
                uint256 stakeEndStep = userStake.startStep + userStake.steps;

                // Loop over the rewards untill the end of the stake
                for (uint256 stakeStep = userStake.lastClaimedRewardStep; stakeStep < stakeEndStep; stakeStep++) {
                    // Exit the loop if no more rewards are available
                    // (Current step isn't claimable as it's not yet immutable)
                    if (stakeStep >= stableRewards.length) {
                        userStake.lastClaimedRewardStep = stakeStep;
                        break;
                    }
                }
            }
        }
    }

    /*
     * 100% = 1e18
     * Example calc:
     * 28 days late = 0% penalty
     * 58 days late => (58 days - 28days) => 30 * 1e16 / 1 => 3e17 => 30%
     * 118 days late => (118 days - 28days) => 90 * 1e16 / 1 => 9e17 => 90%
     * 1% penalty / day after 28 days
     *
     * Returns the penalty type and the rate
     * Early penalty = false
     * No penalty = false
     * Late penalty = true
     *
     */
    function _getPenaltyTypeAndRate(uint256 actualSteps, uint256 expectedSteps) internal view returns (bool, uint256) {
        // no penalty
        if (actualSteps == expectedSteps) {
            return (false, 0);
        }

        // early penalty
        if (actualSteps < expectedSteps) {
            return (false, ((expectedSteps - actualSteps) * 1e18) / expectedSteps);
        }

        // late penalty
        uint256 lateSteps = actualSteps - expectedSteps;

        if (lateSteps <= SETTINGS.MAX_LATE_STEPS) {
            return (true, 0);
        }
        uint256 penaltyLateSteps = lateSteps - SETTINGS.MAX_LATE_STEPS;

        uint256 latePenalty = (penaltyLateSteps * 1e16) / SETTINGS.LATE_PENALTY_RATE_PER_STEP;

        // Prevent overflow
        if (latePenalty > 1e18) {
            return (true, 1e18);
        }

        return (true, latePenalty);
    }

    function _getActualSteps(uint256 _currentTimestamp, uint256 _startDate) internal view returns (uint256) {
        return (_currentTimestamp - _startDate) / SETTINGS.STEP_DURATION;
    }

    function _getCurrentStep(uint256 _currentTimestamp) internal view returns (uint256) {
        return _getActualSteps(_currentTimestamp, SETTINGS.START_DATE);
    }
}

