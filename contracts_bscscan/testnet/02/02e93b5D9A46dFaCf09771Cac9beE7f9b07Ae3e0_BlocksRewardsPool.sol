import "hardhat/console.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

contract BlocksRewardsPool is Ownable {

    // Info of each user.
    struct UserInfo {
        uint256 amount;     // How many block.areas or LP tokens user provided.
        uint256 rewardDebt; // What user is not eligible for plus already harvested
        uint256 pendingRewards; // Rewards assigned, but not yet claimed
        uint256 lastRewardCalculatedBlock; // When pending rewards were last calculated for user
    }

    struct SpaceInfo {
        // mapping(address => uint256) amountOfBlocksOwnedBy; // owners of blocks (with amounts) so we know how to calculate rewards
        uint256 spaceId;
        uint256 amountOfBlocksBought; // Number of all blocks bought on this space
        address contractAddress;           // Address of space contract.
        uint256 lastRewardBlock;
        uint256 blsPerBlockAreaPerBlock; // We will start with 830000 Gwei -> 830000000000000 wei (which is approx 24 BLS/1 block.area / day)
        uint256 accBlsPerBlockArea;
    }

    SpaceInfo[] public spaceInfo;
    mapping(uint256 => mapping(address => UserInfo)) public userInfo;
    mapping(address => bool) public spacesByAddress;
    uint256 public allAllocationBlocks; // Amount of all blocks from all spaces that are getting BLS rewards

    event SpaceAdded(uint256 indexed spaceId, address indexed space, address indexed addedBy);

    modifier onlySpace() {
        require(spacesByAddress[msg.sender] == true, "Not a space.");
        _;
    }

    // constructor(){}

    function spacesLength() external view returns (uint256) {
        return spaceInfo.length;
    }

    function addSpace(address _spaceContract, uint256 _blsPerBlockAreaPerBlock) public { // TODO: only owner
        spacesByAddress[_spaceContract] = true;
        uint256 spaceId = spaceInfo.length;
        SpaceInfo storage newSpace = spaceInfo.push();
        newSpace.contractAddress = _spaceContract;
        newSpace.spaceId = spaceId;
        newSpace.blsPerBlockAreaPerBlock = _blsPerBlockAreaPerBlock;
        emit SpaceAdded(spaceId, _spaceContract, msg.sender);
    }

    function pendingBlsTokens(uint256 _spaceId, address _user) public view returns(uint256){
        SpaceInfo storage space = spaceInfo[_spaceId];
        UserInfo storage user = userInfo[_spaceId][_user];
        uint256 accBlsPerBlockArea = space.accBlsPerBlockArea;
        console.log("accBlsPerBlockArea1", accBlsPerBlockArea);
        console.log("block.number", block.number);
        console.log("space.lastRewardBlock", space.lastRewardBlock);
        if (block.number > space.lastRewardBlock && space.amountOfBlocksBought != 0) {
            uint256 multiplier = block.number - space.lastRewardBlock;
            console.log("Multiplier", multiplier);
            uint256 blsRewards = multiplier * space.blsPerBlockAreaPerBlock;
            accBlsPerBlockArea = accBlsPerBlockArea + (blsRewards * 1e12);
            console.log("accBlsPerBlockArea", accBlsPerBlockArea);
        }
        return user.amount * accBlsPerBlockArea / 1e12 - user.rewardDebt;
    }

    function updateSpace(uint256 _spaceId) internal {
        SpaceInfo storage space = spaceInfo[_spaceId];
        if (block.number <= space.lastRewardBlock) {
            return;
        }

        uint256 multiplier = block.number - space.lastRewardBlock;
        uint256 blsRewards = multiplier * space.blsPerBlockAreaPerBlock;
        space.accBlsPerBlockArea = space.accBlsPerBlockArea + (blsRewards * 1e12);
        space.lastRewardBlock = block.number;
    }

    function blockAreaBoughtOnSpace(uint256 _spaceId, address _buyer, uint256 _numberOfBlocks, uint256[] calldata _blocksBought, address[] calldata _previousBlockOwners) public payable onlySpace {
    
        SpaceInfo storage space = spaceInfo[_spaceId];
        UserInfo storage user = userInfo[_spaceId][_buyer];
        updateSpace(_spaceId);
        user.amount += _numberOfBlocks;
        user.rewardDebt = user.amount * space.accBlsPerBlockArea / 1e12;
        //remove blocks from previous owners that this guy took over. Max 42 loops
        uint256 amountOfBlocksToRemove;
        for(uint256 i = 0; i < _blocksBought.length; i++){
            if(_previousBlockOwners[i] == address(0x0))
                continue; // We shall not care about 0x0 address

            if(i < _numberOfBlocks){
                userInfo[_spaceId][_previousBlockOwners[i]].amount--;
                amountOfBlocksToRemove++;
            }else{
                break; // If we already reached full array we stop
            }
        }
        if(amountOfBlocksToRemove < _numberOfBlocks){
            // Then amount of blocks on space changed, so lets update state
            spaceInfo[_spaceId].amountOfBlocksBought = spaceInfo[_spaceId].amountOfBlocksBought - amountOfBlocksToRemove + _numberOfBlocks;
            allAllocationBlocks += _numberOfBlocks - amountOfBlocksToRemove;
        }

        // At this point we have all stuff to calculate rewards. We have how many blocks does a specific user (address has). And we know how many are all of blocks in space bought.
        console.log("Money received:", msg.value);
    }

    // TODO: This shall be removed after testnet. If you see this, contact devs immediately
    function withdrawTestOnly() external onlyOwner{
        uint contractBalance = address(this).balance;
        (bool success, ) = msg.sender.call{value: contractBalance}("");
        require(success, "Transfer failed.");
    }

    // function claim(uint256 _spaceId) public {
    //     SpaceInfo storage space = spaceInfo[_spaceId];
    //     UserInfo storage user = userInfo[_spaceId][msg.sender];
    //     updateSpace(_spaceId);

    //     uint256 pending = user.amount * space.accBlsPerBlockArea / 1e12 - user.rewardDebt;
    //     if (pending > 0 || user.pendingRewards > 0) {
    //         user.pendingRewards = user.pendingRewards + pending;
    //         uint256 claimedAmount = safeBlsTransfer(msg.sender, user.pendingRewards, pid);
    //         emit Claim(msg.sender, pid, claimedAmount);
    //         user.pendingRewards = user.pendingRewards.sub(claimedAmount);
    //         user.lastClaim = block.timestamp;
    //         pool.rewardsAmount = pool.rewardsAmount.sub(claimedAmount);
    //     }
    //     user.rewardDebt = user.amount.mul(pool.accWaultXPerShare).div(1e12);
    // }

    // // Safe eleven transfer function, just in case if rounding error causes pool to not have enough ELEVENs.
    // function safeBlsTransfer(address _to, uint256 _amount) internal {
    //     uint256 elevenBal = eleven.balanceOf(address(this));
    //     if (_amount > elevenBal) {
    //         eleven.transfer(_to, elevenBal);
    //     } else {
    //         eleven.transfer(_to, _amount);
    //     }
    // }
    // uint256 accBlsPerBlockArea = space.accBlsPerBlockArea;
    // if (block.number > space.lastRewardBlock && space.amountOfBlocksBought != 0) {
    //     uint256 multiplier = block.number - space.lastRewardBlock;
    //     console.log("Multiplier", multiplier);
    //     uint256 blsRewards = multiplier * space.blsPerBlockAreaPerBlock;
    //     accBlsPerBlockArea = accBlsPerBlockArea + (blsRewards * 1e12);
    //     console.log("accBlsPerBlockArea", accBlsPerBlockArea);
    // }
    // return user.amount * accBlsPerBlockArea / 1e12 - user.rewardDebt;
}

