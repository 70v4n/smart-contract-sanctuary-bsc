    mapping(address => uint256) private _tOwned;

    uint256 private constant MAX = ~uint256(0);
    uint256 internal constant _tTotal = 299792458000000;

    uint256 private _rTotal = (MAX - (MAX % _tTotal));
    uint256 private _tFeeTotal;

    uint256 private _rewardPercentage = 5;
    uint256 private _previousRewardPercentage = _rewardPercentage;

    mapping(address => uint256) private previousSale;

    uint256 private _maxTxAmount = _tTotal / 200;
    uint256 private dumpThreshold = ((_tTotal) * (5)) / (10**3);

    constructor(address _newOwner, address dexAddr) public override {
        transferOwnership(_newOwner);

        _rOwned[owner()] = _rTotal;
        emit Transfer(address(0), owner(), _tTotal);

        setupUni(dexAddr);
    }

    function totalSupply() public view override returns (uint256) {
        return _tTotal;
    }

    function balanceOf(address account) public override view returns (uint256) {
        if (_isExcluded[account]) return _tOwned[account];
        return tokenFromReflection(_rOwned[account]);
    }

    function allowance(address tokenOwner, address spender) external override view returns (uint256) {
        return _allowances[tokenOwner][spender];
    }

    function increaseAllowance(address spender, uint256 addedValue) external virtual returns (bool) {
