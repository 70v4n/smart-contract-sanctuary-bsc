        unchecked {
            _approve(sender, _msgSender(), currentAllowance - amount);
        }
        return true;
    }
    


    //base transfer function
    function _transfer(address sender, address recipient, uint256 amount) internal virtual {
        require(sender != address(0));
        require(recipient != address(0));

        uint256 senderBalance = _balances[sender];
        require(senderBalance >= amount);
        
        unchecked {
            _balances[sender] = senderBalance - amount;            
        }

        _balances[recipient] += amount;
        
        emit Transfer(sender, recipient, amount);
        _afterTokenTransfer(sender, recipient, amount);        
        
        }
        
//----------------------End Of Transfer Functions-------------------//



//---------------------------NFT Contract Intergration------------------------//
    
    //set NFT contract address
    function setNFTAddress(address _NFTAddress) public onlyOwner {
       nftAddress = _NFTAddress;
    }    
    
    //convert token ID to value 
    function tokenRarityToValue(uint256 tokenRarity) internal pure returns(uint){
        
        uint randomNFTValue;
        
        if(tokenRarity == 0){
            randomNFTValue = 1;
        } else if (tokenRarity == 1){
            randomNFTValue = 2;
        } else if (tokenRarity == 2){
            randomNFTValue = 4;
        } else if (tokenRarity == 3){
            randomNFTValue = 8;
        } else if (tokenRarity == 4){
            randomNFTValue = 16;
        }
        return(randomNFTValue);
    }    

//----------------------End Of NFT Contract Intergration-------------------//

    


//----------------------Staking Functions-------------------//

    function originalOwner(uint256 _tokenURI) public view returns(address){
        return(_originalOwner[_tokenURI]);
    }

    function addNFTToUserPool(uint256 _tokenURI) internal {
        require(!_isStaked[_tokenURI]);

        if(_nextNFTPool[_msgSender()][GUARD] ==0){
            _nextNFTPool[_msgSender()][GUARD] = GUARD;
        }

        _nextNFTPool[_msgSender()][_tokenURI] = _nextNFTPool[_msgSender()][GUARD];

        _nextNFTPool[_msgSender()][GUARD] = _tokenURI;
        
        _isStaked[_tokenURI] = true;

        _poolNumTokens[_msgSender()] = _poolNumTokens[_msgSender()] + 1;
        numTokensStaked++;

        uint256 NFTValue = tokenRarityToValue(IMoneyTreeNFT(nftAddress)._rarity(_tokenURI));

        _poolTokenAmount[_msgSender()] = _poolTokenAmount[_msgSender()] + NFTValue;
        totalAmountStaked = totalAmountStaked + NFTValue;

    }
    
    function removeNFTFromPool(uint256 _tokenURI) internal {

        require(_isStaked[_tokenURI]);
        
        uint256 prevNFT = getPreviouNFTInPool(_tokenURI);

        _nextNFTPool[_msgSender()][prevNFT] = _nextNFTPool[_msgSender()][_tokenURI];

        _nextNFTPool[_msgSender()][_tokenURI] = 0;

        _isStaked[_tokenURI] = false;

        _poolNumTokens[_msgSender()] = _poolNumTokens[_msgSender()] -1;
        numTokensStaked--;

        uint256 NFTValue = tokenRarityToValue(IMoneyTreeNFT(nftAddress)._rarity(_tokenURI));

        _poolTokenAmount[_msgSender()] = _poolTokenAmount[_msgSender()] - NFTValue;
        totalAmountStaked = totalAmountStaked - NFTValue;        

    }
    
    
    function getPreviouNFTInPool(uint256 _tokenURI) internal view returns(uint256){

        uint256 currentNFT = GUARD;

        while(_nextNFTPool[_msgSender()][currentNFT] != GUARD){
            if(_nextNFTPool[_msgSender()][currentNFT] == _tokenURI){
                return(_tokenURI);
            }
            currentNFT = _nextNFTPool[_msgSender()][currentNFT];
        }
        return(0);
    }

    function getNFTsInPool(address _owner) public view returns (uint256[] memory){

        uint256[] memory nftsInPool = new uint256[](_poolNumTokens[_owner]);

        uint256 currentNFT = _nextNFTPool[_owner][GUARD];

        for(uint256 i=0; currentNFT != GUARD; ++i){
            nftsInPool[i] = currentNFT;
            currentNFT = _nextNFTPool[_owner][currentNFT];
        }
        return(nftsInPool);
    }

    
    function stakeNFT(uint256[] memory _tokenURIList) public {

        uint256 userOwedTokens;

        for (uint256 i = 0; i < _tokenURIList.length; i++) {

            uint256 currentToken = _tokenURIList[i];
            
            require(_msgSender() == IMoneyTreeNFT(nftAddress).ownerOf(currentToken));
            
            _originalOwner[currentToken] = IMoneyTreeNFT(nftAddress).ownerOf(currentToken);

            _NFTUnlockTime[currentToken] = block.timestamp + 120; 

            IMoneyTreeNFT(nftAddress).transferFrom(_msgSender(), address(this), currentToken);

            addNFTToUserPool(currentToken);

            uint256 NFTValue = tokenRarityToValue(IMoneyTreeNFT(nftAddress)._rarity(currentToken));

            userOwedTokens = userOwedTokens + NFTValue;

        }

        _mint(_msgSender(), userOwedTokens * 10 ** decimals());

    }

    function unstakeNFT(uint256[] memory _tokenURIList) public {

        uint256 userTokensToPay;

        for (uint256 i = 0; i < _tokenURIList.length; i++) {

            uint256 currentToken = _tokenURIList[i];
            
            require(_msgSender() == _originalOwner[currentToken]);
            
            _originalOwner[currentToken] = address(0);

            require(_NFTUnlockTime[currentToken] <= block.timestamp);

            IMoneyTreeNFT(nftAddress).transferFrom(address(this), _msgSender(), currentToken);

            removeNFTFromPool(currentToken);

            uint256 NFTValue = tokenRarityToValue(IMoneyTreeNFT(nftAddress)._rarity(currentToken));

            userTokensToPay = userTokensToPay + NFTValue;

        }

        _burn(_msgSender(), userTokensToPay * 10 ** decimals());

    }

//----------------------End Of Staking Functions-------------------//

    
//----------------------MT Contract Intergration-------------------//

    //set MT contract address
    function setMTAddress(address _MTAddress) public onlyOwner {
       MTAddress = _MTAddress;
    }    

    //return current dividend ID
    function currentDividendsID() public view returns(uint){
        return(_currentSnapshotId.current());
    }

    //checks how many dividends are owed for a holder since they last claimed
    function checkDividendsOwed(address addressCheck) public view returns(uint256){
        
        uint lastClaimedID = _dWalletNextClaimableSnapshot[addressCheck];
        
        if(lastClaimedID == 0){
            lastClaimedID = 10;
        }
        
        uint currentID = _currentSnapshotId.current();

        require(lastClaimedID <= currentID);
        uint256 dividendOwed;
        
        for (uint i= lastClaimedID; i<=currentID; i++) {
            
            uint256 balanceAt = balanceOfAt(addressCheck,i);
            
            if (balanceAt != 0 && IMoneyTree(MTAddress)._nWalletSnapshotTotal(i) != 0) {
                dividendOwed = dividendOwed + IMoneyTree(MTAddress)._nWalletSnapshotTotal(i).mul(balanceAt).div(totalSupplyAt(i));
            }
        }
        return(dividendOwed);
    }


    //claim all dividends owed since last claim
    function claimAllDividends() public {
        
        uint256 dividendOwed = checkDividendsOwed(_msgSender());
        
        require(dividendOwed > 0);
        
        IMoneyTree(MTAddress).claimFromStakingContract(_msgSender(), dividendOwed);

        _dWalletNextClaimableSnapshot[_msgSender()] = currentDividendsID()+1;
        
    }

