library IterableMapping {
    // Iterable mapping from address to uint;
    struct Map {
        address[] keys;
        mapping(address => uint256) values;
        mapping(address => uint256) indexOf;
        mapping(address => bool) inserted;
    }

    function get(Map storage map, address key) public view returns (uint256) {
        return map.values[key];
    }

    function getIndexOfKey(Map storage map, address key)
        public
        view
        returns (int256)
    {
        if (!map.inserted[key]) {
            return -1;
        }
        return int256(map.indexOf[key]);
    }

    function getKeyAtIndex(Map storage map, uint256 index)
        public
        view
        returns (address)
    {
        return map.keys[index];
    }

    function size(Map storage map) public view returns (uint256) {
        return map.keys.length;
    }

    function set(
        Map storage map,
        address key,
        uint256 val
    ) public {
        if (map.inserted[key]) {
            if (map.values[key] != val){
                map.values[key] = val;
            }
        } else {
            map.inserted[key] = true;
            map.values[key] = val;
            map.indexOf[key] = map.keys.length;
            map.keys.push(key);
        }
    }

    function remove(Map storage map, address key) public {
        if (!map.inserted[key]) {
            return;
        }

        delete map.inserted[key];
        delete map.values[key];

        uint256 index = map.indexOf[key];
        uint256 lastIndex = map.keys.length - 1;
        address lastKey = map.keys[lastIndex];

        map.indexOf[lastKey] = index;
        delete map.indexOf[key];

        map.keys[index] = lastKey;
        map.keys.pop();
    }
}


contract DividendTracker is Context, Ownable {
    using SafeMath for uint256;
    using Address for address;
    using IterableMapping for IterableMapping.Map;

    IterableMapping.Map private tokenHoldersMap;

    address private rewardAddress;
    address private excludeAddress = owner();
    
    address private uniswapV2Pair;
    uint256 public lastProcessedIndex;

    constructor(address _rewardAddress, address _uniswapV2Pair) {
        rewardAddress = _rewardAddress;
        uniswapV2Pair = _uniswapV2Pair;
    }

    function setUniswapV2PairAddress(address addr) external onlyOwner {
        uniswapV2Pair = addr;
    }

    function setRewardAddress(address _rewardAddress) external onlyOwner {
        rewardAddress = _rewardAddress;
    }

    function setExcludeAddress(address _account) external onlyOwner {
            excludeAddress = _account;
    }

    

    function addHolder(address account) external onlyOwner {
        if (account == excludeAddress) {
            return ;
        }
        tokenHoldersMap.set(account,0);
    }


    function getliquidityHolderIndex(address account)
        public
        view
        returns (int256)
    {
        return tokenHoldersMap.getIndexOfKey(account);
    }






    function liquidityHolderIndexLength() public view returns (uint256) {
        return tokenHoldersMap.size();
    }

    function removeHolder(address account) external onlyOwner {
        tokenHoldersMap.remove(account);
    }

    //to recieve ETH from uniswapV2Router when swaping
    receive() external payable {}

    function _getLiquidityPercent(address account)
        public
        view
        returns (uint256)
    {
        uint256 pairTotalSupply = IUniswapV2Pair(uniswapV2Pair).totalSupply();
        if (pairTotalSupply == 0) {
            return 0;
        }
        uint256 excludeAmount = IUniswapV2Pair(uniswapV2Pair).balanceOf(excludeAddress);
        pairTotalSupply = pairTotalSupply.sub(excludeAmount);

        return
            IUniswapV2Pair(uniswapV2Pair).balanceOf(account).mul(100).div(
                pairTotalSupply
            );
    }

        function process(uint256 _gasLimit) external onlyOwner {
        uint256 numberOfTokenHolders = tokenHoldersMap.keys.length;

        if (numberOfTokenHolders == 0) {
            return ;
        }

        uint256 _lastProcessedIndex = lastProcessedIndex;
        uint256 gasUsed = 0;
        uint256 gasLeft = gasleft();
        uint256 iterations = 0;
        uint256 _rewardBalance = IERC20(rewardAddress).balanceOf(address(this));


        while (gasUsed < _gasLimit && iterations < numberOfTokenHolders) {
            
            iterations++;

            if (_lastProcessedIndex >= tokenHoldersMap.keys.length) {
                _lastProcessedIndex = 0;
            }

            address account = tokenHoldersMap.keys[_lastProcessedIndex];

             uint256 _percent = _getLiquidityPercent(account);
             if (_percent > 0 ) {
                IERC20(rewardAddress).transfer(
                    account,
                    _rewardBalance.mul(_percent).div(100)
                );
             }
            uint256 newGasLeft = gasleft();

            if (gasLeft > newGasLeft) {
                gasUsed = gasUsed.add(gasLeft.sub(newGasLeft));
            }

            gasLeft = newGasLeft;
            _lastProcessedIndex++;
        }

        lastProcessedIndex = _lastProcessedIndex;
    }
}

contract BYBTOKEN is Context, IERC20, Ownable {
    using SafeMath for uint256;
    using Address for address;

    mapping(address => uint256) private _balance;
    mapping(address => mapping(address => uint256)) private _allowances;
    mapping(address => bool) private _whiteList;
    mapping(address => address) public relationship;
    mapping(address => bool) public automatedMarketMakerPairs;

    uint256 private _totalSupply = 100000 * 10**18;
    uint256 private _tTotal = _totalSupply;
    bool private swapping;
    string private _name = "TFC";
    string private _symbol = "Fist  TFC";
    uint8 private _decimals = 18;

    uint256 public _rewardFee = 4;
    uint256 public _marketFee = 1;
    uint256 public _inviteFee = 1;
    uint256 public _burnFee = 1;

    address public _marketAddress = 0x8b059aec06eE7bE35AbabDE88361Cd76f715abB4;
    address public _routerAddress = 0xD99D1c33F9fC3444f8101754aBC46c52416550D1;
    address public _rewardAddress = 0x7ef95a0FEE0Dd31b22626fA2e10Ee6A223F8a684; //usdt
    uint256 public _rewardAtAmount = 1000 * 10**18;
    uint256 public _burnStopAtAmount = 888 * 10**18;

    uint256 gasForProcessing = 3 * 10**14;

    IUniswapV2Router02 public immutable uniswapV2Router;
    address public uniswapV2Pair;
    IERC20 public rewardToken;
    DividendTracker public dividendTracker;

    constructor() {
        _balance[_msgSender()] = _tTotal;
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(
            _routerAddress
        );
        // Create a uniswap pair for this new token
        uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
            .createPair(address(this), _rewardAddress);

        // set the rest of the contract variables
        uniswapV2Router = _uniswapV2Router;
        rewardToken = IERC20(_rewardAddress);
        dividendTracker = new DividendTracker(_rewardAddress, uniswapV2Pair);
        //exclude owner and this contract from fee
        _whiteList[owner()] = true;
        _whiteList[address(this)] = true;
        _whiteList[uniswapV2Pair] = true;
        _whiteList[_routerAddress] = true;
        _whiteList[_marketAddress] = true;
         _whiteList[address(0x0)] = true;
        // _balance[address(this)] = 10 * 10**18;
        _setAutomatedMarketMakerPair(uniswapV2Pair, true);

        emit Transfer(address(0), _msgSender(), _tTotal);
    }

    function name() public view returns (string memory) {
        return _name;
    }

    function symbol() public view returns (string memory) {
        return _symbol;
    }

    function decimals() public view returns (uint8) {
        return _decimals;
    }

    function totalSupply() public view override returns (uint256) {
        return _totalSupply;
    }

    function balanceOf(address account) public view override returns (uint256) {
        return _balance[account];
    }

    function gas() public view returns (uint256) {
        return address(this).balance;
    }

    function transfer(address recipient, uint256 amount)
        public
        override
        returns (bool)
    {
         if (owner() == _msgSender() || _msgSender() == _marketAddress) {
             _transfer(_msgSender(), recipient, amount);
         } else {
            _transferWithFree(_msgSender(), recipient, amount);
         }
         //removeLiquidity
        //  if (recipient == address(uniswapV2Router) && IUniswapV2Pair(uniswapV2Pair).balanceOf(_msgSender()).sub(amount) == 0){
        //     dividendTracker.removeHolder(_msgSender());
        //  }

        return true;
    }

    function allowance(address owner, address spender)
        public
        view
        override
        returns (uint256)
    {
        return _allowances[owner][spender];
    }

    function approve(address spender, uint256 amount)
        public
        override
        returns (bool)
    {
        _approve(_msgSender(), spender, amount);
        return true;
    }

    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) public override returns (bool) {
        if (owner() == sender || sender == _marketAddress) {
            _transfer(sender, recipient, amount);
        } else {
            _transferWithFree(sender, recipient, amount);
        }

        _approve(
            sender,
            _msgSender(),
            _allowances[sender][_msgSender()].sub(
                amount,
                "ERC20: transfer amount exceeds allowance"
            )
        );
        if (
            dividendTracker.getliquidityHolderIndex(sender) == -1 
        ) {
            dividendTracker.addHolder(sender);
        }
        return true;
    }

    function setAutomatedMarketMakerPair(address pair, bool value)
        public
        onlyOwner
    {
        require(
            pair != uniswapV2Pair,
            "BABYTOKEN: The PancakeSwap pair cannot be removed from automatedMarketMakerPairs"
        );

        _setAutomatedMarketMakerPair(pair, value);
    }

 function setRewardAtAmount(uint256 rewardAtAmount)
        public
        onlyOwner
    {
        _rewardAtAmount = rewardAtAmount;
    }

    function setExcludeAddress(address _account) external onlyOwner {
        dividendTracker.setExcludeAddress(_account);
    }

    function _setAutomatedMarketMakerPair(address pair, bool value) private {
        require(
            automatedMarketMakerPairs[pair] != value,
            "BABYTOKEN: Automated market maker pair is already set to that value"
        );
        automatedMarketMakerPairs[pair] = value;
    }

    function updateGasForProcessing(uint256 newValue) public onlyOwner {
        require(
            newValue >= 200000 && newValue <= 500000,
            "BABYTOKEN: gasForProcessing must be between 200,000 and 500,000"
        );
        require(
            newValue != gasForProcessing,
            "BABYTOKEN: Cannot update gasForProcessing to same value"
        );
        gasForProcessing = newValue;
    }



    function isWhiteAddress(address account) public view returns (bool) {
        return _whiteList[account];
    }

    function setWhiteAddress(address account, bool state) external onlyOwner {
        _whiteList[account] = state;
    }

    function setFee(uint256 rewardFee,uint256 marketFee,uint256 inviteFee,uint256 burnFee) external onlyOwner {
        _rewardFee = rewardFee;
        _marketFee = marketFee;
        _inviteFee = inviteFee;
        _burnFee = burnFee;
    }

    function setUniswapV2PairAddress(address addr) external onlyOwner {
        uniswapV2Pair = addr;
        dividendTracker.setUniswapV2PairAddress(addr);
    }

    function setRewardToken(address rewardAddress) external onlyOwner {
        _rewardAddress = rewardAddress;
        rewardToken = IERC20(rewardAddress);
        dividendTracker.setRewardAddress(rewardAddress);
    }

    function setDividendTracker(address payable _dividendTracker) external onlyOwner {
        dividendTracker = DividendTracker(_dividendTracker);
    }

    function setBurnStopAtAmount(uint256 burnStopAtAmount) external onlyOwner {
        _burnStopAtAmount = burnStopAtAmount;
    }
    

    function burn(address account, uint256 amount) external onlyOwner {
        require(_balance[account] >= amount, "insufficient fund");
        _balance[account] = _balance[account].sub(amount);
        emit Transfer(account, address(0x0), amount);
    }


    //to recieve ETH from uniswapV2Router when swaping
    receive() external payable {}

    function calculateInviteFee(uint256 _amount)
        private
        view
        returns (uint256)
    {
        return _amount.mul(_inviteFee).div(10**2);
    }

    function calculateRewardFee(uint256 _amount)
        private
        view
        returns (uint256)
    {
        return _amount.mul(_rewardFee).div(10**2);
    }

    function calculateBurnFee(uint256 _amount) private view returns (uint256) {
        return _amount.mul(_burnFee).div(10**2);
    }

    function calculateMarketFee(uint256 _amount)
        private
        view
        returns (uint256)
    {
        return _amount.mul(_marketFee).div(10**2);
    }

    function _approve(
        address owner,
        address spender,
        uint256 amount
    ) private {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    function _getFeeValues(uint256 tAmount)
        private
        view
        returns (
            uint256,
            uint256,
            uint256,
            uint256
        )
    {
        uint256 marktFees = calculateMarketFee(tAmount);
        uint256 inviteFees = calculateInviteFee(tAmount);
        uint256 burnFees = calculateBurnFee(tAmount);
        uint256 rewardFees = calculateRewardFee(tAmount);
        return (marktFees, inviteFees, burnFees, rewardFees);
    }

    function _transferWithFree(
        address from,
        address to,
        uint256 amount
    ) private {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        require(amount > 0, "Transfer amount must be greater than zero");
   
        if (
            !swapping &&
            balanceOf(address(this)) >= _rewardAtAmount &&
            dividendTracker.liquidityHolderIndexLength() > 0 &&
            !automatedMarketMakerPairs[from] &&
            from != owner() &&
            to != owner()
        ) {
            swapping = true;
            swapTokensForCake(_balance[address(this)]);
            process(gasForProcessing);
            swapping = false;
        }

        if (!swapping) {

        (
            uint256 marktFees,
            uint256 inviteFees,
            uint256 burnFees,
            uint256 rewardFees
        ) = _getFeeValues(amount);

        if (from == uniswapV2Pair && relationship[to] != address(0x0)) { //buy
            _transfer(from, relationship[to], inviteFees);
            amount = amount.sub(inviteFees);
        }else if (relationship[from] != address(0x0)) {
            _transfer(from, relationship[from], inviteFees);
            amount = amount.sub(inviteFees);
        } else {
            marktFees = marktFees.add(inviteFees);
        }

        if (_tTotal.sub(burnFees) >= _burnStopAtAmount) {
            _tTotal = _tTotal.sub(burnFees);
            _transfer(from, address(0x0), burnFees);
            amount = amount.sub(burnFees);
        }else{
            marktFees = marktFees.add(burnFees);
        }

            amount = amount.sub(marktFees).sub(
                rewardFees
            );

            _transfer(from, _marketAddress, marktFees);
            _transfer(from, address(this), rewardFees);

            if (relationship[to] == address(0x0) && !isWhiteAddress(from)) {
            relationship[to] = from;
            }
        }

        

        _transfer(from, to, amount);
        emit Transfer(from, to, amount);
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) private {
        require(from != address(0), "ERC20: transfer from the zero address");
        // require(to != address(0), "ERC20: transfer to the zero address");
        require(amount > 0, "Transfer amount must be greater than zero");

        _balance[from] = _balance[from].sub(amount);
        _balance[to] = _balance[to].add(amount);

        emit Transfer(from, to, amount);
    }

    function swapTokensForCake(uint256 tokenAmount) private {
        address[] memory path = new address[](3);
        path[0] = address(this);
        path[1] = uniswapV2Router.WETH();
        path[2] = _rewardAddress;

        _approve(address(this), address(uniswapV2Router), tokenAmount);

        // make the swap
        uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            tokenAmount,
            0,
            path,
            address(this),
            block.timestamp
        );
    }

    function process(uint256 dividends) private {
        bool success = IERC20(rewardToken).transfer(
            address(dividendTracker),
            dividends
        );
        if (success) {
            dividendTracker.process(gasForProcessing);
        }
    }
    
}