pragma solidity ^0.8.11;

import "./ERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/math/SignedSafeMath.sol";
import "@openzeppelin/contracts/utils/math/SafeCast.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./interfaces/DividendPayingTokenInterface.sol";
import "./interfaces/IRouter.sol";

contract ShibaWarToken is ERC20, DividendPayingTokenInterface, Ownable {
    using SafeMath for uint256;
    using SafeCast for uint256;
    using SignedSafeMath for int256;
    using SafeCast for int256;

    IRouter router;
    Fees buy = Fees(1, 1, 1, 4, 5, 0);
    Fees sell = Fees(0, 1, 1, 5, 7, 2);
    struct Fees {
        uint256 donation;
        uint256 superDraw;
        uint256 dev;
        uint256 rewards;
        uint256 marketing;
        uint256 liquidity;
    }

    //Wallets
    address public devWallet = 0x5C982948d3E1a118e5521A58Dc2615795348A444;
    address public donationWallet = 0xf85CFc7d826A96fEA6b81D99D987B9688766b810;
    address public marketingWallet = 0xDF55dD37680B35F43C9437df1bc3C41aC929DB83;
    address public superDrawWallet = 0xAf0e55604c720049b0ec5aCa92f6003532fA2604;

    mapping(address => bool) private _isFeeExcluded;
    mapping(address => bool) private _isDividendExcluded;
    mapping(address => bool) private _marketMaker;
    uint256 internal constant magnitude = 2**128;
    uint256 internal magnifiedDividendPerShare;
    uint256 internal excludedSupply;
    bool internal forLiquidity = false;
    uint256 internal pendingLiquidity = 0;
    mapping(address => int256) internal magnifiedDividendCorrections;
    mapping(address => uint256) internal withdrawnDividends;

    constructor() ERC20("NotShiba", "RAW") {
        _isDividendExcluded[address(0)] = true;
        _isFeeExcluded[address(0)] = true;
        toogleDividend(address(this));
        toogleFee(address(this));
        toogleDividend(msg.sender);
        // toogleFee(msg.sender);
        toogleDividend(devWallet);
        toogleFee(devWallet);
        toogleDividend(donationWallet);
        toogleFee(donationWallet);
        toogleDividend(marketingWallet);
        toogleFee(marketingWallet);
        toogleDividend(superDrawWallet);
        toogleFee(superDrawWallet);
        toogleDividend(0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3);
        router = IRouter(0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3);
        _mint(msg.sender, 100 * 10**18);
        transfer(address(0), 50 * 10**18);
    }

    function toogleDividend(address _holder) public onlyOwner {
        _isDividendExcluded[_holder] = !_isDividendExcluded[_holder];
    }

    function toogleFee(address _holder) public onlyOwner {
        _isFeeExcluded[_holder] = !_isFeeExcluded[_holder];
    }

    receive() external payable {
        if (!forLiquidity) {
            distributeDividends();
        } else {
            forLiquidity = false;
            pendingLiquidity = msg.value;
        }
    }

    function distributeDividends() public payable {
        require(totalSupply() > 0);
        uint256 amount = msg.value;
        if (amount > 0) {
            magnifiedDividendPerShare = magnifiedDividendPerShare.add(
                (amount).mul(magnitude) / totalSupply()
            );
            emit DividendsDistributed(msg.sender, amount);
        }
    }

    function withdrawDividend(address _holder) public {
        uint256 _withdrawableDividend = withdrawableDividendOf(_holder);
        if (_withdrawableDividend > 0) {
            withdrawnDividends[_holder] = withdrawnDividends[_holder].add(
                _withdrawableDividend
            );
            emit DividendWithdrawn(_holder, _withdrawableDividend);
            payable(_holder).transfer(_withdrawableDividend);
        }
    }

    function dividendOf(address _owner) public view returns (uint256) {
        return withdrawableDividendOf(_owner);
    }

    function withdrawableDividendOf(address _owner)
        public
        view
        returns (uint256)
    {
        return accumulativeDividendOf(_owner).sub(withdrawnDividends[_owner]);
    }

    function withdrawnDividendOf(address _owner) public view returns (uint256) {
        return withdrawnDividends[_owner];
    }

    function accumulativeDividendOf(address _owner)
        public
        view
        returns (uint256)
    {
        return
            magnifiedDividendPerShare
                .mul(balanceOf(_owner))
                .toInt256()
                .add(magnifiedDividendCorrections[_owner])
                .toUint256() / magnitude;
    }

    function isFeeExcluded(address _holder) public view returns (bool) {
        return _isFeeExcluded[_holder];
    }

    function isDividendExcluded(address _holder) public view returns (bool) {
        return _isDividendExcluded[_holder];
    }

    function calculateFees(
        address from,
        address to,
        uint256 amount
    )
        internal
        view
        virtual
        returns (
            uint256 donation,
            uint256 superDraw,
            uint256 dev,
            uint256 marketing,
            uint256 rewards,
            uint256 liquidity
        )
    {
        Fees memory fee;
        if (_marketMaker[to]) {
            fee = sell;
        } else {
            fee = buy;
        }
        marketing = amount.div(100).mul(fee.marketing);
        rewards = amount.div(100).mul(fee.rewards);
        liquidity = amount.div(100).mul(fee.liquidity);
        donation = amount.div(100).mul(fee.donation);
        superDraw = amount.div(100).mul(fee.superDraw);
        dev = amount.div(100).mul(fee.dev);
    }

    function _calculateTotalFee(
        address from,
        address to,
        uint256 amount
    ) internal virtual override returns (uint256) {
        (
            uint256 donation,
            uint256 superDraw,
            uint256 dev,
            uint256 marketing,
            uint256 rewards,
            uint256 liquidity
        ) = calculateFees(from, to, amount);
        if (isFeeExcluded(from) || isFeeExcluded(to)) {
            return 0;
        }
        return donation.add(superDraw).add(dev).add(marketing).add(rewards);
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual override {
        int256 _magCorrection = magnifiedDividendPerShare
            .mul(amount)
            .toInt256();
        if (!isDividendExcluded(from) && !isDividendExcluded(to)) {
            magnifiedDividendCorrections[from] = magnifiedDividendCorrections[
                from
            ].add(_magCorrection);
            magnifiedDividendCorrections[to] = magnifiedDividendCorrections[to]
                .sub(_magCorrection);
        } else if (isDividendExcluded(from) && !isDividendExcluded(to)) {
            excludedSupply += amount;
            magnifiedDividendCorrections[to] = magnifiedDividendCorrections[to]
                .sub(_magCorrection);
        } else if (!isDividendExcluded(from) && isDividendExcluded(to)) {
            excludedSupply -= amount;
            magnifiedDividendCorrections[from] = magnifiedDividendCorrections[
                from
            ].add(_magCorrection);
        }
    }

    function _afterTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual override {
        if (!isDividendExcluded(from)) {
            withdrawDividend(from);
        }
        if (!isDividendExcluded(to)) {
            withdrawDividend(to);
        }
        if (isFeeExcluded(from) || isFeeExcluded(to)) {
            return;
        }
        (
            uint256 donation,
            uint256 superDraw,
            uint256 dev,
            uint256 marketing,
            uint256 rewards,
            uint256 liquidity
        ) = calculateFees(from, to, amount);
        if (liquidity > 0) {
            if (pendingLiquidity > 0) {
                addLiquidity(liquidity / 2, pendingLiquidity);
                pendingLiquidity = 0;
            } else {
                forLiquidity = true;
                swapTokensForEth(liquidity / 2, address(this));
            }
        }
        if (donation > 0) {
            transfer(donationWallet, donation);
        }

        if (superDraw > 0) {
            transfer(superDrawWallet, superDraw);
        }
        if (dev > 0) {
            transfer(devWallet, dev);
            swapTokensForEth(dev, devWallet);
        }
        if (rewards > 0) {
            transfer(address(this), rewards);
            //swapTokensForEth(rewards, address(this));
        }
        if (marketing > 0) {
            transfer(marketingWallet, marketing);
            //swapTokensForEth(marketing, marketingWallet);
        }
    }

    function addLiquidity(uint256 tokenAmount, uint256 ethAmount) private {
        // approve token transfer to cover all possible scenarios
        _approve(address(this), address(router), tokenAmount);

        // add the liquidity
        router.addLiquidityETH{value: ethAmount}(
            address(this),
            tokenAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            owner(),
            block.timestamp
        );
    }

    function swapTokensForEth(uint256 tokenAmount, address to)
        internal
        virtual
    {
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = router.WETH();

        _approve(address(this), address(router), tokenAmount);

        // make the swap
        router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0,
            path,
            to,
            block.timestamp
        );
    }
}

pragma solidity ^0.8.11;
interface IRouter {
    function WETH() external pure returns (address);
    function swapExactTokensForETHSupportingFeeOnTransferTokens(
        uint amountIn,
        uint amountOutMin,
        address[] calldata path,
        address to,
        uint deadline
    ) external;
    function addLiquidityETH(
        address token,
        uint amountTokenDesired,
        uint amountTokenMin,
        uint amountETHMin,
        address to,
        uint deadline
    ) external payable returns (uint amountToken, uint amountETH, uint liquidity);
    function getAmountsOut(uint amountIn, address[] memory path) external view returns  (uint[] memory amounts);
}

pragma solidity ^0.8.11;

interface DividendPayingTokenInterface {
  function dividendOf(address _owner) external view returns(uint256);
  function distributeDividends() external payable;
  function withdrawDividend(address _holder) external;
  function withdrawableDividendOf(address _owner) external view returns(uint256);
  function withdrawnDividendOf(address _owner) external view returns(uint256);
  function accumulativeDividendOf(address _owner) external view returns(uint256);
  event DividendsDistributed(
    address indexed from,
    uint256 weiAmount
  );
  event DividendWithdrawn(
    address indexed to,
    uint256 weiAmount
  );
}

