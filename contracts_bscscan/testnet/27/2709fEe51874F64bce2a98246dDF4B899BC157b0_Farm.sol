pragma solidity ^0.8.0;

import "@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router02.sol";
import "@uniswap/v2-core/contracts/interfaces/IUniswapV2Factory.sol";
import "@uniswap/v2-core/contracts/interfaces/IUniswapV2Pair.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./interfaces/IACryptoSwap.sol";
import "./interfaces/IPlanetFinance.sol";
import "hardhat/console.sol";

contract Farm is ERC20, Ownable, ReentrancyGuard {
    IERC20 _usdt = IERC20(0x55d398326f99059fF775485246999027B3197955);
    IERC20 _usdc = IERC20(0x8AC76a51cc950d9822D68b83fE1Ad97B32Cd580d);
    IERC20 _bsw = IERC20(0x965F527D9159dCe6288a2219DB51fc6Eef120dD1);

    IACryptoSwap _stableSwap =
        IACryptoSwap(0x99c92765EfC472a9709Ced86310D64C4573c4b77); // ACryptoS stable swap
    IUniswapV2Router02 _router =
        IUniswapV2Router02(0x3a6d8cA21D1CF76F653A67577FA0D27453350dD8); // biswap router
    IPlanetFinance _masterChef =
        IPlanetFinance(0xDbc1A13490deeF9c3C12b44FE77b503c1B061739); // biswap masterchef
    IUniswapV2Pair _dex =
        IUniswapV2Pair(0x1483767E665B3591677Fd49F724bf7430C18Bf83); // biswap LPs

    uint256 public constant MAX_PERCENT = 10000;

    int128 public constant _usdcIndex = 4; // ACryptoS usdc's base token id
    int128 public constant _usdtIndex = 2; // ACryptoS bsc usd (usdt) base token id
    uint256 public constant _poolId = 4; // biswap usdc<->usdt pool id

    uint256 public constant initialNumberShares = 1000000000000;
    uint256 public TotalStaked;
    uint256 public PPS;
    uint256 public devFee = 2000;
    address public devAddress = 0xF90fF6d484331399f4eAa13f73D03b8B18eA1373;

    constructor() public ERC20("Farm", "FM") { }

    // Admin makes a deposit to set the price per share on first deposit.
    function adminInitialDeposit(uint256 _amount) public onlyOwner {
        require(TotalStaked == 0);
        // Convert 50% usdc on ACryptoS on BSC (+0.05% for slippage)
        uint256 swapAmount = _amount / 2 * 10003 / MAX_PERCENT;

        _usdc.transferFrom(address(msg.sender), address(this), _amount);
        _usdc.approve(address(_stableSwap), swapAmount);

        uint256 received = _stableSwap.exchange_underlying(
            _usdcIndex,
            _usdtIndex,
            swapAmount,
            _stableSwap.get_dy_underlying(_usdcIndex, _usdtIndex, swapAmount)
        );

        console.log("admin dep swap: ", _amount/2 - ((_amount/2) - received), (_amount/2) - received, received);

        // Create LP token 50 USDT - 50 USDC on planet finance
        (uint256 amountA, uint256 amountB, uint256 liquidity) = addLiquidity(
            _usdt.balanceOf(address(this)),
            _usdc.balanceOf(address(this))
        );
        _dex.approve(address(_masterChef), liquidity);
        _masterChef.deposit(_poolId, liquidity);

        TotalStaked += liquidity;

        _mint(msg.sender, initialNumberShares);
        updatePPS();
    }

    function updatePPS() internal {
        PPS = NAV() / totalSupply();
    }

    function lpValue(uint256 _lpTokens) public view returns (uint256) {
        (uint256 reserve0, uint256 reserve1, ) = _dex.getReserves();
        uint256 totalValue = getQuote(reserve0) + getQuote(reserve1);
        return _lpTokens * (totalValue / _dex.totalSupply());
    }

    function usdcToLpValue(uint256 _usdcTokens) public view returns (uint256) {
        (uint256 reserve0, uint256 reserve1, ) = _dex.getReserves();
        uint256 totalValue = getQuote(reserve0) + getQuote(reserve1);
        return _usdcTokens / (totalValue / _dex.totalSupply());
    }

    function NAV() public view returns (uint256) {
        return lpValue(TotalStaked);
    }

    /*
     *  Deposit USDC into autofarm.
     */
    function deposit(uint256 _amount) public nonReentrant {
        // Harvest rewards if any previous deposits have been made, and then deposit provided liquidity + harvested
        _reinvest();
        // Convert 50% usdc on ACryptoS on BSC (0.05% for slippage)
        uint256 swapAmount = _amount / 2 * 10003 / MAX_PERCENT;

        _usdc.transferFrom(address(msg.sender), address(this), _amount);
        _usdc.approve(address(_stableSwap), swapAmount);

        // console.log("dep swap: ", swapAmount);
        uint256 received = _stableSwap.exchange_underlying(
            _usdcIndex,
            _usdtIndex,
            swapAmount,
            _stableSwap.get_dy_underlying(_usdcIndex, _usdtIndex, swapAmount)
        );
        console.log("dep swap: ", received);
        // Create LP token 50 USDT - 50 USDC on planet finance
        (uint256 amountA, uint256 amountB, uint256 liquidity) = addLiquidity(
            _usdt.balanceOf(address(this)),
            _usdc.balanceOf(address(this))
        );

        _dex.approve(address(_masterChef), liquidity);
        _masterChef.deposit(_poolId, liquidity);
        TotalStaked += liquidity;

        uint256 sharesIssued = lpValue(liquidity) / PPS;

        _mint(msg.sender, sharesIssued);
    }

    function reinvest() public nonReentrant {
        _reinvest();
    }

    function _reinvest() private {
        uint256 reward = _masterChef.pendingBSW(_poolId, address(this));
        if (reward > 0) {
            _masterChef.withdraw(_poolId, 0);

            // swap 60% aqua to usdc (40 + 20)
            uint256 usdcAmount = (reward * (6000)) / MAX_PERCENT;

            address[] memory path = new address[](2);
            path[0] = address(_bsw);
            path[1] = address(_usdc);

            uint256 usdcAmountReceived = swap(usdcAmount, path);
            uint256 sentFee = (usdcAmountReceived * devFee) / 6000;
            usdcAmountReceived -= sentFee;

            // send 20% usdc to fee
            _usdc.transfer(devAddress, sentFee);

            // swap 40% aqua to usdt
            uint256 ustAmount = reward - usdcAmount;

            path = new address[](2);
            path[0] = address(_bsw);
            path[1] = address(_usdt);

            uint256 ustAmountReceived = swap(ustAmount, path);

            // Create LP token 40% USDC + 40% USDT
            (
                uint256 amountA,
                uint256 amountB,
                uint256 liquidityR
            ) = addLiquidity(ustAmountReceived, usdcAmountReceived);
            TotalStaked += liquidityR;
            // Stake reinvested LP
            updatePPS();

            // 3. Stake LP token
            _dex.approve(address(_masterChef), liquidityR);
            _masterChef.deposit(_poolId, liquidityR);

            // console.log(
            //     _masterChef.userInfo(_poolId, address(this)).amount,
            //     _dex.balanceOf(address(this)),
            //     "reward:",
            //     reward
            // );
        }
    }

    function withdraw(uint256 _shares) public nonReentrant {
        require(_shares >= 1);
        _reinvest();

        require(balanceOf(msg.sender) >= _shares);

        uint256 toWithdraw = usdcToLpValue(_shares * PPS);
        TotalStaked -= toWithdraw;

        _burn(msg.sender, _shares);

        _masterChef.withdraw(_poolId, toWithdraw);

        (uint256 amountA, uint256 amountB) = removeLiquidity(toWithdraw);

        address[] memory path = new address[](2);
        path[0] = address(_usdt);
        path[1] = address(_usdc);
        uint256 receivedUSDC = swap(amountA, path) + amountB;

        _usdc.transfer(msg.sender, receivedUSDC);
        updatePPS();
    }

    function adminWithdraw() public onlyOwner {
        require(totalSupply() == initialNumberShares);

        _masterChef.withdraw(_poolId, TotalStaked);

        (uint256 amountA, uint256 amountB) = removeLiquidity(TotalStaked);

        address[] memory path = new address[](2);
        path[0] = address(_usdt);
        path[1] = address(_usdc);
        uint256 receivedUSDC = swap(amountA, path) + amountB;

        _usdc.transfer(msg.sender, receivedUSDC);
        updatePPS();
    }

    function addLiquidity(uint256 amountUST, uint256 amountUSDC)
        private
        returns (
            uint256 amountA,
            uint256 amountB,
            uint256 liquidity
        )
    {
        _usdt.approve(address(_router), amountUST);
        _usdc.approve(address(_router), amountUSDC);

        console.log("addLiq");
        console.log(
            _usdt.balanceOf(address(this)),
            _usdc.balanceOf(address(this)),
            amountUST,
            amountUSDC
        );
        (amountA, amountB, liquidity) = _router.addLiquidity(
            address(_usdt),
            address(_usdc),
            amountUST,
            amountUSDC,
            // TODO: is this ok or not
            (amountUST * 9000) / MAX_PERCENT,
            (amountUSDC * 9000) / MAX_PERCENT,
            address(this),
            block.timestamp + 1200
        );
        _usdt.transfer(msg.sender, _usdt.balanceOf(address(this)));
        _usdc.transfer(msg.sender, _usdc.balanceOf(address(this)));
    }

    function removeLiquidity(uint256 _liquidity)
        private
        returns (uint256 amountA, uint256 amountB)
    {
        _dex.approve(address(_router), _liquidity);
        (amountA, amountB) = _router.removeLiquidity(
            address(_usdt),
            address(_usdc),
            _liquidity,
            // TODO: is this ok or not ->  ADD EXPECTED AMOUNT
            lpValue(_liquidity) / 2,
            lpValue(_liquidity) / 2,
            address(this),
            block.timestamp + 1200
        );
    }

    function swap(uint256 aquaAmount, address[] memory path)
        private
        returns (uint256 received)
    {
        uint256[] memory amountsOut = _router.getAmountsOut(aquaAmount, path);
        IERC20(path[0]).approve(address(_router), aquaAmount);
        uint256[] memory amounts = _router.swapExactTokensForTokens(
            aquaAmount,
            amountsOut[path.length - 1],
            path,
            address(this),
            block.timestamp + 1200
        );
        return amounts[path.length - 1];
    }

    function getQuote(uint256 _amountIn)
        internal
        view
        returns (uint256 amountUSDC)
    {
        (uint256 reserve0, uint256 reserve1, ) = _dex.getReserves();
        amountUSDC = _router.quote(_amountIn, reserve0, reserve1);
    }
}

