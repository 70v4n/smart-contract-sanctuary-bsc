    _clearPayees();
    _addPayees(payees, shares_);
  }

  function _addPayees(address[] memory payees, uint256[] memory shares_) private {
    require(_payees.length == 0, 'PaymentSplitter: payees should be cleared first');
    require(payees.length == shares_.length, 'PaymentSplitter: payees and shares length mismatch');
    require(payees.length > 0, 'PaymentSplitter: no payees');
    for (uint256 i = 0; i < payees.length; i++) _addPayee(payees[i], shares_[i]);
  }

