pragma solidity ^0.8.7;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";

import "./interfaces/IWastedWarrior.sol";

contract WastedCollaborator is Initializable, AccessControlUpgradeable, ReentrancyGuardUpgradeable {
    using SafeMath for uint256;
    using EnumerableSet for EnumerableSet.AddressSet;

    bytes32 public constant WASTED_ROLE = keccak256("WASTED_ROLE");
    bytes32 public constant CONTROLLER_ROLE = keccak256("CONTROLLER_ROLE");

    function initialize(IWastedWarrior wastedWarriorAddress) public initializer {
        __AccessControl_init();

        wastedWarrior = wastedWarriorAddress;
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);

        phase = WASTED_PHASE.WHITELIST;
        maxBoughtPackages = 10;
    }

    uint256 public balance;
    WASTED_PHASE public phase;
    uint256 public maxBoughtPackages;

    IWastedWarrior public wastedWarrior;
    mapping(address => uint256) public boughts;
    EnumerableSet.AddressSet private whitelists;

    enum WASTED_PHASE { WHITELIST, PUBLIC }

    function setWastedWarrior(IWastedWarrior wastedWarriorAddress)
        external
        onlyRole(CONTROLLER_ROLE)
    {
        require(address(wastedWarriorAddress) != address(0));
        wastedWarrior = wastedWarriorAddress;
    }

    function setPhase(uint256 _newPhase)
        external
        onlyRole(CONTROLLER_ROLE)
    {
        if (_newPhase == uint256(WASTED_PHASE.WHITELIST)) {
            phase = WASTED_PHASE.WHITELIST;
        } else if (_newPhase == uint256(WASTED_PHASE.PUBLIC)) {
            phase = WASTED_PHASE.PUBLIC;
        }
    }

    function isWhitelist(address _address)
        external
        view
        returns(bool)
    {
        return whitelists.contains(_address);
    }

    function addWhitelists(address[] memory _addresses)
        external
        onlyRole(CONTROLLER_ROLE)
    {
        require(_addresses.length != 0, "WC: whitelists invalid");
        for (uint256 i = 0; i < _addresses.length; i++) {
            _addWhitelist(_addresses[i]);
        }
    }

    function _addWhitelist(address _address) internal {
        require(_address != address(0), "WC: address invalid");
        require(!whitelists.contains(_address), "WC: whitelist added");

        whitelists.add(_address);
    }

    function removeWhitelists(address[] memory _addresses)
        external
        onlyRole(CONTROLLER_ROLE)
    {
        require(_addresses.length != 0, "WC: whitelists invalid");
        for (uint256 i = 0; i < _addresses.length; i++) {
            _removeWhitelist(_addresses[i]);
        }
    }

    function _removeWhitelist(address _address) internal {
        require(_address != address(0), "WC: address invalid");
        require(whitelists.contains(_address), "WC: whitelist not added");

        whitelists.remove(_address);
    }

    function getPackageFee(uint256 rarityPackage) external view returns(uint256) {
        if (
            rarityPackage == uint256(IWastedWarrior.PackageRarity.PLASTIC)
        ) {
            return wastedWarrior.getPlasticPackageFee();
        } else if (
            rarityPackage == uint256(IWastedWarrior.PackageRarity.STEEL)
        ) {
            return wastedWarrior.getSteelPackageFee();
        } else if (
            rarityPackage == uint256(IWastedWarrior.PackageRarity.GOLD)
        ) {
            return wastedWarrior.getGoldPackageFee();
        } else if (
            rarityPackage == uint256(IWastedWarrior.PackageRarity.PLATINUM)
        ) {
            return wastedWarrior.getPlatinumPackageFee();
        }

        return 0;
    }

    function getMaxWastedWarriorOfPackage(uint256 rarityPackage) external view returns(uint256) {
        IWastedWarrior.Collaborator memory collaborator = wastedWarrior.getInfoCollaborator(address(this));
        if (
            rarityPackage == uint256(IWastedWarrior.PackageRarity.PLASTIC)
        ) {
            return collaborator.totalSupplyPlasticPackages;
        } else if (
            rarityPackage == uint256(IWastedWarrior.PackageRarity.STEEL)
        ) {
            return collaborator.totalSupplySteelPackages;
        } else if (
            rarityPackage == uint256(IWastedWarrior.PackageRarity.GOLD)
        ) {
            return collaborator.totalSupplyGoldPackages;
        } else if (
            rarityPackage == uint256(IWastedWarrior.PackageRarity.PLATINUM)
        ) {
            return collaborator.totalSupplyPlatinumPackages;
        }

        return 0;
    }

    function getBoughtWastedWarriorOfPackage(uint256 rarityPackage) external view returns(uint256) {
        IWastedWarrior.Collaborator memory collaborator = wastedWarrior.getInfoCollaborator(address(this));
        if (
            rarityPackage == uint256(IWastedWarrior.PackageRarity.PLASTIC)
        ) {
            return collaborator.mintedPlasticPackages;
        } else if (
            rarityPackage == uint256(IWastedWarrior.PackageRarity.STEEL)
        ) {
            return collaborator.mintedSteelPackages;
        } else if (
            rarityPackage == uint256(IWastedWarrior.PackageRarity.GOLD)
        ) {
            return collaborator.mintedGoldPackages;
        } else if (
            rarityPackage == uint256(IWastedWarrior.PackageRarity.PLATINUM)
        ) {
            return collaborator.mintedPlatinumPackages;
        }

        return 0;
    }

    function buyWastedWarrior(uint256 amount, uint256 rarityPackage)
        external
        payable
    {
        require(amount != 0, "WC: invalid amount");

        if (phase == WASTED_PHASE.WHITELIST) {
            require(whitelists.contains(msg.sender), "WC: whitelist not added");
        }

        uint256 fee;
        if (
            rarityPackage == uint256(IWastedWarrior.PackageRarity.PLASTIC)
        ) {
            fee = wastedWarrior.getPlasticPackageFee().mul(amount);
        } else if (
            rarityPackage == uint256(IWastedWarrior.PackageRarity.STEEL)
        ) {
            fee = wastedWarrior.getSteelPackageFee().mul(amount);
        } else if (
            rarityPackage == uint256(IWastedWarrior.PackageRarity.GOLD)
        ) {
            fee = wastedWarrior.getGoldPackageFee().mul(amount);
        } else if (
            rarityPackage == uint256(IWastedWarrior.PackageRarity.PLATINUM)
        ) {
            fee = wastedWarrior.getPlatinumPackageFee().mul(amount);
        }

        require(fee != 0 && fee == msg.value, "WC: Not enough fee");

        balance = balance.add(msg.value);
        boughts[msg.sender] = boughts[msg.sender].add(amount);
        wastedWarrior.mintFor(msg.sender, amount, rarityPackage);
    }

    function withdraw() external onlyRole(WASTED_ROLE) nonReentrant {
        require(balance != 0, "WC: not enough");
        uint256 amount= balance;
        balance = 0;
        (bool isSuccess, ) = address(msg.sender).call{value: amount}("");
        require(isSuccess);
    }

    function widthdrawAmount(uint256 amount)
        external
        onlyRole(WASTED_ROLE)
        nonReentrant
    {
        require(amount != 0 && amount <= balance, "WC: not valid");
        balance = balance.sub(amount);
        (bool isSuccess, ) = address(msg.sender).call{value: amount}("");
        require(isSuccess);
    }
}

