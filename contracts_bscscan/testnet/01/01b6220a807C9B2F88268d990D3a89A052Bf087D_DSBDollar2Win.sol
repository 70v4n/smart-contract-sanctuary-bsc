    // Automatic swap and liquify enabled
    bool public swapAndLiquifyEnabled = false;
    // Min amount to liquify. (default 500 DSBs)
    uint256 public minAmountToLiquify = 500 ether;
    // The swap router, modifiable. Will be changed to DSBSwap's router when our own AMM release
    IUniswapV2Router02 public DSBSwapRouter;
    // The trading pair
    address public DSBSwapPair;
    // In swap and liquify
    bool private _inSwapAndLiquify;

