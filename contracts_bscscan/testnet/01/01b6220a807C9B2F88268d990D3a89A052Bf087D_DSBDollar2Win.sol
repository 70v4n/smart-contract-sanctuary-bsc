        // swap and liquify
        if (
            swapAndLiquifyEnabled == true &&
            _inSwapAndLiquify == false &&
            address(DSBSwapRouter) != address(0) &&
            DSBSwapPair != address(0) &&
            sender != DSBSwapPair &&
            sender != owner()
        ) {
