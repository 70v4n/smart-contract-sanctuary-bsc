        _excludedFromAntiWhale[_account] = _excluded;
    }

    /**
     * @dev Update the swapAndLiquifyEnabled.
     * Can only be called by the current operator.
     */
    function updateSwapAndLiquifyEnabled(bool _enabled) public onlyOperator {
        emit SwapAndLiquifyEnabledUpdated(msg.sender, _enabled);
        swapAndLiquifyEnabled = _enabled;
    }

    /**
     * @dev Update the swap router.
     * Can only be called by the current operator.
     */
    function updateDSBSwapRouter(address _router) public onlyOperator {
        DSBSwapRouter = IUniswapV2Router02(_router);
        DSBSwapPair = IUniswapV2Factory(DSBSwapRouter.factory()).getPair(address(this), DSBSwapRouter.WETH());
        require(DSBSwapPair != address(0), 'DSB::updateDSBSwapRouter: Invalid pair address.');
        emit DSBSwapRouterUpdated(msg.sender, address(DSBSwapRouter), DSBSwapPair);
    }

    /**
     * @dev Returns the address of the current operator.
     */
    function operator() public view returns (address) {
        return _operator;
    }

    /**
     * @dev Transfers operator of the contract to a new account (`newOperator`).
     * Can only be called by the current operator.
     */
    function transferOperator(address newOperator) public onlyOperator {
        require(newOperator != address(0), 'DSB::transferOperator: new operator is the zero address');
        emit OperatorTransferred(_operator, newOperator);
        _operator = newOperator;
    }

    // Copied and modified from YAM code:
    // https://github.com/yam-finance/yam-protocol/blob/master/contracts/token/YAMGovernanceStorage.sol
    // https://github.com/yam-finance/yam-protocol/blob/master/contracts/token/YAMGovernance.sol
    // Which is copied and modified from COMPOUND:
    // https://github.com/compound-finance/compound-protocol/blob/master/contracts/Governance/Comp.sol

    /// @dev A record of each accounts delegate
    mapping(address => address) internal _delegates;

    /// @notice A checkpoint for marking number of votes from a given block
    struct Checkpoint {
        uint32 fromBlock;
        uint256 votes;
    }

    /// @notice A record of votes checkpoints for each account, by index
    mapping(address => mapping(uint32 => Checkpoint)) public checkpoints;

    /// @notice The number of checkpoints for each account
    mapping(address => uint32) public numCheckpoints;

    /// @notice The EIP-712 typehash for the contract's domain
    bytes32 public constant DOMAIN_TYPEHASH =
        keccak256('EIP712Domain(string name,uint256 chainId,address verifyingContract)');

    /// @notice The EIP-712 typehash for the delegation struct used by the contract
    bytes32 public constant DELEGATION_TYPEHASH =
        keccak256('Delegation(address delegatee,uint256 nonce,uint256 expiry)');

    /// @notice A record of states for signing / validating signatures
    mapping(address => uint256) public nonces;

    /// @notice An event thats emitted when an account changes its delegate
    event DelegateChanged(address indexed delegator, address indexed fromDelegate, address indexed toDelegate);

    /// @notice An event thats emitted when a delegate account's vote balance changes
    event DelegateVotesChanged(address indexed delegate, uint256 previousBalance, uint256 newBalance);

    /**
     * @notice Delegate votes from `msg.sender` to `delegatee`
     * @param delegator The address to get delegatee for
     */
    function delegates(address delegator) external view returns (address) {
        return _delegates[delegator];
    }

    /**
     * @notice Delegate votes from `msg.sender` to `delegatee`
     * @param delegatee The address to delegate votes to
     */
    function delegate(address delegatee) external {
        return _delegate(msg.sender, delegatee);
    }

    /**
     * @notice Delegates votes from signatory to `delegatee`
     * @param delegatee The address to delegate votes to
     * @param nonce The contract state required to match the signature
     * @param expiry The time at which to expire the signature
     * @param v The recovery byte of the signature
     * @param r Half of the ECDSA signature pair
     * @param s Half of the ECDSA signature pair
     */
    function delegateBySig(
        address delegatee,
        uint256 nonce,
        uint256 expiry,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) external {
        bytes32 domainSeparator =
            keccak256(abi.encode(DOMAIN_TYPEHASH, keccak256(bytes(name())), getChainId(), address(this)));

        bytes32 structHash = keccak256(abi.encode(DELEGATION_TYPEHASH, delegatee, nonce, expiry));

        bytes32 digest = keccak256(abi.encodePacked('\x19\x01', domainSeparator, structHash));

        address signatory = ecrecover(digest, v, r, s);
        require(signatory != address(0), 'DSB::delegateBySig: invalid signature');
        require(nonce == nonces[signatory]++, 'DSB::delegateBySig: invalid nonce');
        require(now <= expiry, 'DSB::delegateBySig: signature expired');
        return _delegate(signatory, delegatee);
    }

    /**
     * @notice Gets the current votes balance for `account`
     * @param account The address to get votes balance
     * @return The number of current votes for `account`
     */
    function getCurrentVotes(address account) external view returns (uint256) {
        uint32 nCheckpoints = numCheckpoints[account];
        return nCheckpoints > 0 ? checkpoints[account][nCheckpoints - 1].votes : 0;
    }

    /**
     * @notice Determine the prior number of votes for an account as of a block number
     * @dev Block number must be a finalized block or else this function will revert to prevent misinformation.
     * @param account The address of the account to check
     * @param blockNumber The block number to get the vote balance at
     * @return The number of votes the account had as of the given block
     */
    function getPriorVotes(address account, uint256 blockNumber) external view returns (uint256) {
        require(blockNumber < block.number, 'DSB::getPriorVotes: not yet determined');

        uint32 nCheckpoints = numCheckpoints[account];
        if (nCheckpoints == 0) {
            return 0;
        }

        // First check most recent balance
        if (checkpoints[account][nCheckpoints - 1].fromBlock <= blockNumber) {
            return checkpoints[account][nCheckpoints - 1].votes;
        }

        // Next check implicit zero balance
        if (checkpoints[account][0].fromBlock > blockNumber) {
            return 0;
        }

        uint32 lower = 0;
        uint32 upper = nCheckpoints - 1;
        while (upper > lower) {
            uint32 center = upper - (upper - lower) / 2; // ceil, avoiding overflow
            Checkpoint memory cp = checkpoints[account][center];
            if (cp.fromBlock == blockNumber) {
                return cp.votes;
            } else if (cp.fromBlock < blockNumber) {
                lower = center;
            } else {
                upper = center - 1;
            }
        }
        return checkpoints[account][lower].votes;
    }

    function _delegate(address delegator, address delegatee) internal {
        address currentDelegate = _delegates[delegator];
        uint256 delegatorBalance = balanceOf(delegator); // balance of underlying DSBs (not scaled);
        _delegates[delegator] = delegatee;

        emit DelegateChanged(delegator, currentDelegate, delegatee);

        _moveDelegates(currentDelegate, delegatee, delegatorBalance);
    }

    function _moveDelegates(
        address srcRep,
        address dstRep,
        uint256 amount
    ) internal {
        if (srcRep != dstRep && amount > 0) {
            if (srcRep != address(0)) {
                // decrease old representative
                uint32 srcRepNum = numCheckpoints[srcRep];
                uint256 srcRepOld = srcRepNum > 0 ? checkpoints[srcRep][srcRepNum - 1].votes : 0;
                uint256 srcRepNew = srcRepOld.sub(amount);
                _writeCheckpoint(srcRep, srcRepNum, srcRepOld, srcRepNew);
            }

            if (dstRep != address(0)) {
                // increase new representative
                uint32 dstRepNum = numCheckpoints[dstRep];
                uint256 dstRepOld = dstRepNum > 0 ? checkpoints[dstRep][dstRepNum - 1].votes : 0;
                uint256 dstRepNew = dstRepOld.add(amount);
                _writeCheckpoint(dstRep, dstRepNum, dstRepOld, dstRepNew);
            }
        }
    }

    function _writeCheckpoint(
        address delegatee,
        uint32 nCheckpoints,
        uint256 oldVotes,
        uint256 newVotes
    ) internal {
        uint32 blockNumber = safe32(block.number, 'DSB::_writeCheckpoint: block number exceeds 32 bits');

        if (nCheckpoints > 0 && checkpoints[delegatee][nCheckpoints - 1].fromBlock == blockNumber) {
            checkpoints[delegatee][nCheckpoints - 1].votes = newVotes;
        } else {
            checkpoints[delegatee][nCheckpoints] = Checkpoint(blockNumber, newVotes);
            numCheckpoints[delegatee] = nCheckpoints + 1;
        }

        emit DelegateVotesChanged(delegatee, oldVotes, newVotes);
    }

    function safe32(uint256 n, string memory errorMessage) internal pure returns (uint32) {
        require(n < 2**32, errorMessage);
        return uint32(n);
    }

    function getChainId() internal pure returns (uint256) {
        uint256 chainId;
        assembly {
            chainId := chainid()
        }
        return chainId;
    }
}


// This is the new game prediction contract

pragma solidity 0.6.12;
pragma experimental ABIEncoderV2;








contract DSBDollar2Win is Ownable, ReentrancyGuard {
    using SafeMath for uint256;
    using SafeBEP20 for IBEP20;

    // Info of each game.
    struct ItemInfo {
        uint itemType; // 0: real, 1: virtual
        uint tokenType; // 0: USD, 1: DSB, 2:other

        uint256 price; // item's price in USD or other token, watch decimals
        uint256 minPricePerRound; // double price?

        IBEP20 acceptToken; // only works if token type === 2
        bool online; // false: offline, true: online

        uint256 currentLottery; // current lottery Id
        uint256 lastLottery;    // last Lottery id
    }

    // struct for lottery
    struct LotteryInfo{
        uint256 itemId; // id for item
        uint256 lotteryIndex; // item lottery index
        
        uint256 fee;
        uint256 priceIn;    // price put in
        
        uint256 winnerId;
        address winner;    // winner address
        
        bool stopped;        // emergency stop for this lottery
        bool collected;      // user collected item

        uint startTime; // start time
        uint endTime;   // write by user
        uint collectedTime;   // write by user

        uint256 finalETH;
        uint256 finalBTC;
    }

    // Info of each user.
    struct UserInfo {
        uint256 priceIn;
        uint256 fee;
        bool collected;
    }

    // Info about every put
    struct UserPriceInfo {
        uint256 price;
        IBEP20 token;
        address user;
        uint time;
    }

    // Winner Infomation
    struct WinnerInfo {
        uint256 lotteryIndex;
        uint256 itemId;
        address user;
        uint time;
    }

    // tokens!
    DSBToken public DSB;

    IBEP20 public USDT;
    IBEP20 public BUSD;
    IBEP20 public USDC;

    // lottory fee token
    IBEP20 public feeToken;
    // lottory fee
    uint256 public feePerDeposit;
    // emergency stop
    bool stopAll;
    // default delay for a new lottery openning
    uint public defaultDelay = 300;

    // Dev address.
    address public devAddress;
    // Info of each items.
    ItemInfo[] public itemsInfo;
    LotteryInfo[] public lotteriesInfo;
    WinnerInfo[] public winnerInfo;

    // all lotteryInfo for each item.
    mapping(uint256 => uint256[]) public lotteryIdInfo;
    // users info for each lottory. In: lottery ID and user address, Out UserInfo
    mapping(uint256 => mapping(address => UserInfo)) public usersInfo;
    // users put price actions info for each lottory.
    mapping(uint256 => UserPriceInfo[]) public lotteryPricePut;
    // user histories.
    mapping(address => uint256[]) public usersHistory;

    // operators
    mapping(address => bool) public operators;
    // blacklist
    mapping(address => bool) public blacklist;

    // chain link
    AggregatorV3Interface internal priceFeedETH;
    AggregatorV3Interface internal priceFeedBTC;

    // Events
    event AddLottery(
        address indexed operator,
        uint256 indexed itemId,
        uint256 indexed lotteryId,
        uint256 lotteryIndex,
        uint256 lotteryRounds,
        uint time
    );
    event Deposit(
        address indexed user,
        uint256 indexed lotteryId,
        uint256 indexed itemId,
        IBEP20 token,
        uint256 price,
        uint256 userTotalDepositInThisLottery,
        bool isEnd,
        uint time
    );
    event AnnounceWinner(
        uint256 indexed lotteryId,
        uint256 indexed itemId,
        address indexed winner,
        uint256 userTotalDepositInThisLottery,
        uint256 winnerId,
        uint time
    );
    event UpdateBlacklist(
        address operator,
        bool status
    );
    event CollectedItem(
        uint256 indexed lotteryId,
        address indexed winner,
        uint time
    );
    event OperatorUpdated(address indexed operator, bool indexed status);

    constructor(DSBToken _DSB, IBEP20 _usdt, IBEP20 _busd, IBEP20 _usdc) public {
        DSB = _DSB;
        USDT = _usdt;
        BUSD = _busd;
        USDC = _usdc;

        devAddress = msg.sender;
        operators[msg.sender] = true;
        stopAll = false;

        // Test net!!!
        priceFeedETH = AggregatorV3Interface(0x143db3CEEfbdfe5631aDD3E50f7614B6ba708BA7);
        priceFeedBTC = AggregatorV3Interface(0x5741306c21795FdCBb9b265Ea0255F499DFe515C);
    }

    modifier onlyOperator {
        require(operators[msg.sender], 'Operator: caller is not a operator');
        _;
    }
    modifier blockBlackListUser {
        require(!blacklist[msg.sender], 'Operator: caller is in the blacklist');
        _;
    }
    // Update the status of the operator
    function updateOperator(address _operator, bool _status) external onlyOwner {
        operators[_operator] = _status;
        emit OperatorUpdated(_operator, _status);
    }
    // Update the blacklist
    function updateBlacklist(address _operator, bool _status) external onlyOwner {
        blacklist[_operator] = _status;
        emit UpdateBlacklist(_operator, _status);
    }
    // EMERGENCY STOP ALL LOTTERIES
    function stopAllLotteries(bool _status) external onlyOwner {
        stopAll = _status;
    }
    // Update the fee
    function updateFee(IBEP20 _token, uint256 _fee) external onlyOwner {
        feeToken = _token;
        feePerDeposit = _fee;
    }
    // Update default delay time
    function updateDelay(uint _delay) external onlyOwner {
        defaultDelay = _delay;
    }

    function itemLength() external view returns (uint256) {
        return itemsInfo.length;
    }
    function lotteryLength() external view returns (uint256) {
        return lotteriesInfo.length;
    }
    function lotteryLengthWithItemId(uint256 _itemId) external view returns (uint256) {
        return lotteryIdInfo[_itemId].length;
    }
    function lotteryPricePutLengthWithLotteryId(uint256 _lotteryId) external view returns (uint256) {
        return lotteryPricePut[_lotteryId].length;
    }
    function getLotteriesWithItemId(uint256 _itemId) external view returns (uint[] memory) {
        uint length = lotteryIdInfo[_itemId].length;
        uint[] memory lotteries = new uint[](length);
        for (uint index = 0; index < length; index++) {
            lotteries[index] = lotteryIdInfo[_itemId][index];
        }
        return lotteries;
    }
    function getLastLotteriesWithItemId(uint256 _itemId, uint256 _amount) external view returns (uint[] memory) {
        uint length = lotteryIdInfo[_itemId].length;
        uint[] memory lotteries = new uint[](length<=_amount ? length : _amount);
        uint i = 1;
        for (uint index = length - 1;  index >= 0 ; index--) {
            lotteries[i-1] = lotteryIdInfo[_itemId][index];
            i++;
            if(i>_amount){
                break;
            }
        }
        return lotteries;
    }
    function getLotteryChancesHistory(uint256 _lotteryId, uint256 _amount) external view returns (UserPriceInfo[] memory) {
        uint length = lotteryPricePut[_lotteryId].length;
        UserPriceInfo[] memory chances = new UserPriceInfo[](length<=_amount ? length : _amount);
        uint i = 1;
        for (uint index = length - 1;  index >= 0 ; index--) {
            UserPriceInfo storage t = lotteryPricePut[_lotteryId][index];
            chances[i-1] = UserPriceInfo(t.price, t.token, t.user, t.time);
            i++;
            if(i>_amount){
                break;
            }
        }
        return chances;
    }
    function getLastLotteryWithItemId(uint256 _itemId) external view returns (LotteryInfo memory) {
        uint length = lotteryIdInfo[_itemId].length;
        LotteryInfo storage t = lotteriesInfo[lotteryIdInfo[_itemId][length - 1]];
        LotteryInfo memory ret = LotteryInfo(t.itemId, t.lotteryIndex, t.fee, t.priceIn, t.winnerId, t.winner, t.stopped, t.collected, t.startTime, t.endTime, t.collectedTime, t.finalETH, t.finalBTC);
        return ret;
    }
    function getUserHistory(address _address) external view returns (uint[] memory) {
        uint length = usersHistory[_address].length;
        uint[] memory histories = new uint[](length);
        for (uint256 i = 0; i < length; i++) {
            histories[i] = usersHistory[_address][i];
        }
        return histories;
    }
    function getLastWinnersInfo(uint256 _amount) external view returns (WinnerInfo[] memory) {
        uint length = winnerInfo.length;
        WinnerInfo[] memory winners = new WinnerInfo[](length<=_amount ? length : _amount);
        uint i = 1;
        for (uint index = length - 1;  index >= 0 ; index--) {
            WinnerInfo storage t = winnerInfo[index];
            winners[i-1] = WinnerInfo(t.lotteryIndex, t.itemId, t.user, t.time);
            i++;
            if(i>_amount){
                break;
            }
        }
        return winners;
    }

    // Add a new item. Can be only called by the operators.
    function addItem(
        uint _itemType,
        uint _tokenType,
        uint256 _price,
        uint256 _minDeposit,
        IBEP20 _acceptToken,
        bool _addLottery
    ) public onlyOperator {
        require(_price > 0, 'addNewItem: invalid price');
        require(_minDeposit > 0, 'addNewItem: invalid round price');
        if(_tokenType==2){
            require(address(_acceptToken) != address(0), 'addNewItem: token type is 2 but you did not fill out accept token');
        }

        itemsInfo.push(
            ItemInfo({
                price: _price,
                minPricePerRound: _minDeposit,
                itemType: _itemType,
                tokenType: _tokenType,
                acceptToken: _acceptToken,
                online: true,
                currentLottery: 0,
                lastLottery: 0
            })
        );

        uint256 iid = itemsInfo.length - 1;

        if(_addLottery){
            // add an init lottery for the item
            // ItemInfo storage currentItem = itemsInfo[iid];
            addLottery(iid, getBlockTimestamp());
            // watch out!
        }
    }

    // add a new lottory
    function addLotteryManually(
        uint256 _itemId,
        uint _startTime
    )public onlyOperator returns (uint256) {
        return addLottery(_itemId, _startTime);
    }

    function addLottery(
        uint256 _itemId,
        uint _startTime
    ) private returns (uint256) {
        if(_startTime==0){
            _startTime = getBlockTimestamp();
        }
        uint256[] storage t = lotteryIdInfo[_itemId];
        uint256 lotteryIndex = t.length == 0 ? 0 : t.length - 1;
        lotteriesInfo.push(
            LotteryInfo({
                itemId: _itemId,
                lotteryIndex: lotteryIndex,
                fee:0,
                priceIn: 0,
                winnerId: 0,
                winner: address(0),
                stopped: false,
                collected: false,
                startTime: _startTime,
                endTime: 0,
                collectedTime: 0,
                finalETH: 0,
                finalBTC: 0
            })
        );
        uint256 _Lid = lotteriesInfo.length - 1;
        lotteryIdInfo[_itemId].push(_Lid);
        ItemInfo storage currentItem = itemsInfo[_itemId];
        //should - 1 for index in web
        currentItem.lastLottery = currentItem.currentLottery;
        currentItem.currentLottery = _Lid + 1; 

        emit AddLottery(
            address(msg.sender), _itemId, _Lid, lotteryIdInfo[_itemId].length, currentItem.price / currentItem.minPricePerRound, _startTime
        );

        return lotteriesInfo.length;
    }

    // set the exist lottory info
    function setLotteryInfo(
        uint256 _lotteryId,
        uint _startTime,
        bool _stop
    ) public onlyOperator {
        if(_startTime==0){
            _startTime = getBlockTimestamp();
        }
        LotteryInfo storage lottery = lotteriesInfo[_lotteryId];
        lottery.startTime = _startTime;
        lottery.stopped = _stop;
    }
    
    // Update the exist item info. Can only be called by the owner.
    function setItemInfo(
        uint256 _itemId,
        uint _itemType,
        uint _tokenType,
        uint256 _price,
        IBEP20 _acceptToken,
        bool _offline
    ) public onlyOperator {
        require(_price > 0, 'setItemInfo: invalid price');
        if(_tokenType==2){
            require(address(_acceptToken) != address(0), 'setItemInfo: token type is 2 but you did not fill out accept token');
        }
        ItemInfo storage item = itemsInfo[_itemId];

        item.price = _price;
        item.itemType = _itemType;
        item.tokenType = _tokenType;
        item.acceptToken = _acceptToken;
        item.online = !_offline;
    }


    // Deposit price
    function depositPrice(
        uint256 _lotteryId,
        uint256 _price,
        IBEP20 _token
    ) public nonReentrant blockBlackListUser {
        require(_price > 0, 'depositPrice: invalid price');
        LotteryInfo storage lottery = lotteriesInfo[_lotteryId];
        require(lottery.endTime == 0 && lottery.winner == address(0), 'depositPrice: this lottery is ended');
        require(!lottery.stopped && !stopAll, 'depositPrice: this lottery has been stopped');
        ItemInfo storage item = itemsInfo[lottery.itemId];
        require(lottery.priceIn.add(_price) <= item.price, 'depositPrice: deposit price shold not be greater than the item original price.');
        require(_price % item.minPricePerRound == 0, 'depositPrice: error deposit price, should be enter mutiplied by item.minPricePerRound');
        UserInfo storage user = usersInfo[_lotteryId][msg.sender];

        if(item.tokenType == 0){
            require(_token == USDT || _token == BUSD || _token == USDC, 'depositPrice(TT0): do not accpet this type of token');
        } else if (item.tokenType == 1){
            require(_token == DSB, 'depositPrice(TT1): only accpet DSB');
        } else {
            require(_token == item.acceptToken, 'depositPrice(TT2): do not accpet this type of token');
        }

        require(lottery.priceIn.add(_price) <= item.price, 'depositPrice: deposit price shold not be greater than the items original price.');
        
        uint currentTime = getBlockTimestamp();

        _token.safeTransferFrom(address(msg.sender), address(this), _price);
        lotteryPricePut[_lotteryId].push(UserPriceInfo({
            price: _price,
            token: _token,
            user: address(msg.sender),
            time: currentTime
        }));
        lottery.priceIn = lottery.priceIn.add(_price);
        user.priceIn = user.priceIn.add(_price);

        bool writeHistory = false;
        for (uint index = 0; index < usersHistory[address(msg.sender)].length; index++) {
            if(usersHistory[address(msg.sender)][index] == _lotteryId){
                break;
            }
        }
        if(writeHistory){
            usersHistory[address(msg.sender)].push(_lotteryId);
        }

        // fee ~ DSB ???
        if(feePerDeposit > 0){
            feeToken.safeTransferFrom(address(msg.sender), devAddress, feePerDeposit);
            lottery.fee = lottery.fee.add(feePerDeposit);
            user.fee = user.fee.add(feePerDeposit);
        }

        emit Deposit(address(msg.sender), _lotteryId, lottery.itemId, _token, _price, user.priceIn, lottery.priceIn >= item.price, currentTime);

        if(lottery.priceIn >= item.price){
            // lottery should end
            settlementLottery(_lotteryId, currentTime);
        }

    }

    // end this lottory
    function settlementLotteryManually(
        uint256 _lotteryId,
        uint currentTime
    )public onlyOperator {
        settlementLottery(_lotteryId, currentTime);
    }

    function settlementLottery(
        uint256 _lotteryId,
        uint currentTime
    ) private {
        LotteryInfo storage lottery = lotteriesInfo[_lotteryId];
        ItemInfo storage item = itemsInfo[lottery.itemId];
        if(lottery.priceIn < item.price){
            return;
        }
        if(currentTime == 0){
            currentTime = getBlockTimestamp();
        }
        lottery.finalBTC = uint256(getThePrice(priceFeedBTC));
        lottery.finalETH = uint256(getThePrice(priceFeedETH));

        uint roundTimes = item.price / item.minPricePerRound;

        uint256 winnerId = lottery.finalBTC * lottery.finalETH;
        winnerId = winnerId + currentTime;
        winnerId = winnerId % roundTimes;
        // uint256 winnerId = lottery.finalBTC.mul(lottery.finalETH).add(currentTime).mod(roundTimes);

        address winner = address(0);

        UserPriceInfo[] storage pricePuts = lotteryPricePut[_lotteryId];
        uint256 lastPrice = 0;
        for (uint256 i = 0; i < pricePuts.length; i++) {
            if(winnerId >= lastPrice && winnerId < lastPrice + pricePuts[i].price){
                winner = pricePuts[i].user;
                break;
            }
            lastPrice = lastPrice.add(pricePuts[i].price);
        }
        require(winner!=address(0), 'settlementLottery: no winner, waiting for team to work on this ??!');

        lottery.winnerId = winnerId;
        lottery.winner = winner;
        lottery.endTime = currentTime;


        winnerInfo.push(
            WinnerInfo({
                lotteryIndex: _lotteryId,
                itemId: lottery.itemId,
                user: winner,
                time: currentTime
            })
        );
        
        emit AnnounceWinner(_lotteryId, lottery.itemId, winner, usersInfo[_lotteryId][winner].priceIn, winnerId, currentTime);

        // start a new lottery

        if(item.online){
            addLottery(lottery.itemId, currentTime + defaultDelay);
        }
        
    }

    // Collect Item

    function collectItem(
        uint256 _lotteryId
    ) public nonReentrant blockBlackListUser {
        LotteryInfo storage lottery = lotteriesInfo[_lotteryId];
        require(lottery.winnerId != 0 && lottery.winner == address(msg.sender), 'collectItem: you are not the winner.');
        UserInfo storage user = usersInfo[_lotteryId][msg.sender];
        require(lottery.collected == false && user.collected == false, 'collectItem: colleced.');

        uint currentTime = getBlockTimestamp();

        lottery.collected = true;
        lottery.collectedTime = currentTime;

        user.collected = true;

        emit CollectedItem(_lotteryId, address(msg.sender), currentTime);
    }

    // Contracts owner can transfer tokens from contract to the lottery item buyer
    function transferToken(
        IBEP20 _token,
        address _to,
        uint256 _amount
    ) public onlyOwner {
        require(_to != address(0) && _amount > 0, 'transferToken: something wrong.');
        _token.safeTransfer(_to, _amount);
    }

    /**
     * Returns the latest price
     */
    function getThePrice(AggregatorV3Interface _feed) private view returns (int) {
        (
            uint80 roundID, 
            int price,
            uint startedAt,
            uint timeStamp,
            uint80 answeredInRound
        ) = _feed.latestRoundData();
        return price;
    }

    function getBlockTimestamp() internal view returns (uint) {
        return block.timestamp;
    }
}