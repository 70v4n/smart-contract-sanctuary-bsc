        emit SwapAndLiquifyEnabledUpdated(_enabled);
    }
    receive() external payable {}


    function _getValues(uint256 tAmount, bool isBuy, bool isSell) private pure returns (uint256, uint256, uint256) {
    
        uint256 marketingFee = calculateMarketingFee(tAmount,isBuy,isSell);
        uint256 liquidityFee = calculateLiquidityFee(tAmount,isBuy, isSell);
        uint256 taxedAmount = tAmount.sub(marketingFee).sub(liquidityFee);
        return (taxedAmount, marketingFee, liquidityFee);
    }

    
    function _takeLiquidity(uint256 tLiquidity) private {
        _balances[address(this)] = _balances[address(this)].add(tLiquidity);
    }

    function _takeMarketingFee(uint256 marketingFee) private {
        _balances[MarketingWallet] = _balances[MarketingWallet].add(marketingFee);
    }
    
    function calculateMarketingFee(uint256 _amount,bool isBuy, bool isSell) private pure  returns (uint256) {
        if(isSell){
            return _amount.mul(4).div(10**2);
        } else if(isBuy){
			return _amount.mul(4).div(10**2);
        } else { 
            return 0;
        }     
    }

    function calculateLiquidityFee(uint256 _amount,bool isBuy, bool isSell) private pure  returns (uint256) {

        if(isSell){
            return _amount.mul(8).div(10**2);
        } else if(isBuy){
			return _amount.mul(6).div(10**2);
        } else { 
            return 0;
        }     

    }
    
    
    function isExcludedFromFee(address account) public view returns(bool) {
        return _isExcludedFromFee[account];
    }

    function _approve(address owner, address spender, uint256 amount) private {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) private {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        require(amount > 0, "Transfer amount must be greater than zero");
        if(from != owner() && to != owner())
            require(amount <= _maxTxAmount, "Transfer amount exceeds the maxTxAmount.");
        uint256 contractTokenBalance = balanceOf(address(this));
        
        if(contractTokenBalance >= _maxTxAmount)
        {
            contractTokenBalance = _maxTxAmount;
        }
        
        bool overMinTokenBalance = contractTokenBalance >= numTokensSellToAddToLiquidity;
        if (
            overMinTokenBalance &&
            !inSwapAndLiquify &&
            from != uniswapV2Pair &&
            swapAndLiquifyEnabled
        ) {
            contractTokenBalance = numTokensSellToAddToLiquidity;
            swapAndLiquify(contractTokenBalance);
        }

    

        //Manually Excluded adresses are transfering tax and lock free
        bool isExcluded = (_isExcludedFromFee[from] || _isExcludedFromFee[to]);

        bool isContractTransfer=(from==address(this) || to==address(this));
        
        //transfers between PancakeRouter and PancakePair are tax and lock free
        address pancakeRouter=address(uniswapV2Router);
        bool isLiquidityTransfer = ((from == uniswapV2Pair && to == pancakeRouter) 
        || (to == uniswapV2Pair && from == pancakeRouter));

        //differentiate between buy/sell/transfer to apply different taxes/restrictions
        bool isBuy=from==uniswapV2Pair|| from == pancakeRouter;
        bool isSell=to==uniswapV2Pair|| to == pancakeRouter;

        if(isContractTransfer || isLiquidityTransfer || isExcluded){
            _feelessTransfer(from, to, amount);
        }
        else{ 
            _taxedTransfer(from,to,amount,isBuy,isSell);
        }

    }

    
    function _taxedTransfer(address sender, address recipient, uint256 amount,bool isBuy,bool isSell) private{
        uint256 recipientBalance = _balances[recipient];
        uint256 senderBalance = _balances[sender];
        require(senderBalance >= amount, "Transfer exceeds balance");
         if(isSell){
            //Sells can't exceed the sell limit(21,000 Tokens at start, can be updated to circulating supply)
            require(amount<=_maxTxAmount,"Dump protection");
        } else if(isBuy){
            //Checks If the recipient balance(excluding Taxes) would exceed Balance Limit
            require(recipientBalance+amount<=_maxWalletAmount,"whale protection");
			require(amount <= _maxTxAmount,"Tx amount exceeding max buy amount");
        }
        else{
          //Checks If the recipient balance(excluding Taxes) would exceed Balance Limit
            require(recipientBalance+amount<=_maxWalletAmount,"whale protection");
        }
        (uint256 afterTaxAmount, uint256 liquidityFee, uint256 marketingFee) = _getValues(amount, isBuy, isSell);
     

        _balances[sender] = _balances[sender].sub(afterTaxAmount);
        _balances[recipient] = _balances[recipient].add(afterTaxAmount);        
        _takeLiquidity(liquidityFee);
        _takeMarketingFee(marketingFee);
        emit Transfer(sender,recipient,afterTaxAmount);

    }

    //Feeless transfer only transfers and autostakes
    function _feelessTransfer(address sender, address recipient, uint256 amount) private{
        uint256 senderBalance = _balances[sender];
        require(senderBalance >= amount, "Transfer exceeds balance");
    
        _balances[sender] = _balances[sender].sub(amount);
        _balances[recipient] = _balances[recipient].add(amount);
        
        emit Transfer(sender,recipient,amount);

    }

    function swapAndLiquify(uint256 contractTokenBalance) private lockTheSwap {
        uint256 half = contractTokenBalance.div(2);
        uint256 otherHalf = contractTokenBalance.sub(half);
        uint256 initialBalance = address(this).balance;
        swapTokensForEth(half);
        uint256 newBalance = address(this).balance.sub(initialBalance);
        addLiquidity(otherHalf, newBalance);
    
        emit SwapAndLiquify(half, newBalance, otherHalf);
    }

    function swapTokensForEth(uint256 tokenAmount) private {
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = uniswapV2Router.WETH();

        _approve(address(this), address(uniswapV2Router), tokenAmount);
        uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0,
            path,
            address(this),
            block.timestamp
        );
    }

    function addLiquidity(uint256 tokenAmount, uint256 ethAmount) private {
        _approve(address(this), address(uniswapV2Router), tokenAmount);
        uniswapV2Router.addLiquidityETH{value: ethAmount}(
            address(this),
            tokenAmount,
            0,
            0,
            owner(),
            block.timestamp
        );
    }

}