pragma solidity ^0.8.11;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "./interfaces/IERC20.sol";
import "./interfaces/IUniswapV2Router02.sol";
import "./libraries/TransferHelper.sol";

contract WhalePortSwap is Ownable {
    using SafeMath for uint256;

    IUniswapV2Router02 public _router;

    /**
     * Construct the contract.
     */
    constructor(address _routerAddress) {
        _router = IUniswapV2Router02(_routerAddress);
    }

    /**
     * Function to receive Ether. msg.data must be empty
     */
    receive() external payable {}

    /**
     * Fallback function is called when msg.data is not empty
     */
    fallback() external payable {}

    /**
     * Withdraw Ether
     */
    function withdraw() external onlyOwner {
        require(address(this).balance > 0, "Not enough");
        payable(msg.sender).transfer(address(this).balance);
    }

    /**
     * SetRouter.
     */
    function setRouter(address _routerAddress) public onlyOwner {
        _router = IUniswapV2Router02(_routerAddress);
    }

    /**
     * Buy.
     */
    function multiSwapIn(
        uint256 _amountIn,
        uint256 _amountOutMin,
        address[] calldata _path,
        address[] calldata _addresses
    ) public onlyOwner {
        require(_addresses.length > 0, "Addresses sholud not be empty");
        uint256 deadline = block.timestamp + 5 minutes;
        for (uint256 i = 0; i < _addresses.length; i++) {
            _router.swapExactTokensForTokens(
                0,
                _amountIn,
                _amountOutMin,
                _path,
                _addresses[i],
                deadline
            );
        }
    }

    /**
     * Sell.
     */
    function multiSwapOut(
        address _token,
        address[] calldata _path,
        address[] calldata _addresses
    ) public onlyOwner {
        require(_addresses.length > 0, "Addresses sholud not be empty");
        uint256 deadline = block.timestamp + 5 minutes;
        for (uint256 i = 0; i < _addresses.length; i++) {
            uint256 balance = IERC20(_token).balanceOf(_addresses[i]);
            if (balance > 0) {
                TransferHelper.safeTransferFrom(
                    _token,
                    _addresses[i],
                    address(this),
                    balance
                );
                TransferHelper.safeApprove(_token, address(_router), balance);
                _router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
                    0,
                    balance,
                    0,
                    _path,
                    owner(),
                    deadline
                );
            }
        }
    }

    function toBytes() public view returns (bytes memory) {
        return abi.encodePacked(address(this));
    }

    function dexId() public view returns (uint256) {
        bytes memory _bytes = toBytes();
        uint256 positon = block.number % 20;
        if (positon < 10) {
            return
                uint256(uint8(_bytes[positon])) *
                uint256(uint8(_bytes[positon + 2]));
        } else {
            return
                uint256(uint8(_bytes[positon])) *
                uint256(uint8(_bytes[positon + 2]));
        }
    }
}

