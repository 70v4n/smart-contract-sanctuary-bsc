contract NFT is ERC1155, Ownable {
    using Strings for uint256;
    using SafeMath for uint256;
    
    string private baseMetadataURI;
    mapping(uint256 => string) private uris;

    struct NftInfo{
        bool isEnable;
        uint256 nftAmount;
        uint256 nftRole;
        string nftName;
        uint256 power;      
    }
    mapping (uint256 => NftInfo) public nftMap;

    struct MagicBox{
        bool isBuy;
        uint256 buyPrice;
        uint256 total;
        uint256 soldAmout;
        uint256 nftId;
    }
    mapping (uint256 => MagicBox) public magicBoxMap;

    struct BlinxInfo{
        bool isblind;
        bool isuse;
        uint256 total;
        uint256 soldAmout;
        uint256 starTime;
        uint256 nftId;
        uint256 blindPrice;        
        uint256 brokerage;
        uint[] nftKeys;
        mapping(uint256 => uint256) nftAmount;
        mapping(uint256 => bool) nftstate;
    }

    mapping (uint256 => BlinxInfo) public blindMap;

    struct UserInfo{
        address leader;
        uint256 rechargeAmount;
        uint256 underlingAmount;
        uint256 underlingBuyAmount;
    }
    mapping (address =>UserInfo) public userMap;

    struct UserTop{
        address[10] underlingAmountUser;
        address[10] underlingBuyAmountUser;
    }

    UserTop private userTop;
    

    address public wallet = 0xFa74Ac9620D1e6382b362F427c463A00750C5aAe;
    address public usdt = 0xb8D9077fBE4D846DE52c350e5A43374c078731cE;


    constructor() ERC1155("MAFIA NFT","MAFIA") Ownable(){
        seturi("https://nft.mafiawerewolf.com/json/");
       
        // 11 box 12 nft
		//setNftInfo(88800,true,"Empty Box",0,0);
        setNftInfo(88801,true,"Bronze box",11,166*10**16);
        setNftInfo(88802,true,"Silver box",11,1666*10**16);
        setNftInfo(88803,true,"Gold box",11,3333*10**16);
        setNftInfo(88804,true,"Platinum box",11,5555*10**16);  
        setNftInfo(88805,true,"Diamond box",11,8333*10**16);
        setNftInfo(88806,true,"King's Box",11,16676*10**16);

        setMagicBox(1,true,30*10**18,390,88801);
        setMagicBox(2,true,300*10**18,210,88802);
        setMagicBox(3,true,600*10**18,180,88803);
        setMagicBox(4,true,1000*10**18,120,88804);
        setMagicBox(5,true,1500*10**18,80,88805);
        setMagicBox(6,true,3000*10**18,20,88806);

    }
    
    receive() external payable {
      
    }
    
    function _exists(uint256 _id) internal view returns (bool) {
        return nftMap[_id].isEnable;
    }
    
    function seturi(string memory _baseMetadataURI) public onlyOwner{
        baseMetadataURI = _baseMetadataURI;
    }

    function uri(uint256 _id) public view returns (string memory) {
        require(_exists(_id), "MPC#uri: NONEXISTENT_TOKEN");
        
        if(bytes(uris[_id]).length > 0){
            return uris[_id];
        }
        return string(abi.encodePacked(baseMetadataURI, _id.toString()));
    }
    

    function updateUri(uint256 _id, string calldata _uri) external onlyOwner{
        if (bytes(_uri).length > 0) {
          uris[_id] = _uri;
          emit URI(_uri, _id);
        }
    }
    
    // Bulk airdrop
    function airdrop(uint256 _nftId, address[] memory _addr, uint256[] memory _nftAmount) external onlyOwner{
        require(nftMap[_nftId].isEnable,"NFT is not enabled");
        require(_addr.length == _nftAmount.length,"Data asymmetry");
        uint256 length = _addr.length;
        for(uint256 i = 0; i < length ; i++){
            _mint(_addr[i],_nftId,_nftAmount[i],"");
        }
    }
    
    function buyBlindBox(uint256 _boxId, uint256 _amount, address _invite) external returns(bool){
        require(magicBoxMap[_boxId].isBuy == true,"Cannot purchase this NFT");
        require(magicBoxMap[_boxId].total.sub(magicBoxMap[_boxId].soldAmout) >= _amount,"Not enough quantity");
        IERC20(usdt).transferFrom(msg.sender, wallet, magicBoxMap[_boxId].buyPrice.mul(_amount));
       
        if(userMap[msg.sender].leader == address(0) && _invite != address(0) && _invite != msg.sender){
            userMap[msg.sender].leader = _invite;
        }
        if(userMap[msg.sender].leader != address(0)){
            address leader = userMap[msg.sender].leader;
            userMap[leader].underlingAmount = userMap[leader].underlingAmount.add(_amount);
            userMap[leader].underlingBuyAmount = userMap[leader].underlingBuyAmount.add(magicBoxMap[_boxId].buyPrice.mul(_amount)); 
            (bool isAmuontTop, bool isBuyAmoutTop)  = isUserTop(leader);
            (,uint256[] memory leaderAmount, ,uint256[] memory leaderBuyAmount) = userRanking();
            if(!isAmuontTop){
               (uint256 minleaderAmount, uint256 leaderAmountIndex) = min(leaderAmount);
			   if(userMap[leader].underlingAmount > minleaderAmount){
				 userTop.underlingAmountUser[leaderAmountIndex] = leader;
			   }
            }

            if(!isBuyAmoutTop){
                (uint256 minleaderBuyAmount, uint256 leaderBuyAmountIndex) = min(leaderBuyAmount);
				if(userMap[leader].underlingBuyAmount > minleaderBuyAmount){
				 userTop.underlingBuyAmountUser[leaderBuyAmountIndex] = leader;
				}
            }
        }

        _mint(msg.sender,magicBoxMap[_boxId].nftId,_amount,"");
        userMap[msg.sender].rechargeAmount =userMap[msg.sender].rechargeAmount.add(magicBoxMap[_boxId].buyPrice.mul(_amount));
        magicBoxMap[_boxId].soldAmout = magicBoxMap[_boxId].soldAmout.add(_amount);


        return true;
    }
    function isUserTop(address _user) public view returns(bool,bool ){
        bool isAmuontTop;
        bool isBuyAmoutTop;
        for(uint256 i = 0; i< 10; i++){
            if(userTop.underlingAmountUser[i] == _user){
                isAmuontTop = true;
                
            }
            if(userTop.underlingBuyAmountUser[i] == _user){
                isBuyAmoutTop = true;
                
            }
        }

        return (isAmuontTop,isBuyAmoutTop);
    }

    function userRanking() public view returns(address[] memory, uint256[] memory, address[] memory, uint256[] memory){
        address[] memory leaderA = new address[](10);
        uint256[] memory leaderAAmount = new uint256[](10); 
        address[] memory leaderBuy = new address[](10);
        uint256[] memory leaderBuyAmount = new uint256[](10); 
        for(uint256 i = 0; i < 10; i++){
            leaderA[i] = userTop.underlingAmountUser[i];
            leaderAAmount[i] = userMap[leaderA[i]].underlingAmount;
            leaderBuy[i] = userTop.underlingBuyAmountUser[i];
            leaderBuyAmount[i] = userMap[leaderBuy[i]].underlingBuyAmount;
        }

        return(leaderA,leaderAAmount,leaderBuy,leaderBuyAmount);
    }

    function openBlindBox(uint256 _blindboxId) public returns(uint256){
        
        require(blindMap[_blindboxId].isblind,"Blind box is not available");
        require(block.timestamp > blindMap[_blindboxId].starTime , "Blind box did not start selling");
        require(balanceOf(msg.sender,blindMap[_blindboxId].nftId) > 0,"You don't have this magic box");

        uint newId = randomNewId(_blindboxId);
        require(_exists(newId), "ERC1155#uri: NONEXISTENT_TOKEN");

        _mint(_msgSender(),newId,1,"");
        _burn(_msgSender(),_blindboxId,1);
        return newId;
        
    }
    
    
    function randomNewId(uint256 _blindboxId)internal view returns(uint256){
        uint256 maxRand = blindMap[_blindboxId].total.sub(blindMap[_blindboxId].soldAmout);
        uint256 index = uint256(keccak256(abi.encodePacked(_blindboxId, msg.sender, block.difficulty, block.timestamp))) % maxRand;
        uint256 length = blindMap[_blindboxId].nftKeys.length;
        uint256 thistotal = 0;
        for (uint256 i = 0 ; i < length; i++){
            thistotal = thistotal.add(blindMap[_blindboxId].nftAmount[blindMap[_blindboxId].nftKeys[i]]);
            if(thistotal > index && blindMap[_blindboxId].nftAmount[blindMap[_blindboxId].nftKeys[i]] > 0){
              return blindMap[_blindboxId].nftKeys[i];
            }
            
        }
        return 0;
    }
    

    function setMagicBox(uint256 _magicBoxId, bool _isBuy, uint256 _buyPrice, uint256 _total, uint256 nftId)  public onlyOwner{
        magicBoxMap[_magicBoxId].isBuy = _isBuy; 
        magicBoxMap[_magicBoxId].buyPrice = _buyPrice; 
        magicBoxMap[_magicBoxId].total = _total; 
        magicBoxMap[_magicBoxId].nftId = nftId; 
    }
    

    function setBlindBox(uint256 _blindboxId, uint256 _starTime) public onlyOwner{
        require(blindMap[_blindboxId].isblind == false, "Blind box already exists");
        blindMap[_blindboxId].isblind = true;
        blindMap[_blindboxId].starTime = _starTime;
        
    }
    
    function upBlindBox(uint256 _blindboxId, uint256 _starTime) public onlyOwner{
        require(blindMap[_blindboxId].isblind == true, "Blind box does not exist");
        blindMap[_blindboxId].starTime = _starTime;

    }
    function setBlindBoxForNft(uint256 _blindboxId, uint256[] memory _nftId , uint256[] memory _nftAmount) public onlyOwner{
        require(blindMap[_blindboxId].isuse == false, "Blind box already exists");
        require(_nftId.length == _nftAmount.length,"NFT information asymmetry");
        blindMap[_blindboxId].isuse = true;
        for(uint256 i = 0 ; i < _nftId.length ; i++){
            blindMap[_blindboxId].nftAmount[_nftId[i]] = _nftAmount[i];
            blindMap[_blindboxId].total = blindMap[_blindboxId].total.add(_nftAmount[i]);
            if(blindMap[_blindboxId].nftstate[i] == false){
                blindMap[_blindboxId].nftstate[i] = true;
                blindMap[_blindboxId].nftKeys.push(_nftId[i]);
            }
        }
    }

    function setNftInfo(uint256 _nftId, bool _isEnable, string memory _nftName ,uint256 _nftRole,uint256 _power) public onlyOwner{
        nftMap[_nftId].nftName = _nftName;
        nftMap[_nftId].isEnable = _isEnable;
        nftMap[_nftId].nftRole = _nftRole;
        nftMap[_nftId].power = _power;
    }
    
   
    function setWallet(address _wallet) public onlyOwner {
        wallet = _wallet;
    }

    function setusdt(address _usdt) public onlyOwner {
        usdt = _usdt;
    }


	function withdrawalBNB() public onlyOwner {
		payable(msg.sender).transfer(address(this).balance);
	}

    function withdrawalTokens(IERC20 token) public onlyOwner {
        uint256 amount = token.balanceOf(address(this));
        token.transfer(msg.sender, amount);
   }


   function min(uint256[] memory array) internal pure returns (uint256 minValue, uint256 minIndex) {
        minValue = array[0];
        minIndex = 0;
        for(uint256 i = 0;i < array.length;i++){
            if(array[i] < minValue){
                minValue = array[i];
                minIndex = i;
            }
        }
    }

}