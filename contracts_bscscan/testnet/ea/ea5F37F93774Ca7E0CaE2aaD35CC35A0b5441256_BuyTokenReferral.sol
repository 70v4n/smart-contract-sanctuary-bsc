pragma solidity ^0.8.0;

import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

contract Referral is Context, AccessControl {
    using SafeMath for uint256;

    mapping(address => uint256) private balances; // in Tokens
    mapping(address => uint256) private lifetimeReferrals; // in life time referrals
    mapping(address => bool) private _authorized; //authorized to mint
    mapping(address => address) private referredBy;
    mapping(address => uint256) private referralTerms;

    bool isActive;

    bytes32 internal constant GOVERNANCE_ROLE = keccak256("GOVERNANCE_ROLE");

    event BalanceClaimed(address user, uint256 amount);
    event AddReferralBalance(
        address referrer,
        uint256 referrerAmount,
        address user,
        uint256 userAmount
    );
    event NewReferral(address referrer, address user, uint256 percent);

    uint256 public totalReferredAmount;

    address public rewardToken; //address for reward token

    modifier onlyAuth() {
        require(
            _authorized[_msgSender()] == true,
            "Authority: caller is not Authorized"
        );
        _;
    }

    constructor(address _rewardToken) {
        rewardToken = _rewardToken;
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(GOVERNANCE_ROLE, _msgSender());
    }

    /**
     * @dev Sets turns on and off the referral system;
     */
    function setStatus(bool _option)
        public
        onlyRole(GOVERNANCE_ROLE)
        returns (bool)
    {
        isActive = _option;
        return isActive;
    }

    /**
     * @dev Sets turns on and off the referral system;
     */
    function getStatus() external view returns (bool) {
        return isActive;
    }

    /**
     * @dev Sets the addresses of the contracts that can call the payreferral function ( our farm, pool, swap etc);
     */
    function setAuth(address _user, bool _option)
        public
        onlyRole(GOVERNANCE_ROLE)
    {
        _authorized[_user] = _option;
    }

    function checkAuth(address _user)
        public
        view
        onlyRole(GOVERNANCE_ROLE)
        returns (bool)
    {
        bool isAuth = _authorized[_user];
        return isAuth;
    }

    /**
     * @dev returns the _users current balance in rewards
     */
    function checkLifetimeReferrals(address _user)
        external
        view
        returns (uint256)
    {
        return lifetimeReferrals[_user];
    }

    function checkUserReferrer(address _user) external view returns (address) {
        return referredBy[_user];
    }

    /**
     * @dev returns the _users current balance in rewards
     */
    function checkCurrentBalance(address _user) public view returns (uint256) {
        require(_user != address(0));
        return balances[_user];
    }

    /**
     * @dev Allows the user to claim their referral balances
     */
    function claimBalance() public {
        require(balances[_msgSender()] > 0);
        require(isActive, "Referral System is not currently Active");
        uint256 balanceToClaim = balances[_msgSender()];
        balances[_msgSender()] = 0;
        IERC20(rewardToken).transfer(_msgSender(), balanceToClaim);
        emit BalanceClaimed(_msgSender(), balanceToClaim);
    }

    function addAmount(address _referred, uint256 _amount) external onlyAuth {
        uint256 percent = referralTerms[_referred];
        uint256 userReward = _amount.mul(percent).div(10000);
        uint256 commission = _amount.sub(userReward);
        uint256 referrerAmount = balances[referredBy[_referred]].add(
            commission
        );
        balances[referredBy[_referred]] = referrerAmount;
        balances[_referred] = balances[_referred].add(userReward);
        totalReferredAmount += _amount;
        emit AddReferralBalance(
            referredBy[_referred],
            referrerAmount,
            _referred,
            userReward
        );
    }

    function addReferral(
        address _referrer, // address that is referring user
        address _referred, // user that is being referred
        uint256 _percent //percent of reward going to user
    ) external onlyAuth {
        require(_percent <= 10000, "Can't be over 100%");
        require(_referrer != _referred, "User Cant recommend themselves");
        require(referredBy[_referred] == address(0), "Already has a referrer.");
        referredBy[_referred] = _referrer;
        referralTerms[_referred] = _percent;
        lifetimeReferrals[_referrer] += 1;
        emit NewReferral(_referrer, _referred, _percent);
    }
}

