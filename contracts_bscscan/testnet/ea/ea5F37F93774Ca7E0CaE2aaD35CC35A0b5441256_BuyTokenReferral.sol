pragma solidity ^0.8.0;

import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./utils/IBabyDogeV2Factory.sol";
import "./utils/IBabyDogeV2Pair.sol";
import "./utils/IBabyDogeV2Router02.sol";
import "./utils/IReferral.sol";
import "./Referral.sol";

// TODO WRITE COMMENTS

contract BuyTokenReferral is ReentrancyGuard, AccessControl {
    using SafeMath for uint256;
    bytes32 internal constant GOVERNANCE_ROLE = keccak256("GOVERNANCE_ROLE");

    address public ROUTER;
    address public TOKEN;
    address public REFERRAL;
    uint256 public rewardPercent;

    event Purchase(address buyer, uint256 amount);

    constructor(
        address _token,
        address _referral,
        address _router,
        uint256 _rewardPercent
    ) {
        TOKEN = _token;
        REFERRAL = _referral;
        ROUTER = _router;
        rewardPercent = _rewardPercent;
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(GOVERNANCE_ROLE, _msgSender());
    }

    // Receive ETH when swapping
    // TODO EMIT AN EVENT WHEN ETH ARRIVES
    receive() external payable {}

    function setReferral(address _referral) public onlyRole(GOVERNANCE_ROLE) {
        REFERRAL = _referral;
    }

    function setRouter(address _router) public onlyRole(GOVERNANCE_ROLE) {
        ROUTER = _router;
    }

    function setReward(uint256 _rewardPercent)
        public
        onlyRole(GOVERNANCE_ROLE)
    {
        rewardPercent = _rewardPercent;
    }

    function buyTokens(
        uint256 _amountOutMin,
        address _referrer,
        uint256 _percent
    ) external payable nonReentrant {
        address FromTokenContractAddress = IBabyDogeV2Router02(ROUTER).WETH();
        address ToTokenContractAddress = TOKEN;
        address[] memory path = new address[](2);
        path[0] = FromTokenContractAddress;
        path[1] = ToTokenContractAddress;
        uint256 balanceBefore = IERC20(TOKEN).balanceOf(_msgSender());
        uint256 deadline = block.timestamp.add(1200);

        IBabyDogeV2Router02(ROUTER)
            .swapExactETHForTokensSupportingFeeOnTransferTokens{
            value: msg.value
        }(_amountOutMin, path, _msgSender(), deadline);

        uint256 balanceAfter = IERC20(TOKEN).balanceOf(_msgSender());

        uint256 totalPurchasedAmount = balanceAfter - balanceBefore;
        emit Purchase(_msgSender(), totalPurchasedAmount);

        if (IReferral(REFERRAL).getStatus() && _referrer != address(0)) {
            if (
                IReferral(REFERRAL).checkUserReferrer(_msgSender()) ==
                address(0)
            ) {
                IReferral(REFERRAL).addReferral(
                    _referrer,
                    _msgSender(),
                    _percent
                );
            } else {
                uint256 amountOfReward = totalPurchasedAmount
                    .mul(rewardPercent)
                    .div(10000);
                IReferral(REFERRAL).addAmount(_msgSender(), amountOfReward);
            }
        }
    }

    function getQuote(uint256 amountA) public view returns (uint256) {
        address lpToken = IBabyDogeV2Factory(
            IBabyDogeV2Router02(ROUTER).factory()
        ).getPair(TOKEN, IBabyDogeV2Router02(ROUTER).WETH());
        (uint112 reserveA, uint112 reserveB, ) = IBabyDogeV2Pair(lpToken)
            .getReserves();

        return IBabyDogeV2Router02(ROUTER).quote(amountA, reserveA, reserveB);
    }
}

