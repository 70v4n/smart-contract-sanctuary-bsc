pragma solidity ^0.8.2;

import "./IBEP20.sol";
import "./IMarketplace.sol";
import "./Ownable.sol";
import "./ERC721.sol";
import "./ERC721Pausable.sol";
import "./IRobotCore.sol";
import "./ReentrancyGuard.sol";

contract RobotPackageGen1 is Ownable, ERC721Pausable, ReentrancyGuard {
    struct Package {
        address owner;
        uint256 packageId; // id of the package
        uint8 robotQuantity; // quantity of robots on this package
        PackageType packageType; //package type
        uint64 birthTime; // The timestamp from the block when this package came into existence.
    }

    enum PackageType {
        Default,
        First,
        Second,
        Third,
        Fourth,
        Fifth,
        Sixth
    }

    /*** STORAGE ***/
    /// @dev An array containing the package struct for all packages in existence. The ID
    ///  of each package is actually an index into this array.
    Package[] packages;

    event PackageCreatedGen1(
        address to,
        uint256 packId,
        uint8 packageType,
        uint256 date,
        bool isPresale
    );

    event UnpackedGen1(
        address owner,
        uint256 packId,
        uint8 packageType,
        uint256 date
    );

    /* PACKAGE VARIABLES */
    uint256 private nextPackageTokenId = 1;

    mapping(address => uint8) walletPackages;
    mapping(PackageType => uint8) private _packageTypeRobots;
    mapping(PackageType => uint256) private _packageTypePrice;
    mapping(address => bool) private banBlackList;

    uint256 private priceFirstPackage = 0.2 * 10**18;
    uint256 private priceSecondPackage = 0.5 * 10**18;
    uint256 private priceThirdPackage = 0.9 * 10**18;
    uint256 private openPackagePrice = 5 * 10**18;
    uint8 private totalPackagesOwner = 100;

    uint32 firstPackageCount;
    uint32 secondPackageCount;
    uint32 thirdPackageCount;
    uint32 firstPackageLimit = 400;
    uint32 secondPackageLimit = 200;
    uint32 thirdPackageLimit = 100;

    address public _mekaAddress;
    address public _robotCoreAddress;
    bool public canUnpack;
    bool public isPackageAvailable;

    constructor() ERC721("Mekabot Pkg", "MekabotPkg") {
        //Package Configs
        canUnpack = false;
        isPackageAvailable = false;

        setPackageTypePrice(PackageType.Fourth, priceFirstPackage);
        setPackageTypePrice(PackageType.Fifth, priceSecondPackage);
        setPackageTypePrice(PackageType.Sixth, priceThirdPackage);

        setPackageTypeRobots(PackageType.Fourth, 1);
        setPackageTypeRobots(PackageType.Fifth, 2);
        setPackageTypeRobots(PackageType.Sixth, 3);
    }

    function isBlacklisted(address addr) external view returns (bool) {
        return banBlackList[addr];
    }

    function setBlackList(address wallet, bool isBanned) external onlyOwner {
        banBlackList[wallet] = isBanned;
    }

    function setPackageLimit(uint8 limit) external onlyOwner {
        totalPackagesOwner = limit;
    }

    function setTokenAddress(address _addr) external onlyOwner {
        _mekaAddress = _addr;
    }

    function setRobotAddress(address robotAddress) external onlyOwner {
        _robotCoreAddress = robotAddress;
    }

    function setFirstPackageLimit(uint32 quantity) external onlyOwner {
        firstPackageLimit = quantity;
    }

    function setSecondPackageLimit(uint32 quantity) external onlyOwner {
        secondPackageLimit = quantity;
    }

    function setThirdPackageLimit(uint32 quantity) external onlyOwner {
        thirdPackageLimit = quantity;
    }

    function setPackageTypePrice(PackageType _packageType, uint256 amount)
        public
        onlyOwner
    {
        _packageTypePrice[_packageType] = amount;
    }

    function setPackageTypeRobots(PackageType _packageType, uint8 quantity)
        public
        onlyOwner
    {
        _packageTypeRobots[_packageType] = quantity;
    }

    function updateCanUnpack() external onlyOwner {
        canUnpack = !canUnpack;
    }

    function updatePackageAvailable() external onlyOwner {
        isPackageAvailable = !isPackageAvailable;
    }

    modifier canOpen() {
        require(canUnpack, "can not open");
        _;
    }

    modifier packageAvailable() {
        require(isPackageAvailable, "package unavailable");
        _;
    }

    function withdrawBalance() external onlyOwner {
        IBEP20(_mekaAddress).transfer(owner(), getBalance());
    }

    function withdrawBnb() external onlyOwner {
        uint256 amount = address(this).balance;

        (bool success, ) = payable(owner()).call{value: amount}("");
        require(success, "Failed to send Ether");
    }

    function getBalance() public view returns (uint256) {
        return IBEP20(_mekaAddress).balanceOf(address(this));
    }

    function getBnbBalance() public view returns (uint256) {
        return address(this).balance;
    }

    function pause() public whenNotPaused onlyOwner {
        _pause();
    }

    function unpause() public whenPaused onlyOwner {
        _unpause();
    }

    /* ROBOT PACKAGE */
    function _mintPackage(
        address _to,
        uint256 _tokenId,
        PackageType _packageType,
        bool isPresale
    ) private {
        Package memory _package = Package({
            owner: _to,
            packageType: _packageType,
            packageId: _tokenId,
            robotQuantity: _packageTypeRobots[_packageType],
            birthTime: uint64(block.timestamp)
        });

        _safeMint(_to, _tokenId, "");
        packages.push(_package);

        emit PackageCreatedGen1(
            _to,
            _package.packageId,
            uint8(_package.packageType),
            block.timestamp,
            isPresale
        );
    }

    function createPackage(PackageType _packageType, bool _isPresale)
        external
        payable
        nonReentrant
        packageAvailable
        whenNotPaused
    {
        require(
            walletPackages[msg.sender] < totalPackagesOwner,
            "wallet-package-limit"
        );
        require(
            _packageType >= PackageType.Fourth,
            "for gen 0 use other contract"
        );
        require(!isContract(msg.sender), "contract not allowed");
        require(msg.value == _packageTypePrice[_packageType], "INVALID AMOUNT");

        if (_packageType == PackageType.Fourth) {
            require(firstPackageCount < firstPackageLimit, "sold out");
            firstPackageCount++;
        } else if (_packageType == PackageType.Fifth) {
            require(secondPackageCount < secondPackageLimit, "sold out");
            secondPackageCount++;
        } else {
            require(thirdPackageCount < thirdPackageLimit, "sold out");
            thirdPackageCount++;
        }

        _mintPackage(msg.sender, nextPackageTokenId, _packageType, _isPresale);
        walletPackages[msg.sender] = walletPackages[msg.sender] + 1;
        nextPackageTokenId++;
    }

    function createPackageExtra(
        address _to,
        PackageType _packageType,
        bool _isPresale
    ) external onlyOwner {
        require(_packageType != PackageType.Default, "invalid package");
        require(_to != address(0), "ERC721: mint to the zero address");
        require(
            _packageType >= PackageType.Fourth,
            "for gen 0 use other contract"
        );
        _mintPackage(_to, nextPackageTokenId, _packageType, _isPresale);
        nextPackageTokenId++;
    }

    function openPackage(uint256 _amount, uint256 _tokenId)
        external
        canOpen
        whenNotPaused
    {
        require(!isContract(msg.sender), "can not be a contract");
        require(_amount == openPackagePrice, "INVALID AMOUNT");
        require(ownerOf(_tokenId) == msg.sender, "not the owner");
        require(
            IBEP20(_mekaAddress).transferFrom(
                msg.sender,
                address(this),
                _amount
            )
        );

        Package memory package = packages[_tokenId - 1];

        IRobotCore(_robotCoreAddress).createRobotFromBundle(
            msg.sender,
            uint8(package.packageType),
            package.robotQuantity
        );

        _burn(_tokenId);

        emit UnpackedGen1(
            msg.sender,
            _tokenId,
            uint8(package.packageType),
            block.timestamp
        );
    }

    /**
     * @dev Identify an address is user address or contract address
     */
    function isContract(address _address) private view returns (bool) {
        uint32 size;
        assembly {
            size := extcodesize(_address)
        }
        return (size > 0);
    }

    function getPackagesCount()
        external
        view
        returns (
            uint32 _firstPackageCount,
            uint32 _secondPackageCount,
            uint32 _thirdPackageCount
        )
    {
        _firstPackageCount = firstPackageCount;
        _secondPackageCount = secondPackageCount;
        _thirdPackageCount = thirdPackageCount;
    }

    function getPackage(uint256 _id)
        public
        view
        returns (Package memory package)
    {
        package = packages[_id - 1];
    }

    function transferFrom(
        address _from,
        address _receiver,
        uint256 _tokenId
    ) public override {
        require(!isContract(_receiver), "can not be contract");
        require(!banBlackList[_from], "address-blacklisted");
        require(!banBlackList[_receiver], "address-blacklisted");

        super.transferFrom(_from, _receiver, _tokenId);
    }
}

