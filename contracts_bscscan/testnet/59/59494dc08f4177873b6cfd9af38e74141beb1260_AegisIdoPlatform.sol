contract AegisIdoPlatform is Initializable, OwnableUpgradeable, ReentrancyGuardUpgradeable {
    using SafeERC20Upgradeable for IERC20Upgradeable;
    using ABDKMathQuad for uint256;
    using ABDKMathQuad for bytes16;
    // using AddressUpgradeable for address;

    event Received(address, uint256);
    event AllocationReserved(
        uint256 indexed idoId,
        address token,
        address indexed recipient,
        uint256 deposited,
        uint256 reserved
    );
    event OverpayRefundIssued(uint256 indexed idoId, address token, address indexed recipient, uint256 amount);
    event RefundIssued(uint256 indexed idoId, address token, address indexed recipient, uint256 amount);
    event AllocationDistributed(uint256 indexed idoId, address token, address indexed recipient, uint256 amount);

    AggregatorV3Interface internal priceFeed;
    IERC20Upgradeable internal platformToken;
    IERC20Upgradeable internal holdingToken;
    address internal whitelistAuthority;
    address payable internal contributionWallet;

    uint256 internal constant SECONDS_PER_DAY = 86400;
    uint256 internal constant BNB_DECIMALS = 18;

    struct IdoInfo {
        IERC20Upgradeable token;
        address tokenPool;
        uint256 decimals;
        uint256 swapRate;
        uint256 swapRateUsd;
        uint256 minCap;
        uint256 maxCap;
        uint256 minCapUsd;
        uint256 maxCapUsd;
        uint256 launchDateTs;
        uint256 claimDateTs;
        uint256 firstDayVestingBasisPts;
        uint256 monthlyVestingBasisPts;
        uint256 minBuy;
        uint256 maxBuy;
        uint256 minBuyUsd;
        uint256 maxBuyUsd;
        uint256 weiRaisedSoFar;
        bool isUsdBased;
        bool isUsdRateLocked;
        bool isWhitelistEnabled;
        bool isVestingEnabled;
        bool isRefunding;
    }

    struct IdoInfoInput {
        address tokenPool;
        uint256 decimals;
        uint256 swapRate;
        uint256 swapRateUsd;
        uint256 minCap;
        uint256 maxCap;
        uint256 minCapUsd;
        uint256 maxCapUsd;
        uint256 launchDateTs;
        uint256 claimDateTs;
        uint256 firstDayVestingBasisPts;
        uint256 monthlyVestingBasisPts;
        uint256 minBuy;
        uint256 maxBuy;
        uint256 minBuyUsd;
        uint256 maxBuyUsd;
        bool isUsdBased;
        bool isWhitelistEnabled;
        bool isVestingEnabled;
    }

    struct IdoInfoInputChanges {
        bool tokenPool;
        bool decimals;
        bool swapRate;
        bool swapRateUsd;
        bool minCap;
        bool maxCap;
        bool minCapUsd;
        bool maxCapUsd;
        bool launchDateTs;
        bool claimDateTs;
        bool firstDayVestingBasisPts;
        bool monthlyVestingBasisPts;
        bool minBuy;
        bool maxBuy;
        bool minBuyUsd;
        bool maxBuyUsd;
        bool isUsdBased;
        bool isWhitelistEnabled;
        bool isVestingEnabled;
    }

    struct UserContributions {
        uint256 weiContributed;
        uint256 tokensReserved;
        uint256 tokensDistributed;
    }

    struct RefundStatus {
        uint256 refundRemaining;
        uint256 refundClaimed;
    }

    struct SignedCirculatingSupply {
        uint256 circulatingSupply;
        uint256 supplyExpiry;
        uint8 v;
        bytes32 r;
        bytes32 s;
    }

    uint256 public lastIdoId;
    mapping(uint256 => mapping(address => UserContributions)) internal idoContributionsByAddress;
    mapping(uint256 => mapping(address => uint256)) internal allocationModifierBasisPointsByAddress;
    mapping(uint256 => mapping(address => uint256)) internal idoRefundsByAddress;
    mapping(uint256 => mapping(address => bool)) internal idoWhitelists;
    mapping(uint256 => IdoInfo) internal idosById;

    function initialize() public initializer {
        __Ownable_init();
        __ReentrancyGuard_init();
    }

    /**
     * Sets the platform token (Aegis Pad token) address. Used to determin what percentage the user holds,
     * which is used to calculate max allotment amount.
     */
    function setPlatformToken(IERC20Upgradeable token) external onlyOwner {
        platformToken = token;
    }

    /**
     * BNB/USD address in Kovan testnet: 0x9326BFA02ADD2366b30bacB125260Af641031331
     * BNB/USD address in mainnet: 0x0567F2323251f0Aab15c8dFb1967E4e8A7D42aeE
     *
     * ref: https://docs.chain.link/docs/get-the-latest-price/
     */
    function setPriceFeedAddress(AggregatorV3Interface _priceFeed) external onlyOwner {
        priceFeed = _priceFeed;
    }

    /**
     *  Returns (usd, decimal) for latest usd per bnb.
     *
     *  roundId uint80, answer int256, startedAt uint256, updatedAt uint256, answeredInRound uint80
     *  roundId|uint80 :  36893488147419273196
     *  answer|int256 :  63060110149
     *  startedAt|uint256 :  1636615513
     *  updatedAt|uint256 :  1636615513
     *  answeredInRound|uint80 :  36893488147419273196
     */
    function getUsdPerBnb() internal view returns (uint256, uint256) {
        /* (uint80 roundID, int256 price, uint256 startedAt, uint256 timeStamp, uint80 answeredInRound) */
        (, int256 price, , , ) = priceFeed.latestRoundData();
        uint256 decimals = priceFeed.decimals();

        return (uint256(price), decimals);
    }

    /**
     * Sets the token to be used as a required holding token.
     * This is considered different than the platform token,
     * in case we want to require a stake/lock token be held in relation to the Aegis token.
     */
    function setHoldingToken(IERC20Upgradeable token) external onlyOwner {
        holdingToken = token;
    }

    function setWhitelistAuthority(address addr) external onlyOwner {
        whitelistAuthority = addr;
    }

    function setContributionWallet(address addr) external onlyOwner {
        contributionWallet = payable(addr);
    }

    function getIdo(uint256 idoId) internal view returns (IdoInfo storage) {
        IdoInfo storage idoInfo = idosById[idoId];
        require(address(idoInfo.token) != address(0x00), "Unknown IDO");

        return idoInfo;
    }

    function createIdo(
        IERC20Upgradeable _token,
        IdoInfoInput calldata _input,
        IdoInfoInputChanges calldata _inputFields
    ) external onlyOwner returns (uint256) {
        lastIdoId++;

        IdoInfo storage idoInfo = idosById[lastIdoId];
        idoInfo.token = _token;

        updateIdoSettings(lastIdoId, _input, _inputFields);

        return lastIdoId;
    }

    function getMaxAllocationForBalance(
        IdoInfo storage idoInfo,
        uint256 platformBalance,
        SignedCirculatingSupply memory supply
    ) internal view returns (uint256) {
        if (platformBalance == 0) {
            return 0;
        }

        require(supply.supplyExpiry > block.timestamp, "Token calculation expired");

        bytes32 messageExpiryHashHex = keccak256(abi.encodePacked(supply.circulatingSupply, supply.supplyExpiry));
        bytes32 messageDigest = keccak256(abi.encodePacked("\x19Ethereum Signed Message:\n32", messageExpiryHashHex));

        address signer = ecrecover(messageDigest, supply.v, supply.r, supply.s);
        require(signer == whitelistAuthority, "Token calculation invalid");

        // uint256 maxCap = 300 ether;
        uint256 maxCap = idoInfo.maxCap;

        /**
         * Perform our math using a floating point library, since native solidity deals in whole numbers only
         *
         * ($AGSPAD / CIRULATINGSUPPLY) * idoMaxCap
         * (platformBalance / circulatingSupply) * idoMaxCap
         **/
        uint256 result = ABDKMathQuad.toUInt(
            ABDKMathQuad.fromUInt(platformBalance).div(ABDKMathQuad.fromUInt(supply.circulatingSupply)).mul(
                ABDKMathQuad.fromUInt(maxCap)
            )
        );

        // Cap allocation to the IDOs max per user
        if (result > idoInfo.maxBuy) {
            result = idoInfo.maxBuy;
        }

        // Check if start time has elapsed, and apply +10% allocation amount for every hour passed
        if (idoInfo.launchDateTs > 0 && block.timestamp > idoInfo.launchDateTs) {
            uint256 hoursElapsed = (block.timestamp - idoInfo.launchDateTs) / 3600;

            if (hoursElapsed > 0) {
                // uint256 additionalAllocation = (hoursElapsed * 10) / result;
                uint256 additionalAllocation = ABDKMathQuad.toUInt(
                    ABDKMathQuad.fromUInt(hoursElapsed * 10).div(ABDKMathQuad.fromUInt(100)).mul(
                        ABDKMathQuad.fromUInt(result)
                    )
                );

                result += additionalAllocation;
            }
        }

        return result;
    }

    function setUsersMaxAllocationModifierBasisPoints(
        uint256 idoId,
        address user,
        uint256 modiferBasisPts
    ) external onlyOwner {
        // Verify IDO existence
        getIdo(idoId);

        allocationModifierBasisPointsByAddress[idoId][user] = modiferBasisPts;
    }

    function getUsersMaxAllocationModifierBasisPoints(uint256 idoId, address user)
        external
        view
        onlyOwner
        returns (uint256)
    {
        // Verify IDO existence
        getIdo(idoId);

        return allocationModifierBasisPointsByAddress[idoId][user];
    }

    // Returns the max allocaiton a user can contribure in wei
    function getUsersMaxAllocation(
        uint256 idoId,
        address user,
        uint256 circulatingSupply,
        uint256 supplyExpiry,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) public view returns (uint256) {
        SignedCirculatingSupply memory supply = SignedCirculatingSupply(circulatingSupply, supplyExpiry, v, r, s);

        IdoInfo storage idoInfo = getIdo(idoId);
        uint256 platformBalance = platformToken.balanceOf(user);

        uint256 baseMaxAllocation = getMaxAllocationForBalance(idoInfo, platformBalance, supply);
        uint256 allocationModifierBasisPts = allocationModifierBasisPointsByAddress[idoId][user];

        if (allocationModifierBasisPts > 0) {
            return (baseMaxAllocation * allocationModifierBasisPts) / 100;
        }

        return baseMaxAllocation;
    }

    // Returns the max allocaiton a user can contribure in wei, minus their allocated so far
    function getUsersRemainingAllocationLimit(
        uint256 idoId,
        address user,
        uint256 circulatingSupply,
        uint256 supplyExpiry,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) public view returns (uint256) {
        uint256 userMaxAllocation = getUsersMaxAllocation(idoId, user, circulatingSupply, supplyExpiry, v, r, s);
        UserContributions storage userInfo = idoContributionsByAddress[idoId][msg.sender];

        return userMaxAllocation - userInfo.weiContributed;
    }

    function getIdoDetails(uint256 idoId) external view returns (IdoInfo memory) {
        IdoInfo storage idoInfo = getIdo(idoId);

        return idoInfo;
    }

    function getTokenCountForPrice(IdoInfo storage idoInfo, uint256 weiAmount) internal view returns (uint256) {
        uint256 normalizedWei;

        // Convert wei decimals to target token decimals, then multiply by swapRate
        if (idoInfo.decimals < BNB_DECIMALS) {
            uint256 divisor = 10**(BNB_DECIMALS - idoInfo.decimals);
            normalizedWei = weiAmount / divisor;
        } else if (idoInfo.decimals == BNB_DECIMALS) {
            normalizedWei = weiAmount;
        } else {
            uint256 multiplier = 10**(idoInfo.decimals - BNB_DECIMALS);
            normalizedWei = weiAmount * multiplier;
        }

        return normalizedWei * idoInfo.swapRate;
    }

    function passesWhitelist(uint256 idoId, IdoInfo storage idoInfo) internal view returns (bool) {
        return !idoInfo.isWhitelistEnabled || idoWhitelists[idoId][msg.sender] == true;
    }

    function addToWhitelist(
        uint256 idoId,
        address addr,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) external {
        IdoInfo storage idoInfo = getIdo(idoId);

        require(idoInfo.isWhitelistEnabled, "Whitelist is not enabled");
        require(idoWhitelists[idoId][addr] == false, "Address already whitelisted");

        bytes32 messageHashHex = keccak256(abi.encodePacked(idoId, addr));
        bytes32 messageDigest = keccak256(abi.encodePacked("\x19Ethereum Signed Message:\n32", messageHashHex));
        address signer = ecrecover(messageDigest, v, r, s);

        require(signer == whitelistAuthority, "Failed validating signer");
        idoWhitelists[idoId][addr] = true;
    }

    function adminAddToWhitelist(uint256 idoId, address addr) external onlyOwner {
        IdoInfo storage idoInfo = getIdo(idoId);

        require(idoInfo.isWhitelistEnabled, "Whitelist is not enabled");
        require(idoWhitelists[idoId][addr] == false, "Address already whitelisted");

        idoWhitelists[idoId][addr] = true;
    }

    function isWhitelistedOrPermitted(uint256 idoId, address addr) external view returns (bool) {
        IdoInfo storage idoInfo = getIdo(idoId);

        return !idoInfo.isWhitelistEnabled || idoWhitelists[idoId][addr] == true;
    }

    function purchaseTokens(
        uint256 idoId,
        uint256 circulatingSupply,
        uint256 supplyExpiry,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) public payable nonReentrant {
        IdoInfo storage idoInfo = getIdo(idoId);

        require(passesWhitelist(idoId, idoInfo), "Address is not whitelisted");

        require(idoInfo.launchDateTs > 0 && idoInfo.launchDateTs <= block.timestamp, "Not open for sale yet");
        require(idoInfo.isRefunding == false, "Not in sale window");
        require(_isInClaimableWindow(idoInfo) == false, "Not in sale window");
        require(idoInfo.isUsdBased == false || idoInfo.isUsdRateLocked == true, "Exchange rate not initialized");
        require(idoInfo.maxCap == 0 || idoInfo.weiRaisedSoFar < idoInfo.maxCap, "Max raise already reached");

        uint256 holdingBalance = holdingToken.balanceOf(msg.sender);
        require(holdingBalance > 0, "User holds no AEGIS tokens");

        UserContributions storage userInfo = idoContributionsByAddress[idoId][msg.sender];
        uint256 userMaxAllocation = getUsersMaxAllocation(idoId, msg.sender, circulatingSupply, supplyExpiry, v, r, s);

        require(userInfo.weiContributed < userMaxAllocation, "Already hit max contribution");
        require(userInfo.weiContributed + msg.value >= idoInfo.minBuy, "Min purchase not met");

        uint256 marketCapLeft = idoInfo.maxCap - idoInfo.weiRaisedSoFar;

        uint256 permittedWei = userMaxAllocation - userInfo.weiContributed;
        if (permittedWei > msg.value) {
            permittedWei = msg.value;
        }

        // If this transaction will leave us with less than a single token worth of BNB, then round the market cap up
        if (marketCapLeft < permittedWei) {
            permittedWei = marketCapLeft;
        }

        idoInfo.weiRaisedSoFar += permittedWei;
        uint256 tokensToAllocate = getTokenCountForPrice(idoInfo, permittedWei);

        userInfo.tokensReserved += tokensToAllocate;
        userInfo.weiContributed += permittedWei;

        // Forward contribution funds to pre-defined wallet
        contributionWallet.transfer(permittedWei);
        emit AllocationReserved(idoId, address(idoInfo.token), msg.sender, permittedWei, tokensToAllocate);

        // Refund if applicable
        if (permittedWei < msg.value) {
            payable(msg.sender).transfer(msg.value - permittedWei);
            emit OverpayRefundIssued(idoId, address(idoInfo.token), msg.sender, msg.value - permittedWei);
        }
    }

    function getTotalTokensReserved(uint256 idoId) external view returns (uint256) {
        getIdo(idoId);
        UserContributions storage userInfo = idoContributionsByAddress[idoId][msg.sender];

        return userInfo.tokensReserved;
    }

    function getUsersTotalTokensReserved(uint256 idoId, address user) external view onlyOwner returns (uint256) {
        getIdo(idoId);
        UserContributions storage userInfo = idoContributionsByAddress[idoId][user];

        return userInfo.tokensReserved;
    }

    function getTotalVestedTokens(IdoInfo storage idoInfo, UserContributions storage userInfo)
        internal
        view
        returns (uint256)
    {
        if (!idoInfo.isVestingEnabled) {
            return userInfo.tokensReserved;
        }

        uint256 elapsedDays = (block.timestamp - idoInfo.claimDateTs) / SECONDS_PER_DAY;
        uint256 dailyVestingBasisPts = idoInfo.monthlyVestingBasisPts / 30;
        uint256 currentVestedBasisPts = idoInfo.firstDayVestingBasisPts + (elapsedDays * dailyVestingBasisPts);

        // Cap at 100% vesting
        if (currentVestedBasisPts > 10000) {
            currentVestedBasisPts = 10000;
        }

        // Return tokens reserved multiplied by percent/basis points
        return (userInfo.tokensReserved * currentVestedBasisPts) / 10000;
    }

    function getWithdrawableTokenCount(uint256 idoId) external view returns (uint256) {
        IdoInfo storage idoInfo = getIdo(idoId);

        // Claim date not started yet, return 0
        if (idoInfo.claimDateTs > block.timestamp) {
            return 0;
        }

        UserContributions storage userInfo = idoContributionsByAddress[idoId][msg.sender];

        return getTotalVestedTokens(idoInfo, userInfo) - userInfo.tokensDistributed;
    }

    function withdrawVestedTokens(uint256 idoId) external nonReentrant {
        IdoInfo storage idoInfo = getIdo(idoId);

        require(idoInfo.claimDateTs <= block.timestamp, "Claiming period not started yet");
        require(idoInfo.weiRaisedSoFar >= idoInfo.minCap, "Minimum raise not met");

        UserContributions storage userInfo = idoContributionsByAddress[idoId][msg.sender];

        uint256 totalVested = getTotalVestedTokens(idoInfo, userInfo);
        require(totalVested > 0, "User has no tokens allocated");

        uint256 withdrawableTokens = getTotalVestedTokens(idoInfo, userInfo) - userInfo.tokensDistributed;
        require(withdrawableTokens > 0, "User has no tokens remaining");

        userInfo.tokensDistributed += withdrawableTokens;
        idoInfo.token.transferFrom(contributionWallet, msg.sender, withdrawableTokens);

        emit AllocationDistributed(idoId, address(idoInfo.token), msg.sender, withdrawableTokens);
    }

    function refundAllocation(uint256 idoId) external nonReentrant {
        IdoInfo storage idoInfo = getIdo(idoId);

        require(idoInfo.isRefunding, "Not in refunding window");

        UserContributions storage userInfo = idoContributionsByAddress[idoId][msg.sender];
        uint256 refundedSoFar = idoRefundsByAddress[idoId][msg.sender];

        require(refundedSoFar < userInfo.weiContributed, "Already fully refunded");
        require(address(this).balance > 0, "No balance left for refunds");

        uint256 refundToSend = userInfo.weiContributed - refundedSoFar;

        if (refundToSend > address(this).balance) {
            refundToSend = address(this).balance;
        }

        idoRefundsByAddress[idoId][msg.sender] = refundedSoFar + refundToSend;
        payable(msg.sender).transfer(refundToSend);

        emit RefundIssued(idoId, address(idoInfo.token), msg.sender, refundToSend);
    }

    function getRefundStatus(uint256 idoId) external view returns (RefundStatus memory) {
        IdoInfo storage idoInfo = getIdo(idoId);

        require(idoInfo.isRefunding, "Not in refunding window");

        UserContributions storage userInfo = idoContributionsByAddress[idoId][msg.sender];

        uint256 refundClaimed = idoRefundsByAddress[idoId][msg.sender];
        uint256 refundRemaining = userInfo.weiContributed - refundClaimed;

        return RefundStatus(refundRemaining, refundClaimed);
    }

    // Checks whether allowance has been granted to the IDO contract from the `contributionWallet` address for a specified IDO token
    function isApprovedForClaiming(uint256 idoId) external view returns (bool) {
        IdoInfo storage idoInfo = getIdo(idoId);

        // Return true if our contract's allowance of the contributionWallet is equal or greater than the balance of the contributionWallet
        return
            idoInfo.token.allowance(contributionWallet, address(this)) >= idoInfo.token.balanceOf(contributionWallet);
    }

    function lockUsdRates(uint256 idoId) external onlyOwner {
        IdoInfo storage idoInfo = getIdo(idoId);

        require(address(priceFeed) != address(0x00), "Price feed not configured");

        // Don't allow re-locking rates if IDO has begun
        require(
            idoInfo.isUsdRateLocked == false || idoInfo.launchDateTs == 0 || idoInfo.launchDateTs > block.timestamp,
            "Rates already locked active"
        );

        (uint256 usdPerBnb, uint256 usdDecimals) = getUsdPerBnb();
        uint256 multiplier = 10**usdDecimals;

        bytes16 oneEth = ABDKMathQuad.fromUInt(1 ether);
        bytes16 _usdPerBnb = ABDKMathQuad.fromUInt(usdPerBnb);

        idoInfo.minBuy = ABDKMathQuad.toUInt(
            ABDKMathQuad.fromUInt(idoInfo.minBuyUsd * multiplier).div(_usdPerBnb).mul(oneEth)
        );
        idoInfo.maxBuy = ABDKMathQuad.toUInt(
            ABDKMathQuad.fromUInt(idoInfo.maxBuyUsd * multiplier).div(_usdPerBnb).mul(oneEth)
        );
        idoInfo.minCap = ABDKMathQuad.toUInt(
            ABDKMathQuad.fromUInt(idoInfo.minCapUsd * multiplier).div(_usdPerBnb).mul(oneEth)
        );
        idoInfo.maxCap = ABDKMathQuad.toUInt(
            ABDKMathQuad.fromUInt(idoInfo.maxCapUsd * multiplier).div(_usdPerBnb).mul(oneEth)
        );
        idoInfo.swapRate = ABDKMathQuad.toUInt(
            ABDKMathQuad.fromUInt(idoInfo.swapRateUsd * multiplier).div(_usdPerBnb).mul(oneEth)
        );

        idoInfo.isUsdRateLocked = true;
    }

    function enableLaunch(uint256 idoId) external onlyOwner {
        IdoInfo storage idoInfo = getIdo(idoId);

        idoInfo.launchDateTs = block.timestamp;
    }

    function _isInClaimableWindow(IdoInfo storage idoInfo) internal view returns (bool) {
        return
            idoInfo.claimDateTs > 0 &&
            idoInfo.claimDateTs <= block.timestamp &&
            idoInfo.weiRaisedSoFar >= idoInfo.minCap;
    }

    function isInClaimableWindow(uint256 idoId) external view returns (bool) {
        IdoInfo storage idoInfo = getIdo(idoId);

        return _isInClaimableWindow(idoInfo);
    }

    function enableIdoClaiming(uint256 idoId) external onlyOwner {
        IdoInfo storage idoInfo = getIdo(idoId);

        require(idoInfo.weiRaisedSoFar >= idoInfo.minCap, "Minimum raise not met");
        require(idoInfo.isRefunding == false, "Already in refunding state");

        bool isClaimingApproved = idoInfo.token.allowance(contributionWallet, address(this)) >=
            idoInfo.token.balanceOf(contributionWallet);
        require(isClaimingApproved, "Transfers must be approved first");

        idoInfo.claimDateTs = block.timestamp;
    }

    receive() external payable onlyOwner {
        emit Received(msg.sender, msg.value);
    }

    function enableIdoRefunding(uint256 idoId) external payable onlyOwner {
        IdoInfo storage idoInfo = getIdo(idoId);

        require(_isInClaimableWindow(idoInfo) == false, "Already in claiming state");
        require(msg.value > 0 || address(this).balance > 0, "No funds provided for refunds");

        idoInfo.isRefunding = true;
    }

    /**
     * Single function for applying all requested fields which are directly writable
     */
    function updateIdoSettings(
        uint256 idoId,
        IdoInfoInput calldata _input,
        IdoInfoInputChanges calldata _inputFields
    ) public onlyOwner {
        IdoInfo storage idoInfo = getIdo(idoId);

        if (_inputFields.tokenPool) {
            idoInfo.tokenPool = _input.tokenPool;
        }

        if (_inputFields.decimals) {
            idoInfo.decimals = _input.decimals;
        }

        if (_inputFields.swapRate) {
            idoInfo.swapRate = _input.swapRate;
        }

        if (_inputFields.swapRateUsd) {
            idoInfo.swapRateUsd = _input.swapRateUsd;
        }

        if (_inputFields.minCap) {
            idoInfo.minCap = _input.minCap;
        }

        if (_inputFields.maxCap) {
            idoInfo.maxCap = _input.maxCap;
        }

        if (_inputFields.minCapUsd) {
            idoInfo.minCapUsd = _input.minCapUsd;
        }

        if (_inputFields.maxCapUsd) {
            idoInfo.maxCapUsd = _input.maxCapUsd;
        }

        if (_inputFields.launchDateTs) {
            idoInfo.launchDateTs = _input.launchDateTs;
        }

        if (_inputFields.claimDateTs) {
            idoInfo.claimDateTs = _input.claimDateTs;
        }

        if (_inputFields.firstDayVestingBasisPts) {
            idoInfo.firstDayVestingBasisPts = _input.firstDayVestingBasisPts;
        }

        if (_inputFields.monthlyVestingBasisPts) {
            idoInfo.monthlyVestingBasisPts = _input.monthlyVestingBasisPts;
        }

        if (_inputFields.minBuy) {
            idoInfo.minBuy = _input.minBuy;
        }

        if (_inputFields.maxBuy) {
            idoInfo.maxBuy = _input.maxBuy;
        }

        if (_inputFields.minBuyUsd) {
