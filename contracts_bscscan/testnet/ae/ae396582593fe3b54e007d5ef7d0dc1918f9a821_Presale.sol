contract Presale is ReentrancyGuard, Context {
    using SafeMath for uint256;
    
    struct Sale {
        address investor;
        uint quantity;
    }
    Sale[] public sales;
    uint256 public min;
    uint256 public max;
    uint public availableTokens;
    uint256 public totalSold;
    address public  admin;
    bool public isActive;
    uint256 public price;
    address payable private presaleAddress;
    event TokensPurchased(address indexed purchaser, address indexed beneficiary, uint256 value, uint256 amount);

// 0x2dC375d8c2c36ce78409A520ec96F917243DFfF5
// 5000,1936146
    
    constructor(address payable wallet_){
        admin = msg.sender;
        presaleAddress = payable(wallet_);
    }

    
    function getSale(address _investor) external view returns(uint) {
    uint total = 0;
      for(uint i = 0; i < sales.length; i++) {
        if(sales[i].investor == _investor) {
          total = total.add(sales[i].quantity);
        }
      }
      return total;
    }
    
    
    function buy(uint256 quantity) external payable presaleActive() {
        require(msg.value > 0, "Presalesale: amount is 0");
        require(msg.sender != address(0), "Presale: beneficiary is the zero address");
        require(quantity >= min && quantity <= max, 'Quantity should be within min and max');
        require(quantity <= availableTokens, 'Not enough tokens left for sale');
        presaleAddress.transfer(msg.value);
        sales.push(Sale(
            msg.sender,
            quantity
        ));
        availableTokens = availableTokens.sub(quantity);
        totalSold = totalSold.add(quantity);
        emit TokensPurchased(msg.sender, presaleAddress, msg.value, quantity);
    }
    
    
    function startPresale(uint256 availableTokens_, uint256 max_, uint256 min_) external onlyAdmin(){
        availableTokens = availableTokens_;
        max = max_;
        min = min_;
        isActive = true;
    }
    
    function wallet() public view returns (address payable) {
        return presaleAddress;
    }
    
    modifier onlyAdmin(){
        require(msg.sender == admin, 'Only admin can perform this action');
        _;
    }
    
    modifier presaleActive() {
        require(isActive == true, "Presale has ended or not active");
        _;
    }
    
}