pragma solidity >=0.8.0 <0.9.0;

import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "hardhat/console.sol";

import "./Utils.sol";

contract Boom is Ownable {
    using Counters for Counters.Counter;
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    enum Progress {
        None,
        BoomStart,
        BoomEnd,
        BoomWinner
    }

    enum Prize {
        None,
        FirstPrize,
        SecondPrize,
        ThirdPrize,
        StarPrize
    }

    struct BoomInfo {
        uint256 index;
        address user;
        string boxId;
        Prize prize;
    }

    uint256 public constant TOKEN_FEE_AMOUNT = 0 * 10**18;
    // (0->199)+1 ==> 1/200
    uint256 public constant PROBABILITY = 199;
    uint256 public constant PROBABILITY_WIN_NUMBER = 1;

    uint256 public constant PRIZE_EXCELLENT_QUANTITY = 1;
    uint256 public constant PRIZE_FIRST_QUANTITY = 10;
    uint256 public constant PRIZE_SECOND_QUANTITY = 50;
    uint256 public constant PRIZE_THIRD_QUANTITY = 100;
    uint256 public constant PRIZE_FIRST_AMOUNT = 3 * 10**18;
    uint256 public constant PRIZE_SECOND_AMOUNT = 2 * 10**18;
    uint256 public constant PRIZE_THIRD_AMOUNT = 1 * 10**18;

    uint256 public constant NFT721_TOKENID_MIN = 1;
    uint256 public constant NFT721_TOKENID_MAX = 2000;

    uint256 public constant NFT1155_TOKENID_MIN = 1;
    uint256 public constant NFT1155_TOKENID_MAX = 4;

    IERC20 private token20;
    IERC721 private token721;
    IERC1155 private token1155;

    address public admin;
    Progress public progress;

    Counters.Counter private _currentIndex;
    uint256 private nonce;

    //<boxId,index>
    mapping(string => uint256) private boxIds;
    // <address user,index[]>
    mapping(address => uint256[]) private users;
    // <index,BommInfo>
    mapping(uint256 => BoomInfo) public boomInfo;
    // <Prize,Amount>
    mapping(Prize => uint256) public prizeAmount;
    // <user,Amount>
    mapping(address => uint256) public claimedAmount;
    // [index..]
    uint256[] public winnerFirstPrize;
    uint256[] public winnerSecondPrize;
    uint256[] public winnerThirdPrize;
    uint256 public winnerStarPrize;

    event BoomEvent(
        address indexed user,
        uint256 index,
        string boxId,
        Prize prize
    );

    event ClaimEvent(address indexed user, uint256 amount);

    /** ****************************** modifier ****************************** */
    modifier onlyAdmin() {
        require(_msgSender() == admin, "caller only admin");
        _;
    }

    modifier canBoom(
        uint256 _tokenId,
        uint256 _first,
        uint256 _second
    ) {
        require(progress == Progress.BoomStart, "error: no start");
        require(address(_msgSender()) != address(0), "error: address");
        require(checkNft1155(_tokenId) == true, "error: tokenId not found");

        require(_first < _second, "error: sorting error");

        require(
            _first >= NFT721_TOKENID_MIN,
            "error:  _first >= minimum value"
        );
        require(
            _first <= NFT721_TOKENID_MAX,
            "error:  _first <= maximum value"
        );
        require(
            _second >= NFT721_TOKENID_MIN,
            "error:  _first >= minimum value"
        );
        require(
            _second <= NFT721_TOKENID_MAX,
            "error:  _first <= maximum value"
        );

        require(
            token721.ownerOf(_first) == _msgSender(),
            "error: not have token"
        );
        require(
            token721.ownerOf(_second) == _msgSender(),
            "error: not have token"
        );
        _;
    }

    constructor(
        address _admin,
        address _token20,
        address _token721,
        address _token1155
    ) {
        admin = _admin;
        token20 = IERC20(_token20);
        token721 = IERC721(_token721);
        token1155 = IERC1155(_token1155);
        progress = Progress.None;

        prizeAmount[Prize.FirstPrize] = PRIZE_FIRST_AMOUNT;
        prizeAmount[Prize.SecondPrize] = PRIZE_SECOND_AMOUNT;
        prizeAmount[Prize.ThirdPrize] = PRIZE_THIRD_AMOUNT;
    }

    /** ****************************** user ****************************** */
    function play(
        uint256 _tokenId,
        uint256 _fir,
        uint256 _sec
    ) external canBoom(_tokenId, _fir, _sec) returns (Prize) {
        //TODO TEST canBoom(_tokenId,_fir, _sec)

        string memory boxId = Utils.uint2string(_fir, _sec);
        require(boxIds[boxId] == 0, "error: already boom");

        if (TOKEN_FEE_AMOUNT > 0) {
            require(
                token20.balanceOf(_msgSender()) > TOKEN_FEE_AMOUNT,
                "error:  transfer amount exceeds balance"
            );
            token20.safeTransferFrom(
                _msgSender(),
                address(this),
                TOKEN_FEE_AMOUNT
            );
        }
        _currentIndex.increment();
        uint256 nextIndex = _currentIndex.current();
        Prize p = Prize.None;
        boxIds[boxId] = nextIndex;
        users[_msgSender()].push(nextIndex);

        if (_randomWin() == PROBABILITY_WIN_NUMBER) {
            p = _randomPrize();
            if (p == Prize.FirstPrize) winnerFirstPrize.push(nextIndex);
            else if (p == Prize.SecondPrize) winnerSecondPrize.push(nextIndex);
            else if (p == Prize.ThirdPrize) winnerThirdPrize.push(nextIndex);
            else console.log("no prize");
            console.log(nextIndex, boxId);
        }
        boomInfo[nextIndex] = BoomInfo(nextIndex, _msgSender(), boxId, p);

        emit BoomEvent(msg.sender, nextIndex, boxId, p);
        return p;
    }

    function claim(uint256 _tokenId) external returns (bool) {
        require(checkNft1155(_tokenId) == true, "error: tokenId not found");

        uint256 len = users[_msgSender()].length;
        require(len > 0, "error: not bomm");

        uint256 amount = 0;
        for (uint256 index = 0; index < len; index++) {
            uint256 id = users[_msgSender()][index];
            BoomInfo storage b = boomInfo[id];
            if (b.user == _msgSender()) {
                if (
                    b.prize == Prize.FirstPrize ||
                    b.prize == Prize.SecondPrize ||
                    b.prize == Prize.ThirdPrize
                ) amount += _getPrizeAmount(b.prize);
            }
        }
        if (amount > 0) {
            token20.safeTransfer(_msgSender(), amount);
            claimedAmount[_msgSender()] = amount;
            emit ClaimEvent(_msgSender(), amount);
            return true;
        }
        return false;
    }

    /** ****************************** check ****************************** */
    function checkBoom(
        uint256 _tokenId,
        uint256 _fir,
        uint256 _sec
    ) external view canBoom(_tokenId, _fir, _sec) returns (uint256) {
        //TODO TEST canBoom(_tokenId,_fir, _sec)

        string memory boxId = Utils.uint2string(_fir, _sec);
        return boxIds[boxId];
    }

    function checkNft1155(uint256 _tokenId) public view returns (bool) {
        require(_tokenId >= NFT1155_TOKENID_MIN, "error : tokenId");
        require(_tokenId <= NFT1155_TOKENID_MAX, "error : tokenId");
        if (token1155.balanceOf(_msgSender(), _tokenId) > 0) return true;
        return false;
    }

    /** ****************************** internal ****************************** */
    function _getPrizeAmount(Prize _p) internal view returns (uint256) {
        //TODO
        uint256 amount = prizeAmount[_p];
        if (amount > 0) return amount;
        return 0;
    }

    function _randomWin() internal returns (uint256) {
        uint256 index = uint256(
            keccak256(
                abi.encodePacked(
                    nonce,
                    msg.sender,
                    block.difficulty,
                    block.timestamp
                )
            )
        ) % PROBABILITY;
        nonce++;
        return index + 1;
    }

    function _randomPrize() internal view returns (Prize) {
        uint256 total = 0;
        uint256[3] memory quan;
        if (winnerFirstPrize.length < PRIZE_FIRST_QUANTITY) {
            total += PRIZE_FIRST_QUANTITY;
            quan[0] = PRIZE_FIRST_QUANTITY;
        }
        if (winnerSecondPrize.length < PRIZE_SECOND_QUANTITY) {
            total += PRIZE_SECOND_QUANTITY;
            quan[1] = quan[0] + PRIZE_SECOND_QUANTITY;
        }
        if (winnerThirdPrize.length < PRIZE_THIRD_QUANTITY) {
            total += PRIZE_THIRD_QUANTITY;
            quan[2] = quan[0] + quan[1] + PRIZE_THIRD_QUANTITY;
        }

        if (total > 0) {
            uint256 id = _currentIndex.current();
            uint256 index = uint256(
                keccak256(
                    abi.encodePacked(
                        nonce + id + total,
                        msg.sender,
                        block.difficulty,
                        block.timestamp
                    )
                )
            ) % total;
            uint256 random = index + 1;
            // console.log(_currentIndex.current(), total);
            // console.log(random, quan[0], quan[1], quan[2]);
            if (quan[0] > 0 && random <= quan[0]) {
                return Prize.FirstPrize;
            } else if (quan[1] > 0 && random <= quan[1]) {
                return Prize.SecondPrize;
            } else if (quan[2] > 0 && random <= quan[2]) {
                return Prize.ThirdPrize;
            }
        }
        return Prize.None;
    }

    /** ****************************** onlyAdmin ****************************** */
    function setProgress(Progress _value) external onlyAdmin {
        progress = _value;
    }

    function BoomWinner(uint256 salt) external onlyAdmin {
        //TODO Chainlink
        require(progress == Progress.BoomEnd, "error: no end");
        require(_currentIndex.current() > 0, "error: no boom");
        uint256 prob = _currentIndex.current();

        uint256 index = uint256(
            keccak256(
                abi.encodePacked(
                    nonce + salt,
                    msg.sender,
                    block.difficulty,
                    block.timestamp
                )
            )
        ) % prob;
        winnerStarPrize = index + 1;
        progress = Progress.BoomWinner;
    }

    function withdraw() external onlyAdmin {
        //TODO
        require(progress == Progress.BoomWinner, "error: no BoomWinner");
        token20.safeTransfer(owner(), token20.balanceOf(address(this)));
    }

    /** ****************************** get ****************************** */
    function boomCount() public view returns (uint256) {
        return _currentIndex.current();
    }

    function winnerCount() public view returns (uint256) {
        return
            winnerFirstPrize.length +
            winnerSecondPrize.length +
            winnerThirdPrize.length;
    }

    function winnerFirstPrizeLength() public view returns (uint256) {
        return winnerFirstPrize.length;
    }

    function winnerSecondPrizeLength() public view returns (uint256) {
        return winnerSecondPrize.length;
    }

    function winnerThirdPrizeLength() public view returns (uint256) {
        return winnerThirdPrize.length;
    }
}

