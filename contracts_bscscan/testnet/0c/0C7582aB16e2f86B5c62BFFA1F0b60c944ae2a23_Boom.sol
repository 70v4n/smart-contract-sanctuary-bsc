pragma solidity >=0.8.0 <0.9.0;

library Utils {
    function uint2string(uint256 _a, uint256 _b)
        internal
        pure
        returns (string memory)
    {
        bytes memory a = toBytes(uintToBytes(_a));
        bytes memory b = toBytes(uintToBytes(_b));
        string memory newStr = new string(a.length + b.length + 1);
        bytes memory bret = bytes(newStr);

        uint256 k = 0;
        for (uint256 i = 0; i < a.length; i++) {
            bret[k++] = a[i];
        }

        bret[k++] = ",";
        for (uint256 i = 0; i < b.length; i++) {
            bret[k++] = b[i];
        }

        return string(newStr);
    }

    function uintToBytes(uint256 v) internal pure returns (bytes32 ret) {
        if (v == 0) {
            ret = "0";
        } else {
            while (v > 0) {
                ret = bytes32(uint256(ret) / (2**8));
                ret |= bytes32(((v % 10) + 48) * 2**(8 * 31));
                v /= 10;
            }
        }
        return ret;
    }

    function toBytes(bytes32 _data) internal pure returns (bytes memory) {
        return abi.encodePacked(_data);
    }
}

