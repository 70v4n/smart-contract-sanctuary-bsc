pragma experimental ABIEncoderV2;
contract MyCrowdsaleDeployer {
    struct crowdSaleDetails {
        address _crowdsale;
        address _token;
    }
    
    address[] private allCrowdsales;
    
    mapping (address => crowdSaleDetails[]) public tokens;
    
    function DeployContract(uint256 _rate, address _token, address payable _fundReceiver, address _ReceiveTokenAddr, uint256 _cap, uint256 _startingTime, uint256 _endingTime) public {
        // create a token
        MyToken token = MyToken(_token);

        MyCrowdsale crowdsale = new MyCrowdsale(
            _rate,
            _fundReceiver,
            _ReceiveTokenAddr,
            token,           
            msg.sender,
            _cap,
            _startingTime,
            _endingTime
        );
        
        crowdSaleDetails memory details;
        
        details._crowdsale = address(crowdsale);
        details._token = address(token);
        allCrowdsales.push(address(crowdsale));
        
        tokens[msg.sender].push(details);
    }
    
    function getBlockTime() public view returns(uint256) {
        return block.timestamp;
    }
    
    function getCrowdsaleList() public view returns(crowdSaleDetails[] memory) {
        return tokens[msg.sender];
    }
    
    function getCrowdsales() public view returns(address[] memory) {
        return allCrowdsales;
    }
}