contract AllkaBNBRandom{
    
    bytes32 public constant RANDOMNESS_REQUESTER = keccak256("RANDOMNESS_REQUESTER");
    using SafeMath for uint256;
    bytes32 public keyHash;
    uint256 public fee;
    uint256 public seed;
    uint256 public currenseed;
    uint256 public randomdata;
    // uint256 public seed = มีการ set ค่าเข้าไม่ไม่ทราบค่า ลองตั้ง 9999;

    function setSeed(uint256 seedNumber) external {
        seed = seedNumber;
    }

    function setrandomdata(uint256 exrandomdata) external {
        randomdata = exrandomdata;
    }

    function randemo(uint256 bestrate) external view returns (uint){
        return randomdata.mod(1000) + bestrate;
    }

    function getRandomSeed(uint256 inseed ,uint256 attack ,uint256 winrate) external view returns (uint256){
            uint256 successChance = winrate + attack * 10 / 100;
            uint256 winer = inseed.mod(1000) + successChance;
            return winer;
    }
    
}