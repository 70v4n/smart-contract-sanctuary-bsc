/////////////////////////////////////////////////////////
//
//  DogeVille Bank 
//  Allows deposit/withdrawal of $DVILLE
//  Allows and monitors purchase of $DVILLE from PCS
//  
///////////////////////////////////////////////////////////

contract DVILLE_Bank is Context, AccessControlEnumerable, ReentrancyGuard, Pausable {

    // Dead address
    address public _dead = 0x000000000000000000000000000000000000dEaD;

    // Deposits
    mapping(address => uint256) public _ledger;

    // Token Address 
    address public _tokenAddress;
    address payable public devWallet = payable(0xA97B53fE4d7de9201D0d78E14721C6bbfe71F0AD);
    IERC20 public DVILLEToken;

    // PCS SETTINGS
    IUniswapV2Router02 public _uniswapV2Router;
    
    // EVENTS
    event Deposit(address indexed _to, uint256 _token);
    event Withdrawal(address indexed _to, uint256 _token);
    event Purchase(address indexed _to, uint256 _token);

    constructor()  {

        // SETUP PCS
        IUniswapV2Router02 uniswapV2Router = IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
        _uniswapV2Router = uniswapV2Router;

        // Admin role
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);

        _pause();
    
    }
    
    ////////////////////////////////////
    // MODIFIERS
    ////////////////////////////////////  

    modifier whenNotPausedOrAdmin() {
         if (!hasRole(DEFAULT_ADMIN_ROLE, _msgSender()))
         {
            require(!paused(), "GM : Contract is Locked");
         }
        
        _;
    }

    // Admin Only commands
    modifier OnlyAdmin(){
           require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "GM : Restricted to Admin");        
        _;
    }
    
    //////////////////////////////////////
    // GET/SET VALUES
    //////////////////////////////////////

    function setTokenAddress( address Address_) OnlyAdmin external {
     
        _tokenAddress = Address_;
  DVILLEToken = IERC20(_tokenAddress);
    }

///////////////////////////
// Frees locked tokens/BNB
/////////////////////////////

function disperseBalanceToAdmin() whenPaused OnlyAdmin public {
   uint256 balance = address(this).balance;
   
   devWallet.transfer(balance);  
}

function disperseTokenBalanceToAdmin(address address_) whenPaused OnlyAdmin public {
    IERC20 _token = IERC20(address_);

    _token.transfer(address(devWallet), _token.balanceOf(address(this)));

}

////////////////////////////
// Helper Functions
////////////////////////////

function pauseGM() OnlyAdmin whenNotPaused public {
    _pause();
}

function unpauseGM() OnlyAdmin whenPaused public {
    _unpause();
}

function balanceOf(address add_) public view returns (uint256)
{
    return _ledger[add_];
}

    ////////////////////////////////////
    // User Functions
    ////////////////////////////////////
    
    function purchaseTokens(uint256 min_) nonReentrant whenNotPausedOrAdmin external payable
    {
       require(msg.value > 100000000,"BANK : Insufficent BNB for Purchase");
        uint256 bnbAmount = msg.value;
        
        // Swap Path to Token
        address[] memory path = new address[](2);
        path[0] = _uniswapV2Router.WETH();
        path[1] = address(_tokenAddress);

        _uniswapV2Router.swapExactETHForTokensSupportingFeeOnTransferTokens{value: bnbAmount}( 
            min_, 
            path,
            address(msg.sender),
            block.timestamp + 360
        );

        emit Purchase(msg.sender, msg.value);

    }

    // Admin can force user to withdraw all funds
    function adminWithdrawal(address add_) nonReentrant OnlyAdmin external {
        uint256 val_ = _ledger[add_];
        emit Withdrawal(add_, val_);
        _ledger[msg.sender] = 0;

        DVILLEToken.transfer(address(add_), val_);
    }

    function withdrawFromBank(uint256 val_) nonReentrant whenNotPausedOrAdmin external {
        require(_ledger[msg.sender] >= val_, "BANK : Insufficient Balance");

        _ledger[msg.sender] = _ledger[msg.sender] - val_;
        emit Withdrawal(msg.sender, val_);
        
        DVILLEToken.transfer(address(msg.sender), val_);

    }
    
    function depositToBank(uint256 val_) nonReentrant whenNotPausedOrAdmin external{
        
        _ledger[msg.sender] = _ledger[msg.sender] + val_;
        emit Deposit(msg.sender, val_);

        DVILLEToken.transferFrom(address(msg.sender), address(this), val_);
                
    } 

 
}

abstract contract IERC20 {


    function totalSupply() external view virtual returns (uint256);

    function balanceOf(address account) external view virtual returns (uint256);

    function allowance(address owner, address spender) external view virtual returns (uint256);


    function transfer(address recipient, uint256 amount) external virtual returns (bool);

    function approve(address spender, uint256 amount) external virtual returns (bool);

    function transferFrom(address sender, address recipient, uint256 amount) external virtual returns (bool);



    event Transfer(address indexed from, address indexed to, uint256 value);

    event Approval(address indexed owner, address indexed spender, uint256 value);

}