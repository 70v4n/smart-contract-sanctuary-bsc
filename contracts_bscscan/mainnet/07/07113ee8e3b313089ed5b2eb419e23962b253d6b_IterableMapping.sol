        require(automatedMarketMakerPairs[pair] != value, "Baby: Automated market maker pair is already set to that value");
        automatedMarketMakerPairs[pair] = value;

        if(value) {
            for(uint i = 0; i < dividendTracker.length; i++)
                dividendTracker[i].excludeFromDividends(pair);
        }

        emit SetAutomatedMarketMakerPair(pair, value);
    }

