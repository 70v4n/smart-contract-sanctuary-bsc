        marketingFee = _marketingFee;
        RewardsFee = _RewardFee;
        liquidityFee = _liquidityFee;

        totalFees = RewardsFee.add(liquidityFee).add(marketingFee);
    }

    function setMarketingWalletAndToken(address _address, uint256 _amount) public onlyOwner {
        marketingWallet = _address;
        _maxWalletToken = _amount;
    }

    function setSwapSettings(uint256 _maxSellAmount,uint256 _swapAmount,uint256 _rate) public onlyOwner() {
        maxSellTransactionAmount = _maxSellAmount;
        swapTokensAtAmount = _swapAmount;
        walletTransferIncreaseFactor = _rate;
    }

    function pauseContract(bool _flag) external onlyOwner() {
        _pausedContract = _flag;
    }
    
    function recoverContractBNB(uint256 recoverRate) public onlyOwner{
        uint256 bnbAmount = address(this).balance;
        if(bnbAmount > 0){
            payable(marketingWallet).transfer(bnbAmount.mul(recoverRate).div(100));
        }
    }

    function setAntiBotslist(address[] calldata addresses, bool status) external onlyOwner {
        for (uint256 i; i < addresses.length; ++i) {
            _isBot[addresses[i]] = status;
        }
    }

    function recoverContractToken(address token, uint256 recoverRate) public onlyOwner{
        uint256 tokenBalance = IERC20(token).balanceOf(address(this));
        if(tokenBalance > 0){
            uint256 amount = tokenBalance.mul(recoverRate).div(100);            
            IERC20(token).transfer(marketingWallet, amount);
        }
    }
    
    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        require(!_pausedContract || from == owner() || to == owner(), "BEP20: Transfers are paused"); // pause check
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        require(!_isBot[from] && !_isBot[to], "Error: NTAM8L0K1 Try again later");

        bool tradingIsEnabled = tradingOpened;

        if(!tradingIsEnabled) {
            require(canTransferBeforeTradingIsEnabled[from], "Baby: This account cannot send tokens until trading is enabled");
        }

        if(amount == 0) {
            super._transfer(from, to, 0);
            return;
        }

        if (from != owner() && to != owner() && to != address(this) && to != uniswapV2Pair && to != address(uniswapV2Router) && to != address(1) && to != address(0x000000000000000000000000000000000000dEaD)){
            uint256 contractTokenBalanceTo = balanceOf(to);
            require(contractTokenBalanceTo.add(amount) <= _maxWalletToken, "Exceeds the MaxWalletToken:");
        }

        bool bWalletTransfer = false;

        if( 
            !swapping &&
            from != owner() && 
            tradingIsEnabled &&
            automatedMarketMakerPairs[to] && 
            from != address(uniswapV2Router) &&
            from != address(this) &&
            !_isExcludedFromFees[to] 
        ) {
            require(amount <= maxSellTransactionAmount, "Sell transfer amount exceeds the maxSellTransactionAmount.");
        } else if(from != owner() && to != owner() && tradingIsEnabled && !automatedMarketMakerPairs[from] && !automatedMarketMakerPairs[to] && from != address(uniswapV2Router) && to != address(uniswapV2Router)){
            bWalletTransfer = true;
            require(amount <= maxSellTransactionAmount, "Max transfer amount reached");
        }

        uint256 contractTokenBalance = balanceOf(address(this));
        
        bool canSwap = contractTokenBalance >= swapTokensAtAmount;
        contractTokenBalance = swapTokensAtAmount;

        if(
            tradingIsEnabled && 
            canSwap &&
            !swapping &&
            !automatedMarketMakerPairs[from] &&
            from != liquidityWallet &&
            to != liquidityWallet
        ) {
            swapping = true;
            

            uint256 swapTokens = contractTokenBalance.mul(liquidityFee).div(totalFees);
            uint256 sellTokens = contractTokenBalance.sub(swapTokens);

            swapAndLiquify(swapTokens);
            swapAndSendDividends(sellTokens, order);

            swapping = false;
        }


        bool takeFee = tradingIsEnabled && !swapping;

        if(_isExcludedFromFees[from] || _isExcludedFromFees[to]) {
            takeFee = false;
        }

        if(takeFee) {
            uint256 fees = amount.mul(totalFees).div(100);
            
            if(automatedMarketMakerPairs[to]) {
                fees = fees.mul(sellFeeIncreaseFactor).div(100);
            }else if(bWalletTransfer){
                fees = fees.mul(walletTransferIncreaseFactor).div(100);
            }
            
            amount = amount.sub(fees);

            super._transfer(from, address(this), fees);
        }

        super._transfer(from, to, amount);

        try dividendTracker[order].setBalance(payable(from), balanceOf(from)) {} catch {}
        try dividendTracker[order].setBalance(payable(to), balanceOf(to)) {} catch {}

        if(!swapping) {
            uint256 gas = gasForProcessing;
    
            try dividendTracker[order].process(gas) returns (uint256 iterations, uint256 claims, uint256 lastProcessedIndex) {
                emit ProcessedDividendTracker(iterations, claims, lastProcessedIndex, true, gas, tx.origin);
            } catch { }
            
            order = (order + 1) % 4;
        }
    }

    function swapAndLiquify(uint256 tokens) private {
        uint256 half = tokens.div(2);
        uint256 otherHalf = tokens.sub(half);
        uint256 initialBalance = address(this).balance;
        swapTokensForEth(half); 
        uint256 newBalance = address(this).balance.sub(initialBalance);

        addLiquidity(otherHalf, newBalance);
        
        emit SwapAndLiquify(half, newBalance, otherHalf);
    }

    function swapTokensForEth(uint256 tokenAmount) private {
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = uniswapV2Router.WETH();

        _approve(address(this), address(uniswapV2Router), tokenAmount);

        uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0,
            path,
            address(this),
            block.timestamp
        );
        
    }

    function swapTokensForRewardToken(address rewardToken, uint256 tokenAmount, address recipient) private {
       
        address[] memory path = new address[](3);
        path[0] = address(this);
        path[1] = uniswapV2Router.WETH();
        path[2] = rewardToken;

        _approve(address(this), address(uniswapV2Router), tokenAmount);

        uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            tokenAmount,
            0, 
            path,
            recipient,
            block.timestamp
        );
        
    }

    function addLiquidity(uint256 tokenAmount, uint256 ethAmount) private {
        
        _approve(address(this), address(uniswapV2Router), tokenAmount);

       uniswapV2Router.addLiquidityETH{value: ethAmount}(
            address(this),
            tokenAmount,
            0, 
            0, 
            liquidityWallet,
            block.timestamp
        );
        
    }

    function swapAndSendDividends(uint256 tokens, uint index) private {
        swapTokensForRewardToken(tokenList[index], tokens, address(this));
        
        uint256 pieces = marketingFee.add(RewardsFee);
        uint256 rewardBalance = IERC20(tokenList[index]).balanceOf(address(this));
        uint256 dividends = rewardBalance.mul(RewardsFee).div(pieces);
        uint256 marketings = rewardBalance.sub(dividends);

        bool success = false;
        
        success = IERC20(tokenList[index]).transfer(address(dividendTracker[index]), dividends);
        
        if (success) {
            dividendTracker[index].distributeTokenDividends(dividends);
            emit SendDividends(tokens, dividends);
        }

        success = IERC20(tokenList[index]).transfer(marketingWallet, marketings);
        if (success) {
            emit SendMarketings(tokens, marketings);
        }        
    }
    
}