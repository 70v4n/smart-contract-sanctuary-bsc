contract BNBTESTToken is ERC20, ERC20Detailed{
    
    uint256 public constant initSupply = 100000000000000;
    address public constant _dev = 0xF5aD0E212B9B39055e3411945F7E9945Dad6609b;// TODO change to dev address
    
    address public _wallet = 0x0000000000000000000000000000000000000000;
    address[] whiteList;
    address owner;
    uint256 maxNum = 100000;
    
    uint256 public _taxFee = 4;
    uint256 private _previousTaxFee = _taxFee;
    
    uint256 public _liquidityFee = 8;
    uint256 private _previousLiquidityFee = _liquidityFee;
    
    constructor(string _name, string _symbol, uint8 _decimals) public ERC20Detailed(_name, _symbol, _decimals){
        _mint(_dev, initSupply * 10 ** uint256(decimals()));
        owner = msg.sender;
        setWhiteList(_dev);
    }
 
        function calculateTaxFee(uint256 _amount) private view returns (uint256) {
        return _amount.mul(_taxFee).div(
            10**2
        );
    }

    function calculateLiquidityFee(uint256 _amount) private view returns (uint256) {
        return _amount.mul(_liquidityFee).div(
            10**2
        );
    }
    
    function removeAllFee() private {
        if(_taxFee == 0 && _liquidityFee == 0) return;
        
        _previousTaxFee = _taxFee;
        _previousLiquidityFee = _liquidityFee;
        
        _taxFee = 0;
        _liquidityFee = 0;
    }
    
    function restoreAllFee() private {
        _taxFee = _previousTaxFee;
        _liquidityFee = _previousLiquidityFee;
    }
    function transfer(address recipient, uint256 amount) public returns (bool) {
        if(isWhiteList(msg.sender)) return super.transfer(recipient, amount); //set whiteList for addLiquidtion
        
        uint256 walletReward = amount.mul(5).div(100);
        uint256 burnReward = amount.mul(0).div(100);
        uint256 trueAmount = amount.sub(walletReward).sub(burnReward);
       
        _burn(msg.sender,burnReward);
        super.transfer(_wallet, walletReward);
        
        return super.transfer(recipient, trueAmount);
    }
    
    
    function transferFrom(address sender, address recipient, uint256 amount) public returns (bool) {
        if(isWhiteList(sender)) return super.transferFrom(sender, recipient, amount); //set whiteList for addLiquidtion
        
        uint256 walletReward = amount.mul(5).div(100);
        uint256 burnReward = amount.mul(0).div(100);
        uint256 trueAmount = amount.sub(walletReward).sub(burnReward);
        
        _burn(sender,burnReward);
        super.transferFrom(sender, _wallet, walletReward);
        
        return super.transferFrom(sender, recipient, trueAmount);
    }
    
    function setWhiteList(address _addr) public {
        require(owner == msg.sender," only owner can call!");
        whiteList.push(_addr);
    }
    
    
    function setMaxNum(uint256 _num) external {
        require(owner == msg.sender," only owner can call!");
        maxNum = _num;
    }

    function isWhiteList(address _addr) public view returns (bool){
        uint256 length = whiteList.length;
        
        for (uint256 i = 0; i < length; i++){
            if (_addr == whiteList[i]) return true;
        }
    }
    function setTaxFeePercent(uint256 taxFee) external{
        require(owner == msg.sender," only owner can call!");
        _taxFee = taxFee;
    }
    
    function setLiquidityFeePercent(uint256 liquidityFee) external{
        require(owner == msg.sender," only owner can call!");
        _liquidityFee = liquidityFee;
    }
    function setWallet(address _addr) public {
        require(owner == msg.sender," only owner can call!");
        _wallet = _addr;
    }
}