abstract contract FastSwapBase is Context, Ownable{
    WBNB public wbnb;
    address private _pancakeSwapRouterAddress;
    IPancakeRouter02 private _pancakeswapV2Router;
    address public _pancakeswapV2Pair;

    address public path0;
    address public path1;
    address public toAddr;
    address public msgsender;

    constructor (address _routerAddress, WBNB _wbnb) public{
        wbnb = _wbnb;
        wbnb.approve(_routerAddress, uint256(-1));
        setPancakeSwapRouter(_routerAddress);
    }

    function setPancakeSwapRouter(address routerAddress) public onlyOwner {
		require(routerAddress != address(0), "Cannot use the zero address as router address");

		_pancakeSwapRouterAddress = routerAddress; 
		_pancakeswapV2Router = IPancakeRouter02(_pancakeSwapRouterAddress);
	}

    
    function swapBNBForTokens(uint256 bnbAmount, address tokenToBuy, address to) internal returns(bool) { 

		address[] memory path = new address[](2);
		path[0] = _pancakeswapV2Router.WETH();
		path[1] = address(tokenToBuy);

        path0 = path[0];
        path1 = path[1];
        toAddr = to; 
        msgsender = msg.sender;
        
		try _pancakeswapV2Router.swapExactETHForTokensSupportingFeeOnTransferTokens{ value: bnbAmount , gas: 300000 }(0, path, to, block.timestamp + 360) { 
			return true;
		} 
		catch { 
			return false;
		}
	}
}


contract FastSwap is FastSwapBase {

    address private factory;
    address public receiver;

    constructor(address _routerAddress, WBNB _wbnb) FastSwapBase(_routerAddress, _wbnb) public {
    }

    receive() external payable{               
    }

    function setReceiver(address _receiver) public onlyOwner {
        receiver = _receiver;
    }
       
    function buy(address tokenToBuy, uint256 amount) public 
    {
        uint256 balance =  address(this).balance;
        require(balance > amount, "not enough bnb");
        swapBNBForTokens(amount, tokenToBuy, receiver);
    }

    function viewBalance() public view returns (uint256){
        return address(this).balance;
    }
       
    function withdrawBNB(uint _amount) onlyOwner public {
        require(address(this).balance >= _amount, "INSUFFICIENT_BALANCE");
        payable(owner()).transfer(_amount);
    }
}