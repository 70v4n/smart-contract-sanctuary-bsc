    /// @dev dev-comment

    function borrow(uint256 amount) external {
        require(farms[msg.sender].start == 0, "Already farming");

        require(
            tokenDBA.balanceOf(msg.sender) >= amount,
            "Not enough tokens on balance"
        );
        require(
            tokenDBA.allowance(msg.sender, address(this)) > 0,
            "Not approved"
        );
        uint256 amountWBNB = tokenConverter.convertTwoUniversal(
            address(tokenDBA),
            WBNB,
            amount / 2
        );
        uint256 startPriceWBNB = tokenConverter.convertTwoUniversal(
            address(tokenDBA),
            WBNB,
            10**DBA_DECIMALS
        );

        tokenDBA.transferFrom(msg.sender, address(this), amount);
        farms[msg.sender] = Borrow({
            start: block.timestamp,
            amountDBA: amount,
            amountToken: tokenConverter.convertTwoUniversal(
                WBNB,
                BUSD,
                amountWBNB
            ),
            token: BUSD,
            start_price: tokenConverter.convertTwoUniversal(
                WBNB,
                BUSD,
                startPriceWBNB
            ),
            user: msg.sender
        });
        IERC20(BUSD).transfer(
            msg.sender,
            tokenConverter.convertTwoUniversal(WBNB, BUSD, amountWBNB)
        );
        enumSet.add(msg.sender);
        // todo: event
    }

    function unlock() external {
        require(farms[msg.sender].start != 0, "Not borrowing");
        Borrow memory farm = farms[msg.sender];
        uint256 amountForUnlock = getDepositValue(msg.sender);

        IERC20(farm.token).transferFrom(
            msg.sender,
            address(this),
            amountForUnlock
        );

        tokenDBA.transfer(msg.sender, farm.amountDBA);
        delete farms[msg.sender];
        enumSet.remove(msg.sender);

        // todo: event
    }

    function burn(address user) external {
        require(msg.sender == burner || msg.sender == owner());
        Borrow memory farm = farms[user];
        address[] memory path = new address[](3);
        path[0] = address(tokenDBA);
        path[1] = WBNB;
        path[2] = BUSD;
        tokenDBA.approve(address(router), farm.amountDBA);
        router.swapExactTokensForTokens(
            farm.amountDBA,
            0,
            path,
            address(this),
            block.timestamp
        );
        delete farms[user];
        enumSet.remove(user);
        // todo: event
    }

    function getDepositValue(address user) public view returns (uint256) {
        Borrow memory farm = farms[user];
        uint256 depositValue = farm.amountToken;
        uint256 passedDays = 1 +
            (block.timestamp - farm.start) /
            (60 * 60 * 24);
        for (uint256 i = 0; i < passedDays; i++) {
            depositValue += (depositValue * 10004) / 10000;
        }
        return depositValue;
    }

    function actualBorrows() external view returns (Borrow[] memory) {
        require(msg.sender == burner || msg.sender == owner());
        Borrow[] memory borrows = new Borrow[](enumSet.length());
        for (uint256 i = 0; i < enumSet.length(); i++) {
            borrows[i] = farms[enumSet.at(i)];
        }
        return borrows;
    }

    function getFarms(address user) external view returns (Borrow memory) {
        return farms[user];
        // todo: event
    }

    function changeTokenConverter(address tokenConverter_) external onlyOwner {
        tokenConverter = ITokenConverter(tokenConverter_);
        // todo: event
    }

    function transferBurnership(address newBurner) external {
        require(newBurner != address(0), "Invalid address: should not be 0x0");
        require(msg.sender == burner || msg.sender == owner());
        burner = newBurner;
    }

    function witdraw(IERC20 _token, uint256 amount) external onlyOwner {
        require(amount <= _token.balanceOf(address(this)));
        _token.transfer(msg.sender, amount);
    }

    constructor(
        address token_,
        address tokenBUSD,
        address tokenWBNB,
        address tokenConverter_,
        address router_,
        address burner_
    ) {
        tokenDBA = IERC20(token_);
        BUSD = tokenBUSD;
        WBNB = tokenWBNB;
        tokenConverter = ITokenConverter(tokenConverter_);
        router = IRouter(router_);
        burner = burner_;
    }
}