		emit RemovedFromTax(_operator, account);
        _TaxExcluded.add(account);
    }
	
    function UpdateIncludeAccountToTax(address account) public onlyOperator {
		require(account != address(0),"PANTHER::UpdateIncludeAccountToTax:Zero address");
