    /**
     * @dev Update the swap router
     * Can only be called by the current operator
     */
    function updatePantherSwapRouter(address _router) public onlyOperator {
        pantherSwapRouter = IUniswapV2Router02(_router);
        pantherSwapPair = IUniswapV2Factory(pantherSwapRouter.factory()).getPair(address(this), pantherSwapRouter.WETH());
        require(pantherSwapPair != address(0), "PANTHER::updatePantherSwapRouter: Invalid pair address.");
        emit PantherSwapRouterUpdated(_operator, address(pantherSwapRouter), pantherSwapPair);
    }

    /**
     * @dev Withdraw other tokens and leftover BNB in contract
     * Can only be called by the current operator
	 * Panther balance cannot be withdrawn from contract
     */

	function withdrawLostTokens(address tokenAddress, address to, uint256 amount) public onlyOperator{		
        require(to != address(0),"PANTHER::withdrawLostTokens:Recipient cannot be zero address");
        require(tokenAddress != address(this),"PANTHER::withdrawLostTokens:Token balance can only be used for swapAndLiquify");
        require(tokenAddress != address(0),"PANTHER::withdrawLostTokens:Token address cannot be zero address");
		IBEP20 token = IBEP20(tokenAddress);
		uint256 balance = token.balanceOf(address(this));		
		require(balance >= 0,"PANTHER::withdrawLostTokens:Provided token has no balance to withdraw");
		require(amount >= 0,"PANTHER::withdrawLostTokens:No token amount to withdraw");
		require(amount <= balance,"PANTHER::withdrawLostTokens:Insufficient balance to withdraw");
		emit LostTokenTransferred(_operator, tokenAddress, balance, amount);
		token.transfer(to, amount);
    }
	
	function withdrawLeftOverBNB(address payable to) public onlyOperator{
        require(to != address(0),"PANTHER::withdrawLeftOverBNB:To address cannot be zero address");
		uint256 balance = address(this).balance;
		require(balance >= 0,"PANTHER::withdrawLeftOverBNB:No left over BNB to withdraw");
		emit LeftOverBNBTransferred(_operator, to, balance);
		to.transfer(balance);
	}
	
