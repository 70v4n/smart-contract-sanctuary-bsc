// File: contracts/PantherToken.sol

// PantherToken

contract PantherToken is BEP20 {
	
    using Address for address;
    using EnumerableSet for EnumerableSet.AddressSet;	
	
    //Max transfer tax rate: 20%.
    uint16 public constant MAXIMUM_TRANSFER_TAX_RATE = 2000;
	
    //Transfer tax rate in basis points. (default 5%)
    uint16 public transferTaxRate = 1000;

    //Sell tax rate in basis points. (default 10%)
    uint16 public transferSellTaxRate = 1000;

    //Buy tax rate in basis points. (default 5%)
    uint16 public transferBuyTaxRate = 500;
	
    //Burn rate % of transfer tax. (50% of transferTaxRate).
    uint16 public burnRate = 50;

    //Burn address
    address private constant BURN_ADDRESS = 0x000000000000000000000000000000000000dEaD;
	
	//Max supply of Panther Token (1 Billion) 
	uint256 private constant MAXIMUM_SUPPLY = 1000000000 * 10 ** 18;
	
    //Max transfer amount rate in basis points. (default is 0.5% of total supply)
    uint16 public maxTransferAmountRate = 50;
	
    //Automatic swap and liquify enabled
    bool public swapAndLiquifyEnabled = false;
	
    //Min amount to liquify. (default 500 PANTHERs)
    uint256 public minAmountToLiquify = 500 ether;
	
    //The swap router is modifiable. It will be changed to when our own AMM is released
    IUniswapV2Router02 public pantherSwapRouter;
	
    //The trading pair
    address public pantherSwapPair;
	
    //In swap and liquify
    bool private _inSwapAndLiquify;

    //The operator can perform all updates
    address private _operator;
	
    //The operator can perform all updates
    address private _pendingOperator;
	
	//Addresses excluded from taxes
	EnumerableSet.AddressSet private _TaxExcluded;
	
    //Addresses excluded from transfer limit
	EnumerableSet.AddressSet private _TransferLimitExcluded;
	
	//Events
    event OperatorUpdated(address indexed previousOperator, address indexed newOperator);
    event OperatorChangePending(address indexed currentOperator, address indexed pendingOperator);
	event OperatorChangeAccepted(address indexed oldOperator, address indexed newOperator);
	event OperatorReset(address indexed previousOperator, address indexed ownerOperator);
    event SwapAndLiquify(uint256 tokensSwapped, uint256 ethReceived, uint256 tokensIntoLiqudity);
    event TransferTaxRateUpdated(address indexed operator, uint256 previousRate, uint256 newRate);
    event TransferSellTaxRateUpdated(address indexed operator, uint256 previousRate, uint256 newRate);
    event TransferBuyTaxRateUpdated(address indexed operator, uint256 previousRate, uint256 newRate);
    event BurnRateUpdated(address indexed operator, uint256 previousRate, uint256 newRate);
    event MaxTransferAmountRateUpdated(address indexed operator, uint256 previousRate, uint256 newRate);
	event RemovedFromTransferLimit(address indexed operator, address indexed AddressRemoved);
	event AddedToTransferLimit(address indexed operator, address indexed AddressAdded);
	event RemovedFromTax(address indexed operator, address indexed AddressRemoved);
	event AddedToTax(address indexed operator, address indexed AddressAdded);
	event MinAmountToLiquifyUpdated(address indexed operator, uint256 previousAmount, uint256 newAmount);
    event SwapAndLiquifyEnabledUpdated(address indexed operator, bool enabled);
    event PantherSwapRouterUpdated(address indexed operator, address indexed router, address indexed pair);
	event LostTokenTransferred(address indexed operator, address indexed TokenAddress, uint256 TokenBalance, uint256 AmountTokenTransfered);
    event LeftOverBNBTransferred(address indexed operator, address indexed RecipientAddress, uint256 AmountBNBTransfered);
    event TokensForETH(uint256 amountIn, address[] path);
    event LiquidityAdded(uint256 AddedTokenAmount, uint256 AddedBNBAmount);

    modifier onlyOperator() {
        require(_operator == msg.sender, "operator: caller is not the operator");
        _;
    }
	
    modifier transferLimit(address sender, address recipient, uint256 amount) {
        if (maxTransferAmount() > 0) {
            if (
				!isExcludedFromTransferLimit(sender)
                && !isExcludedFromTransferLimit(recipient)
            ) {
                require(amount <= maxTransferAmount(), "PANTHER::transferLimit: Transfer amount exceeds the maxTransferAmount");
            }
        }
        _;
    }

    modifier lockTheSwap {
        _inSwapAndLiquify = true;
        _;
        _inSwapAndLiquify = false;
    }

    modifier transferTaxFree {
        uint16 _transferTaxRate = transferTaxRate;
        transferTaxRate = 0;
        _;
        transferTaxRate = _transferTaxRate;
    }

    /**
     * @notice Constructs the PantherToken contract.
     */
	 
    constructor() public BEP20("PantherSwap Token", "PANTHER") {
		
        _operator = _msgSender();
        emit OperatorUpdated(address(0), _operator);
		
		_TransferLimitExcluded.add(msg.sender);
		_TransferLimitExcluded.add(address(0));
		_TransferLimitExcluded.add(address(this));
		_TransferLimitExcluded.add(BURN_ADDRESS);
		
		_TaxExcluded.add(msg.sender);
		_TaxExcluded.add(address(0));
		_TaxExcluded.add(address(this));
		_TaxExcluded.add(BURN_ADDRESS);

		//Mint the maximum supply of Panther tokens at deployment
		
		_mint(msg.sender, MAXIMUM_SUPPLY);
	}
	
    /// @dev overrides transfer function to meet tokenomics of PANTHER
    function _transfer(address sender, address recipient, uint256 amount) internal virtual override transferLimit(sender, recipient, amount) {
		
		//Get the PantherSwap router address
		address pantherSwapRouterAddress = address(pantherSwapRouter);

        //Manually excluded adresses are tax free
        bool isExcluded = (isExcludedFromTax(sender) || isExcludedFromTax(recipient));
		
        //Transactions from and to the contract are tax free
        bool isContractTransfer = (sender == address(this) || recipient == address(this));
        
        //Transfers between pantherSwapRouter and pantherSwapPair are tax free
        bool isLiquidityTransfer = ((sender == pantherSwapPair && recipient == pantherSwapRouterAddress) || (recipient == pantherSwapPair && sender == pantherSwapRouterAddress));

        //Buy and sell transfers attract different taxes
        bool isSell= (recipient == pantherSwapPair || recipient == pantherSwapRouterAddress);
        bool isBuy = (sender == pantherSwapPair || sender == pantherSwapRouterAddress);
		
		//Swap and liquify
        if (
            swapAndLiquifyEnabled == true
            && _inSwapAndLiquify == false
            && pantherSwapRouterAddress != address(0)
            && pantherSwapPair != address(0)			
        ) {
            swapAndLiquify();
        }

        if (recipient == BURN_ADDRESS) {
			
			_burn(sender, amount);
			amount = 0;

        } else if (transferTaxRate == 0 || isExcluded || isContractTransfer || isLiquidityTransfer){
			
			super._transfer(sender, recipient, amount);
			amount = 0;			
			
		} else {
			
			TaxedTransfers(sender, recipient, amount, isSell, isBuy);
			amount = 0;
        }
    }
	
    /// @dev liquidity generating transaction

	function TaxedTransfers(address sender, address recipient, uint256 amount, bool isSell, bool isBuy) private {
		
			uint16 rateTax = transferTaxRate;
			if(isSell) rateTax = transferSellTaxRate;
			if(isBuy) rateTax = transferBuyTaxRate;
			
			//Sell Transactions: 
			//Sell tax is completely burned
			//Default sell tax is 12% of the total amount
         
			if(isSell){
				
				uint256 taxAmount = amount.mul(rateTax).div(10000);
				uint256 sendAmount = amount.sub(taxAmount);				
				_burn(sender, taxAmount);
				super._transfer(sender, recipient, sendAmount);
				
			//Buy Transactions: 
			//Part of the buy tax is burned and the rest is added to the contract for liquidity
			//Default buy tax is 5% of the total amount
			//Default burn is 50% of the buy tax
				
			}else if(isBuy){
				
				uint256 taxAmount = amount.mul(rateTax).div(10000);
				uint256 burnAmount = taxAmount.mul(burnRate).div(100);
				uint256 liquidityAmount = taxAmount.sub(burnAmount);
				uint256 sendAmount = amount.sub(taxAmount);		
				_burn(sender, burnAmount);			
				super._transfer(sender, address(this), liquidityAmount);
				super._transfer(sender, recipient, sendAmount);
				
				
			}else{
				
			//Default Transfer Transactions: 
			//Tax is completely burned
			//Default transfer tax is 10% of the total amount

				uint256 taxAmount = amount.mul(rateTax).div(10000);
				uint256 sendAmount = amount.sub(taxAmount);				
				_burn(sender, taxAmount);
				super._transfer(sender, recipient, sendAmount);
				
			}
	}


    /// @dev Swap and liquify
    function swapAndLiquify() private lockTheSwap transferTaxFree {
        uint256 contractTokenBalance = balanceOf(address(this));
        uint256 maxTransferAmount = maxTransferAmount();
        contractTokenBalance = contractTokenBalance > maxTransferAmount ? maxTransferAmount : contractTokenBalance;

        if (contractTokenBalance >= minAmountToLiquify) {
            // only min amount to liquify
            uint256 liquifyAmount = minAmountToLiquify;

            // split the liquify amount into halves
            uint256 half = liquifyAmount.div(2);
            uint256 otherHalf = liquifyAmount.sub(half);

            // capture the contract's current ETH balance
            // this is so that we can capture exactly the amount of ETH that the
            // swap creates, and not make the liquidity event include any ETH that
            // has been manually sent to the contract
            uint256 initialBalance = address(this).balance;

            // swap tokens for ETH
            swapTokensForEth(half);

            // how much ETH did we just swap into?
            uint256 newBalance = address(this).balance.sub(initialBalance);

            // add liquidity
            addLiquidity(otherHalf, newBalance);

            emit SwapAndLiquify(half, newBalance, otherHalf);
        }
    }

    /// @dev Swap tokens for eth
    function swapTokensForEth(uint256 tokenAmount) private {
        // generate the pantherSwap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = pantherSwapRouter.WETH();

        _approve(address(this), address(pantherSwapRouter), tokenAmount);

        // make the swap
        pantherSwapRouter.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            address(this),
            block.timestamp
        );
		
        emit TokensForETH(tokenAmount, path);		
    }
	
    /// @dev Add liquidity
    function addLiquidity(uint256 tokenAmount, uint256 ethAmount) private {
        // approve token transfer to cover all possible scenarios
        _approve(address(this), address(pantherSwapRouter), tokenAmount);

        // add the liquidity
        pantherSwapRouter.addLiquidityETH{value: ethAmount}(
            address(this),
            tokenAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            operator(),
            block.timestamp
        );
		
		emit LiquidityAdded(tokenAmount, ethAmount);
    }
	
    /**
     * @dev Returns the address of the current operator
     */
    function operator() public view returns (address) {
        return _operator;
    }
	
    /**
     * @dev Returns the pending operator
     */
    function operatorPending() public view returns (address) {
        return _pendingOperator;
    }

    /**
     * @dev Returns the max transfer amount.
     */
    function maxTransferAmount() public view returns (uint256) {
        return totalSupply().mul(maxTransferAmountRate).div(10000);
    }

    /**
     * @dev Returns transfer limit status for an address
     */
    function isExcludedFromTransferLimit(address account) public view returns (bool) {		
		return _TransferLimitExcluded.contains(account);
        
    }
	
    /**
     * @dev Returns tax status for an address
     */
    function isExcludedFromTax(address account) public view returns (bool) {		
		return _TaxExcluded.contains(account);
        
    }
	
    /**
     * @dev Transfers the operator of the contract to a pending operator
	 * Pending operator must accept before the change can be made
     * UpdateOperatorPending can only be called by the current operator
	 * UpdateOperatorAccept can only be called by the pending operator
     */

    function UpdateOperatorPending(address pendingOperator) public onlyOperator {
        require(pendingOperator != address(0), "PANTHER::UpdateOperatorPending: New operator cannot be the zero address");
        require(pendingOperator != _operator, "PANTHER::UpdateOperatorPending: Current operator address cannot be used");
        emit OperatorChangePending(_operator, pendingOperator);
        _pendingOperator = pendingOperator;
    }
	
    function UpdateOperatorAccept() public {
        require(msg.sender == _pendingOperator, "PANTHER::UpdateOperatorAccept: Only the pendingOperator can call the function");
        emit OperatorChangeAccepted(_operator, _pendingOperator);
		_operator = msg.sender;
		_pendingOperator = address(0);
    }
	
    /**
     * @dev Owner of the contract can become the operator
	 * Can only be called by the owner
     */	
	 
    function UpdateOperatorReset() public onlyOwner{
        require(msg.sender == owner(), "PANTHER::UpdateOperatorReset: Only the owner can call the function");
        require(_operator != owner(), "PANTHER::UpdateOperatorReset: Operator is the owner");
        emit OperatorReset(_operator, owner());
		_operator = owner();
    }
	
    /**
     * @dev Update the transfer tax rate
     * Can only be called by the current operator
     */
    function updateTransferTaxRate(uint16 _transferTaxRate) public onlyOperator {
        require(_transferTaxRate <= MAXIMUM_TRANSFER_TAX_RATE, "PANTHER::updateTransferTaxRate: Transfer tax rate must not exceed the maximum rate.");
        emit TransferTaxRateUpdated(_operator, transferTaxRate, _transferTaxRate);
        transferTaxRate = _transferTaxRate;
    }
	
    /**
     * @dev Update the transfer sell tax rate
     * Can only be called by the current operator
     */
    function updateTransferSellTax(uint16 _transferSellTax) public onlyOperator {
        require(_transferSellTax <= MAXIMUM_TRANSFER_TAX_RATE, "PANTHER::updateTransferSellTax: Transfer tax rate must not exceed the maximum rate.");
        emit TransferSellTaxRateUpdated(_operator, transferTaxRate, _transferSellTax);
        transferSellTaxRate = _transferSellTax;
    }
	
    /**
     * @dev Update the transfer buy tax rate
     * Can only be called by the current operator
     */
    function updateTransferBuyTax(uint16 _transferBuyTax) public onlyOperator {
        require(_transferBuyTax <= MAXIMUM_TRANSFER_TAX_RATE, "PANTHER::updateTransferBuyTax: Transfer tax rate must not exceed the maximum rate.");
        emit TransferBuyTaxRateUpdated(_operator, transferBuyTaxRate, _transferBuyTax);
        transferBuyTaxRate = _transferBuyTax;
    }

    /**
     * @dev Update the burn rate
     * Can only be called by the current operator
     */
    function updateBurnRate(uint16 _burnRate) public onlyOperator {
        require(_burnRate <= 100, "PANTHER::updateBurnRate: Burn rate must not exceed the maximum rate.");
        emit BurnRateUpdated(_operator, burnRate, _burnRate);
        burnRate = _burnRate;
    }

    /**
     * @dev Update the max transfer amount rate
     * Can only be called by the current operator
     */
    function updateMaxTransferAmountRate(uint16 _maxTransferAmountRate) public onlyOperator {
        require(_maxTransferAmountRate <= 10000, "PANTHER::updateMaxTransferAmountRate: Max transfer amount rate must not exceed the maximum rate.");
        emit MaxTransferAmountRateUpdated(_operator, maxTransferAmountRate, _maxTransferAmountRate);
        maxTransferAmountRate = _maxTransferAmountRate;
    }

    /**
     * @dev Include or exclude accounts from tax and transfer limit (eg. CEX, MasterChef)
     * Can only be called by the current operator
     */

    function UpdateRemoveFromTransferLimit(address account) public onlyOperator {
		require(account != address(0),"PANTHER::UpdateRemoveFromTransferLimit:Zero address");
		emit RemovedFromTransferLimit(_operator, account);
        _TransferLimitExcluded.remove(account);
    }

    function UpdateAddedToTransferLimit(address account) public onlyOperator {
