contract GMPD_SWAP is Ownable {
    event Exchange(address indexed user, uint256 indexed bnftId, uint256 indexed gmpdId);
    event Pause(bool status);

    bool public pause = true;
    IERC721 public gmpd;
    IERC721 public bnft;
    address devs;
    mapping (uint256 => uint256) bnftToGmpd;

    constructor(
        IERC721 _gmpd,
        IERC721 _bnft,
        address _devs
    ) {
        gmpd = _gmpd;
        bnft = _bnft;
        devs = _devs;
    }

    function swap(
        uint256 _tokenId
    ) external {
        require(!pause, "Contract on pause");
        require(bnftToGmpd[_tokenId] != 0, "Wrong token");
        require(bnft.ownerOf(_tokenId) == msg.sender, "User not an owner");

        bnft.safeTransferFrom(msg.sender, devs, _tokenId);
        gmpd.safeTransferFrom(devs, msg.sender, bnftToGmpd[_tokenId]);

        emit Exchange(msg.sender, _tokenId, bnftToGmpd[_tokenId]);
    }

    function setMapping(uint256[] memory _gmpdIds, uint256[] memory _bnftIds) external onlyOwner {
        require(_gmpdIds.length == _bnftIds.length, "Wrong params");
        for (uint256 i = 0; i < _gmpdIds.length; i++) {
            bnftToGmpd[_bnftIds[i]] = _gmpdIds[i];
        }
    }

    function swithcPause() external onlyOwner {
        pause = !pause;
        emit Pause(pause);
    }
}