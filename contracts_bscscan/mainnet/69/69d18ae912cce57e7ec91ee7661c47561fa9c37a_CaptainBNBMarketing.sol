contract CaptainBNBMarketing is Ownable {
    string public _name = "CaptainBNBMarketing";
    string public _symbol = "CaptainBNBMarketing";
    
    uint256 public unlock = 1661001900; // Saturday, 20 August 2022 23:25:00 GMT+10:00
    
    constructor () {
    }

    receive() external payable { }

    function withdrawReflection(address recipient) public onlyOwner{
        uint256 amountBNB = address(this).balance;
        payable(recipient).transfer(amountBNB);
    }
    
    function withdrawToken(address targetToken, address recipient) public onlyOwner{
        require(block.timestamp >= unlock);
        IERC20 token = IERC20(targetToken);
        uint256 amount = token.balanceOf(address(this));
        if(amount > 0)
            token.transfer(recipient, amount);
    }
}