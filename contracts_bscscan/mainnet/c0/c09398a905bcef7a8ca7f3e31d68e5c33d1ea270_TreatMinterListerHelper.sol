interface TreatMinterV2 {
    function balanceOf(address owner, uint256 id) external view returns (uint256);
    function safeBatchTransferFrom(address _from, address _to, uint256[] calldata _ids, uint256[] calldata _amounts, bytes calldata _data) external;
    function create(uint256 _maxSupply, uint256 _initialSupply, string calldata _uri, bytes calldata _data, address _performerAddress) external returns (uint256);
    function isPerformer(address account) external view returns (bool);
}

contract TreatMinterListerHelper is Ownable {
    using SafeMath for uint256;
    TreatMinterV2 public treatMinterV2;
    bool public pausedTreats;

    event TotwNftsCreated(uint256[] maxSupplys, address creatorAddress);

    constructor(TreatMinterV2 _TreatMinterV2Address) public {
        treatMinterV2 = _TreatMinterV2Address;
        pausedTreats = false;
    }
    
    function createTreats(uint256[] memory _maxSupplys, address _creatorAddress) public onlyOwner {
        require(pausedTreats == false, "Contract Paused");
        for(uint256 i = 0; i < _maxSupplys.length; i++) {
            treatMinterV2.create(_maxSupplys[i],0,"","0x69",_creatorAddress);
        }
        emit TotwNftsCreated(_maxSupplys,_creatorAddress);
    }

    function setPaused(bool _paused) public onlyOwner {
        pausedTreats = _paused;
    }
}