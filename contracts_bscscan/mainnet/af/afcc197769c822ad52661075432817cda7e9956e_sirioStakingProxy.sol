interface IMasterChefBSC {
    function pendingCake(uint256 pid, address user) external view returns (uint256);

    function deposit(uint256 pid, uint256 amount) external;

    function withdraw(uint256 pid, uint256 amount) external;

    function emergencyWithdraw(uint256 pid) external;
}

interface IWbnb{
    function deposit() payable external;
}

interface IMDEXMining{
    function takerWithdraw() external;
}

contract sirioStakingProxy {
    using SafeMath for uint256;
    
    IERC20 usdt;
    IERC20 wbnb;
    IWbnb wbnbAddress;
    IERC20 LPToken;
    IERC20 mdex;
    IMdexRouter mdexRouter;
    IMasterChefBSC pool;
    IMDEXMining mdexMining;
    address payable communityWallet;
    uint256 LPAmount;
    
    constructor (IERC20 _usdt,address _mdexRouter, address payable _communityWallet,address _wbnb, address _LPToken,address _pool,address _mdex, address _mdexMining){
        usdt=IERC20(_usdt);
        mdexRouter=IMdexRouter(_mdexRouter);
        wbnb=IERC20(_wbnb);
        wbnbAddress=IWbnb(_wbnb);
        communityWallet=payable (_communityWallet);
        LPToken=IERC20(_LPToken);
        pool=IMasterChefBSC(_pool);
        LPAmount=0;
        mdex=IERC20(_mdex);
        mdexMining=IMDEXMining(_mdexMining);

    }
    
    function approveAndAddLiquidity () payable public{
        require(msg.sender==communityWallet,"permission denied");
        uint256 amountToSwap=msg.value/2;
        address[] memory path = new address[](2);
        path[0]=address(wbnb);
        path[1]=address(usdt);
        mdexRouter.swapExactETHForTokens{value : amountToSwap}(0,path, address(this), block.timestamp+1000);
        usdt.approve(address(mdexRouter),100000000000000000000000000000000000000000);
        wbnb.approve(address(mdexRouter),100000000000000000000000000000000000000000);
        wbnbAddress.deposit{value:address(this).balance}();
        mdexRouter.addLiquidity(address(wbnb),address(usdt),wbnb.balanceOf(address(this)),usdt.balanceOf(address(this)),0,0,address(this),block.timestamp+1000);
    }
    
    function stake(uint256 poolId) public{
        require(msg.sender==communityWallet,"permission denied");
        LPToken.approve(address(pool),100000000000000000000000000000000000000000);
        LPAmount=LPAmount.add(LPToken.balanceOf(address(this)));
        pool.deposit(poolId,LPToken.balanceOf(address(this)));
    }
    
    function unstake (uint256 poolId) public{
        require(msg.sender==communityWallet,"permission denied");
        pool.withdraw(poolId,LPAmount);
        LPAmount=0;
    }
    
    function getLPAmount() view public returns(uint256){
        return LPAmount;
    }
    
    function removeLiq() public{
        require(msg.sender==communityWallet,"permission denied");
        LPToken.approve(address(mdexRouter),100000000000000000000000000000000000000000);
        mdexRouter.removeLiquidityETH(address(usdt),LPToken.balanceOf(address(this))-1,0,0,address(this),block.timestamp+1000);

    }
    
    function returnTokens() public{
        require(msg.sender==communityWallet,"permission denied");
        communityWallet.transfer(address(this).balance);
        usdt.transfer(communityWallet,usdt.balanceOf(address(this)));
        mdex.transfer(communityWallet,mdex.balanceOf(address(this)));
    }
    
    function getRewards (uint256 poolId) public{
        require(msg.sender==communityWallet,"permission denied");
        pool.withdraw(poolId,0);
        mdex.transfer(communityWallet,mdex.balanceOf(address(this)));
    }
    
    function getSwapRewards() public{
        require(msg.sender==communityWallet,"permission denied");
        mdexMining.takerWithdraw();
        mdex.transfer(communityWallet,mdex.balanceOf(address(this)));
    }
    
    receive() external payable {}
    
}