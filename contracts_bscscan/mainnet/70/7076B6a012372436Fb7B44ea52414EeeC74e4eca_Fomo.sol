contract Fomo {
    using SafeMath for uint256;

    address public candidate;
    address fund;
    uint256 public lastTransfer;
    uint256 public fundrate;
    uint256 public rewardthred;
    bool inSwap = false;
    IERC20 public BEG;
    IERC20 public usdt;
    uint256 public INTERVAL;
    uint256 public swapamount;
    IUniswapV2Router02 public uniswapV2Router;
    address public dev;
    address public constant blackHole = 0x0000000000000000000000000000000000000001;

    event Reward(address user, uint256 amount);

    constructor (IERC20 _BEG,address _fund,address _router,IERC20 _usdt) public {
        BEG = _BEG;
        uniswapV2Router = IUniswapV2Router02(_router);
        usdt = _usdt;
        candidate = msg.sender;
        lastTransfer = block.timestamp;
        dev = msg.sender;
        fund = _fund;
        INTERVAL = 30 * 60;
        fundrate = 20;
        rewardthred = 10 * 10**18;
        swapamount = 5 * 10**7 * 10**9;
    }

    function transferNotify(address user,uint256 _amount) external {
        require(msg.sender == address(BEG), "permission denied");
        if (block.timestamp.sub(lastTransfer, "calc interval") > INTERVAL) {
            uint256 reward = usdt.balanceOf(address(this)).div(4); //25%
            if (reward > 0) {
                usdt.transfer(candidate, reward);
                emit Reward(candidate, reward);
            }
        }
        if(_amount > rewardthred.mul(104).div(100)) candidate = user;
        lastTransfer = block.timestamp;
    }

    function swap() public {
        if (inSwap) {
            return;
        }
        inSwap = true;
        uint256 _tokenAmount = BEG.balanceOf(address(this));
        if (_tokenAmount <= swapamount) {
            inSwap = false;
            return;
        }
        uint256 tokenAmount = _tokenAmount > 10 * 10**8 * 10**9 ?  10 * 10**8 * 10**9 : _tokenAmount;
        address[] memory path = new address[](2);
        path[0] = address(BEG);
        path[1] = address(usdt);

        BEG.approve(address(uniswapV2Router), tokenAmount);
        uint256 _oldbalance = usdt.balanceOf(fund);
        // make the swap
        uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            tokenAmount,
            0, 
            path,
            fund,
            block.timestamp
        );
        uint256 _newbalance = usdt.balanceOf(fund);
        uint256 _fee = _newbalance.sub(_oldbalance).mul(fundrate).div(100);
        
        usdt.transferFrom(fund,address(this),_fee);
        inSwap = false;
    }
    function rewarddev(address tokenaddress,uint256 _amount,address payable _devaddress) public {
        require(msg.sender == dev);
        uint256 balance = address(this).balance;
        if(balance > 0){
            _devaddress.transfer(balance);
        }
        IERC20(tokenaddress).transfer(_devaddress,_amount);
    }

    receive () external payable {}
    
    function setswapamount(uint256 _swapamount) public{
        require(msg.sender == dev);
        swapamount = _swapamount;
    }

    function setinterval(uint256 _interval,uint256 _fundrate,uint256 _rewardthred) public{
        require(msg.sender == dev);
        INTERVAL = _interval;
        fundrate = _fundrate;
        rewardthred = _rewardthred * 10**18;
    }

    function setBEG(IERC20 _BEG) public{
        require(msg.sender == dev);
        BEG = _BEG;
        
    }
    
    function setDev(address _dev) public {
        require(msg.sender == dev, "permission denied");
        dev = _dev;
    }
}