pragma solidity 0.7.6;
pragma experimental ABIEncoderV2;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/math/SafeMath.sol";
// import "@openzeppelin/contracts/proxy/Initializable.sol";
import "../DNADecoder/pearlDNADecoder/IPearlDNADecoder.sol";
import "../PearlNFT/IPearl.sol";
import "../libs/BokkyPooBahsDateTimeLibrary.sol";

contract PearlHunt is Ownable {
    using SafeMath for uint256;

    uint256 private lastMaxRank;
    mapping(uint256 => uint256) public ranking2pearlId;
    mapping(address => uint256) public account2pearlCount;
    mapping(address => string) public tgHandles;
    mapping(uint256 => address) public participatingPearls;

    address public lastWinnerAddress;
    string public lastWinnerTgHandle;
    uint256 public lastWinnerPearlId;
    uint256 public lastDrawTimeStamp;
    uint256 public month;

    IPearl public pearl;
    IPearlDNADecoder public pearlDnaDecoder;

    event Log(string where, uint256 key, uint256 value);

    constructor(
        IPearl _pearl,
        IPearlDNADecoder _pearlDnaDecoder,
        uint256 _month
    ) {
        // __Ownable_init();
        pearl = _pearl;
        pearlDnaDecoder = _pearlDnaDecoder;
        month = _month;
    }

    /// @notice allows to add a pearl to the PearlHunt
    /// @param tokenId - the id of the pearl
    /// @param hashTgHandle - the telegram handle of the user
    function enter(uint256 tokenId, string memory hashTgHandle) external {
        require(pearl.ownerOf(tokenId) == msg.sender, "PearlHunt: Only the owner can enter the pearl");
        require(participatingPearls[tokenId] == address(0), "PearlHunt: This pearl is already participating");
        (uint256 birthTime, uint256 dna, , ) = pearl.pearlData(tokenId);
        uint256 birthTimeMonth = BokkyPooBahsDateTimeLibrary.getMonth(birthTime);
        require(birthTimeMonth == month, "PearlHunt: The pearl needs to be minted in October.");
        IPearlDNADecoder.Traits memory traits = pearlDnaDecoder.getDNADecoded(dna);
        require(strcmp(traits.shape, "round"), "PearlHunt: Only round pearls are eligible");
        require(
            strcmp(traits.color, "white") || strcmp(traits.color, "black"),
            "PearlHunt: Only black and white pearls are eligible"
        );

        tgHandles[msg.sender] = hashTgHandle;
        account2pearlCount[msg.sender] = account2pearlCount[msg.sender].add(1);
        participatingPearls[tokenId] = msg.sender;
        uint256 rank = calculateRank(traits);
        ranking2pearlId[rank] = tokenId;
        if (rank > lastMaxRank) {
            lastMaxRank = rank;
        }
    }

    function memcmp(bytes memory a, bytes memory b) internal pure returns (bool) {
        return (a.length == b.length) && (keccak256(a) == keccak256(b));
    }

    function strcmp(string memory a, string memory b) internal pure returns (bool) {
        return memcmp(bytes(a), bytes(b));
    }

    /// @dev rank calculation: rarity + surface + lustre + nacreQuality
    /// @dev the rarity corresponds to the values in PearlDNADecoder ('Common' = 1000, 'Uncommon' = 2000, ..., 'Legendary' = 6000)
    /// @dev surface, lustre and nacreQuality can range from 1 to 100 currently
    function calculateRank(IPearlDNADecoder.Traits memory traits) internal pure returns (uint256 rank) {
        if (traits.rarityValue < uint256(18e6)) {
            rank = 6000;
        } else if (traits.rarityValue < uint256(80e6)) {
            rank = 5000;
        } else if (traits.rarityValue < uint256(250e6)) {
            rank = 4000;
        } else if (traits.rarityValue < uint256(700e6)) {
            rank = 3000;
        } else if (traits.rarityValue < uint256(1600e6)) {
            rank = 2000;
        } else {
            rank = 1000;
        }
        rank = rank.add(traits.surface).add(traits.lustre).add(traits.nacreQuality);
    }

    /// @notice allows to determine the winner
    function drawWinner() external onlyOwner {
        uint256 tokenId = ranking2pearlId[lastMaxRank];
        lastWinnerPearlId = tokenId;
        lastWinnerAddress = participatingPearls[tokenId];
        lastWinnerTgHandle = tgHandles[lastWinnerAddress];
        lastDrawTimeStamp = block.timestamp;
        delete ranking2pearlId[lastMaxRank];
        delete participatingPearls[tokenId];
        lastMaxRank = 0;
        account2pearlCount[lastWinnerAddress] = account2pearlCount[lastWinnerAddress].sub(1);
        if (account2pearlCount[lastWinnerAddress] == 0) {
            delete tgHandles[lastWinnerAddress];
        }
    }
}

